		line-height='0%'>Hide Stichologia<span class='key' data-key='template.titles_en_US_dedes|clps.stichologia.bottom' hidden='hidden'></span> </fo:block>
		</fo:table-cell>
		</fo:table-row>
		<fo:table-row>
		<fo:table-cell 
		border-right='thin solid silver' 
		padding-left='0px' 
		padding-right='5px' 
		vertical-align='top'>
		<fo:block linefeed-treatment='preserve' white-space-collapse='false' white-space-treatment='preserve'> </fo:block>
		</fo:table-cell>
		</fo:table-row>
