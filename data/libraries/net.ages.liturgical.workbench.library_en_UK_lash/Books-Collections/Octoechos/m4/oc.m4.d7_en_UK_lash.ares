ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m4.d3_en_UK_lash.ocVE.Aposticha3.text

ocVE.Aposticha2.mode = misc_en_UK_lash.Mode4
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.text = oc.m4.d4_en_UK_lash.ocVE.Aposticha3.text

ocVE.Aposticha3.mode = misc_en_UK_lash.Mode4
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.text = oc.m4.d5_en_UK_lash.ocVE.Aposticha3.text

ocVE.Aposticha4.mode = misc_en_UK_lash.Mode4
ocVE.Aposticha4.melody = ""
ocVE.Aposticha4.text = "With the spirits of the righteous made perfect in death give rest, O Saviour, to the souls of your servants; keeping them for the life of blessedness with you, O Lover of mankind."

ocVE.AposTheotokion.mode = misc_en_UK_lash.Mode4
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.text = "As we have you as hope and protection, O Mother of God, we are not afraid of the assaults of enemies, for you save our souls."

ocVE.ApolTheotokionVM.mode = misc_en_UK_lash.Mode4
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.text = oc.m4.d1_en_UK_lash.ocVE.ApolTheotokionVM.text

