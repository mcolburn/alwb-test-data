ocMA.OdeW4C11.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C11.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C11.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.MiEnPotamois.name
ocMA.OdeW4C11.text = "Loose the bonds of my passions, Lord, and binding me with repentance, make me a partaker of your good things."

ocMA.OdeW4C12.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C12.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C12.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.MiEnPotamois.name
ocMA.OdeW4C12.text = "Light for me bright ways of repentance, O Christ, my Sun, and dispel the deep night of my evils."

ocMA.OdeW4C13.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C13.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C13.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.MiEnPotamois.name
ocMA.OdeW4C13.text = "You appeared as burning coals, wise Athletes, reducing to ashes the fuel of godlessness, and leading those in darkness to the light."

ocMA.OdeW4C14.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C14.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C14.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.MiEnPotamois.name
ocMA.OdeW4C14.text = "Imitating Christ’s death, O Martyrs, you were acquainted with pains of many kinds; therefore you inherited life divine."

ocMA.OdeW4C1T.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C1T.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C1T.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.MiEnPotamois.name
ocMA.OdeW4C1T.text = "Give me, Pure Virgin, true repentance; calm the raging of the passions, O protection of those who sin."

ocMA.OdeW4C21.mode = misc_en_UK_lash.Mode3
ocMA.OdeW4C21.ode = misc_en_UK_lash.Ode4
ocMA.OdeW4C21.melody = misc_en_UK_lash.NotMetered //he.h.m3_en_UK_lash.ToXenonKaiAporriton.name
