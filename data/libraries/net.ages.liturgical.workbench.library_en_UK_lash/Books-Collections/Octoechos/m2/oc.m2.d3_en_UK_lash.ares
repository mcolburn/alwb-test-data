octrMA.Triadic1.mode = misc_en_UK_lash.Mode2
octrMA.Triadic1.text = oc.m2.d2_en_UK_lash.octrMA.Triadic1.text

octrMA.Triadic2.mode = misc_en_UK_lash.Mode2
