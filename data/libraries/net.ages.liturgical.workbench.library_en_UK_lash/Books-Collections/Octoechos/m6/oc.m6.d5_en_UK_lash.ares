ocMA.Kathisma13.mode = misc_en_UK_lash.Mode6
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.text = "Open the gate of compassion to us, blessed Mother of God; hoping in you, may we not fail. Through you may we be delivered from adversities, for you are the salvation of the Christian race."

