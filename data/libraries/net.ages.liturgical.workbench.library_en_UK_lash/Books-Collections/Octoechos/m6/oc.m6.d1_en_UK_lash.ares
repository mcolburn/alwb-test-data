ocLI.BeatitudesBoth.mode = misc_en_UK_lash.Mode6
ocLI.BeatitudesBoth.melody = misc_en_UK_lash.NotMetered //he.h.m6_en_UK_lash.IToChaireDiAngelou.name
ocLI.BeatitudesBoth.text = ""

