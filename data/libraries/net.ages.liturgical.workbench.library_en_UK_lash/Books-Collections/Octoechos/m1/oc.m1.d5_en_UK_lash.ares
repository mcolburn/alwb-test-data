ocMA.OdeW5C2T.ode = misc_en_UK_lash.Ode5
ocMA.OdeW5C2T.melody = misc_en_UK_lash.NotMetered //he.h.m1_en_UK_lash.ToFaeinonIminExanateilon.name
ocMA.OdeW5C2T.text = ""

ocMA.OdeW6C1H.mode = misc_en_UK_lash.Mode1
ocMA.OdeW6C1H.ode = misc_en_UK_lash.Ode6
ocMA.OdeW6C1H.melody = misc_en_UK_lash.HeirmosNotMetered
ocMA.OdeW6C1H.text = he.h.m1_en_UK_lash.EkyklosenImas.text

ocMA.OdeW6C11.mode = misc_en_UK_lash.Mode1
ocMA.OdeW6C11.ode = misc_en_UK_lash.Ode6
ocMA.OdeW6C11.melody = misc_en_UK_lash.NotMetered //he.h.m1_en_UK_lash.EkyklosenImas.name
ocMA.OdeW6C11.text = "You caught the nations in spiritual nets to know the One who was made known for the refashioning of mortals, O Apostles blessed by God."

ocMA.OdeW6C12.mode = misc_en_UK_lash.Mode1
ocMA.OdeW6C12.ode = misc_en_UK_lash.Ode6
ocMA.OdeW6C12.melody = misc_en_UK_lash.NotMetered //he.h.m1_en_UK_lash.EkyklosenImas.name
ocMA.OdeW6C12.text = "My wretched soul, my miserable soul, my unrepentant soul, repent and cry out to Christ: I have sinned, be merciful, O Lover of mankind, at the prayers of the Apostles, for you are supremely good."

ocMA.OdeW6C13.mode = misc_en_UK_lash.Mode1
ocMA.OdeW6C13.ode = misc_en_UK_lash.Ode6
ocMA.OdeW6C13.melody = misc_en_UK_lash.NotMetered //he.h.m1_en_UK_lash.EkyklosenImas.name
ocMA.OdeW6C13.text = "All-powerful Christ, who Israel of old drew waters from a rock for Israel, dissolving my hardness, make me carry channels of tears at the prayers of the Apostles, for are most merciful."

ocMA.OdeW6C1T.mode = misc_en_UK_lash.Mode1
ocMA.OdeW6C1T.ode = misc_en_UK_lash.Ode6
ocMA.OdeW6C1T.melody = misc_en_UK_lash.NotMetered //he.h.m1_en_UK_lash.EkyklosenImas.name
ocMA.OdeW6C1T.text = "O Virgin, entreat as Fashioner and God the One who through loving-kindness was well pleased to be born from you, to save from temptations and disasters those who ever hope in you."

