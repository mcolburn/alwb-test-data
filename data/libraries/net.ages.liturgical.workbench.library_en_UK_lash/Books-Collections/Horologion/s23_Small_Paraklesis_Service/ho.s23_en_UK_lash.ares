hoCA.PostGospelBoth.mode = misc_en_UK_lash.Mode2
hoCA.PostGospelBoth.melody  = ""
hoCA.PostGospelBoth.text = ho.s03_en_UK_lash.hoMA.PostGospelTheotokos.text

hoCA.PostGospelVerse.text = ""

hoCA.PostGospelHymn1.mode = misc_en_UK_lash.Mode6
hoCA.PostGospelHymn1.melody = he.a.m6_en_UK_lash.OlinApothemenoi.name
hoCA.PostGospelHymn1.text = "Sovereign and all-holy one, * do not trust me to protection * which is merely human, * but accept the pleading of your supplicant, * anguish has hold of me, * nor can I endure * the hostile demons’ arrows; * no shelter can I find, * no, nor place of refuge, wretch that I am, * from every side I am assailed, * and, save you, I find none that offers solace, * Queen of all creation, * Protection of the faithful and our hope, * do not despise my entreaty, * but take action for my good."

hoCA.PostGospelHymn2.mode = misc_en_UK_lash.Mode6
hoCA.PostGospelHymn2.melody  = ""
hoCA.PostGospelHymn2.text = oc.m6.d3_en_UK_lash.ocMA.AposTheotokion.text

hoCA.PostGospelHymn3.mode = misc_en_UK_lash.Mode6
hoCA.PostGospelHymn3.melody  = ""
hoCA.PostGospelHymn3.text = oc.m6.d3_en_UK_lash.ocVE.AposTheotokion.text

