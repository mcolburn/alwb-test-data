meMA.Ode3C11.ode = misc_en_UK_lash.Ode3
meMA.Ode3C11.mode = misc_en_UK_lash.Mode8
meMA.Ode3C11.melody = misc_en_UK_lash.NotMetered //he.h.m8_en_UK_lash.OStereosasKatArchas.name
meMA.Ode3C11.text = "From a child you were seized with love for God, collecting round you children of your own age and forming them into a band you trained them spiritually, most wisely weaning them from childish ways."

meMA.Ode3C12.ode = misc_en_UK_lash.Ode3
meMA.Ode3C12.mode = misc_en_UK_lash.Mode8
meMA.Ode3C12.melody = misc_en_UK_lash.NotMetered //he.h.m8_en_UK_lash.OStereosasKatArchas.name
meMA.Ode3C12.text = "Smitten with love of wisdom, you reached the fair city of Constantine, O God-inspired; and like a wise, melodious bee you gathered seasonable words and abandoned what was superfluous."

meMA.Ode3C13.ode = misc_en_UK_lash.Ode3
meMA.Ode3C13.mode = misc_en_UK_lash.Mode8
meMA.Ode3C13.melody = misc_en_UK_lash.NotMetered //he.h.m8_en_UK_lash.OStereosasKatArchas.name
meMA.Ode3C13.text = "Piloting your spiritual ship, blest Father, with the sail of the Cross you accomplished shrewdly and nimbly the difficult voyage of life and you were brought to the divine havens of calm."

meMA.Ode3C1T.ode = misc_en_UK_lash.Ode3
meMA.Ode3C1T.mode = misc_en_UK_lash.Mode8
meMA.Ode3C1T.melody = misc_en_UK_lash.NotMetered //he.h.m8_en_UK_lash.OStereosasKatArchas.name
meMA.Ode3C1T.text = "He who was ineffably begotten of the Father before the ages, in the last times came forth from your womb and made our nature divine, O Virgin Mother, conspicuously bringing to himself the companies of the ascetics."

meMA.Ode3C21.ode = misc_en_UK_lash.Ode3
meMA.Ode3C21.mode = misc_en_UK_lash.Mode2
meMA.Ode3C21.melody = misc_en_UK_lash.NotMetered //he.h.m2_en_UK_lash.SyToLogoSouKyrie.name
meMA.Ode3C21.text = ""

meMA.Ode3C22.ode = misc_en_UK_lash.Ode3
meMA.Ode3C22.mode = misc_en_UK_lash.Mode2
meMA.Ode3C22.melody = misc_en_UK_lash.NotMetered //he.h.m2_en_UK_lash.SyToLogoSouKyrie.name
