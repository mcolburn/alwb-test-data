peMA.Synaxarion.commemoration.text = "On this day, the seventh Sunday of Pascha, we celebrate the First Ecumenical Synod in Nicaea of the three hundred eighteen God-bearing Fathers."
peMA.Synaxarion.couplet1.title = titles_en_US_dedes.Stichoi
peMA.Synaxarion.couplet1.verse1 = "Luminous stars of the spiritual sky,"
peMA.Synaxarion.couplet1.verse2 = "illumine my mind with the light of your rays."
peMA.Synaxarion.couplet2.title = "Against Arius"
peMA.Synaxarion.couplet2.verse1 = "Arius be a stranger to God's glory,"
peMA.Synaxarion.couplet2.verse2 = "for calling the Son a stranger to the Father's essence."
peMA.Synaxarion.lastline.text = "By the intercession of the three hundred and eighteen God-bearing Fathers, O Christ our God, have mercy on us. Amen."

