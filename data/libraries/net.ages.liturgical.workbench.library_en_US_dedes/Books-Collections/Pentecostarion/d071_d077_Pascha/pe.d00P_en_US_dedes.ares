pePA.Ode8H.ode = misc_en_US_dedes.Ode8
pePA.Ode8H.mode = misc_en_US_dedes.Mode1
pePA.Ode8H.melody = misc_en_US_dedes.Heirmos
pePA.Ode8H.text = he.h.m1_en_US_dedes.AftiIKliti.text

pePA.Ode81.ode = misc_en_US_dedes.Ode8
pePA.Ode81.mode = misc_en_US_dedes.Mode1
pePA.Ode81.melody = he.h.m1_en_US_dedes.AftiIKliti.name
pePA.Ode81.text = "Come on this illustrious day of Resurrection and day of Christ's Kingdom, let us receive the communion of the joy divine from the new fruit of the vine, for eternal life, and highly extol Him as God unto the ages."

pePA.Ode82.ode = misc_en_US_dedes.Ode8
pePA.Ode82.mode = misc_en_US_dedes.Mode1
pePA.Ode82.melody = he.h.m1_en_US_dedes.AftiIKliti.name
pePA.Ode82.text = "Lift up your eyes round about You and see, O Zion. Behold, like beacons shining with light divine have your children come to you from the West, from the North and from seaward and from the East. In you they extol Christ and bless Him to the ages."

pePA.Ode83.ode = misc_en_US_dedes.Ode8
pePA.Ode83.mode = misc_en_US_dedes.Mode1
pePA.Ode83.melody = he.h.m1_en_US_dedes.AftiIKliti.name
pePA.Ode83.text = "Father, ruler over all, and Logos and Spirit, O Nature divine, in three Hypostases united, transcending essence and divinity, in You have we been baptized; we praise and bless and worship You, unto all the ages."

pePA.Ode8T1.ode = misc_en_US_dedes.Ode8
pePA.Ode8T1.mode = misc_en_US_dedes.Mode1
pePA.Ode8T1.melody = he.h.m1_en_US_dedes.AftiIKliti.name
