peMA.Ode9C2H1.text = he.h.m4_en_US_dedes.ChairoisAnassa1.text
peMA.Ode9C2H2.text = he.h.m4_en_US_dedes.ChairoisAnassa2.text
peMA.Ode9C2H3.text = he.h.m4_en_US_dedes.ChairoisAnassa3.text
peMA.Ode9C2H4.text = he.h.m4_en_US_dedes.ChairoisAnassa4.text
peMA.Ode9C2H5.text = he.h.m4_en_US_dedes.ChairoisAnassa5.text

peMA.Ode9C21.ode = misc_en_US_dedes.Ode9
peMA.Ode9C21.mode = misc_en_US_dedes.Mode4
peMA.Ode9C21.melody = he.h.m4_en_US_dedes.ChairoisAnassa.name
peMA.Ode9C21.text = "It is right to tell of the Maiden who produced life; * she alone concealed within her womb the Logos, * who remedied mortal nature that was ailing. * And being now on the right hand of the Father * enthroned, He has sent the grace of the Spirit." 
peMA.Ode9C211.text = "It is right to tell of the Maiden who produced life;"
peMA.Ode9C212.text = "She alone concealed within her womb the Logos,"
peMA.Ode9C213.text = "Who remedied mortal nature that was ailing."
peMA.Ode9C214.text = "And being now on the right hand of the Father"
peMA.Ode9C215.text = "Enthroned, He has sent the grace of the Spirit."

peMA.Ode9C22.ode = misc_en_US_dedes.Ode9
peMA.Ode9C22.mode = misc_en_US_dedes.Mode4
peMA.Ode9C22.melody = he.h.m4_en_US_dedes.ChairoisAnassa.name
peMA.Ode9C22.text = "We on whom has breathed grace that flows from the Godhead, * dazzling and resplendent now for undergoing * a strange and most majestical transformation, * know that the wise, indivisible, tri-shining * Essence is equipotent; Which we glorify." 
peMA.Ode9C221.text = "We on whom has breathed grace that flows from the Godhead,"
peMA.Ode9C222.text = "Dazzling and resplendent now for undergoing"
peMA.Ode9C223.text = "A strange and most majestical transformation,"
peMA.Ode9C224.text = "Know that the wise, indivisible, tri-shining"
peMA.Ode9C225.text = "Essence is equipotent; Which we glorify."

peMA.Exaposteilarion1.mode = misc_en_US_dedes.Mode3
peMA.Exaposteilarion1.melody = he.a.m3_en_US_dedes.OOuranonToisAstrois.name
peMA.Exaposteilarion1.text = "O all-holy Spirit * who from the Father issues forth, * and through the Son was indwelling * in the unlettered Disciples, * sanctify all who acknowledge * You to be God, and save us."

peMA.Exaposteilarion2.mode = misc_en_US_dedes.Mode3
peMA.Exaposteilarion2.melody = he.a.m3_en_US_dedes.OOuranonToisAstrois.name
peMA.Exaposteilarion2.text = "Light is the Father, light the Logos; * light is the Holy Spirit, * which was sent to the Apostles * in the form of fiery tongues, * and through which the entire world is guided by the light * to worship the Holy Trinity."

peMA.Lauds1.mode = misc_en_US_dedes.Mode4
peMA.Lauds1.melody = ""
peMA.Lauds1.text = "Today, all the nations in the city of David saw paradoxical things, when the Holy Spirit descended in fiery tongues, according to the utterance of Luke the theologian. For he said, when the Disciples of Christ were gathered together, a sound came like the rush of a mighty wind, and it filled all the house where they were sitting. And they all began to speak with strange words, strange dogmas, strange teachings of the Holy Trinity."

peMA.Lauds2.mode = misc_en_US_dedes.Mode4
peMA.Lauds2.melody = ""
peMA.Lauds2.text = "The Holy Spirit forever was, and is and will be, never beginning, never ending, but forever ranked and numbered with the Father and the Son. It is life and life-giving, light and giver of light; goodness itself and the source of goodness; through whom the Father is known and the Son is glorified, and by all is known the one power, one order, one worship of the Holy Trinity."

peMA.Lauds3.mode = misc_en_US_dedes.Mode4
peMA.Lauds3.melody = ""
peMA.Lauds3.text = "The Holy Spirit is light and life, and a living noetic fount. Spirit of wisdom, Spirit of understanding; good, upright, noetic, ruling, purging offences; God and deifying; He is fire issuing from fire, uttering, inciting, distributing the gifts of grace. Through Him the Prophets and the Apostles of God and the Martyrs all were crowned. Strange to hear, strange to see, fire distributed for the apportioning of gifts."

peMA.LaudsGlory.mode = misc_en_US_dedes.Mode6
peMA.LaudsGlory.melody = misc_en_US_dedes.Idiomelon
peMA.LaudsGlory.text = prayers_en_US_dedes.VasilefOuranie.text

peLI.Antiphon2.text = "Save us, O good Comforter."

