ocVE.Aposticha1.mode = misc_en_US_dedes.Mode4
ocVE.Aposticha1.melody = misc_en_US_dedes.Automelon
ocVE.Aposticha1.text = he.a.m4_en_US_dedes.IthelonDakrysinExaleipsai.text

ocVE.Aposticha2.mode = misc_en_US_dedes.Mode4
ocVE.Aposticha2.melody = he.a.m4_en_US_dedes.IthelonDakrysinExaleipsai.name
ocVE.Aposticha2.text = "Who has been tossed by a storm and hastened * to this calm and quiet harbor whose life has not been saved? * Or who was afflicted with pain and illness * and ran unto this clinic whose health was not restored? * O Creator of all things * and Physician of the ailing, * save me before I utterly perish, O Lord my God."

ocVE.Aposticha3.mode = misc_en_US_dedes.Mode4
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.text = "O Christ our God, who are glorified in the memorials of Your Saints, being importuned by them, send down Your great mercy unto us."

ocVE.AposTheotokion.mode = misc_en_US_dedes.Mode4
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.text = "Rejoice, Cloud of light; rejoice, O radiant Lampstand; rejoice, Jar of manna; O Rod of Aaron, rejoice; rejoice, O holy Mountain; rejoice, O Paradise; O holy Table, rejoice; rejoice, O mystical Gate; rejoice. O hope of all."

