ocMA.Kathisma13.mode = misc_en_US_dedes.Mode6
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.text = "Open the gate of compassion for us, O blessed Theotokos. For hoping in you, let us not fail in our aim. Through you may we be delivered from adversities. For you are the salvation of the Christian race."

