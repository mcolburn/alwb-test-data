trMA.Kontakion1.mode = misc_en_US_dedes.Mode8
trMA.Kontakion1.melody = he.a.m8_en_US_dedes.OsAparchas.name
trMA.Kontakion1.text = "O Lord, you made the host of God-bearing Saints shine as preachers of godliness and silencers of godlessness, and they lit up the entire earth. By their entreaties, preserve us in peace, as we glorify and magnify You and chant and sing to You: Alleluia."

trMA.Oikos1.text = "I observed the delights of earthly life, and considered what goes on in peoples' minds; and I weighed their pain, and I deemed their life unhappy. Only you did I call blessed, you Saints who chose the good portion, to suffer with Christ and remain with Him, and ever sing with the Prophet David: Alleluia."

trMA.Synaxarion.commemoration.text = "On the Saturday of Cheese-fare week, we commemorate all the holy men and women who excelled in asceticism."
trMA.Synaxarion.couplet1.title = titles_en_US_dedes.Stichoi
trMA.Synaxarion.couplet1.verse1 = "I offer these words as an abiding libation"
trMA.Synaxarion.couplet1.verse2 = "to the souls of the Righteous whose memory abides forever."
trMA.Synaxarion.lastline.text = "By the intercessions of all Your monastic Saints, O Christ our God, have mercy on us. Amen."

