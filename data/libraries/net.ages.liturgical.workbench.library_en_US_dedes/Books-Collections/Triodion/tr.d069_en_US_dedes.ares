trH1.Idiomelon1.melody = ""
trH1.Idiomelon1.text = trHW.Antiphon122.text

trH1.Idiomelon2.mode = misc_en_US_dedes.Mode8
trH1.Idiomelon2.melody = ""
trH1.Idiomelon2.text = "You were led as a sheep to slaughter, O Christ the King, and as an innocent lamb, You were nailed to a cross by lawless men, for our sins, O Lord who love humanity."

trH1.Idiomelon3.mode = misc_en_US_dedes.Mode8
trH1.Idiomelon3.melody = ""
trH1.Idiomelon3.text = trHW.Antiphon071.text

trH1.Kontakion.mode = misc_en_US_dedes.Mode8
