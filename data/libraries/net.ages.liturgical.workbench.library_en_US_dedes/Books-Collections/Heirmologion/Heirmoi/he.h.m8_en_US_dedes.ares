AsomenToKyrio.text = ""

AvyssosAmartion.name = "The abyss of many sins."
AvyssosAmartion.mode = misc_en_US_dedes.Mode8
AvyssosAmartion.ode = misc_en_US_dedes.Ode6
AvyssosAmartion.text = "The abyss of many sins * and the storm of my transgressions assail me * and forcibly compel me to the deep sea of hopelessness. * But I pray You to extend * Your mighty arm to me, * as to Peter, * and from corruption call me back, O Lord."

ChitonaMoiParaschou.name = ""
ChitonaMoiParaschou.mode = misc_en_US_dedes.Mode8
ChitonaMoiParaschou.ode = misc_en_US_dedes.Ode6
ChitonaMoiParaschou.text = ""

EfrixePasaAkoi.name = "All hearers shuddered."
EfrixePasaAkoi.mode = misc_en_US_dedes.Mode8
EfrixePasaAkoi.ode = misc_en_US_dedes.Ode9
EfrixePasaAkoi.text = "All hearers shuddered when they heard * the unheard of condescension of God to us: * that voluntarily * the Most High came down, and even bodily, * and from a Virgin's womb was born, * becoming a man from her. * Hence, the immaculate * Theotokos we believers now magnify."

EisakikoaKyrie.name = "I have heard the report."
EisakikoaKyrie.mode = misc_en_US_dedes.Mode8
EisakikoaKyrie.ode = misc_en_US_dedes.Ode4
EisakikoaKyrie.text = "I have heard the report, O Lord, * of Your dispensation's amazing mystery. * I considered well Your mighty works; * wherefore Your divinity I glorified."

EisakikoaKyrieTinAkoinSou.name = ""
EisakikoaKyrieTinAkoinSou.mode = misc_en_US_dedes.Mode8
EisakikoaKyrieTinAkoinSou.ode = misc_en_US_dedes.Ode4
EisakikoaKyrieTinAkoinSou.text = ""

