IdeteIdeteOPalaiTonIsrail.ode = misc_en_US_dedes.Ode2
IdeteIdeteOPalaiTonIsrail.text = ""

IlasthitiMoiSotir.name = "Be gracious to me."
IlasthitiMoiSotir.mode = misc_en_US_dedes.Mode8
IlasthitiMoiSotir.ode = misc_en_US_dedes.Ode6
IlasthitiMoiSotir.text = "Be gracious to me, O Lord, * for many are my iniquities; * and bring me up from the deep * of evils, I supplicate. * For I have cried out to You, God of my salvation. * O my Savior, hearken unto me."

InaTiMeAposo.name = "Why, O Lord, have You cast me."
InaTiMeAposo.mode = misc_en_US_dedes.Mode8
InaTiMeAposo.ode = misc_en_US_dedes.Ode5
InaTiMeAposo.text = "Why, O Lord, have You cast me * away from Your countenance, O never-setting Light, * and this foreign darkness * now has covered me, mis'rable that I am? * Do, I pray, convert me, and to the light of Your commandments * guide my ways, I entreat You most fervently."

KyrieOTheosImon.name = "O Lord our God, give peace."
KyrieOTheosImon.mode = misc_en_US_dedes.Mode8
KyrieOTheosImon.ode = misc_en_US_dedes.Ode5
KyrieOTheosImon.text = "O Lord our God, give peace to us, for You have rendered all. * O Lord our God, we pray, take possession of us. * Lord, O Lord, we do not know any other God except for You, and we name Your name."

KyriosTheotokon.name = "You are the Theotokos."
KyriosTheotokon.mode = misc_en_US_dedes.Mode8
KyriosTheotokon.ode = misc_en_US_dedes.Ode9
KyriosTheotokon.text = "You are the Theotokos * lit'rally, O Virgin; * and we confess it, for we have been saved through you * whom we, O pure one, with choirs of angels magnify."

MegalynomenSeTinMiteraTouFotos.name = ""
MegalynomenSeTinMiteraTouFotos.mode = misc_en_US_dedes.Mode8
MegalynomenSeTinMiteraTouFotos.ode = misc_en_US_dedes.Ode9
