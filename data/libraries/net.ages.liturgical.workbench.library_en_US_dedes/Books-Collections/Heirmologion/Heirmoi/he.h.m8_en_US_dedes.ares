OiTisChaldaiasKaminou.mode = misc_en_US_dedes.Mode8
OiTisChaldaiasKaminou.ode = misc_en_US_dedes.Ode7
OiTisChaldaiasKaminou.text = ""

OrousParilthes.name = ""
OrousParilthes.mode = misc_en_US_dedes.Mode8
OrousParilthes.ode = misc_en_US_dedes.Ode9
OrousParilthes.text = ""

OrthrizeiToPnevmaMou.name = ""
OrthrizeiToPnevmaMou.mode = misc_en_US_dedes.Mode8
OrthrizeiToPnevmaMou.ode = misc_en_US_dedes.Ode5
OrthrizeiToPnevmaMou.text = ""

OrthrizontesVoomenSoi.name = ""
OrthrizontesVoomenSoi.mode = misc_en_US_dedes.Mode8
OrthrizontesVoomenSoi.ode = misc_en_US_dedes.Ode5
OrthrizontesVoomenSoi.text = ""

OsTonProfitinErryso.name = ""
OsTonProfitinErryso.mode = misc_en_US_dedes.Mode8
OsTonProfitinErryso.ode = misc_en_US_dedes.Ode6
OsTonProfitinErryso.text = ""

OsYdataThalassis.name = "The waves of life."
OsYdataThalassis.mode = misc_en_US_dedes.Mode8
OsYdataThalassis.ode = misc_en_US_dedes.Ode6
OsYdataThalassis.text = "The waves of life are tossing me like the waters of the sea, O Friend of humanity. * I cry to You, O Logos, as once did Jonah, * to raise my life from corruption, O Lord and only compassionate."

OukEstinAgios.name = ""
OukEstinAgios.mode = misc_en_US_dedes.Mode8
OukEstinAgios.ode = misc_en_US_dedes.Ode3
OukEstinAgios.text = ""

OuraniasApsidos.name = "O divine Master Builder."
OuraniasApsidos.mode = misc_en_US_dedes.Mode8
OuraniasApsidos.ode = misc_en_US_dedes.Ode3
OuraniasApsidos.text = "O divine Master Builder of the celestial vault, * as the only Lover of mankind and Founder of the Church, * establish me, O Lord, in love for You, the believers' * firm support and ultimate object of all desire."

PaidesEvraion.name = "Servants, descendants."
PaidesEvraion.mode = misc_en_US_dedes.Mode8
PaidesEvraion.ode = misc_en_US_dedes.Ode7
PaidesEvraion.text = "Servants, descendants of the Hebrews, * bravely trampled down the flame within the furnace; * and they changed into dew * the fire, and they chanted: * Blessed are You, O Lord, the God * of our fathers to the ages."

PaidesTheoseveis.name = ""
PaidesTheoseveis.mode = misc_en_US_dedes.Mode8
PaidesTheoseveis.ode = misc_en_US_dedes.Ode7
PaidesTheoseveis.text = ""

ProsTonDynamenonSozein.name = ""
ProsTonDynamenonSozein.mode = misc_en_US_dedes.Mode8
ProsTonDynamenonSozein.ode = misc_en_US_dedes.Ode6
ProsTonDynamenonSozein.text = ""

ProsecheOurane.name = ""
ProsecheOurane.mode = misc_en_US_dedes.Mode8
ProsecheOurane.ode = misc_en_US_dedes.Ode2
ProsecheOurane.text = ""

RavdosEisTypon.name = "The rod is taken."
RavdosEisTypon.mode = misc_en_US_dedes.Mode8
RavdosEisTypon.ode = misc_en_US_dedes.Ode3
RavdosEisTypon.text = "The rod is taken to be an archetype of the mystery; * for it blossomed and thus chose the priest from others. * And now the tree of the Cross, unto the formerly barren Church, * has blossomed forth, * as power and a firm support."

SeTinApeirogamon.name = ""
SeTinApeirogamon.mode = misc_en_US_dedes.Mode8
SeTinApeirogamon.ode = misc_en_US_dedes.Ode9
SeTinApeirogamon.text = ""

SeTinOuranou.name = ""
SeTinOuranou.mode = misc_en_US_dedes.Mode8
SeTinOuranou.ode = misc_en_US_dedes.Ode9
SeTinOuranou.text = ""

StavronCharaxasMosis.name = "When Moses inscribed."
StavronCharaxasMosis.mode = misc_en_US_dedes.Mode8
StavronCharaxasMosis.ode = misc_en_US_dedes.Ode1
StavronCharaxasMosis.text = "When Moses inscribed a cross * with his rod extended, * he split the Red Sea for the host * of Israel that went on foot. * Then crosswise he once again * struck it and united it on Pharaoh's chariots, * portraying on its surface the invincible weapon. * Therefore to Christ our God * let us all sing, for He has been glorified."

StereomaMouSotir.name = ""
StereomaMouSotir.mode = misc_en_US_dedes.Mode8
StereomaMouSotir.ode = misc_en_US_dedes.Ode3
StereomaMouSotir.text = ""

StereosonImonTonNoun.name = ""
StereosonImonTonNoun.mode = misc_en_US_dedes.Mode8
StereosonImonTonNoun.ode = misc_en_US_dedes.Ode3
StereosonImonTonNoun.text = ""

SyEiToStereoma.name = "Lord, You are the firm."
SyEiToStereoma.mode = misc_en_US_dedes.Mode8
SyEiToStereoma.ode = misc_en_US_dedes.Ode3
SyEiToStereoma.text = "Lord, You are the firm support * of those who have recourse unto You. You are the light * of those who are benighted. * And my spirit sings praise to You."

SyMouIschys.name = "You are my strength."
SyMouIschys.mode = misc_en_US_dedes.Mode8
SyMouIschys.ode = misc_en_US_dedes.Ode4
SyMouIschys.text = "You are my strength. * You are my power and might, O Lord. * You, my God, are also my exultant joy, * Who deigned to visit our poverty, * without ever leaving the Father's bosom at any time. * To You, then, do I cry out with Habakkuk the Prophet: * To Your power be glory, O Lover of man."

SynechomenonDexaiMe.name = ""
SynechomenonDexaiMe.mode = misc_en_US_dedes.Mode8
SynechomenonDexaiMe.ode = misc_en_US_dedes.Ode6
SynechomenonDexaiMe.text = ""

ThalassanEpixas.name = "Lord, You congealed the sea."
ThalassanEpixas.mode = misc_en_US_dedes.Mode8
ThalassanEpixas.ode = misc_en_US_dedes.Ode1
ThalassanEpixas.text = "Lord, You congealed the sea, and Pharaoh the arrogant with all his chariots You drowned. * And You saved Your people, who did not get wet at all. * And You led them to the mountain of sanctification as they shouted, * \"We will sing to You, O Lord our God, * a victory canticle, to You the Mighty One in war.\""

TheouSynkatavasin.name = "The fire in Babylon."
TheouSynkatavasin.mode = misc_en_US_dedes.Mode8
TheouSynkatavasin.ode = misc_en_US_dedes.Ode7
TheouSynkatavasin.text = "The fire in Babylon * withdrew in fear from the condescension of God. * For this reason the Servants * within the furnace as in a moist meadow danced * with feet exultant, and sang that immortal hymn: * O God of our fathers, blessed are You!"

TinAgninEndoxos.name = "The pure Theotokos."
TinAgninEndoxos.mode = misc_en_US_dedes.Mode8
TinAgninEndoxos.ode = misc_en_US_dedes.Ode9
TinAgninEndoxos.text = "The pure Theotokos who in her womb * conceived the divine fire * without being burned, let us honor gloriously, O peoples, * magnifying her with never-silent hymns."

TinDeisinEkcheo.name = "My prayer."
TinDeisinEkcheo.mode = misc_en_US_dedes.Mode8
TinDeisinEkcheo.ode = misc_en_US_dedes.Ode6
TinDeisinEkcheo.text = "My prayer * shall I pour out before the Lord, * and to Him shall I proclaim my afflictions, * in that my soul has been flooded with evils, * and very near unto Hades my life has drawn. * Therefore, like Jonah I entreat: * Raise me up from corruption, O Lord my God."

