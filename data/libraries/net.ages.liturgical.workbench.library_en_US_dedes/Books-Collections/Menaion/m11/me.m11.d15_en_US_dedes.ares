meCommemoration.text = "Memory of the Holy Martyrs and Confessors Guria, Samonas and Habib." // full title of day
meShortCommemoration1.text = ""
meInsert1 = "the holy and glorious Martyrs and Confessors Guria, Samonas and Habib," // for use in petitions and dismissal
meInsert2 = "whose memory we are observing," // for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "The Holy Martyrs and Confessors Gouria, Samonas, and Abibos."
meLE.commemoration2 = ""
