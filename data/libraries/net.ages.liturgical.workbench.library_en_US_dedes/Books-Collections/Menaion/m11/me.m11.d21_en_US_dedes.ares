meMA.PostGospelGlory.mode = misc_en_US_dedes.Mode2
meMA.PostGospelGlory.melody = ""
meMA.PostGospelGlory.text = "Today the animate temple of the great King comes into the Temple, to prepare herself to become His divine dwelling. O peoples, be exultant."

