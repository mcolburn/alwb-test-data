meDate.text = "November 8"
meCommemoration.text = "The Synaxis in Honor of the Chief Commanders Michael and Gabriel, and All the Bodiless Powers." // full title of day
meShortCommemoration1.text = ""
meInsert1 = "the holy and glorious Supreme Commanders Michael and Gabriel, and the other bodiless hosts," // for use in petitions and dismissal
meInsert2 = "whose Synaxis we celebrate;" // for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "The Synaxis of Archangels Michael and Gabriel and of All the Heavenly Powers."
meLE.commemoration2 = ""
