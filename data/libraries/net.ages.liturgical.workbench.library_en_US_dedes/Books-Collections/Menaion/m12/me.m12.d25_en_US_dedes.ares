meMA.Ode9C1M07.text = ""

meMA.Ode9C2M01.text = "Today the holy Virgin gives birth to the Master within the cave and grotto."
meMA.Ode9C2M02.text = "Today our Lord and Master is born as an infant; His Mother is a virgin."
