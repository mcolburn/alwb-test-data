A_Resource_Whose_Name = sy.m06.d19_en_US_dedes

S01.commemoration.text = "On the June 19 we commemorate the Holy Apostle Jude."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Zosimas."

S03.commemoration.text = "On this day we commemorate  our devout Father Zeno."

S04.commemoration.text = "On this day we also commemorate our righteous Father Paisios the Great of Egypt."

S99.lastline.text= "By their holy intercessions, Christ God, have mercy on us. Amen."