A_Resource_Whose_Name = sy.m06.d06_en_US_dedes

S01.commemoration.text = "On June 6 we commemorate our devout Father Hilarion the New, Abbot of the Dalmation Monastery."

S02.commemoration.text = "On this day we also commemorate our devout Father Attalus the Wonderworker."

S03.commemoration.text = "On this day we also commemorate the Holy five Virgins and their comrades."

S99.lastline.text= "By their holy intercessions, Christ God, have mercy on us. Amen."