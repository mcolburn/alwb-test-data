A_Resource_Whose_Name = sy.m12.d12_en_US_dedes

S01.commemoration.text = "On December 12 we commemorate our devout Father Spyridon the Wonder-worker, Bishop of Trimythous."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Synetos."

S03.commemoration.text = "On this day we also commemorate our father among the saints Alexander Archbishop of Jerusalem, the Hieromartyr."

S04.commemoration.text = "On this day we also commemorate our devout fathers Amonathas and Anthos."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us and save us. Amen."