S00.days = "The Month of December, which has 31 days."
S00.hours = "The day has 9 hours, the night has 15 hours."

S01.commemoration.text = "On December 1, we commemorate the holy Prophet Nahum."

S02.commemoration.text = "On this day we also commemorate devout Philaretos the Merciful."

S03.commemoration.text = "On this day we also commemorate the holy Martyr Ananias the Persian."

S04.commemoration.text = "On this day we also commemorate devout Anthony the New."

S05.commemoration.text = "On this day Saint Onisimos, Archbishop of Ephesus, died in peace."

S06.commemoration.text = "On this day we also commemorate our father among the saints Theocletos, Archbishop of Lakedaemonia."

S07.commemoration.text = "We also commemorate our fathers among the saints the Archbishops of Ephesus Ananias and Solochon."

S08.commemoration.text = ""

