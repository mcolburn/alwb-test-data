A_Resource_Whose_Name = sy.m08.d03_en_US_dedes 

S01.commemoration.text = "On August 3, we commemorate our devout fathers Dalmatus, Faustus, and Isaakius."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Stephan, Pope of Rome."

S03.commemoration.text = "On this day we also commemorate our devout father John the Confessor, Abbot of the Monastery of Patalaria."

S04.commemoration.text = "On this day, Saint Salome the Myrrh-bearer died in peace."

S05.commemoration.text = "On this day, the devout Theoclito died in peace."

S99.lastline.text= "By the intercessions of Your Saints, O God, have mercy on us. Amen."
