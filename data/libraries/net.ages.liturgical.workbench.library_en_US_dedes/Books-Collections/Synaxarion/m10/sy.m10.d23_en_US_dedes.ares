A_Resource_Whose_Name = sy.m10.d23_en_US_dedes 

S01.commemoration.text = "On October 23, we commemorate the holy Hieromartyr and Apostle James, the Brother of the Lord."

S02.commemoration.text = "On this day we also commemorate our devout father Ignatius, Patriarch of Constantinople."

S03.commemoration.text = "On this day we also commemorate our devout father Makarios the Roman."

S04.commemoration.text = "Our devout father Nikephoros, the founder of the Monastery of Charsianos, died in peace."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."
