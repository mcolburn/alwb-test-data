A_Resource_Whose_Name = sy.m10.d13_en_US_dedes 

S01.commemoration.text = "On October 13, we commemorate the holy Martyrs Carpus, Papylus, Agathodorus, and Agathoniki."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Florentius."

S03.commemoration.text = "On this day we also commemorate the holy Martyr Dioskorus."

S04.commemoration.text = "On this day we also commemorate our devout father Niketas, the Patrician and Confessor."

S05.commemoration.text = "On this day we also commemorate the holy Martyr Benjamin the Deacon."

S06.commemoration.text = "On this day we commemorate the holy Martyr Antigonus who died by fire."

S07.commemoration.text = "The holy New Martyr Chryse, who contested in a villige called Slatena in the eparchy of Moglena (Edessa) in the year 1795, died by dimemberment."

S99.lastline.text= "By the intercessions of Your saints, O God, have mercy on us. Amen."
