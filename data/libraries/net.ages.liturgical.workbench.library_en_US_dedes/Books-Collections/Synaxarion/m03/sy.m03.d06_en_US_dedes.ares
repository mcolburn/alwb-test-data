A_Resource_Whose_Name = sy.m03.d06_en_US_dedes

S01.commemoration.text = "On March 6, we commemorate the holy Martyrs who witnessed in Amorion: Theodore, Kallistos, Theophilos, Bassoes, and those with them."

S02.commemoration.text = "On this day we commemorate our devout fater Arcadios."

S03.commemoration.text = "On this day we commemorate devout Saint Hesychios the Wonder-worker."

S04.commemoration.text = "On this day Saint Efphrosynos died when boiling water was poured down his throat."

S05.commemoration.text = "On this day we commemorate the finding of the precious Cross by blessed St. Helen."

S06.commemoration.text = "On this day we commemorate the finding of the precious nails."

S07.commemoration.text = "On this day Saints Julian and Evoulos died by the sword."

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By the intercessions of Your saints, O God, have mercy on us. Amen."