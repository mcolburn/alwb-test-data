A_Resource_Whose_Name = sy.m04.d30_en_US_dedes

S01.commemoration.text = "On April 30 we commemorate the holy and glorious Apostles James, the brother of St. John the Theologian."

S02.commemoration.text = "On this day we also commemorate our devout father Clement the ecclesiastical poet."

S03.commemoration.text = "On this day we also commemorate the holy Martyr Maximus."

S04.commemoration.text = "On this day we also commemorate the finding of the relices of the holy Hieromartyr Vasilefs, Bishop of Amasia."

S05.commemoration.text = "On this day we also commemorate our father among the saints Donatus, Bishop of Euroia."

S06.commemoration.text = "On this day the holy Neo-martyr Argyri, who confessed Christ in Constantinople and was tortured for it, died in prison in the year 1725."

S07.commemoration.text = ""

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."