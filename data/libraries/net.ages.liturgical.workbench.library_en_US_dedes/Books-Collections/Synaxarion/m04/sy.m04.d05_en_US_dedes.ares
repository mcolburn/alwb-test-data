A_Resource_Whose_Name = sy.m04.d05_en_US_dedes

S01.commemoration.text = "On April 5 we commemorate the holy martyrs Claudius, Diodoros, Victor, Victorinus, Pappias, Serapion, and Nikephoros."

S02.commemoration.text = "On this day we also commemorate our devout mother Theodora of Thessaloniki."

S03.commemoration.text = "On this day we also commemorate the holy martyrs Theodora and Didymos."

S04.commemoration.text = "On this day we also commemorate the holy martyr Thermos."

S05.commemoration.text = "On this day we also commemorate the holy women, a Lady and her Servant."

S06.commemoration.text = "On this day we also commemorate the holy martyr Pompey."

S07.commemoration.text = "On this day, Saint Zeno died after they smeared him with pitch, threw him into fire, and ran him through with a spear while he was in the fire."

S08.commemoration.text = "On this day we also commemorate the holy martyrs Maximus and Terence."

S09.commemoration.text = "On this day we also commemorate the holy five young women martyrs of Lesbos."

S10.commemoration.text = "On this day St. George the Neomartyr, who contested in New Ephesus in the year 1801, died by the sword."

S99.lastline.text= "By the intercessions of Your saints, Christ our God, have mercy on us. Amen."