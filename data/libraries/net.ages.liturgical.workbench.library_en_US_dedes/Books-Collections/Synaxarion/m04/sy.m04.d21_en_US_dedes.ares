A_Resource_Whose_Name = sy.m04.d21_en_US_dedes

S01.commemoration.text = "On April 21 we commemorate the holy Hieromartyr and bishop Januarius; the deacons Proculus, Sosius, and Faustus; the reader Desiderius; and Eutychius and Acutius."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Alexandra the Queen."

S03.commemoration.text = "On this day we also commemorate the holy Martyrs Apollo, Isaakios, and Quadratus, who died by hunger and the sword."

S04.commemoration.text = "On this day we also commemorate our father among the saints Maximian, Patriarch of Constantinople."

S05.commemoration.text = "On this day we also commemorate our devout father Anastasios of Sinai."

S06.commemoration.text = ""

