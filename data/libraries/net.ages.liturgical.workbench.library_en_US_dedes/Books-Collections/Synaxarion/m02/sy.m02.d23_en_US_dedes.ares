A_Resource_Whose_Name = sy.m02.d23_en_US_dedes

S01.commemoration.text = "On February 23, we commemorate the holy Hieromartyr Polycarp, Bishop of Smyrna."

S02.commemoration.text = "On this day we also commemorate our devout fathers John, Moses, Antiochus, and Antoninus."

S03.commemoration.text = "On this day Saint Gorgonia, the sister of St. Gregory the Theologian, died in peace."

S04.commemoration.text = "On this day the holy Martyr Clement died by the sword."

S05.commemoration.text = "On this day the holy Martyr Thea died by the sword."

S06.commemoration.text = "On this day we also commemorate our devout fathers Zebinas, Polychronios, Moses, and Damian."

S07.commemoration.text = "Devout St. Damian, who lived as an ascetic near the holy Monastery of Esphigmenou on Mt. Athos, died in peace in the year 1280."

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."