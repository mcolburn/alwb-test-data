A_Resource_Whose_Name = sy.m11.d10_en_US_dedes 

S01.commemoration.text = "On November 10, we commemorate the holy Apostles of the Seventy: Olympas, Rodion, Sosipater, Tertius, Erastus, and Quartus."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Orestes."

S03.commemoration.text = "On this day we also commemorate our devout father Theosterictos of Symbola. We also commemorate our devout father Nonnos the Bishop, who catechized St. Pelagia."

S04.commemoration.text = "On this day we also commemorate our devout father Martin the Bishop."

S05.commemoration.text = "On this day we also commemorate the holy Hieromartyr Milos the Bishop, and his two disciples."

S06.commemoration.text = "On this day we also commemorate the holy Martyrs Kalliopios, Nirus, and Orion."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us and save us. Amen."
