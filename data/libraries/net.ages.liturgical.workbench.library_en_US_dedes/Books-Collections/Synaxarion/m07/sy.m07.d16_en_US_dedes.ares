A_Resource_Whose_Name = sy.m07.d16_en_US_dedes 

S01.commemoration.text = "On July 16, we commemorate the holy Hieromartyr Athenogenes, Bishop of Pidachthoa."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Faustus."

S03.commemoration.text = "On this day we also commemorate the holy Martyr Antiochus, brother of the holy Martyr Plato."

S04.commemoration.text = "The holy 15,000 Martyrs in Pisidia died by the sword."

S05.commemoration.text = "The holy Martyr Athenogenes died by fire, and several women Martyrs died by the sword."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."
