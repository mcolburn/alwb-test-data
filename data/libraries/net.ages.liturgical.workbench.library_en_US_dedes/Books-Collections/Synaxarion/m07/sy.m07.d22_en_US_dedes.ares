S01.commemoration.text = "On July 22, we commemorate Saint Mary Magdalene, the Myrrh-bearer and Equal to the Apostles."

S02.commemoration.text = "On this day we also commemorate Saint Markella, the devout Martyr."
S02.couplet1.title = 
S02.couplet1.verse1 = ""
S02.couplet1.verse2 = ""
S02.couplet2.verse1 = "Hearing David who utters the saying,"
S02.couplet2.verse2 = "Unrighteousness I have abhorred and hated,"
S02.couplet2.verse3 = "And bringing to mind the incestuous union"
S02.couplet2.verse4 = "Of Lot and the shameful affair that he suffered,"
S02.couplet2.verse5 = "And fleeing from illicit congress with her father,"
S02.couplet2.verse6 = "By his bloody sword the maiden was wounded"
S02.couplet2.verse7 = "And sustained an unjust death, O Christ my Savior."

S02.single1.text = "Markella was slain by her sire, a baneful man."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."