A_Resource_Whose_Name = sy.m01.d12_en_US_dedes

S01.commemoration.text = "On January 12, we commemorate the holy Martyr Tatiana."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Peter Absalom."

S03.commemoration.text = "On this day we also commemorate the holy Martyr Mertius."

S04.commemoration.text = "On this day the holy eight Martyrs from Nicea died by the sword."

S05.commemoration.text = "On this day, the holy Martyr Efthasia died by the sword."

S06.commemoration.text = "On this day we also commemorate our devout father Elias the Wonderworker."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."