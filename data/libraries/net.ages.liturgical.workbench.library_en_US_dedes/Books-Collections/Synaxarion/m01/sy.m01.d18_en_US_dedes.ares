A_Resource_Whose_Name = sy.m01.d18_en_US_dedes

S01.commemoration.text = "On January 18, we commemorate our fathers among the saints Athanasios and Cyril, Patriarchs of Alexandria."

S02.commemoration.text = "On this day we also commemorate the holy Martyr Theodoula."

S03.commemoration.text = "On this day the holy Martyr Xenia died by fire."

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."