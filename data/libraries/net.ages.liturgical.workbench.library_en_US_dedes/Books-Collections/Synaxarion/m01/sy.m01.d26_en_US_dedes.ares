A_Resource_Whose_Name = sy.m01.d26_en_US_dedes

S01.commemoration.text = "On January 26, we commemorate our devout father Xenophon, his wife Maria, and sons Archadios and John."

S02.commemoration.text = "On this day we also commemorate the great earthquake."

S03.commemoration.text = "On this day we also commemorate our devout father Symeon, the so-called Old One."

S04.commemoration.text = "On this day we also commemorate the holy Martyrs Ananias the priest, Peter the prison-guard, and the seven soldiers with them."

S05.commemoration.text = "On this day, devout saint Ammonas died in peace."

S06.commemoration.text = "On this day, devout saint Gabriel died in peace."

S07.commemoration.text = "On this day, the two holy Martyrs in Phrygia died by beating."

S08.commemoration.text = ""

S09.commemoration.text = ""

S10.commemoration.text = ""

S99.lastline.text= "By their holy intercessions, O God, have mercy on us. Amen."