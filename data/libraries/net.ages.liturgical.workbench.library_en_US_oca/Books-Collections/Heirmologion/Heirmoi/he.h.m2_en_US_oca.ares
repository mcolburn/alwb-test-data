EisakikoaKyrieTinAkoin.mode = misc_en_US_oca.Mode2
EisakikoaKyrieTinAkoin.ode = misc_en_US_oca.Ode4
EisakikoaKyrieTinAkoin.text = "I have heard, Lord, the news of Your dispensation, * and I have glorified You, only Lover of mankind!"

EleftheraMenIKtisis.name = "The creation finds itself."
EleftheraMenIKtisis.mode = misc_en_US_oca.Mode2
EleftheraMenIKtisis.ode = misc_en_US_oca.Ode8
EleftheraMenIKtisis.text = "The creation finds itself set free; * those in darkness now are made sons of the Light! * Cast out, the prince of darkness groans! * Let the nationsâ€™ inheritance, once in misery, * now eagerly bless Him Who has wrought this change!"
