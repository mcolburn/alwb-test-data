OTonYperNoun.mode = misc_en_US_oca.Mode2
OTonYperNoun.ode = misc_en_US_oca.Ode9
OTonYperNoun.text = "Most pure Bride, blessed Mother, * the wonders of your childbearing pass all understanding! * Through you we have obtained the Salvation of all! * We fittingly rejoice before you, our Benefactor, * bringing a song of thanksgiving as a gift!"
