YpilthonOsNymfona.name = "For the sake of piety."
YpilthonOsNymfona.mode = misc_en_US_oca.Mode1
YpilthonOsNymfona.ode = misc_en_US_oca.Ode7
YpilthonOsNymfona.text = "For the sake of piety, * the children once entered the unbearable flame of the furnace * as though it were a bridal chamber. * They were plainly shown to be holy and chanting together, sang the hymn: * Blessed are You, O God of our fathers!"
