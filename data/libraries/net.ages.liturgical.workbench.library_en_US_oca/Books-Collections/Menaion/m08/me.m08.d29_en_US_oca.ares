meMA.Ode1C11.ode = misc_en_US_oca.Ode1
meMA.Ode1C11.mode = misc_en_US_oca.Mode8
meMA.Ode1C11.melody = misc_en_US_oca.NotMetered //he.h.m8_en_US_oca.YgranDiodefsas.name
meMA.Ode1C11.text = "Let us sing the praises of the holy Forerunner who appeared from a barren womb, conceived as a prophet from fruitless loins."

meMA.Ode1C12.ode = misc_en_US_oca.Ode1
meMA.Ode1C12.mode = misc_en_US_oca.Mode8
meMA.Ode1C12.melody = misc_en_US_oca.NotMetered //he.h.m8_en_US_oca.YgranDiodefsas.name
meMA.Ode1C12.text = "You surpassed the bounds of nature, keeping the laws of righteousness, rebuking lawless intercourse without fearing the bold pride of the king."

meMA.Ode1C13.ode = misc_en_US_oca.Ode1
meMA.Ode1C13.mode = misc_en_US_oca.Mode8
meMA.Ode1C13.melody = misc_en_US_oca.NotMetered //he.h.m8_en_US_oca.YgranDiodefsas.name
meMA.Ode1C13.text = "You fed on the milk of the Law, and as its seal you upheld the place of lawful marriage, standing against abhorrent adultery."

meMA.Ode1C1T.ode = misc_en_US_oca.Ode1
meMA.Ode1C1T.mode = misc_en_US_oca.Mode8
meMA.Ode1C1T.melody = misc_en_US_oca.NotMetered //he.h.m8_en_US_oca.YgranDiodefsas.name
meMA.Ode1C1T.text = "The ranks of angels and mankind praise you in song, unwedded Mother, for you carried their Creator in your arms as a child."

meMA.Ode1C21.ode = misc_en_US_oca.Ode1
meMA.Ode1C21.mode = misc_en_US_oca.Mode8
meMA.Ode1C21.melody = misc_en_US_oca.NotMetered //he.h.m8_en_US_oca.TonIsrailEkDouleias.name
meMA.Ode1C21.text = ""

