meMA.Ode1C2H3.text = he.h.m2_en_US_oca.SteiveiThalassis3.text
meMA.Ode1C2H4.text = he.h.m2_en_US_oca.SteiveiThalassis4.text
meMA.Ode1C2H5.text = he.h.m2_en_US_oca.SteiveiThalassis5.text

meMA.Ode1C21.ode = misc_en_US_oca.Ode1
meMA.Ode1C21.mode = misc_en_US_oca.Mode2
meMA.Ode1C21.melody = misc_en_US_oca.NotMetered //he.h.m2_en_US_oca.SteiveiThalassis.name
meMA.Ode1C21.text = "When the light of Dawn shone on mortal men,  * he came from the desert to the streams of the Jordan.  * You, the Sun's King, bowed Your head before him,  * to snatch our forefather from the land of darkness,  * and cleanse creation from pollution."
meMA.Ode1C211.text = ""
