bode9.title = "Ode Nine"
bode9.version = properties_en_US_nkjv.version.designation
bode9_mag.book = bible_en_US_nkjv.Luke
bode9_mag.heading1 = "Song of the Theotokos"
bode9_mag.chapverse = "(1:46-56)"
bode9_mag.heading2 = "The Maiden Mother sings a hymn to her Son and God."
bode9_mag.intro = "Let us magnify in hymns the Theotokos [and Mother of the Light]."
bode9_mag.v1.number = "(9a:1)"
bode9_mag.v1.text = "My soul magnifies the Lord, and my spirit has rejoiced in God my Savior."
bode9_mag.v2.number = "(9a:2)"
bode9_mag.v2.text = "For He has regarded the lowly state of His maidservant; for behold, henceforth all generations will call me blessed."
bode9_mag.v3.number = "(9a:3)"
bode9_mag.v3.text = "For He who is mighty has done great things for me, and holy is His name. And His mercy is on those who fear Him from generation to generation."
bode9_mag.v4.number = "(9a:4)"
bode9_mag.v4.text = "He has shown strength with His arm; He has scattered the proud in the imagination of their hearts."
bode9_mag.v5.number = "(9a:5)"
bode9_mag.v5.text = "He has put down the mighty from their thrones, and exalted the lowly. He has filled the hungry with good things, and the rich He has sent away empty."
bode9_mag.v6.number = "(9a:6)"
bode9_mag.v6.text = "He has helped His servant Israel, in remembrance of His mercy, as He spoke to our fathers, to Abraham and to his seed forever."
bode9.book = bible_en_US_nkjv.Luke
bode9.heading1 = "The Prayer of Zacharias the Forerunner's Father"
bode9.chapverse = "(1:68-79)"
bode9.heading2 = ""
bode9.intro = "Zacharias blesses the birth of his son."
bode9.v1.number = "(9b:1)"
bode9.v1.text = "Blessed is the Lord God of Israel, for He has visited and redeemed His people."
bode9.v2.number = "(9b:2)"
bode9.v2.text = "And [He] has raised up a horn of salvation for us in the house of His servant David."
bode9.v3.number = "(9b:3)"
bode9.v3.text = "As He spoke by the mouth of His holy prophets, who have been since the world began."
bode9.v4.number = "(9b:4)"
bode9.v4.text = "That we should be saved from our enemies and from the hand of all who hate us."
bode9.v5.number = "(9b:5)"
bode9.v5.text = "To perform the mercy promised to our fathers and to remember His holy covenant."
bode9.v6.number = "(9b:6)"
bode9.v6.text = "The oath which He swore to our father Abraham: to grant us that we [be] delivered from the hand of our enemies."
bode9.v7.number = "(9b:7)"
bode9.v7.text = "[That we] might serve Him without fear, in holiness and righteousness before Him all the days of our life."
bode9.v8.number = "(9b:8)"
bode9.v8.text = "And you, child, will be called the prophet of the Highest; for you will go before the face of the Lord to prepare His ways."
bode9.v9.number = "(9b:9)"
bode9.v9.text = "To give knowledge of salvation to His people by the remission of their sins, through the tender mercy of our God."
bode9.v10.number = "(9b:10)"
bode9.v10.text = "With which the Dayspring from on high has visited us; to give light to those who sit in darkness and the shadow of death."
bode9.v11.number = "(9b:11)"
bode9.v11.text = "To guide our feet into the way of peace."
