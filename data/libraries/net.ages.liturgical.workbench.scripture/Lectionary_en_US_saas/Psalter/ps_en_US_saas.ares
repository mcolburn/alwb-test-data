psa57.v1.text = ""
psa57.v2.number = "(57:2)"
psa57.v2.text = "If, then, you truly speak of righteousness, do you judge rightly, O you sons of men?"
psa57.v3.number = "(57:3)"
psa57.v3.text = "For in your heart you work lawlessness in the earth; Your hands weave unrighteousness."
