psa81.v2.text = "How long will you judge unrighteously and favors those who are sinners?"
psa81.v3.number = "(81:3)"
psa81.v3.text = "Judge the orphan and poor: do justice to the low and needy."
psa81.v4.number = "(81:4)"
psa81.v4.text = "Rescue the needy and deliver the poor out of the hand of the sinner."
psa81.v5.number = "(81:5)"
psa81.v5.text = "They know not, nor understand; they walk on in darkness: all the foundations of the earth shall be shaken."
psa81.v6.number = "(81:6)"
psa81.v6.text = "I have said: you are gods; and all of you children of the Most High."
