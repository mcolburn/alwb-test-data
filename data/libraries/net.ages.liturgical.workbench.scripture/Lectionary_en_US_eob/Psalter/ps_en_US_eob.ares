psa33.v3.text = "My soul shall find glory in the Lord: let the meek hear and rejoice."
psa33.v4.number = "(33:4)"
psa33.v4.text = "Magnify the Lord with me, Let us exalt his name together."
psa33.v5.number = "(33:5)"
psa33.v5.text = "I sought the Lord diligently, He listened to me and delivered me from all my exiles."
