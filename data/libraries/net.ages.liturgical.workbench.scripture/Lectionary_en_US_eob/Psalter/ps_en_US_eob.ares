psa75.v2.text = "God is known in Judea: his name is great in Israel."
psa75.v3.number = "(75:3)"
psa75.v3.text = "And his place has been in peace and his dwelling-place in Zion."
psa75.v4.number = "(75:4)"
psa75.v4.text = "There he broke the power of the bows, the shield, the sword and the battle."
