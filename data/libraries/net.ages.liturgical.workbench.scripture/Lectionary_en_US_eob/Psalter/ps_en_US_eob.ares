psa59.v9.number = "(59:9)"
psa59.v9.text = "Galaad is mine and Manasse is mine; and Ephraim is the strength of my head; Judas is my king; "
psa59.v10.number = "(59:10)"
psa59.v10.text = "Moab is the caldron of my hope; over Idumea will I stretch out my shoe; the Philistines have been subjected to me."
psa59.v11.number = "(59:11)"
psa59.v11.text = "Who will lead me into the fortified city? Who will guide me as far as Idumea?"
