psa75.v10.number = "(75:10)"
psa75.v10.text = "When God arose to judgment, to save all the meek in heart."
psa75.v11.number = "(75:11)"
psa75.v11.text = "For the inward thought of man shall give thanks to you: and the memorial of his inward thought shall keep a feast to you."
psa75.v12.number = "(75:12)"
