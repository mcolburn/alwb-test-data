ps.title = "PSALTER"
ps.version = properties_en_US_rsv.version.designation

psa0.title = ""
psa0.v0.text = ""
kathisma_1.title = "KATHISMA 1"

psa1.title = "Psalm 1"
psa1.version = properties_en_US_rsv.version.designation
psa1.text = ""
psa1.v1.number_rsv = "(1:1)"
psa1.v1.number = "(1:1)"
psa1.v1.text = "Blessed is the man who walks not in the counsel of the wicked, nor stands in the way of sinners, nor sits in the seat of scoffers;"
psa1.v2.number_rsv = "(1:2)"
psa1.v2.number = "(1:2)"
psa1.v2.text = "but his delight is in the law of the LORD, and on his law he meditates day and night."
psa1.v3.number_rsv = "(1:3)"
psa1.v3.number = "(1:3)"
psa1.v3.text = "He is like a treeplanted by streams of water, that yields its fruit in its season, and its leaf does not wither.In all that he does, he prospers."
psa1.v4.number_rsv = "(1:4)"
psa1.v4.number = "(1:4)"
psa1.v4.text = "The wicked are not so, but are like chaff which the wind drives away."
psa1.v5.number_rsv = "(1:5)"
psa1.v5.number = "(1:5)"
psa1.v5.text = "Therefore the wicked will not stand in the judgment, nor sinners in the congregation of the righteous;"
psa1.v6.number_rsv = "(1:6)"
psa1.v6.number = "(1:6)"
psa1.v6.text = "for the LORD knows the way of the righteous, but the way of the wicked will perish."

psa2.title = "Psalm 2"
psa2.version = properties_en_US_rsv.version.designation
psa2.text = ""
psa2.v1.number_rsv = "(2:1)"
psa2.v1.number = "(2:1)"
psa2.v1.text = "Why do the nations conspire, and the peoples plot in vain?"
psa2.v2.number_rsv = "(2:2)"
psa2.v2.number = "(2:2)"
psa2.v2.text = "The kings of the earth set themselves, and the rulers take counsel together, against the LORD and his anointed, saying,"
psa2.v3.number_rsv = "(2:3)"
psa2.v3.number = "(2:3)"
psa2.v3.text = "\"Let us burst their bonds asunder, and cast their cords from us.\""
psa2.v4.number_rsv = "(2:4)"
psa2.v4.number = "(2:4)"
psa2.v4.text = "He who sits in the heavens laughs; the LORD has them in derision."
psa2.v5.number_rsv = "(2:5)"
psa2.v5.number = "(2:5)"
psa2.v5.text = "Then he will speak to them in his wrath, and terrify them in his fury, saying,"
psa2.v6.number_rsv = "(2:6)"
psa2.v6.number = "(2:6)"
psa2.v6.text = "\"I have set my kingon Zion, my holy hill.\""
psa2.v7.number_rsv = "(2:7)"
psa2.v7.number = "(2:7)"
psa2.v7.text = "I will tell of the decree of the LORD:He said to me, \"You are my son, today I have begotten you."
psa2.v8.number_rsv = "(2:8)"
psa2.v8.number = "(2:8)"
psa2.v8.text = "Ask of me, and I will make the nations your heritage, and the ends of the earth your possession."
psa2.v9.number_rsv = "(2:9)"
psa2.v9.number = "(2:9)"
psa2.v9.text = "You shall break them with a rod of iron, and dash them in pieces like a potter\"s vessel.\""
psa2.v10.number_rsv = "(2:10)"
psa2.v10.number = "(2:10)"
psa2.v10.text = "Now therefore, O kings, be wise; be warned, O rulers of the earth."
psa2.v11.number_rsv = "(2:11)"
psa2.v11.number = "(2:11)"
psa2.v11.text = "Serve the LORD with fear, with trembling"
psa2.v12.number_rsv = "(2:12)"
psa2.v12.number = "(2:12)"
psa2.v12.text = "kiss his feet, lest he be angry, and you perish in the way; for his wrath is quickly kindled.Blessed are all who take refuge in him."

psa3.title = "Psalm 3"
psa3.version = properties_en_US_rsv.version.designation
psa3.text = ""
psa3.v0.number_rsv = "(3:0)"
psa3.v0.number = "(3:0)"
psa3.v0.text = "A Psalm of David, when he fled from Absalom his son."
psa3.v1.number_rsv = "(3:1)"
psa3.v1.number = "(3:1)"
psa3.v1.text = "O LORD, how many are my foes! Many are rising against me;"
psa3.v2.number_rsv = "(3:2)"
psa3.v2.number = "(3:2)"
psa3.v2.text = "many are saying of me, there is no help for him in God."
psa3.v3.number_rsv = "(3:3)"
psa3.v3.number = "(3:3)"
psa3.v3.text = "But thou, O LORD, art a shield about me, my glory, and the lifter of my head."
psa3.v4.number_rsv = "(3:4)"
psa3.v4.number = "(3:4)"
psa3.v4.text = "I cry aloud to the LORD, and he answers me from his holy hill."
psa3.v5.number_rsv = "(3:5)"
psa3.v5.number = "(3:5)"
psa3.v5.text = "I lie down and sleep; I wake again, for the LORD sustains me."
psa3.v6.number_rsv = "(3:6)"
psa3.v6.number = "(3:6)"
psa3.v6.text = "I am not afraid of ten thousands of peoplewho have set themselves against me round about."
psa3.v7.number_rsv = "(3:7)"
psa3.v7.number = "(3:7)"
psa3.v7.text = "Arise, O LORD! Deliver me, O my God! For thou dost smite all my enemies on the cheek, thou dost break the teeth of the wicked."
psa3.v8.number_rsv = "(3:8)"
psa3.v8.number = "(3:8)"
psa3.v8.text = "Deliverance belongs to the LORD; thy blessing be upon thy people!"

psa4.title = "Psalm 4"
psa4.version = properties_en_US_rsv.version.designation
psa4.text = ""
psa4.v0.number_rsv = "(4:0)"
psa4.v0.number = "(4:0)"
psa4.v0.text = "To the choirmaster: with stringed instruments. A Psalm of David."
psa4.v1.number_rsv = "(4:1)"
psa4.v1.number = "(4:1)"
psa4.v1.text = "Answer me when I call, O God of my right! Thou hast given me room when I was in distress.Be gracious to me, and hear my prayer."
psa4.v2.number_rsv = "(4:2)"
psa4.v2.number = "(4:2)"
psa4.v2.text = "O men, how long shall my honor suffer shame? How long will you love vain words, and seek after lies?"
psa4.v3.number_rsv = "(4:3)"
psa4.v3.number = "(4:3)"
psa4.v3.text = "But know that the LORD has set apart the godly for himself; the LORD hears when I call to him."
psa4.v4.number_rsv = "(4:4)"
psa4.v4.number = "(4:4)"
psa4.v4.text = "Be angry, but sin not; commune with your own hearts on your beds, and be silent."
psa4.v5.number_rsv = "(4:5)"
psa4.v5.number = "(4:5)"
psa4.v5.text = "Offer right sacrifices, and put your trust in the LORD."
psa4.v6.number_rsv = "(4:6)"
psa4.v6.number = "(4:6)"
psa4.v6.text = "There are many who say, \"O that we might see some good! Lift up the light of thy countenance upon us, O LORD!\""
psa4.v7.number_rsv = "(4:7)"
psa4.v7.number = "(4:7)"
psa4.v7.text = "Thou hast put more joy in my heartthan they have when their grain and wine abound."
psa4.v8.number_rsv = "(4:8)"
psa4.v8.number = "(4:8)"
psa4.v8.text = "In peace I will both lie down and sleep; for thou alone, O LORD, makest me dwell in safety."

psa5.title = "Psalm 5"
psa5.version = properties_en_US_rsv.version.designation
psa5.text = ""
psa5.v0.number_rsv = "(5:0)"
psa5.v0.number = "(5:0)"
psa5.v0.text = "To the choirmaster: for the flutes. A Psalm of David."
psa5.v1.number_rsv = "(5:1)"
psa5.v1.number = "(5:1)"
psa5.v1.text = "Give ear to my words, O LORD; give heed to my groaning."
psa5.v2.number_rsv = "(5:2)"
psa5.v2.number = "(5:2)"
psa5.v2.text = "Hearken to the sound of my cry, my King and my God, for to thee do I pray."
psa5.v3.number_rsv = "(5:3)"
psa5.v3.number = "(5:3)"
psa5.v3.text = "O LORD, in the morning thou dost hear my voice; in the morning I prepare a sacrifice for thee, and watch."
psa5.v4.number_rsv = "(5:4)"
psa5.v4.number = "(5:4)"
psa5.v4.text = "For thou art not a God who delights in wickedness; evil may not sojourn with thee."
psa5.v5.number_rsv = "(5:5)"
psa5.v5.number = "(5:5)"
psa5.v5.text = "The boastful may not stand before thy eyes; thou hatest all evildoers."
psa5.v6.number_rsv = "(5:6)"
psa5.v6.number = "(5:6)"
psa5.v6.text = "Thou destroyest those who speak lies; the LORD abhors bloodthirsty and deceitful men."
psa5.v7.number_rsv = "(5:7)"
psa5.v7.number = "(5:7)"
psa5.v7.text = "But I through the abundance of thy steadfast lovewill enter thy house, I will worship toward thy holy templein the fear of thee."
psa5.v8.number_rsv = "(5:8)"
psa5.v8.number = "(5:8)"
psa5.v8.text = "Lead me, O LORD, in thy righteousnessbecause of my enemies; make thy way straight before me."
psa5.v9.number_rsv = "(5:9)"
psa5.v9.number = "(5:9)"
psa5.v9.text = "For there is no truth in their mouth; their heart is destruction, their throat is an open sepulchre, they flatter with their tongue."
psa5.v10.number_rsv = "(5:10)"
psa5.v10.number = "(5:10)"
psa5.v10.text = "Make them bear their guilt, O God; let them fall by their own counsels; because of their many transgressions cast them out, for they have rebelled against thee."
psa5.v11.number_rsv = "(5:11)"
psa5.v11.number = "(5:11)"
psa5.v11.text = "But let all who take refuge in thee rejoice, let them ever sing for joy; and do thou defend them, that those who love thy name may exult in thee."
psa5.v12.number_rsv = "(5:12)"
psa5.v12.number = "(5:12)"
psa5.v12.text = "For thou dost bless the righteous, O LORD; thou dost cover him with favor as with a shield."

psa6.title = "Psalm 6"
psa6.version = properties_en_US_rsv.version.designation
psa6.text = ""
psa6.v0.number_rsv = "(6:0)"
psa6.v0.number = "(6:0)"
psa6.v0.text = "To the choirmaster: with stringed instruments; according to The Sheminith. A Psalm of David."
psa6.v1.number_rsv = "(6:1)"
psa6.v1.number = "(6:1)"
psa6.v1.text = "O LORD, rebuke me not in thy anger, nor chasten me in thy wrath."
psa6.v2.number_rsv = "(6:2)"
psa6.v2.number = "(6:2)"
psa6.v2.text = "Be gracious to me, O LORD, for I am languishing; O LORD, heal me, for my bones are troubled."
psa6.v3.number_rsv = "(6:3)"
psa6.v3.number = "(6:3)"
psa6.v3.text = "My soul also is sorely troubled.But thou, O LORD -- how long?"
psa6.v4.number_rsv = "(6:4)"
psa6.v4.number = "(6:4)"
psa6.v4.text = "Turn, O LORD, save my life; deliver me for the sake of thy steadfast love."
psa6.v5.number_rsv = "(6:5)"
psa6.v5.number = "(6:5)"
psa6.v5.text = "For in death there is no remembrance of thee; in Sheol who can give thee praise?"
psa6.v6.number_rsv = "(6:6)"
psa6.v6.number = "(6:6)"
psa6.v6.text = "I am weary with my moaning; every night I flood my bed with tears; I drench my couch with my weeping."
psa6.v7.number_rsv = "(6:7)"
psa6.v7.number = "(6:7)"
psa6.v7.text = "My eye wastes away because of grief, it grows weak because of all my foes."
psa6.v8.number_rsv = "(6:8)"
psa6.v8.number = "(6:8)"
psa6.v8.text = "Depart from me, all you workers of evil; for the LORD has heard the sound of my weeping."
psa6.v9.number_rsv = "(6:9)"
psa6.v9.number = "(6:9)"
psa6.v9.text = "The LORD has heard my supplication; the LORD accepts my prayer."
psa6.v10.number_rsv = "(6:10)"
psa6.v10.number = "(6:10)"
psa6.v10.text = "All my enemies shall be ashamed and sorely troubled; they shall turn back, and be put to shame in a moment."

psa7.title = "Psalm 7"
psa7.version = properties_en_US_rsv.version.designation
psa7.text = ""
psa7.v0.number_rsv = "(7:0)"
psa7.v0.number = "(7:0)"
psa7.v0.text = "A Shiggaion of David, which he sang to the LORD concerning Cush a Benjaminite."
psa7.v1.number_rsv = "(7:1)"
psa7.v1.number = "(7:1)"
psa7.v1.text = "O LORD my God, in thee do I take refuge; save me from all my pursuers, and deliver me,"
psa7.v2.number_rsv = "(7:2)"
psa7.v2.number = "(7:2)"
psa7.v2.text = "lest like a lion they rend me, dragging me away, with none to rescue."
psa7.v3.number_rsv = "(7:3)"
psa7.v3.number = "(7:3)"
psa7.v3.text = "O LORD my God, if I have done this, if there is wrong in my hands,"
psa7.v4.number_rsv = "(7:4)"
psa7.v4.number = "(7:4)"
psa7.v4.text = "if I have requited my friend with evilor plundered my enemy without cause,"
psa7.v5.number_rsv = "(7:5)"
psa7.v5.number = "(7:5)"
psa7.v5.text = "let the enemy pursue me and overtake me, and let him trample my life to the ground, and lay my soul in the dust."
psa7.v6.number_rsv = "(7:6)"
psa7.v6.number = "(7:6)"
psa7.v6.text = "Arise, O LORD, in thy anger, lift thyself up against the fury of my enemies; awake, O my God; thou hast appointed a judgment."
psa7.v7.number_rsv = "(7:7)"
psa7.v7.number = "(7:7)"
psa7.v7.text = "Let the assembly of the peoples be gathered about thee; and over it take thy seat on high."
psa7.v8.number_rsv = "(7:8)"
psa7.v8.number = "(7:8)"
psa7.v8.text = "The LORD judges the peoples; judge me, O LORD, according to my righteousnessand according to the integrity that is in me."
psa7.v9.number_rsv = "(7:9)"
psa7.v9.number = "(7:9)"
psa7.v9.text = "O let the evil of the wicked come to an end, but establish thou the righteous, thou who triest the minds and hearts, thou righteous God."
psa7.v10.number_rsv = "(7:10)"
psa7.v10.number = "(7:10)"
psa7.v10.text = "My shield is with God, who saves the upright in heart."
psa7.v11.number_rsv = "(7:11)"
psa7.v11.number = "(7:11)"
psa7.v11.text = "God is a righteous judge, and a God who has indignation every day."
psa7.v12.number_rsv = "(7:12)"
psa7.v12.number = "(7:12)"
psa7.v12.text = "If a man does not repent, God will whet his sword; he has bent and strung his bow;"
psa7.v13.number_rsv = "(7:13)"
psa7.v13.number = "(7:13)"
psa7.v13.text = "he has prepared his deadly weapons, making his arrows fiery shafts."
psa7.v14.number_rsv = "(7:14)"
psa7.v14.number = "(7:14)"
psa7.v14.text = "Behold, the wicked man conceives evil, and is pregnant with mischief, and brings forth lies."
psa7.v15.number_rsv = "(7:15)"
psa7.v15.number = "(7:15)"
psa7.v15.text = "He makes a pit, digging it out, and falls into the hole which he has made."
psa7.v16.number_rsv = "(7:16)"
psa7.v16.number = "(7:16)"
psa7.v16.text = "His mischief returns upon his own head, and on his own pate his violence descends."
psa7.v17.number_rsv = "(7:17)"
psa7.v17.number = "(7:17)"
psa7.v17.text = "I will give to the LORD the thanks due to his righteousness, and I will sing praise to the name of the LORD, the Most High."

psa8.title = "Psalm 8"
psa8.version = properties_en_US_rsv.version.designation
psa8.text = ""
psa8.v0.number_rsv = "(8:0)"
psa8.v0.number = "(8:0)"
psa8.v0.text = "To the choirmaster: according to The Gittith. A Psalm of David."
psa8.v1.number_rsv = "(8:1)"
psa8.v1.number = "(8:1)"
psa8.v1.text = "O LORD, our Lord, how majestic is thy name in all the earth! Thou whose glory above the heavens is chanted"
psa8.v2.number_rsv = "(8:2)"
psa8.v2.number = "(8:2)"
psa8.v2.text = "by the mouth of babes and infants, thou hast founded a bulwark because of thy foes, to still the enemy and the avenger."
psa8.v3.number_rsv = "(8:3)"
psa8.v3.number = "(8:3)"
psa8.v3.text = "When I look at thy heavens, the work of thy fingers, the moon and the stars which thou hast established;"
psa8.v4.number_rsv = "(8:4)"
psa8.v4.number = "(8:4)"
psa8.v4.text = "what is man that thou art mindful of him, and the son of man that thou dost care for him?"
psa8.v5.number_rsv = "(8:5)"
psa8.v5.number = "(8:5)"
psa8.v5.text = "Yet thou hast made him little less than God, and dost crown him with glory and honor."
psa8.v6.number_rsv = "(8:6)"
psa8.v6.number = "(8:6)"
psa8.v6.text = "Thou hast given him dominion over the works of thy hands; thou hast put all things under his feet,"
psa8.v7.number_rsv = "(8:7)"
psa8.v7.number = "(8:7)"
psa8.v7.text = "all sheep and oxen, and also the beasts of the field,"
psa8.v8.number_rsv = "(8:8)"
psa8.v8.number = "(8:8)"
psa8.v8.text = "the birds of the air, and the fish of the sea, whatever passes along the paths of the sea."
psa8.v9.number_rsv = "(8:9)"
psa8.v9.number = "(8:9)"
psa8.v9.text = "O LORD, our Lord, how majestic is thy name in all the earth!"

kathisma_2.title = "KATHISMA 2."

psa9.title = "Psalm 9"
psa9.version = properties_en_US_rsv.version.designation
psa9.text = ""
psa9.v0.number_rsv = "(9:0)"
psa9.v0.number = "(9:0)"
psa9.v0.text = "To the choirmaster: according to Muth-labben. A Psalm of David."
psa9.v1.number_rsv = "(9:1)"
psa9.v1.number = "(9:1)"
psa9.v1.text = "I will give thanks to the LORD with my whole heart; I will tell of all thy wonderful deeds."
psa9.v2.number_rsv = "(9:2)"
psa9.v2.number = "(9:2)"
psa9.v2.text = "I will be glad and exult in thee, I will sing praise to thy name, O Most High."
psa9.v3.number_rsv = "(9:3)"
psa9.v3.number = "(9:3)"
psa9.v3.text = "When my enemies turned back, they stumbled and perished before thee."
psa9.v4.number_rsv = "(9:4)"
psa9.v4.number = "(9:4)"
psa9.v4.text = "For thou hast maintained my just cause; thou hast sat on the throne giving righteous judgment."
psa9.v5.number_rsv = "(9:5)"
psa9.v5.number = "(9:5)"
psa9.v5.text = "Thou hast rebuked the nations, thou hast destroyed the wicked; thou hast blotted out their name for ever and ever."
psa9.v6.number_rsv = "(9:6)"
psa9.v6.number = "(9:6)"
psa9.v6.text = "The enemy have vanished in everlasting ruins; their cities thou hast rooted out; the very memory of them has perished."
psa9.v7.number_rsv = "(9:7)"
psa9.v7.number = "(9:7)"
psa9.v7.text = "But the LORD sits enthroned for ever, he has established his throne for judgment;"
psa9.v8.number_rsv = "(9:8)"
psa9.v8.number = "(9:8)"
psa9.v8.text = "and he judges the world with righteousness, he judges the peoples with equity."
psa9.v9.number_rsv = "(9:9)"
psa9.v9.number = "(9:9)"
psa9.v9.text = "The LORD is a stronghold for the oppressed, a stronghold in times of trouble."
psa9.v10.number_rsv = "(9:10)"
psa9.v10.number = "(9:10)"
psa9.v10.text = "And those who know thy name put their trust in thee, for thou, O LORD, hast not forsaken those who seek thee."
psa9.v11.number_rsv = "(9:11)"
psa9.v11.number = "(9:11)"
psa9.v11.text = "Sing praises to the LORD, who dwells in Zion! Tell among the peoples his deeds!"
psa9.v12.number_rsv = "(9:12)"
psa9.v12.number = "(9:12)"
psa9.v12.text = "For he who avenges blood is mindful of them; he does not forget the cry of the afflicted."
psa9.v13.number_rsv = "(9:13)"
psa9.v13.number = "(9:13)"
psa9.v13.text = "Be gracious to me, O LORD! Behold what I suffer from those who hate me, O thou who liftest me up from the gates of death,"
psa9.v14.number_rsv = "(9:14)"
psa9.v14.number = "(9:14)"
psa9.v14.text = "that I may recount all thy praises, that in the gates of the daughter of ZionI may rejoice in thy deliverance."
psa9.v15.number_rsv = "(9:15)"
psa9.v15.number = "(9:15)"
psa9.v15.text = "The nations have sunk in the pit which they made; in the net which they hid has their own foot been caught."
psa9.v16.number_rsv = "(9:16)"
psa9.v16.number = "(9:16)"
psa9.v16.text = "The LORD has made himself known, he has executed judgment; the wicked are snared in the work of their own hands."
psa9.v17.number_rsv = "(9:17)"
psa9.v17.number = "(9:17)"
psa9.v17.text = "The wicked shall depart to Sheol, all the nations that forget God."
psa9.v18.number_rsv = "(9:18)"
psa9.v18.number = "(9:18)"
psa9.v18.text = "For the needy shall not always be forgotten, and the hope of the poor shall not perish for ever."
psa9.v19.number_rsv = "(9:19)"
psa9.v19.number = "(9:19)"
psa9.v19.text = "Arise, O LORD! Let not man prevail; let the nations be judged before thee!"
psa9.v20.number_rsv = "(9:20)"
psa9.v20.number = "(9:20)"
psa9.v20.text = "Put them in fear, O LORD! Let the nations know that they are but men!"

psa10.title = "Psalm 10"
psa10.version = properties_en_US_rsv.version.designation
psa10.text = ""
psa10.v1.number_rsv = "(10:1)"
psa10.v1.number = "(10:1)"
psa10.v1.text = "Why dost thou stand afar off, O LORD? Why dost thou hide thyself in times of trouble?"
psa10.v2.number_rsv = "(10:2)"
psa10.v2.number = "(10:2)"
psa10.v2.text = "In arrogance the wicked hotly pursue the poor; let them be caught in the schemes which they have devised."
psa10.v3.number_rsv = "(10:3)"
psa10.v3.number = "(10:3)"
psa10.v3.text = "For the wicked boasts of the desires of his heart, and the man greedy for gain curses and renounces the LORD."
psa10.v4.number_rsv = "(10:4)"
psa10.v4.number = "(10:4)"
psa10.v4.text = "In the pride of his countenance the wicked does not seek him; all his thoughts are, \"There is no God.\""
psa10.v5.number_rsv = "(10:5)"
psa10.v5.number = "(10:5)"
psa10.v5.text = "His ways prosper at all times; thy judgments are on high, out of his sight; as for all his foes, he puffs at them."
psa10.v6.number_rsv = "(10:6)"
psa10.v6.number = "(10:6)"
psa10.v6.text = "He thinks in his heart, \"I shall not be moved; throughout all generations I shall not meet adversity.\""
psa10.v7.number_rsv = "(10:7)"
psa10.v7.number = "(10:7)"
psa10.v7.text = "His mouth is filled with cursing and deceit and oppression; under his tongue are mischief and iniquity."
psa10.v8.number_rsv = "(10:8)"
psa10.v8.number = "(10:8)"
psa10.v8.text = "He sits in ambush in the villages; in hiding places he murders the innocent.His eyes stealthily watch for the hapless,"
psa10.v9.number_rsv = "(10:9)"
psa10.v9.number = "(10:9)"
psa10.v9.text = "he lurks in secret like a lion in his covert; he lurks that he may seize the poor, he seizes the poor when he draws him into his net."
psa10.v10.number_rsv = "(10:10)"
psa10.v10.number = "(10:10)"
psa10.v10.text = "The hapless is crushed, sinks down, and falls by his might."
psa10.v11.number_rsv = "(10:11)"
psa10.v11.number = "(10:11)"
psa10.v11.text = "He thinks in his heart, \"God has forgotten, he has hidden his face, he will never see it.\""
psa10.v12.number_rsv = "(10:12)"
psa10.v12.number = "(10:12)"
psa10.v12.text = "Arise, O LORD; O God, lift up thy hand; forget not the afflicted."
psa10.v13.number_rsv = "(10:13)"
psa10.v13.number = "(10:13)"
psa10.v13.text = "Why does the wicked renounce God, and say in his heart, \"Thou wilt not call to account\"?"
psa10.v14.number_rsv = "(10:14)"
psa10.v14.number = "(10:14)"
psa10.v14.text = "Thou dost see; yea, thou dost note trouble and vexation, that thou mayest take it into thy hands; the hapless commits himself to thee; thou hast been the helper of the fatherless."
psa10.v15.number_rsv = "(10:15)"
psa10.v15.number = "(10:15)"
psa10.v15.text = "Break thou the arm of the wicked and evildoer; seek out his wickedness till thou find none."
psa10.v16.number_rsv = "(10:16)"
psa10.v16.number = "(10:16)"
psa10.v16.text = "The LORD is king for ever and ever; the nations shall perish from his land."
psa10.v17.number_rsv = "(10:17)"
psa10.v17.number = "(10:17)"
psa10.v17.text = "O LORD, thou wilt hear the desire of the meek; thou wilt strengthen their heart, thou wilt incline thy ear"
psa10.v18.number_rsv = "(10:18)"
psa10.v18.number = "(10:18)"
psa10.v18.text = "to do justice to the fatherless and the oppressed, so that man who is of the earth may strike terror no more."

psa11.title = "Psalm 11"
psa11.version = properties_en_US_rsv.version.designation
psa11.text = ""
psa11.v0.number_rsv = "(11:0)"
psa11.v0.number = "(11:0)"
psa11.v0.text = "To the choirmaster. Of David."
psa11.v1.number_rsv = "(11:1)"
psa11.v1.number = "(11:1)"
psa11.v1.text = "In the LORD I take refuge; how can you say to me,\"Flee like a bird to the mountains;"
psa11.v2.number_rsv = "(11:2)"
psa11.v2.number = "(11:2)"
psa11.v2.text = "for lo, the wicked bend the bow, they have fitted their arrow to the string, to shoot in the dark at the upright in heart;"
psa11.v3.number_rsv = "(11:3)"
psa11.v3.number = "(11:3)"
psa11.v3.text = "if the foundations are destroyed, what can the righteous do\"?"
psa11.v4.number_rsv = "(11:4)"
psa11.v4.number = "(11:4)"
psa11.v4.text = "The LORD is in his holy temple, the LORD\"s throne is in heaven; his eyes behold, his eyelids test, the children of men."
psa11.v5.number_rsv = "(11:5)"
psa11.v5.number = "(11:5)"
psa11.v5.text = "The LORD tests the righteous and the wicked, and his soul hates him that loves violence."
psa11.v6.number_rsv = "(11:6)"
psa11.v6.number = "(11:6)"
psa11.v6.text = "On the wicked he will rain coals of fire and brimstone; a scorching wind shall be the portion of their cup."
psa11.v7.number_rsv = "(11:7)"
psa11.v7.number = "(11:7)"
psa11.v7.text = "For the LORD is righteous, he loves righteous deeds; the upright shall behold his face."

psa12.title = "Psalm 12"
psa12.version = properties_en_US_rsv.version.designation
psa12.text = ""
psa12.v0.number_rsv = "(12:0)"
psa12.v0.number = "(12:0)"
psa12.v0.text = "To the choirmaster: according to The Sheminith. A Psalm of David."
psa12.v1.number_rsv = "(12:1)"
psa12.v1.number = "(12:1)"
psa12.v1.text = "Help, LORD; for there is no longer any that is godly; for the faithful have vanished from among the sons of men."
psa12.v2.number_rsv = "(12:2)"
psa12.v2.number = "(12:2)"
psa12.v2.text = "Every one utters lies to his neighbor; with flattering lips and a double heart they speak."
psa12.v3.number_rsv = "(12:3)"
psa12.v3.number = "(12:3)"
psa12.v3.text = "May the LORD cut off all flattering lips, the tongue that makes great boasts,"
psa12.v4.number_rsv = "(12:4)"
psa12.v4.number = "(12:4)"
psa12.v4.text = "those who say, \"With our tongue we will prevail, our lips are with us; who is our master?\""
psa12.v5.number_rsv = "(12:5)"
psa12.v5.number = "(12:5)"
psa12.v5.text = "\"Because the poor are despoiled, because the needy groan, I will now arise,\" says the LORD;\"I will place him in the safety for which he longs.\""
psa12.v6.number_rsv = "(12:6)"
psa12.v6.number = "(12:6)"
psa12.v6.text = "The promises of the LORD are promises that are pure, silver refined in a furnace on the ground, purified seven times."
psa12.v7.number_rsv = "(12:7)"
psa12.v7.number = "(12:7)"
psa12.v7.text = "Do thou, O LORD, protect us, guard us ever from this generation."
psa12.v8.number_rsv = "(12:8)"
psa12.v8.number = "(12:8)"
psa12.v8.text = "On every side the wicked prowl, as vileness is exalted among the sons of men."

psa13.title = "Psalm 13"
psa13.version = properties_en_US_rsv.version.designation
psa13.text = ""
psa13.v0.number_rsv = "(13:0)"
psa13.v0.number = "(13:0)"
psa13.v0.text = "To the choirmaster. A Psalm of David."
psa13.v1.number_rsv = "(13:1)"
psa13.v1.number = "(13:1)"
psa13.v1.text = "How long, O LORD? Wilt thou forget me for ever?How long wilt thou hide thy face from me?"
psa13.v2.number_rsv = "(13:2)"
psa13.v2.number = "(13:2)"
psa13.v2.text = "How long must I bear pain in my soul, and have sorrow in my heart all the day?How long shall my enemy be exalted over me?"
psa13.v3.number_rsv = "(13:3)"
psa13.v3.number = "(13:3)"
psa13.v3.text = "Consider and answer me, O LORD my God; lighten my eyes, lest I sleep the sleep of death;"
psa13.v4.number_rsv = "(13:4)"
psa13.v4.number = "(13:4)"
psa13.v4.text = "lest my enemy say, \"I have prevailed over him\"; lest my foes rejoice because I am shaken."
psa13.v5.number_rsv = "(13:5)"
psa13.v5.number = "(13:5)"
psa13.v5.text = "But I have trusted in thy steadfast love; my heart shall rejoice in thy salvation."
psa13.v6.number_rsv = "(13:6)"
psa13.v6.number = "(13:6)"
psa13.v6.text = "I will sing to the LORD, because he has dealt bountifully with me."

psa14.title = "Psalm 14"
psa14.version = properties_en_US_rsv.version.designation
psa14.text = ""
psa14.v0.number_rsv = "(14:0)"
psa14.v0.number = "(14:0)"
psa14.v0.text = "To the choirmaster. Of David."
psa14.v1.number_rsv = "(14:1)"
psa14.v1.number = "(14:1)"
psa14.v1.text = "The fool says in his heart, \"There is no God.\"They are corrupt, they do abominable deeds, there is none that does good."
psa14.v2.number_rsv = "(14:2)"
psa14.v2.number = "(14:2)"
psa14.v2.text = "The LORD looks down from heaven upon the children of men, to see if there are any that act wisely, that seek after God."
psa14.v3.number_rsv = "(14:3)"
psa14.v3.number = "(14:3)"
psa14.v3.text = "They have all gone astray, they are all alike corrupt; there is none that does good, no, not one."
psa14.v4.number_rsv = "(14:4)"
psa14.v4.number = "(14:4)"
psa14.v4.text = "Have they no knowledge, all the evildoerswho eat up my people as they eat bread, and do not call upon the LORD?"
psa14.v5.number_rsv = "(14:5)"
psa14.v5.number = "(14:5)"
psa14.v5.text = "There they shall be in great terror, for God is with the generation of the righteous."
psa14.v6.number_rsv = "(14:6)"
psa14.v6.number = "(14:6)"
psa14.v6.text = "You would confound the plans of the poor, but the LORD is his refuge."
psa14.v7.number_rsv = "(14:7)"
psa14.v7.number = "(14:7)"
psa14.v7.text = "O that deliverance for Israel would come out of Zion! When the LORD restores the fortunes of his people, Jacob shall rejoice, Israel shall be glad.R"

psa15.title = "Psalm 15"
psa15.version = properties_en_US_rsv.version.designation
psa15.text = ""
psa15.v0.number_rsv = "(15:0)"
psa15.v0.number = "(15:0)"
psa15.v0.text = "A Psalm of David."
psa15.v1.number_rsv = "(15:1)"
psa15.v1.number = "(15:1)"
psa15.v1.text = "O LORD, who shall sojourn in thy tent? Who shall dwell on thy holy hill?"
psa15.v2.number_rsv = "(15:2)"
psa15.v2.number = "(15:2)"
psa15.v2.text = "He who walks blamelessly, and does what is right, and speaks truth from his heart;"
psa15.v3.number_rsv = "(15:3)"
psa15.v3.number = "(15:3)"
psa15.v3.text = "who does not slander with his tongue, and does no evil to his friend, nor takes up a reproach against his neighbor;"
psa15.v4.number_rsv = "(15:4)"
psa15.v4.number = "(15:4)"
psa15.v4.text = "in whose eyes a reprobate is despised, but who honors those who fear the LORD; who swears to his own hurt and does not change;"
psa15.v5.number_rsv = "(15:5)"
psa15.v5.number = "(15:5)"
psa15.v5.text = "who does not put out his money at interest, and does not take a bribe against the innocent.He who does these things shall never be moved."

psa16.title = "Psalm 16"
psa16.version = properties_en_US_rsv.version.designation
psa16.text = ""
psa16.v0.number_rsv = "(16:0)"
psa16.v0.number = "(16:0)"
psa16.v0.text = "A Miktam of David."
psa16.v1.number_rsv = "(16:1)"
psa16.v1.number = "(16:1)"
psa16.v1.text = "Preserve me, O God, for in thee I take refuge."
psa16.v2.number_rsv = "(16:2)"
psa16.v2.number = "(16:2)"
psa16.v2.text = "I say to the LORD, \"Thou art my Lord; I have no good apart from thee.\""
psa16.v3.number_rsv = "(16:3)"
psa16.v3.number = "(16:3)"
psa16.v3.text = "As for the saints in the land, they are the noble, in whom is all my delight."
psa16.v4.number_rsv = "(16:4)"
psa16.v4.number = "(16:4)"
psa16.v4.text = "Those who choose another god multiply their sorrows; their libations of blood I will not pour out or take their names upon my lips."
psa16.v5.number_rsv = "(16:5)"
psa16.v5.number = "(16:5)"
psa16.v5.text = "The LORD is my chosen portion and my cup; thou holdest my lot."
psa16.v6.number_rsv = "(16:6)"
psa16.v6.number = "(16:6)"
psa16.v6.text = "The lines have fallen for me in pleasant places; yea, I have a goodly heritage."
psa16.v7.number_rsv = "(16:7)"
psa16.v7.number = "(16:7)"
psa16.v7.text = "I bless the LORD who gives me counsel; in the night also my heart instructs me."
psa16.v8.number_rsv = "(16:8)"
psa16.v8.number = "(16:8)"
psa16.v8.text = "I keep the LORD always before me; because he is at my right hand, I shall not be moved."
psa16.v9.number_rsv = "(16:9)"
psa16.v9.number = "(16:9)"
psa16.v9.text = "Therefore my heart is glad, and my soul rejoices; my body also dwells secure."
psa16.v10.number_rsv = "(16:10)"
psa16.v10.number = "(16:10)"
psa16.v10.text = "For thou dost not give me up to Sheol, or let thy godly one see the Pit."
psa16.v11.number_rsv = "(16:11)"
psa16.v11.number = "(16:11)"
psa16.v11.text = "Thou dost show me the path of life; in thy presence there is fulness of joy, in thy right hand are pleasures for evermore."

psa17.title = "Psalm 17"
psa17.version = properties_en_US_rsv.version.designation
psa17.text = ""
psa17.v0.number_rsv = "(17:0)"
psa17.v0.number = "(17:0)"
psa17.v0.text = "A Prayer of David."
psa17.v1.number_rsv = "(17:1)"
psa17.v1.number = "(17:1)"
psa17.v1.text = "Hear a just cause, O LORD; attend to my cry! Give ear to my prayer from lips free of deceit!"
psa17.v2.number_rsv = "(17:2)"
psa17.v2.number = "(17:2)"
psa17.v2.text = "From thee let my vindication come! Let thy eyes see the right!"
psa17.v3.number_rsv = "(17:3)"
psa17.v3.number = "(17:3)"
psa17.v3.text = "If thou triest my heart, if thou visitest me by night, if thou testest me, thou wilt find no wickedness in me; my mouth does not transgress."
psa17.v4.number_rsv = "(17:4)"
psa17.v4.number = "(17:4)"
psa17.v4.text = "With regard to the works of men, by the word of thy lipsI have avoided the ways of the violent."
psa17.v5.number_rsv = "(17:5)"
psa17.v5.number = "(17:5)"
psa17.v5.text = "My steps have held fast to thy paths, my feet have not slipped."
psa17.v6.number_rsv = "(17:6)"
psa17.v6.number = "(17:6)"
psa17.v6.text = "I call upon thee, for thou wilt answer me, O God; incline thy ear to me, hear my words."
psa17.v7.number_rsv = "(17:7)"
psa17.v7.number = "(17:7)"
psa17.v7.text = "Wondrously show thy steadfast love, O savior of those who seek refugefrom their adversaries at thy right hand."
psa17.v8.number_rsv = "(17:8)"
psa17.v8.number = "(17:8)"
psa17.v8.text = "Keep me as the apple of the eye; hide me in the shadow of thy wings,"
psa17.v9.number_rsv = "(17:9)"
psa17.v9.number = "(17:9)"
psa17.v9.text = "from the wicked who despoil me, my deadly enemies who surround me."
psa17.v10.number_rsv = "(17:10)"
psa17.v10.number = "(17:10)"
psa17.v10.text = "They close their hearts to pity; with their mouths they speak arrogantly."
psa17.v11.number_rsv = "(17:11)"
psa17.v11.number = "(17:11)"
psa17.v11.text = "They track me down; now they surround me; they set their eyes to cast me to the ground."
psa17.v12.number_rsv = "(17:12)"
psa17.v12.number = "(17:12)"
psa17.v12.text = "They are like a lion eager to tear, as a young lion lurking in ambush."
psa17.v13.number_rsv = "(17:13)"
psa17.v13.number = "(17:13)"
psa17.v13.text = "Arise, O LORD! confront them, overthrow them! Deliver my life from the wicked by thy sword,"
psa17.v14.number_rsv = "(17:14)"
psa17.v14.number = "(17:14)"
psa17.v14.text = "from men by thy hand, O LORD, from men whose portion in life is of the world.May their belly be filled with what thou hast stored up for them; may their children have more than enough; may they leave something over to their babes."
psa17.v15.number_rsv = "(17:15)"
psa17.v15.number = "(17:15)"
psa17.v15.text = "As for me, I shall behold thy face in righteousness; when I awake, I shall be satisfied with beholding thy form."

kathisma_3.title = "KATHISMA 3."

psa18.title = "Psalm 18"
psa18.version = properties_en_US_rsv.version.designation
psa18.text = ""
psa18.v0.number_rsv = "(18:0)"
psa18.v0.number = "(18:0)"
psa18.v0.text = "To the choirmaster. A Psalm of David the servant of the LORD, who addressed the words of this song to the LORD on the day when the LORD delivered him from the hand of all his enemies, and from the hand of Saul. He said:"
psa18.v1.number_rsv = "(18:1)"
psa18.v1.number = "(18:1)"
psa18.v1.text = "I love thee, O LORD, my strength."
psa18.v2.number_rsv = "(18:2)"
psa18.v2.number = "(18:2)"
psa18.v2.text = "The LORD is my rock, and my fortress, and my deliverer, my God, my rock, in whom I take refuge, my shield, and the horn of my salvation, my stronghold."
psa18.v3.number_rsv = "(18:3)"
psa18.v3.number = "(18:3)"
psa18.v3.text = "I call upon the LORD, who is worthy to be praised, and I am saved from my enemies."
psa18.v4.number_rsv = "(18:4)"
psa18.v4.number = "(18:4)"
psa18.v4.text = "The cords of death encompassed me, the torrents of perdition assailed me;"
psa18.v5.number_rsv = "(18:5)"
psa18.v5.number = "(18:5)"
psa18.v5.text = "the cords of Sheol entangled me, the snares of death confronted me."
psa18.v6.number_rsv = "(18:6)"
psa18.v6.number = "(18:6)"
psa18.v6.text = "In my distress I called upon the LORD; to my God I cried for help.From his temple he heard my voice, and my cry to him reached his ears."
psa18.v7.number_rsv = "(18:7)"
psa18.v7.number = "(18:7)"
psa18.v7.text = "Then the earth reeled and rocked; the foundations also of the mountains trembledand quaked, because he was angry."
psa18.v8.number_rsv = "(18:8)"
psa18.v8.number = "(18:8)"
psa18.v8.text = "Smoke went up from his nostrils, and devouring fire from his mouth; glowing coals flamed forth from him."
psa18.v9.number_rsv = "(18:9)"
psa18.v9.number = "(18:9)"
psa18.v9.text = "He bowed the heavens, and came down; thick darkness was under his feet."
psa18.v10.number_rsv = "(18:10)"
psa18.v10.number = "(18:10)"
psa18.v10.text = "He rode on a cherub, and flew; he came swiftly upon the wings of the wind."
psa18.v11.number_rsv = "(18:11)"
psa18.v11.number = "(18:11)"
psa18.v11.text = "He made darkness his covering around him, his canopy thick clouds dark with water."
psa18.v12.number_rsv = "(18:12)"
psa18.v12.number = "(18:12)"
psa18.v12.text = "Out of the brightness before himthere broke through his cloudshailstones and coals of fire."
psa18.v13.number_rsv = "(18:13)"
psa18.v13.number = "(18:13)"
psa18.v13.text = "The LORD also thundered in the heavens, and the Most High uttered his voice, hailstones and coals of fire."
psa18.v14.number_rsv = "(18:14)"
psa18.v14.number = "(18:14)"
psa18.v14.text = "And he sent out his arrows, and scattered them; he flashed forth lightnings, and routed them."
psa18.v15.number_rsv = "(18:15)"
psa18.v15.number = "(18:15)"
psa18.v15.text = "Then the channels of the sea were seen, and the foundations of the world were laid bare, at thy rebuke, O LORD, at the blast of the breath of thy nostrils."
psa18.v16.number_rsv = "(18:16)"
psa18.v16.number = "(18:16)"
psa18.v16.text = "He reached from on high, he took me, he drew me out of many waters."
psa18.v17.number_rsv = "(18:17)"
psa18.v17.number = "(18:17)"
psa18.v17.text = "He delivered me from my strong enemy, and from those who hated me; for they were too mighty for me."
psa18.v18.number_rsv = "(18:18)"
psa18.v18.number = "(18:18)"
psa18.v18.text = "They came upon me in the day of my calamity; but the LORD was my stay."
psa18.v19.number_rsv = "(18:19)"
psa18.v19.number = "(18:19)"
psa18.v19.text = "He brought me forth into a broad place; he delivered me, because he delighted in me."
psa18.v20.number_rsv = "(18:20)"
psa18.v20.number = "(18:20)"
psa18.v20.text = "The LORD rewarded me according to my righteousness; according to the cleanness of my hands he recompensed me."
psa18.v21.number_rsv = "(18:21)"
psa18.v21.number = "(18:21)"
psa18.v21.text = "For I have kept the ways of the LORD, and have not wickedly departed from my God."
psa18.v22.number_rsv = "(18:22)"
psa18.v22.number = "(18:22)"
psa18.v22.text = "For all his ordinances were before me, and his statutes I did not put away from me."
psa18.v23.number_rsv = "(18:23)"
psa18.v23.number = "(18:23)"
psa18.v23.text = "I was blameless before him, and I kept myself from guilt."
psa18.v24.number_rsv = "(18:24)"
psa18.v24.number = "(18:24)"
psa18.v24.text = "Therefore the LORD has recompensed me according to my righteousness, according to the cleanness of my hands in his sight."
psa18.v25.number_rsv = "(18:25)"
psa18.v25.number = "(18:25)"
psa18.v25.text = "With the loyal thou dost show thyself loyal; with the blameless man thou dost show thyself blameless;"
psa18.v26.number_rsv = "(18:26)"
psa18.v26.number = "(18:26)"
psa18.v26.text = "with the pure thou dost show thyself pure; and with the crooked thou dost show thyself perverse."
psa18.v27.number_rsv = "(18:27)"
psa18.v27.number = "(18:27)"
psa18.v27.text = "For thou dost deliver a humble people; but the haughty eyes thou dost bring down."
psa18.v28.number_rsv = "(18:28)"
psa18.v28.number = "(18:28)"
psa18.v28.text = "Yea, thou dost light my lamp; the LORD my God lightens my darkness."
psa18.v29.number_rsv = "(18:29)"
psa18.v29.number = "(18:29)"
psa18.v29.text = "Yea, by thee I can crush a troop; and by my God I can leap over a wall."
psa18.v30.number_rsv = "(18:30)"
psa18.v30.number = "(18:30)"
psa18.v30.text = "This God -- his way is perfect; the promise of the LORD proves true; he is a shield for all those who take refuge in him."
psa18.v31.number_rsv = "(18:31)"
psa18.v31.number = "(18:31)"
psa18.v31.text = "For who is God, but the LORD? And who is a rock, except our God? --"
psa18.v32.number_rsv = "(18:32)"
psa18.v32.number = "(18:32)"
psa18.v32.text = "the God who girded me with strength, and made my way safe."
psa18.v33.number_rsv = "(18:33)"
psa18.v33.number = "(18:33)"
psa18.v33.text = "He made my feet like hinds\" feet, and set me secure on the heights."
psa18.v34.number_rsv = "(18:34)"
psa18.v34.number = "(18:34)"
psa18.v34.text = "He trains my hands for war, so that my arms can bend a bow of bronze."
psa18.v35.number_rsv = "(18:35)"
psa18.v35.number = "(18:35)"
psa18.v35.text = "Thou hast given me the shield of thy salvation, and thy right hand supported me, and thy help made me great."
psa18.v36.number_rsv = "(18:36)"
psa18.v36.number = "(18:36)"
psa18.v36.text = "Thou didst give a wide place for my steps under me, and my feet did not slip."
psa18.v37.number_rsv = "(18:37)"
psa18.v37.number = "(18:37)"
psa18.v37.text = "I pursued my enemies and overtook them; and did not turn back till they were consumed."
psa18.v38.number_rsv = "(18:38)"
psa18.v38.number = "(18:38)"
psa18.v38.text = "I thrust them through, so that they were not able to rise; they fell under my feet."
psa18.v39.number_rsv = "(18:39)"
psa18.v39.number = "(18:39)"
psa18.v39.text = "For thou didst gird me with strength for the battle; thou didst make my assailants sink under me."
psa18.v40.number_rsv = "(18:40)"
psa18.v40.number = "(18:40)"
psa18.v40.text = "Thou didst make my enemies turn their backs to me, and those who hated me I destroyed."
psa18.v41.number_rsv = "(18:41)"
psa18.v41.number = "(18:41)"
psa18.v41.text = "They cried for help, but there was none to save, they cried to the LORD, but he did not answer them."
psa18.v42.number_rsv = "(18:42)"
psa18.v42.number = "(18:42)"
psa18.v42.text = "I beat them fine as dust before the wind; I cast them out like the mire of the streets."
psa18.v43.number_rsv = "(18:43)"
psa18.v43.number = "(18:43)"
psa18.v43.text = "Thou didst deliver me from strife with the peoples; thou didst make me the head of the nations; people whom I had not known served me."
psa18.v44.number_rsv = "(18:44)"
psa18.v44.number = "(18:44)"
psa18.v44.text = "As soon as they heard of me they obeyed me; foreigners came cringing to me."
psa18.v45.number_rsv = "(18:45)"
psa18.v45.number = "(18:45)"
psa18.v45.text = "Foreigners lost heart, and came trembling out of their fastnesses."
psa18.v46.number_rsv = "(18:46)"
psa18.v46.number = "(18:46)"
psa18.v46.text = "The LORD lives; and blessed be my rock, and exalted be the God of my salvation,"
psa18.v47.number_rsv = "(18:47)"
psa18.v47.number = "(18:47)"
psa18.v47.text = "the God who gave me vengeanceand subdued peoples under me;"
psa18.v48.number_rsv = "(18:48)"
psa18.v48.number = "(18:48)"
psa18.v48.text = "who delivered me from my enemies; yea, thou didst exalt me above my adversaries; thou didst deliver me from men of violence."
psa18.v49.number_rsv = "(18:49)"
psa18.v49.number = "(18:49)"
psa18.v49.text = "For this I will extol thee, O LORD, among the nations, and sing praises to thy name."
psa18.v50.number_rsv = "(18:50)"
psa18.v50.number = "(18:50)"
psa18.v50.text = "Great triumphs he gives to his king, and shows steadfast love to his anointed, to David and his descendants for ever."

psa19.title = "Psalm 19"
psa19.version = properties_en_US_rsv.version.designation
psa19.text = ""
psa19.v0.number_rsv = "(19:0)"
psa19.v0.number = "(19:0)"
psa19.v0.text = "To the choirmaster. A Psalm of David."
psa19.v1.number_rsv = "(19:1)"
psa19.v1.number = "(19:1)"
psa19.v1.text = "The heavens are telling the glory of God; and the firmament proclaims his handiwork."
psa19.v2.number_rsv = "(19:2)"
psa19.v2.number = "(19:2)"
psa19.v2.text = "Day to day pours forth speech, and night to night declares knowledge."
psa19.v3.number_rsv = "(19:3)"
psa19.v3.number = "(19:3)"
psa19.v3.text = "There is no speech, nor are there words; their voice is not heard;"
psa19.v4.number_rsv = "(19:4)"
psa19.v4.number = "(19:4)"
psa19.v4.text = "yet their voice goes out through all the earth, and their words to the end of the world.In them he has set a tent for the sun,"
psa19.v5.number_rsv = "(19:5)"
psa19.v5.number = "(19:5)"
psa19.v5.text = "which comes forth like a bridegroom leaving his chamber, and like a strong man runs its course with joy."
psa19.v6.number_rsv = "(19:6)"
psa19.v6.number = "(19:6)"
psa19.v6.text = "Its rising is from the end of the heavens, and its circuit to the end of them; and there is nothing hid from its heat."
psa19.v7.number_rsv = "(19:7)"
psa19.v7.number = "(19:7)"
psa19.v7.text = "The law of the LORD is perfect, reviving the soul; the testimony of the LORD is sure, making wise the simple;"
psa19.v8.number_rsv = "(19:8)"
psa19.v8.number = "(19:8)"
psa19.v8.text = "the precepts of the LORD are right, rejoicing the heart; the commandment of the LORD is pure, enlightening the eyes;"
psa19.v9.number_rsv = "(19:9)"
psa19.v9.number = "(19:9)"
psa19.v9.text = "the fear of the LORD is clean, enduring for ever; the ordinances of the LORD are true, and righteous altogether."
psa19.v10.number_rsv = "(19:10)"
psa19.v10.number = "(19:10)"
psa19.v10.text = "More to be desired are they than gold, even much fine gold; sweeter also than honeyand drippings of the honeycomb."
psa19.v11.number_rsv = "(19:11)"
psa19.v11.number = "(19:11)"
psa19.v11.text = "Moreover by them is thy servant warned; in keeping them there is great reward."
psa19.v12.number_rsv = "(19:12)"
psa19.v12.number = "(19:12)"
psa19.v12.text = "But who can discern his errors? Clear thou me from hidden faults."
psa19.v13.number_rsv = "(19:13)"
psa19.v13.number = "(19:13)"
psa19.v13.text = "Keep back thy servant also from presumptuous sins; let them not have dominion over me! Then I shall be blameless, and innocent of great transgression."
psa19.v14.number_rsv = "(19:14)"
psa19.v14.number = "(19:14)"
psa19.v14.text = "Let the words of my mouth and the meditation of my heartbe acceptable in thy sight, O LORD, my rock and my redeemer."

psa20.title = "Psalm 20"
psa20.version = properties_en_US_rsv.version.designation
psa20.text = ""
psa20.v0.number_rsv = "(20:0)"
psa20.v0.number = "(20:0)"
psa20.v0.text = "To the choirmaster. A Psalm of David."
psa20.v1.number_rsv = "(20:1)"
psa20.v1.number = "(20:1)"
psa20.v1.text = "The LORD answer you in the day of trouble! The name of the God of Jacob protect you!"
psa20.v2.number_rsv = "(20:2)"
psa20.v2.number = "(20:2)"
psa20.v2.text = "May he send you help from the sanctuary, and give you support from Zion!"
psa20.v3.number_rsv = "(20:3)"
psa20.v3.number = "(20:3)"
psa20.v3.text = "May he remember all your offerings, and regard with favor your burnt sacrifices!"
psa20.v4.number_rsv = "(20:4)"
psa20.v4.number = "(20:4)"
psa20.v4.text = "May he grant you your heart\"s desire, and fulfil all your plans!"
psa20.v5.number_rsv = "(20:5)"
psa20.v5.number = "(20:5)"
psa20.v5.text = "May we shout for joy over your victory, and in the name of our God set up our banners! May the LORD fulfil all your petitions!"
psa20.v6.number_rsv = "(20:6)"
psa20.v6.number = "(20:6)"
psa20.v6.text = "Now I know that the LORD will help his anointed; he will answer him from his holy heavenwith mighty victories by his right hand."
psa20.v7.number_rsv = "(20:7)"
psa20.v7.number = "(20:7)"
psa20.v7.text = "Some boast of chariots, and some of horses; but we boast of the name of the LORD our God."
psa20.v8.number_rsv = "(20:8)"
psa20.v8.number = "(20:8)"
psa20.v8.text = "They will collapse and fall; but we shall rise and stand upright."
psa20.v9.number_rsv = "(20:9)"
psa20.v9.number = "(20:9)"
psa20.v9.text = "Give victory to the king, O LORD; answer us when we call."

psa21.title = "Psalm 21"
psa21.version = properties_en_US_rsv.version.designation
psa21.text = ""
psa21.v0.number_rsv = "(21:0)"
psa21.v0.number = "(21:0)"
psa21.v0.text = "To the choirmaster. A Psalm of David."
psa21.v1.number_rsv = "(21:1)"
psa21.v1.number = "(21:1)"
psa21.v1.text = "In thy strength the king rejoices, O LORD; and in thy help how greatly he exults!"
psa21.v2.number_rsv = "(21:2)"
psa21.v2.number = "(21:2)"
psa21.v2.text = "Thou hast given him his heart\"s desire, and hast not withheld the request of his li"
ps. 
psa21.v3.number_rsv = "(21:3)"
psa21.v3.number = "(21:3)"
psa21.v3.text = "For thou dost meet him with goodly blessings; thou dost set a crown of fine gold upon his head."
psa21.v4.number_rsv = "(21:4)"
psa21.v4.number = "(21:4)"
psa21.v4.text = "He asked life of thee; thou gavest it to him, length of days for ever and ever."
psa21.v5.number_rsv = "(21:5)"
psa21.v5.number = "(21:5)"
psa21.v5.text = "His glory is great through thy help; splendor and majesty thou dost bestow upon him."
psa21.v6.number_rsv = "(21:6)"
psa21.v6.number = "(21:6)"
psa21.v6.text = "Yea, thou dost make him most blessed for ever; thou dost make him glad with the joy of thy presence."
psa21.v7.number_rsv = "(21:7)"
psa21.v7.number = "(21:7)"
psa21.v7.text = "For the king trusts in the LORD; and through the steadfast love of the Most High he shall not be moved."
psa21.v8.number_rsv = "(21:8)"
psa21.v8.number = "(21:8)"
psa21.v8.text = "Your hand will find out all your enemies; your right hand will find out those who hate you."
psa21.v9.number_rsv = "(21:9)"
psa21.v9.number = "(21:9)"
psa21.v9.text = "You will make them as a blazing ovenwhen you appear.The LORD will swallow them up in his wrath; and fire will consume them."
psa21.v10.number_rsv = "(21:10)"
psa21.v10.number = "(21:10)"
psa21.v10.text = "You will destroy their offspring from the earth, and their children from among the sons of men."
psa21.v11.number_rsv = "(21:11)"
psa21.v11.number = "(21:11)"
psa21.v11.text = "If they plan evil against you, if they devise mischief, they will not succeed."
psa21.v12.number_rsv = "(21:12)"
psa21.v12.number = "(21:12)"
psa21.v12.text = "For you will put them to flight; you will aim at their faces with your bows."
psa21.v13.number_rsv = "(21:13)"
psa21.v13.number = "(21:13)"
psa21.v13.text = "Be exalted, O LORD, in thy strength! We will sing and praise thy power."

psa22.title = "Psalm 22"
psa22.version = properties_en_US_rsv.version.designation
psa22.text = ""
psa22.v0.number_rsv = "(22:0)"
psa22.v0.number = "(22:0)"
psa22.v0.text = "To the choirmaster: according to The Hind of the Dawn. A Psalm of David."
psa22.v1.number_rsv = "(22:1)"
psa22.v1.number = "(22:1)"
psa22.v1.text = "My God, my God, why hast thou forsaken me? Why art thou so far from helping me, from the words of my groaning?"
psa22.v2.number_rsv = "(22:2)"
psa22.v2.number = "(22:2)"
psa22.v2.text = "O my God, I cry by day, but thou dost not answer; and by night, but find no rest."
psa22.v3.number_rsv = "(22:3)"
psa22.v3.number = "(22:3)"
psa22.v3.text = "Yet thou art holy, enthroned on the praises of Israel."
psa22.v4.number_rsv = "(22:4)"
psa22.v4.number = "(22:4)"
psa22.v4.text = "In thee our fathers trusted; they trusted, and thou didst deliver them."
psa22.v5.number_rsv = "(22:5)"
psa22.v5.number = "(22:5)"
psa22.v5.text = "To thee they cried, and were saved; in thee they trusted, and were not disappointed."
psa22.v6.number_rsv = "(22:6)"
psa22.v6.number = "(22:6)"
psa22.v6.text = "But I am a worm, and no man; scorned by men, and despised by the people."
psa22.v7.number_rsv = "(22:7)"
psa22.v7.number = "(22:7)"
psa22.v7.text = "All who see me mock at me, they make mouths at me, they wag their heads;"
psa22.v8.number_rsv = "(22:8)"
psa22.v8.number = "(22:8)"
psa22.v8.text = "\"He committed his cause to the LORD; let him deliver him, let him rescue him, for he delights in him!\""
psa22.v9.number_rsv = "(22:9)"
psa22.v9.number = "(22:9)"
psa22.v9.text = "Yet thou art he who took me from the womb; thou didst keep me safe upon my mother\"s breasts."
psa22.v10.number_rsv = "(22:10)"
psa22.v10.number = "(22:10)"
psa22.v10.text = "Upon thee was I cast from my birth, and since my mother bore me thou hast been my God."
psa22.v11.number_rsv = "(22:11)"
psa22.v11.number = "(22:11)"
psa22.v11.text = "Be not far from me, for trouble is nearand there is none to help."
psa22.v12.number_rsv = "(22:12)"
psa22.v12.number = "(22:12)"
psa22.v12.text = "Many bulls encompass me, strong bulls of Bashan surround me;"
psa22.v13.number_rsv = "(22:13)"
psa22.v13.number = "(22:13)"
psa22.v13.text = "they open wide their mouths at me, like a ravening and roaring lion."
psa22.v14.number_rsv = "(22:14)"
psa22.v14.number = "(22:14)"
psa22.v14.text = "I am poured out like water, and all my bones are out of joint; my heart is like wax, it is melted within my breast;"
psa22.v15.number_rsv = "(22:15)"
psa22.v15.number = "(22:15)"
psa22.v15.text = "my strength is dried up like a potsherd, and my tongue cleaves to my jaws; thou dost lay me in the dust of death."
psa22.v16.number_rsv = "(22:16)"
psa22.v16.number = "(22:16)"
psa22.v16.text = "Yea, dogs are round about me; a company of evildoers encircle me; they have pierced my hands and feet --"
psa22.v17.number_rsv = "(22:17)"
psa22.v17.number = "(22:17)"
psa22.v17.text = "I can count all my bones --they stare and gloat over me;"
psa22.v18.number_rsv = "(22:18)"
psa22.v18.number = "(22:18)"
psa22.v18.text = "they divide my garments among them, and for my raiment they cast lots."
psa22.v19.number_rsv = "(22:19)"
psa22.v19.number = "(22:19)"
psa22.v19.text = "But thou, O LORD, be not far off! O thou my help, hasten to my aid!"
psa22.v20.number_rsv = "(22:20)"
psa22.v20.number = "(22:20)"
psa22.v20.text = "Deliver my soul from the sword, my life from the power of the dog!"
psa22.v21.number_rsv = "(22:21)"
psa22.v21.number = "(22:21)"
psa22.v21.text = "Save me from the mouth of the lion, my afflicted soul from the horns of the wild oxen!"
psa22.v22.number_rsv = "(22:22)"
psa22.v22.number = "(22:22)"
psa22.v22.text = "I will tell of thy name to my brethren; in the midst of the congregation I will praise thee:"
psa22.v23.number_rsv = "(22:23)"
psa22.v23.number = "(22:23)"
psa22.v23.text = "You who fear the LORD, praise him! all you sons of Jacob, glorify him, and stand in awe of him, all you sons of Israel!"
psa22.v24.number_rsv = "(22:24)"
psa22.v24.number = "(22:24)"
psa22.v24.text = "For he has not despised or abhorredthe affliction of the afflicted; and he has not hid his face from him, but has heard, when he cried to him."
psa22.v25.number_rsv = "(22:25)"
psa22.v25.number = "(22:25)"
psa22.v25.text = "From thee comes my praise in the great congregation; my vows I will pay before those who fear him."
psa22.v26.number_rsv = "(22:26)"
psa22.v26.number = "(22:26)"
psa22.v26.text = "The afflicted shall eat and be satisfied; those who seek him shall praise the LORD! May your hearts live for ever!"
psa22.v27.number_rsv = "(22:27)"
psa22.v27.number = "(22:27)"
psa22.v27.text = "All the ends of the earth shall rememberand turn to the LORD; and all the families of the nationsshall worship before him."
psa22.v28.number_rsv = "(22:28)"
psa22.v28.number = "(22:28)"
psa22.v28.text = "For dominion belongs to the LORD, and he rules over the nations."
psa22.v29.number_rsv = "(22:29)"
psa22.v29.number = "(22:29)"
psa22.v29.text = "Yea, to him shall all the proud of the earth bow down; before him shall bow all who go down to the dust, and he who cannot keep himself alive."
psa22.v30.number_rsv = "(22:30)"
psa22.v30.number = "(22:30)"
psa22.v30.text = "Posterity shall serve him; men shall tell of the Lord to the coming generation,"
psa22.v31.number_rsv = "(22:31)"
psa22.v31.number = "(22:31)"
psa22.v31.text = "and proclaim his deliverance to a people yet unborn, that he has wrought it."

psa23.title = "Psalm 23"
psa23.version = properties_en_US_rsv.version.designation
psa23.text = ""
psa23.v0.number_rsv = "(23:0)"
psa23.v0.number = "(23:0)"
psa23.v0.text = "A Psalm of David."
psa23.v1.number_rsv = "(23:1)"
psa23.v1.number = "(23:1)"
psa23.v1.text = "The LORD is my shepherd, I shall not want;"
psa23.v2.number_rsv = "(23:2)"
psa23.v2.number = "(23:2)"
psa23.v2.text = "he makes me lie down in green pastures.He leads me beside still waters;"
psa23.v3.number_rsv = "(23:3)"
psa23.v3.number = "(23:3)"
psa23.v3.text = "he restores my soul.He leads me in paths of righteousnessfor his name\"s sake."
psa23.v4.number_rsv = "(23:4)"
psa23.v4.number = "(23:4)"
psa23.v4.text = "Even though I walk through the valley of the shadow of death, I fear no evil; for thou art with me; thy rod and thy staff, they comfort me."
psa23.v5.number_rsv = "(23:5)"
psa23.v5.number = "(23:5)"
psa23.v5.text = "Thou preparest a table before mein the presence of my enemies; thou anointest my head with oil, my cup overflows."
psa23.v6.number_rsv = "(23:6)"
psa23.v6.number = "(23:6)"
psa23.v6.text = "Surely goodness and mercy shall follow meall the days of my life; and I shall dwell in the house of the LORDfor ever."

psa24.title = "Psalm 24"
psa24.version = properties_en_US_rsv.version.designation
psa24.text = ""
psa24.v0.number_rsv = "(24:0)"
psa24.v0.number = "(24:0)"
psa24.v0.text = "A Psalm of David."
psa24.v1.number_rsv = "(24:1)"
psa24.v1.number = "(24:1)"
psa24.v1.text = "The earth is the LORD\"s and the fulness thereof, the world and those who dwell therein;"
psa24.v2.number_rsv = "(24:2)"
psa24.v2.number = "(24:2)"
psa24.v2.text = "for he has founded it upon the seas, and established it upon the rivers."
psa24.v3.number_rsv = "(24:3)"
psa24.v3.number = "(24:3)"
psa24.v3.text = "Who shall ascend the hill of the LORD? And who shall stand in his holy place?"
psa24.v4.number_rsv = "(24:4)"
psa24.v4.number = "(24:4)"
psa24.v4.text = "He who has clean hands and a pure heart, who does not lift up his soul to what is false, and does not swear deceitfully."
psa24.v5.number_rsv = "(24:5)"
psa24.v5.number = "(24:5)"
psa24.v5.text = "He will receive blessing from the LORD, and vindication from the God of his salvation."
psa24.v6.number_rsv = "(24:6)"
psa24.v6.number = "(24:6)"
psa24.v6.text = "Such is the generation of those who seek him, who seek the face of the God of Jacob."
psa24.v7.number_rsv = "(24:7)"
psa24.v7.number = "(24:7)"
psa24.v7.text = "Lift up your heads, O gates! and be lifted up, O ancient doors! that the King of glory may come in."
psa24.v8.number_rsv = "(24:8)"
psa24.v8.number = "(24:8)"
psa24.v8.text = "Who is the King of glory? The LORD, strong and mighty, the LORD, mighty in battle!"
psa24.v9.number_rsv = "(24:9)"
psa24.v9.number = "(24:9)"
psa24.v9.text = "Lift up your heads, O gates! and be lifted up, O ancient doors! that the King of glory may come in."
psa24.v10.number_rsv = "(24:10)"
psa24.v10.number = "(24:10)"
psa24.v10.text = "Who is this King of glory? The LORD of hosts, he is the King of glory! R"

kathisma_4.title = "KATHISMA 4."

psa25.title = "Psalm 25"
psa25.version = properties_en_US_rsv.version.designation
psa25.text = ""
psa25.v0.number_rsv = "(25:0)"
psa25.v0.number = "(25:0)"
psa25.v0.text = "A Psalm of David."
psa25.v1.number_rsv = "(25:1)"
psa25.v1.number = "(25:1)"
psa25.v1.text = "To thee, O LORD, I lift up my soul."
psa25.v2.number_rsv = "(25:2)"
psa25.v2.number = "(25:2)"
psa25.v2.text = "O my God, in thee I trust, let me not be put to shame; let not my enemies exult over me."
psa25.v3.number_rsv = "(25:3)"
psa25.v3.number = "(25:3)"
psa25.v3.text = "Yea, let none that wait for thee be put to shame; let them be ashamed who are wantonly treacherous."
psa25.v4.number_rsv = "(25:4)"
psa25.v4.number = "(25:4)"
psa25.v4.text = "Make me to know thy ways, O LORD; teach me thy paths."
psa25.v5.number_rsv = "(25:5)"
psa25.v5.number = "(25:5)"
psa25.v5.text = "Lead me in thy truth, and teach me, for thou art the God of my salvation; for thee I wait all the day long."
psa25.v6.number_rsv = "(25:6)"
psa25.v6.number = "(25:6)"
psa25.v6.text = "Be mindful of thy mercy, O LORD, and of thy steadfast love, for they have been from of old."
psa25.v7.number_rsv = "(25:7)"
psa25.v7.number = "(25:7)"
psa25.v7.text = "Remember not the sins of my youth, or my transgressions; according to thy steadfast love remember me, for thy goodness\" sake, O LORD!"
psa25.v8.number_rsv = "(25:8)"
psa25.v8.number = "(25:8)"
psa25.v8.text = "Good and upright is the LORD; therefore he instructs sinners in the way."
psa25.v9.number_rsv = "(25:9)"
psa25.v9.number = "(25:9)"
psa25.v9.text = "He leads the humble in what is right, and teaches the humble his way."
psa25.v10.number_rsv = "(25:10)"
psa25.v10.number = "(25:10)"
psa25.v10.text = "All the paths of the LORD are steadfast love and faithfulness, for those who keep his covenant and his testimonies."
psa25.v11.number_rsv = "(25:11)"
psa25.v11.number = "(25:11)"
psa25.v11.text = "For thy name\"s sake, O LORD, pardon my guilt, for it is great."
psa25.v12.number_rsv = "(25:12)"
psa25.v12.number = "(25:12)"
psa25.v12.text = "Who is the man that fears the LORD? Him will he instruct in the way that he should choose."
psa25.v13.number_rsv = "(25:13)"
psa25.v13.number = "(25:13)"
psa25.v13.text = "He himself shall abide in prosperity, and his children shall possess the land."
psa25.v14.number_rsv = "(25:14)"
psa25.v14.number = "(25:14)"
psa25.v14.text = "The friendship of the LORD is for those who fear him, and he makes known to them his covenant."
psa25.v15.number_rsv = "(25:15)"
psa25.v15.number = "(25:15)"
psa25.v15.text = "My eyes are ever toward the LORD, for he will pluck my feet out of the net."
psa25.v16.number_rsv = "(25:16)"
psa25.v16.number = "(25:16)"
psa25.v16.text = "Turn thou to me, and be gracious to me; for I am lonely and afflicted."
psa25.v17.number_rsv = "(25:17)"
psa25.v17.number = "(25:17)"
psa25.v17.text = "Relieve the troubles of my heart, and bring me out of my distresses."
psa25.v18.number_rsv = "(25:18)"
psa25.v18.number = "(25:18)"
psa25.v18.text = "Consider my affliction and my trouble, and forgive all my sins."
psa25.v19.number_rsv = "(25:19)"
psa25.v19.number = "(25:19)"
psa25.v19.text = "Consider how many are my foes, and with what violent hatred they hate me."
psa25.v20.number_rsv = "(25:20)"
psa25.v20.number = "(25:20)"
psa25.v20.text = "Oh guard my life, and deliver me; let me not be put to shame, for I take refuge in thee."
psa25.v21.number_rsv = "(25:21)"
psa25.v21.number = "(25:21)"
psa25.v21.text = "May integrity and uprightness preserve me, for I wait for thee."
psa25.v22.number_rsv = "(25:22)"
psa25.v22.number = "(25:22)"
psa25.v22.text = "Redeem Israel, O God, out of all his troubles."

psa26.title = "Psalm 26"
psa26.version = properties_en_US_rsv.version.designation
psa26.text = ""
psa26.v0.number_rsv = "(26:0)"
psa26.v0.number = "(26:0)"
psa26.v0.text = "A Psalm of David."
psa26.v1.number_rsv = "(26:1)"
psa26.v1.number = "(26:1)"
psa26.v1.text = "Vindicate me, O LORD, for I have walked in my integrity, and I have trusted in the LORD without wavering."
psa26.v2.number_rsv = "(26:2)"
psa26.v2.number = "(26:2)"
psa26.v2.text = "Prove me, O LORD, and try me; test my heart and my mind."
psa26.v3.number_rsv = "(26:3)"
psa26.v3.number = "(26:3)"
psa26.v3.text = "For thy steadfast love is before my eyes, and I walk in faithfulness to thee."
psa26.v4.number_rsv = "(26:4)"
psa26.v4.number = "(26:4)"
psa26.v4.text = "I do not sit with false men, nor do I consort with dissemblers;"
psa26.v5.number_rsv = "(26:5)"
psa26.v5.number = "(26:5)"
psa26.v5.text = "I hate the company of evildoers, and I will not sit with the wicked."
psa26.v6.number_rsv = "(26:6)"
psa26.v6.number = "(26:6)"
psa26.v6.text = "I wash my hands in innocence, and go about thy altar, O LORD,"
psa26.v7.number_rsv = "(26:7)"
psa26.v7.number = "(26:7)"
psa26.v7.text = "singing aloud a song of thanksgiving, and telling all thy wondrous deeds."
psa26.v8.number_rsv = "(26:8)"
psa26.v8.number = "(26:8)"
psa26.v8.text = "O LORD, I love the habitation of thy house, and the place where thy glory dwells."
psa26.v9.number_rsv = "(26:9)"
psa26.v9.number = "(26:9)"
psa26.v9.text = "Sweep me not away with sinners, nor my life with bloodthirsty men,"
psa26.v10.number_rsv = "(26:10)"
psa26.v10.number = "(26:10)"
psa26.v10.text = "men in whose hands are evil devices, and whose right hands are full of bribes."
psa26.v11.number_rsv = "(26:11)"
psa26.v11.number = "(26:11)"
psa26.v11.text = "But as for me, I walk in my integrity; redeem me, and be gracious to me."
psa26.v12.number_rsv = "(26:12)"
psa26.v12.number = "(26:12)"
psa26.v12.text = "My foot stands on level ground; in the great congregation I will bless the LORD."

psa27.title = "Psalm 27"
psa27.version = properties_en_US_rsv.version.designation
psa27.text = ""
psa27.v0.number_rsv = "(27:0)"
psa27.v0.number = "(27:0)"
psa27.v0.text = "A Psalm of David."
psa27.v1.number_rsv = "(27:1)"
psa27.v1.number = "(27:1)"
psa27.v1.text = "The LORD is my light and my salvation; whom shall I fear? The LORD is the stronghold of my life; of whom shall I be afraid?"
psa27.v2.number_rsv = "(27:2)"
psa27.v2.number = "(27:2)"
psa27.v2.text = "When evildoers assail me, uttering slanders against me, my adversaries and foes, they shall stumble and fall."
psa27.v3.number_rsv = "(27:3)"
psa27.v3.number = "(27:3)"
psa27.v3.text = "Though a host encamp against me, my heart shall not fear; though war arise against me, yet I will be confident."
psa27.v4.number_rsv = "(27:4)"
psa27.v4.number = "(27:4)"
psa27.v4.text = "One thing have I asked of the LORD, that will I seek after; that I may dwell in the house of the LORDall the days of my life, to behold the beauty of the LORD, and to inquire in his temple."
psa27.v5.number_rsv = "(27:5)"
psa27.v5.number = "(27:5)"
psa27.v5.text = "For he will hide me in his shelterin the day of trouble; he will conceal me under the cover of his tent, he will set me high upon a rock."
psa27.v6.number_rsv = "(27:6)"
psa27.v6.number = "(27:6)"
psa27.v6.text = "And now my head shall be lifted upabove my enemies round about me; and I will offer in his tentsacrifices with shouts of joy; I will sing and make melody to the LORD."
psa27.v7.number_rsv = "(27:7)"
psa27.v7.number = "(27:7)"
psa27.v7.text = "Hear, O LORD, when I cry aloud, be gracious to me and answer me!"
psa27.v8.number_rsv = "(27:8)"
psa27.v8.number = "(27:8)"
psa27.v8.text = "Thou hast said, \"Seek ye my face.\"My heart says to thee,\"Thy face, LORD, do I seek.\""
psa27.v9.number_rsv = "(27:9)"
psa27.v9.number = "(27:9)"
psa27.v9.text = "Hide not thy face from me.Turn not thy servant away in anger, thou who hast been my help.Cast me not off, forsake me not, O God of my salvation!"
psa27.v10.number_rsv = "(27:10)"
psa27.v10.number = "(27:10)"
psa27.v10.text = "For my father and my mother have forsaken me, but the LORD will take me up."
psa27.v11.number_rsv = "(27:11)"
psa27.v11.number = "(27:11)"
psa27.v11.text = "Teach me thy way, O LORD; and lead me on a level pathbecause of my enemies."
psa27.v12.number_rsv = "(27:12)"
psa27.v12.number = "(27:12)"
psa27.v12.text = "Give me not up to the will of my adversaries; for false witnesses have risen against me, and they breathe out violence."
psa27.v13.number_rsv = "(27:13)"
psa27.v13.number = "(27:13)"
psa27.v13.text = "I believe that I shall see the goodness of the LORDin the land of the living!"
psa27.v14.number_rsv = "(27:14)"
psa27.v14.number = "(27:14)"
psa27.v14.text = "Wait for the LORD; be strong, and let your heart take courage; yea, wait for the LORD!"

psa28.title = "Psalm 28"
psa28.version = properties_en_US_rsv.version.designation
psa28.text = ""
psa28.v0.number_rsv = "(28:0)"
psa28.v0.number = "(28:0)"
psa28.v0.text = "A Psalm of David."
psa28.v1.number_rsv = "(28:1)"
psa28.v1.number = "(28:1)"
psa28.v1.text = "To thee, O LORD, I call; my rock, be not deaf to me, lest, if thou be silent to me, I become like those who go down to the Pit."
psa28.v2.number_rsv = "(28:2)"
psa28.v2.number = "(28:2)"
psa28.v2.text = "Hear the voice of my supplication, as I cry to thee for help, as I lift up my handstoward thy most holy sanctuary."
psa28.v3.number_rsv = "(28:3)"
psa28.v3.number = "(28:3)"
psa28.v3.text = "Take me not off with the wicked, with those who are workers of evil, who speak peace with their neighbors, while mischief is in their hearts."
psa28.v4.number_rsv = "(28:4)"
psa28.v4.number = "(28:4)"
psa28.v4.text = "Requite them according to their work, and according to the evil of their deeds; requite them according to the work of their hands; render them their due reward."
psa28.v5.number_rsv = "(28:5)"
psa28.v5.number = "(28:5)"
psa28.v5.text = "Because they do not regard the works of the LORD, or the work of his hands, he will break them down and build them up no more."
psa28.v6.number_rsv = "(28:6)"
psa28.v6.number = "(28:6)"
psa28.v6.text = "Blessed be the LORD! for he has heard the voice of my supplications."
psa28.v7.number_rsv = "(28:7)"
psa28.v7.number = "(28:7)"
psa28.v7.text = "The LORD is my strength and my shield; in him my heart trusts; so I am helped, and my heart exults, and with my song I give thanks to him."
psa28.v8.number_rsv = "(28:8)"
psa28.v8.number = "(28:8)"
psa28.v8.text = "The LORD is the strength of his people, he is the saving refuge of his anointed."
psa28.v9.number_rsv = "(28:9)"
psa28.v9.number = "(28:9)"
psa28.v9.text = "O save thy people, and bless thy heritage; be thou their shepherd, and carry them for ever."

psa29.title = "Psalm 29"
psa29.version = properties_en_US_rsv.version.designation
psa29.text = ""
psa29.v0.number_rsv = "(29:0)"
psa29.v0.number = "(29:0)"
psa29.v0.text = "A Psalm of David."
psa29.v1.number_rsv = "(29:1)"
psa29.v1.number = "(29:1)"
psa29.v1.text = "Ascribe to the LORD, O heavenly beings, ascribe to the LORD glory and strength."
psa29.v2.number_rsv = "(29:2)"
psa29.v2.number = "(29:2)"
psa29.v2.text = "Ascribe to the LORD the glory of his name; worship the LORD in holy array."
psa29.v3.number_rsv = "(29:3)"
psa29.v3.number = "(29:3)"
psa29.v3.text = "The voice of the LORD is upon the waters; the God of glory thunders, the LORD, upon many waters."
psa29.v4.number_rsv = "(29:4)"
psa29.v4.number = "(29:4)"
psa29.v4.text = "The voice of the LORD is powerful, the voice of the LORD is full of majesty."
psa29.v5.number_rsv = "(29:5)"
psa29.v5.number = "(29:5)"
psa29.v5.text = "The voice of the LORD breaks the cedars, the LORD breaks the cedars of Lebanon."
psa29.v6.number_rsv = "(29:6)"
psa29.v6.number = "(29:6)"
psa29.v6.text = "He makes Lebanon to skip like a calf, and Si\"rion like a young wild ox."
psa29.v7.number_rsv = "(29:7)"
psa29.v7.number = "(29:7)"
psa29.v7.text = "The voice of the LORD flashes forth flames of fire."
psa29.v8.number_rsv = "(29:8)"
psa29.v8.number = "(29:8)"
psa29.v8.text = "The voice of the LORD shakes the wilderness, the LORD shakes the wilderness of Kadesh."
psa29.v9.number_rsv = "(29:9)"
psa29.v9.number = "(29:9)"
psa29.v9.text = "The voice of the LORD makes the oaks to whirl, and strips the forests bare; and in his temple all cry, \"Glory!\""
psa29.v10.number_rsv = "(29:10)"
psa29.v10.number = "(29:10)"
psa29.v10.text = "The LORD sits enthroned over the flood; the LORD sits enthroned as king for ever."
psa29.v11.number_rsv = "(29:11)"
psa29.v11.number = "(29:11)"
psa29.v11.text = "May the LORD give strength to his people! May the LORD bless his people with peace!"

psa30.title = "Psalm 30"
psa30.version = properties_en_US_rsv.version.designation
psa30.text = ""
psa30.v0.number_rsv = "(30:0)"
psa30.v0.number = "(30:0)"
psa30.v0.text = "A Psalm of David. A Song at the dedication of the Temple."
psa30.v1.number_rsv = "(30:1)"
psa30.v1.number = "(30:1)"
psa30.v1.text = "I will extol thee, O LORD, for thou hast drawn me up, and hast not let my foes rejoice over me."
psa30.v2.number_rsv = "(30:2)"
psa30.v2.number = "(30:2)"
psa30.v2.text = "O LORD my God, I cried to thee for help, and thou hast healed me."
psa30.v3.number_rsv = "(30:3)"
psa30.v3.number = "(30:3)"
psa30.v3.text = "O LORD, thou hast brought up my soul from Sheol, restored me to life from among those gone down to the Pit."
psa30.v4.number_rsv = "(30:4)"
psa30.v4.number = "(30:4)"
psa30.v4.text = "Sing praises to the LORD, O you his saints, and give thanks to his holy name."
psa30.v5.number_rsv = "(30:5)"
psa30.v5.number = "(30:5)"
psa30.v5.text = "For his anger is but for a moment, and his favor is for a lifetime.Weeping may tarry for the night, but joy comes with the morning."
psa30.v6.number_rsv = "(30:6)"
psa30.v6.number = "(30:6)"
psa30.v6.text = "As for me, I said in my prosperity,\"I shall never be moved.\""
psa30.v7.number_rsv = "(30:7)"
psa30.v7.number = "(30:7)"
psa30.v7.text = "By thy favor, O LORD, thou hadst established me as a strong mountain; thou didst hide thy face, I was dismayed."
psa30.v8.number_rsv = "(30:8)"
psa30.v8.number = "(30:8)"
psa30.v8.text = "To thee, O LORD, I cried; and to the LORD I made supplication:"
psa30.v9.number_rsv = "(30:9)"
psa30.v9.number = "(30:9)"
psa30.v9.text = "\"What profit is there in my death, if I go down to the Pit? Will the dust praise thee? Will it tell of thy faithfulness?"
psa30.v10.number_rsv = "(30:10)"
psa30.v10.number = "(30:10)"
psa30.v10.text = "Hear, O LORD, and be gracious to me! O LORD, be thou my helper!\""
psa30.v11.number_rsv = "(30:11)"
psa30.v11.number = "(30:11)"
psa30.v11.text = "Thou hast turned for me my mourning into dancing; thou hast loosed my sackclothand girded me with gladness,"
psa30.v12.number_rsv = "(30:12)"
psa30.v12.number = "(30:12)"
psa30.v12.text = "that my soul may praise thee and not be silent.O LORD my God, I will give thanks to thee for ever."

psa31.title = "Psalm 31"
psa31.version = properties_en_US_rsv.version.designation
psa31.text = ""
psa31.v0.number_rsv = "(31:0)"
psa31.v0.number = "(31:0)"
psa31.v0.text = "To the choirmaster. A Psalm of David."
psa31.v1.number_rsv = "(31:1)"
psa31.v1.number = "(31:1)"
psa31.v1.text = "In thee, O LORD, do I seek refuge; let me never be put to shame; in thy righteousness deliver me!"
psa31.v2.number_rsv = "(31:2)"
psa31.v2.number = "(31:2)"
psa31.v2.text = "Incline thy ear to me, rescue me speedily! Be thou a rock of refuge for me, a strong fortress to save me!"
psa31.v3.number_rsv = "(31:3)"
psa31.v3.number = "(31:3)"
psa31.v3.text = "Yea, thou art my rock and my fortress; for thy name\"s sake lead me and guide me,"
psa31.v4.number_rsv = "(31:4)"
psa31.v4.number = "(31:4)"
psa31.v4.text = "take me out of the net which is hidden for me, for thou art my refuge."
psa31.v5.number_rsv = "(31:5)"
psa31.v5.number = "(31:5)"
psa31.v5.text = "Into thy hand I commit my spirit; thou hast redeemed me, O LORD, faithful God."
psa31.v6.number_rsv = "(31:6)"
psa31.v6.number = "(31:6)"
psa31.v6.text = "Thou hatest those who pay regard to vain idols; but I trust in the LORD."
psa31.v7.number_rsv = "(31:7)"
psa31.v7.number = "(31:7)"
psa31.v7.text = "I will rejoice and be glad for thy steadfast love, because thou hast seen my affliction, thou hast taken heed of my adversities,"
psa31.v8.number_rsv = "(31:8)"
psa31.v8.number = "(31:8)"
psa31.v8.text = "and hast not delivered me into the hand of the enemy; thou hast set my feet in a broad place."
psa31.v9.number_rsv = "(31:9)"
psa31.v9.number = "(31:9)"
psa31.v9.text = "Be gracious to me, O LORD, for I am in distress; my eye is wasted from grief, my soul and my body also."
psa31.v10.number_rsv = "(31:10)"
psa31.v10.number = "(31:10)"
psa31.v10.text = "For my life is spent with sorrow, and my years with sighing; my strength fails because of my misery, and my bones waste away."
psa31.v11.number_rsv = "(31:11)"
psa31.v11.number = "(31:11)"
psa31.v11.text = "I am the scorn of all my adversaries, a horror to my neighbors, an object of dread to my acquaintances; those who see me in the street flee from me."
psa31.v12.number_rsv = "(31:12)"
psa31.v12.number = "(31:12)"
psa31.v12.text = "I have passed out of mind like one who is dead; I have become like a broken vessel."
psa31.v13.number_rsv = "(31:13)"
psa31.v13.number = "(31:13)"
psa31.v13.text = "Yea, I hear the whispering of many --terror on every side! --as they scheme together against me, as they plot to take my life."
psa31.v14.number_rsv = "(31:14)"
psa31.v14.number = "(31:14)"
psa31.v14.text = "But I trust in thee, O LORD, I say, \"Thou art my God.\""
psa31.v15.number_rsv = "(31:15)"
psa31.v15.number = "(31:15)"
psa31.v15.text = "My times are in thy hand; deliver me from the hand of my enemies and persecutors!"
psa31.v16.number_rsv = "(31:16)"
psa31.v16.number = "(31:16)"
psa31.v16.text = "Let thy face shine on thy servant; save me in thy steadfast love!"
psa31.v17.number_rsv = "(31:17)"
psa31.v17.number = "(31:17)"
psa31.v17.text = "Let me not be put to shame, O LORD, for I call on thee; let the wicked be put to shame, let them go dumbfounded to Sheol."
psa31.v18.number_rsv = "(31:18)"
psa31.v18.number = "(31:18)"
psa31.v18.text = "Let the lying lips be dumb, which speak insolently against the righteousin pride and contempt."
psa31.v19.number_rsv = "(31:19)"
psa31.v19.number = "(31:19)"
psa31.v19.text = "O how abundant is thy goodness, which thou hast laid up for those who fear thee, and wrought for those who take refuge in thee, in the sight of the sons of men!"
psa31.v20.number_rsv = "(31:20)"
psa31.v20.number = "(31:20)"
psa31.v20.text = "In the covert of thy presence thou hidest themfrom the plots of men; thou holdest them safe under thy shelterfrom the strife of tongues."
psa31.v21.number_rsv = "(31:21)"
psa31.v21.number = "(31:21)"
psa31.v21.text = "Blessed be the LORD, for he has wondrously shown his steadfast love to mewhen I was beset as in a besieged city."
psa31.v22.number_rsv = "(31:22)"
psa31.v22.number = "(31:22)"
psa31.v22.text = "I had said in my alarm,\"I am driven far from thy sight.\"But thou didst hear my supplications, when I cried to thee for help."
psa31.v23.number_rsv = "(31:23)"
psa31.v23.number = "(31:23)"
psa31.v23.text = "Love the LORD, all you his saints! The LORD preserves the faithful, but abundantly requites him who acts haughtily."
psa31.v24.number_rsv = "(31:24)"
psa31.v24.number = "(31:24)"
psa31.v24.text = "Be strong, and let your heart take courage, all you who wait for the LORD!"

psa32.title = "Psalm 32"
psa32.version = properties_en_US_rsv.version.designation
psa32.text = ""
psa32.v0.number_rsv = "(32:0)"
psa32.v0.number = "(32:0)"
psa32.v0.text = "A Psalm of David. A Maskil."
psa32.v1.number_rsv = "(32:1)"
psa32.v1.number = "(32:1)"
psa32.v1.text = "Blessed is he whose transgression is forgiven, whose sin is covered."
psa32.v2.number_rsv = "(32:2)"
psa32.v2.number = "(32:2)"
psa32.v2.text = "Blessed is the man to whom the LORD imputes no iniquity, and in whose spirit there is no deceit."
psa32.v3.number_rsv = "(32:3)"
psa32.v3.number = "(32:3)"
psa32.v3.text = "When I declared not my sin, my body wasted awaythrough my groaning all day long."
psa32.v4.number_rsv = "(32:4)"
psa32.v4.number = "(32:4)"
psa32.v4.text = "For day and night thy hand was heavy upon me; my strength was dried up as by the heat of summer."
psa32.v5.number_rsv = "(32:5)"
psa32.v5.number = "(32:5)"
psa32.v5.text = "I acknowledged my sin to thee, and I did not hide my iniquity; I said, \"I will confess my transgressions to the LORD\"; then thou didst forgive the guilt of my sin."
psa32.v6.number_rsv = "(32:6)"
psa32.v6.number = "(32:6)"
psa32.v6.text = "Therefore let every one who is godlyoffer prayer to thee; at a time of distress, in the rush of great waters, they shall not reach him."
psa32.v7.number_rsv = "(32:7)"
psa32.v7.number = "(32:7)"
psa32.v7.text = "Thou art a hiding place for me, thou preservest me from trouble; thou dost encompass me with deliverance."
psa32.v8.number_rsv = "(32:8)"
psa32.v8.number = "(32:8)"
psa32.v8.text = "I will instruct you and teach youthe way you should go; I will counsel you with my eye upon you."
psa32.v9.number_rsv = "(32:9)"
psa32.v9.number = "(32:9)"
psa32.v9.text = "Be not like a horse or a mule, without understanding, which must be curbed with bit and bridle, else it will not keep with you."
psa32.v10.number_rsv = "(32:10)"
psa32.v10.number = "(32:10)"
psa32.v10.text = "Many are the pangs of the wicked; but steadfast love surrounds him who trusts in the LORD."
psa32.v11.number_rsv = "(32:11)"
psa32.v11.number = "(32:11)"
psa32.v11.text = "Be glad in the LORD, and rejoice, O righteous, and shout for joy, all you upright in heart!"

kathisma_5.title = "KATHISMA 5."

psa33.title = "Psalm 33"
psa33.version = properties_en_US_rsv.version.designation
psa33.text = ""
psa33.v1.number_rsv = "(33:1)"
psa33.v1.number = "(33:1)"
psa33.v1.text = "Rejoice in the LORD, O you righteous! Praise befits the upright."
psa.v2.number = "(:2)"
psa33.v2.text = "Praise the LORD with the lyre, make melody to him with the harp of ten strings!"
psa33.v3.number_rsv = "(33:3)"
psa33.v3.number = "(33:3)"
psa33.v3.text = "Sing to him a new song, play skilfully on the strings, with loud shouts."
psa33.v4.number_rsv = "(33:4)"
psa33.v4.number = "(33:4)"
psa33.v4.text = "For the word of the LORD is upright; and all his work is done in faithfulness."
psa33.v5.number_rsv = "(33:5)"
psa33.v5.number = "(33:5)"
psa33.v5.text = "He loves righteousness and justice; the earth is full of the steadfast love of the LORD."
psa33.v6.number_rsv = "(33:6)"
psa33.v6.number = "(33:6)"
psa33.v6.text = "By the word of the LORD the heavens were made, and all their host by the breath of his mouth."
psa33.v7.number_rsv = "(33:7)"
psa33.v7.number = "(33:7)"
psa33.v7.text = "He gathered the waters of the sea as in a bottle; he put the deeps in storehouses."
psa33.v8.number_rsv = "(33:8)"
psa33.v8.number = "(33:8)"
psa33.v8.text = "Let all the earth fear the LORD, let all the inhabitants of the world stand in awe of him!"
psa33.v9.number_rsv = "(33:9)"
psa33.v9.number = "(33:9)"
psa33.v9.text = "For he spoke, and it came to be; he commanded, and it stood forth."
psa33.v10.number_rsv = "(33:10)"
psa33.v10.number = "(33:10)"
psa33.v10.text = "The LORD brings the counsel of the nations to nought; he frustrates the plans of the peoples."
psa33.v11.number_rsv = "(33:11)"
psa33.v11.number = "(33:11)"
psa33.v11.text = "The counsel of the LORD stands for ever, the thoughts of his heart to all generations."
psa33.v12.number_rsv = "(33:12)"
psa33.v12.number = "(33:12)"
psa33.v12.text = "Blessed is the nation whose God is the LORD, the people whom he has chosen as his heritage!"
psa33.v13.number_rsv = "(33:13)"
psa33.v13.number = "(33:13)"
psa33.v13.text = "The LORD looks down from heaven, he sees all the sons of men;"
psa33.v14.number_rsv = "(33:14)"
psa33.v14.number = "(33:14)"
psa33.v14.text = "from where he sits enthroned he looks forthon all the inhabitants of the earth,"
psa33.v15.number_rsv = "(33:15)"
psa33.v15.number = "(33:15)"
psa33.v15.text = "he who fashions the hearts of them all, and observes all their deeds."
psa33.v16.number_rsv = "(33:16)"
psa33.v16.number = "(33:16)"
psa33.v16.text = "A king is not saved by his great army; a warrior is not delivered by his great strength."
psa33.v17.number_rsv = "(33:17)"
psa33.v17.number = "(33:17)"
psa33.v17.text = "The war horse is a vain hope for victory, and by its great might it cannot save."
psa33.v18.number_rsv = "(33:18)"
psa33.v18.number = "(33:18)"
psa33.v18.text = "Behold, the eye of the LORD is on those who fear him, on those who hope in his steadfast love,"
psa33.v19.number_rsv = "(33:19)"
psa33.v19.number = "(33:19)"
psa33.v19.text = "that he may deliver their soul from death, and keep them alive in famine."
psa33.v20.number_rsv = "(33:20)"
psa33.v20.number = "(33:20)"
psa33.v20.text = "Our soul waits for the LORD; he is our help and shield."
psa33.v21.number_rsv = "(33:21)"
psa33.v21.number = "(33:21)"
psa33.v21.text = "Yea, our heart is glad in him, because we trust in his holy name."
psa33.v22.number_rsv = "(33:22)"
psa33.v22.number = "(33:22)"
psa33.v22.text = "Let thy steadfast love, O LORD, be upon us, even as we hope in thee."

psa34.title = "Psalm 34"
psa34.version = properties_en_US_rsv.version.designation
psa34.text = ""
psa34.v0.number_rsv = "(34:0)"
psa34.v0.number = "(34:0)"
psa34.v0.text = "A Psalm of David, when he feigned madness before Abimelech, so that he drove him out, and he went away."
psa34.v1.number_rsv = "(34:1)"
psa34.v1.number = "(34:1)"
psa34.v1.text = "I will bless the LORD at all times; his praise shall continually be in my mouth."
psa34.v2.number_rsv = "(34:2)"
psa34.v2.number = "(34:2)"
psa34.v2.text = "My soul makes its boast in the LORD; let the afflicted hear and be glad."
psa34.v3.number_rsv = "(34:3)"
psa34.v3.number = "(34:3)"
psa34.v3.text = "O magnify the LORD with me, and let us exalt his name together!"
psa34.v4.number_rsv = "(34:4)"
psa34.v4.number = "(34:4)"
psa34.v4.text = "I sought the LORD, and he answered me, and delivered me from all my fears."
psa34.v5.number_rsv = "(34:5)"
psa34.v5.number = "(34:5)"
psa34.v5.text = "Look to him, and be radiant; so your faces shall never be ashamed."
psa34.v6.number_rsv = "(34:6)"
psa34.v6.number = "(34:6)"
psa34.v6.text = "This poor man cried, and the LORD heard him, and saved him out of all his troubles."
psa34.v7.number_rsv = "(34:7)"
psa34.v7.number = "(34:7)"
psa34.v7.text = "The angel of the LORD encampsaround those who fear him, and delivers them."
psa34.v8.number_rsv = "(34:8)"
psa34.v8.number = "(34:8)"
psa34.v8.text = "O taste and see that the LORD is good! Happy is the man who takes refuge in him!"
psa34.v9.number_rsv = "(34:9)"
psa34.v9.number = "(34:9)"
psa34.v9.text = "O fear the LORD, you his saints, for those who fear him have no want!"
psa34.v10.number_rsv = "(34:10)"
psa34.v10.number = "(34:10)"
psa34.v10.text = "The young lions suffer want and hunger; but those who seek the LORD lack no good thing."
psa34.v11.number_rsv = "(34:11)"
psa34.v11.number = "(34:11)"
psa34.v11.text = "Come, O sons, listen to me, I will teach you the fear of the LORD."
psa34.v12.number_rsv = "(34:12)"
psa34.v12.number = "(34:12)"
psa34.v12.text = "What man is there who desires life, and covets many days, that he may enjoy good?"
psa34.v13.number_rsv = "(34:13)"
psa34.v13.number = "(34:13)"
psa34.v13.text = "Keep your tongue from evil, and your lips from speaking deceit."
psa34.v14.number_rsv = "(34:14)"
psa34.v14.number = "(34:14)"
psa34.v14.text = "Depart from evil, and do good; seek peace, and pursue it."
psa34.v15.number_rsv = "(34:15)"
psa34.v15.number = "(34:15)"
psa34.v15.text = "The eyes of the LORD are toward the righteous, and his ears toward their cry."
psa34.v16.number_rsv = "(34:16)"
psa34.v16.number = "(34:16)"
psa34.v16.text = "The face of the LORD is against evildoers, to cut off the remembrance of them from the earth."
psa34.v17.number_rsv = "(34:17)"
psa34.v17.number = "(34:17)"
psa34.v17.text = "When the righteous cry for help, the LORD hears, and delivers them out of all their troubles."
psa34.v18.number_rsv = "(34:18)"
psa34.v18.number = "(34:18)"
psa34.v18.text = "The LORD is near to the brokenhearted, and saves the crushed in spirit."
psa34.v19.number_rsv = "(34:19)"
psa34.v19.number = "(34:19)"
psa34.v19.text = "Many are the afflictions of the righteous; but the LORD delivers him out of them all."
psa34.v20.number_rsv = "(34:20)"
psa34.v20.number = "(34:20)"
psa34.v20.text = "He keeps all his bones; not one of them is broken."
psa34.v21.number_rsv = "(34:21)"
psa34.v21.number = "(34:21)"
psa34.v21.text = "Evil shall slay the wicked; and those who hate the righteous will be condemned."
psa34.v22.number_rsv = "(34:22)"
psa34.v22.number = "(34:22)"
psa34.v22.text = "The LORD redeems the life of his servants; none of those who take refuge in him will be condemned.R"

psa35.title = "Psalm 35"
psa35.version = properties_en_US_rsv.version.designation
psa35.text = ""
psa35.v0.number_rsv = "(35:0)"
psa35.v0.number = "(35:0)"
psa35.v0.text = "A Psalm of David."
psa35.v1.number_rsv = "(35:1)"
psa35.v1.number = "(35:1)"
psa35.v1.text = "Contend, O LORD, with those who contend with me; fight against those who fight against me!"
psa35.v2.number_rsv = "(35:2)"
psa35.v2.number = "(35:2)"
psa35.v2.text = "Take hold of shield and buckler, and rise for my help!"
psa35.v3.number_rsv = "(35:3)"
psa35.v3.number = "(35:3)"
psa35.v3.text = "Draw the spear and javelinagainst my pursuers! Say to my soul,\"I am your deliverance!\""
psa35.v4.number_rsv = "(35:4)"
psa35.v4.number = "(35:4)"
psa35.v4.text = "Let them be put to shame and dishonorwho seek after my life! Let them be turned back and confoundedwho devise evil against me!"
psa35.v5.number_rsv = "(35:5)"
psa35.v5.number = "(35:5)"
psa35.v5.text = "Let them be like chaff before the wind, with the angel of the LORD driving them on!"
psa35.v6.number_rsv = "(35:6)"
psa35.v6.number = "(35:6)"
psa35.v6.text = "Let their way be dark and slippery, with the angel of the LORD pursuing them!"
psa35.v7.number_rsv = "(35:7)"
psa35.v7.number = "(35:7)"
psa35.v7.text = "For without cause they hid their net for me; without cause they dug a pit for my life."
psa35.v8.number_rsv = "(35:8)"
psa35.v8.number = "(35:8)"
psa35.v8.text = "Let ruin come upon them unawares! And let the net which they hid ensnare them; let them fall therein to ruin!"
psa35.v9.number_rsv = "(35:9)"
psa35.v9.number = "(35:9)"
psa35.v9.text = "Then my soul shall rejoice in the LORD, exulting in his deliverance."
psa35.v10.number_rsv = "(35:10)"
psa35.v10.number = "(35:10)"
psa35.v10.text = "All my bones shall say,\"O LORD, who is like thee, thou who deliverest the weakfrom him who is too strong for him, the weak and needy from him who despoils him?\""
psa35.v11.number_rsv = "(35:11)"
psa35.v11.number = "(35:11)"
psa35.v11.text = "Malicious witnesses rise up; they ask me of things that I know not."
psa35.v12.number_rsv = "(35:12)"
psa35.v12.number = "(35:12)"
psa35.v12.text = "They requite me evil for good; my soul is forlorn."
psa35.v13.number_rsv = "(35:13)"
psa35.v13.number = "(35:13)"
psa35.v13.text = "But I, when they were sick --I wore sackcloth, I afflicted myself with fasting.I prayed with head bowed on my bosom,"
psa35.v14.number_rsv = "(35:14)"
psa35.v14.number = "(35:14)"
psa35.v14.text = "as though I grieved for my friend or my brother; I went about as one who laments his mother, bowed down and in mourning."
psa35.v15.number_rsv = "(35:15)"
psa35.v15.number = "(35:15)"
psa35.v15.text = "But at my stumbling they gathered in glee, they gathered together against me; cripples whom I knew notslandered me without ceasing;"
psa35.v16.number_rsv = "(35:16)"
psa35.v16.number = "(35:16)"
psa35.v16.text = "they impiously mocked more and more, gnashing at me with their teeth."
psa35.v17.number_rsv = "(35:17)"
psa35.v17.number = "(35:17)"
psa35.v17.text = "How long, O LORD, wilt thou look on? Rescue me from their ravages, my life from the lions!"
psa35.v18.number_rsv = "(35:18)"
psa35.v18.number = "(35:18)"
psa35.v18.text = "Then I will thank thee in the great congregation; in the mighty throng I will praise thee."
psa35.v19.number_rsv = "(35:19)"
psa35.v19.number = "(35:19)"
psa35.v19.text = "Let not those rejoice over mewho are wrongfully my foes, and let not those wink the eyewho hate me without cause."
psa35.v20.number_rsv = "(35:20)"
psa35.v20.number = "(35:20)"
psa35.v20.text = "For they do not speak peace, but against those who are quiet in the landthey conceive words of deceit."
psa35.v21.number_rsv = "(35:21)"
psa35.v21.number = "(35:21)"
psa35.v21.text = "They open wide their mouths against me; they say, \"Aha, Aha! our eyes have seen it!\""
psa35.v22.number_rsv = "(35:22)"
psa35.v22.number = "(35:22)"
psa35.v22.text = "Thou hast seen, O LORD; be not silent! O Lord, be not far from me!"
psa35.v23.number_rsv = "(35:23)"
psa35.v23.number = "(35:23)"
psa35.v23.text = "Bestir thyself, and awake for my right, for my cause, my God and my Lord!"
psa35.v24.number_rsv = "(35:24)"
psa35.v24.number = "(35:24)"
psa35.v24.text = "Vindicate me, O LORD, my God, according to thy righteousness; and let them not rejoice over me!"
psa35.v25.number_rsv = "(35:25)"
psa35.v25.number = "(35:25)"
psa35.v25.text = "Let them not say to themselves,\"Aha, we have our heart\"s desire!\"Let them not say, \"We have swallowed him up.\""
psa35.v26.number_rsv = "(35:26)"
psa35.v26.number = "(35:26)"
psa35.v26.text = "Let them be put to shame and confusion altogetherwho rejoice at my calamity! Let them be clothed with shame and dishonorwho magnify themselves against me!"
psa35.v27.number_rsv = "(35:27)"
psa35.v27.number = "(35:27)"
psa35.v27.text = "Let those who desire my vindicationshout for joy and be glad, and say evermore,\"Great is the LORD, who delights in the welfare of his servant!\""
psa35.v28.number_rsv = "(35:28)"
psa35.v28.number = "(35:28)"
psa35.v28.text = "Then my tongue shall tell of thy righteousnessand of thy praise all the day long."

psa36.title = "Psalm 36"
psa36.version = properties_en_US_rsv.version.designation
psa36.text = ""
psa36.v0.number_rsv = "(36:0)"
psa36.v0.number = "(36:0)"
psa36.v0.text = "To the choirmaster. A Psalm of David, the servant of the LORD."
psa36.v1.number_rsv = "(36:1)"
psa36.v1.number = "(36:1)"
psa36.v1.text = "Transgression speaks to the wickeddeep in his heart; there is no fear of Godbefore his eyes."
psa36.v2.number_rsv = "(36:2)"
psa36.v2.number = "(36:2)"
psa36.v2.text = "For he flatters himself in his own eyesthat his iniquity cannot be found out and hated."
psa36.v3.number_rsv = "(36:3)"
psa36.v3.number = "(36:3)"
psa36.v3.text = "The words of his mouth are mischief and deceit; he has ceased to act wisely and do good."
psa36.v4.number_rsv = "(36:4)"
psa36.v4.number = "(36:4)"
psa36.v4.text = "He plots mischief while on his bed; he sets himself in a way that is not good; he spurns not evil."
psa36.v5.number_rsv = "(36:5)"
psa36.v5.number = "(36:5)"
psa36.v5.text = "Thy steadfast love, O LORD, extends to the heavens, thy faithfulness to the clouds."
psa36.v6.number_rsv = "(36:6)"
psa36.v6.number = "(36:6)"
psa36.v6.text = "Thy righteousness is like the mountains of God, thy judgments are like the great deep; man and beast thou savest, O LORD."
psa36.v7.number_rsv = "(36:7)"
psa36.v7.number = "(36:7)"
psa36.v7.text = "How precious is thy steadfast love, O God! The children of men take refuge in the shadow of thy wings."
psa36.v8.number_rsv = "(36:8)"
psa36.v8.number = "(36:8)"
psa36.v8.text = "They feast on the abundance of thy house, and thou givest them drink from the river of thy delights."
psa36.v9.number_rsv = "(36:9)"
psa36.v9.number = "(36:9)"
psa36.v9.text = "For with thee is the fountain of life; in thy light do we see light."
psa36.v10.number_rsv = "(36:10)"
psa36.v10.number = "(36:10)"
psa36.v10.text = "O continue thy steadfast love to those who know thee, and thy salvation to the upright of heart!"
psa36.v11.number_rsv = "(36:11)"
psa36.v11.number = "(36:11)"
psa36.v11.text = "Let not the foot of arrogance come upon me, nor the hand of the wicked drive me away."
psa36.v12.number_rsv = "(36:12)"
psa36.v12.number = "(36:12)"
psa36.v12.text = "There the evildoers lie prostrate, they are thrust down, unable to rise."

psa37.title = "Psalm 37"
psa37.version = properties_en_US_rsv.version.designation
psa37.text = ""
psa37.v0.number_rsv = "(37:0)"
psa37.v0.number = "(37:0)"
psa37.v0.text = "A Psalm of David."
psa37.v1.number_rsv = "(37:1)"
psa37.v1.number = "(37:1)"
psa37.v1.text = "Fret not yourself because of the wicked, be not envious of wrongdoers!"
psa37.v2.number_rsv = "(37:2)"
psa37.v2.number = "(37:2)"
psa37.v2.text = "For they will soon fade like the grass, and wither like the green herb."
psa37.v3.number_rsv = "(37:3)"
psa37.v3.number = "(37:3)"
psa37.v3.text = "Trust in the LORD, and do good; so you will dwell in the land, and enjoy security."
psa37.v4.number_rsv = "(37:4)"
psa37.v4.number = "(37:4)"
psa37.v4.text = "Take delight in the LORD, and he will give you the desires of your heart."
psa37.v5.number_rsv = "(37:5)"
psa37.v5.number = "(37:5)"
psa37.v5.text = "Commit your way to the LORD; trust in him, and he will act."
psa37.v6.number_rsv = "(37:6)"
psa37.v6.number = "(37:6)"
psa37.v6.text = "He will bring forth your vindication as the light, and your right as the noonday."
psa37.v7.number_rsv = "(37:7)"
psa37.v7.number = "(37:7)"
psa37.v7.text = "Be still before the LORD, and wait patiently for him; fret not yourself over him who prospers in his way, over the man who carries out evil devices!"
psa37.v8.number_rsv = "(37:8)"
psa37.v8.number = "(37:8)"
psa37.v8.text = "Refrain from anger, and forsake wrath! Fret not yourself; it tends only to evil."
psa37.v9.number_rsv = "(37:9)"
psa37.v9.number = "(37:9)"
psa37.v9.text = "For the wicked shall be cut off; but those who wait for the LORD shall possess the land."
psa37.v10.number_rsv = "(37:10)"
psa37.v10.number = "(37:10)"
psa37.v10.text = "Yet a little while, and the wicked will be no more; though you look well at his place, he will not be there."
psa37.v11.number_rsv = "(37:11)"
psa37.v11.number = "(37:11)"
psa37.v11.text = "But the meek shall possess the land, and delight themselves in abundant prosperity."
psa37.v12.number_rsv = "(37:12)"
psa37.v12.number = "(37:12)"
psa37.v12.text = "The wicked plots against the righteous, and gnashes his teeth at him;"
psa37.v13.number_rsv = "(37:13)"
psa37.v13.number = "(37:13)"
psa37.v13.text = "but the LORD laughs at the wicked, for he sees that his day is coming."
psa37.v14.number_rsv = "(37:14)"
psa37.v14.number = "(37:14)"
psa37.v14.text = "The wicked draw the sword and bend their bows, to bring down the poor and needy, to slay those who walk uprightly;"
psa37.v15.number_rsv = "(37:15)"
psa37.v15.number = "(37:15)"
psa37.v15.text = "their sword shall enter their own heart, and their bows shall be broken."
psa37.v16.number_rsv = "(37:16)"
psa37.v16.number = "(37:16)"
psa37.v16.text = "Better is a little that the righteous hasthan the abundance of many wicked."
psa37.v17.number_rsv = "(37:17)"
psa37.v17.number = "(37:17)"
psa37.v17.text = "For the arms of the wicked shall be broken; but the LORD upholds the righteous."
psa37.v18.number_rsv = "(37:18)"
psa37.v18.number = "(37:18)"
psa37.v18.text = "The LORD knows the days of the blameless, and their heritage will abide for ever;"
psa37.v19.number_rsv = "(37:19)"
psa37.v19.number = "(37:19)"
psa37.v19.text = "they are not put to shame in evil times, in the days of famine they have abundance."
psa37.v20.number_rsv = "(37:20)"
psa37.v20.number = "(37:20)"
psa37.v20.text = "But the wicked perish; the enemies of the LORD are like the glory of the pastures, they vanish -- like smoke they vanish away."
psa37.v21.number_rsv = "(37:21)"
psa37.v21.number = "(37:21)"
psa37.v21.text = "The wicked borrows, and cannot pay back, but the righteous is generous and gives;"
psa37.v22.number_rsv = "(37:22)"
psa37.v22.number = "(37:22)"
psa37.v22.text = "for those blessed by the LORD shall possess the land, but those cursed by him shall be cut off."
psa37.v23.number_rsv = "(37:23)"
psa37.v23.number = "(37:23)"
psa37.v23.text = "The steps of a man are from the LORD, and he establishes him in whose way he delights;"
psa37.v24.number_rsv = "(37:24)"
psa37.v24.number = "(37:24)"
psa37.v24.text = "though he fall, he shall not be cast headlong, for the LORD is the stay of his hand."
psa37.v25.number_rsv = "(37:25)"
psa37.v25.number = "(37:25)"
psa37.v25.text = "I have been young, and now am old; yet I have not seen the righteous forsakenor his children begging bread."
psa37.v26.number_rsv = "(37:26)"
psa37.v26.number = "(37:26)"
psa37.v26.text = "He is ever giving liberally and lending, and his children become a blessing."
psa37.v27.number_rsv = "(37:27)"
psa37.v27.number = "(37:27)"
psa37.v27.text = "Depart from evil, and do good; so shall you abide for ever."
psa37.v28.number_rsv = "(37:28)"
psa37.v28.number = "(37:28)"
psa37.v28.text = "For the LORD loves justice; he will not forsake his saints.The righteous shall be preserved for ever, but the children of the wicked shall be cut off."
psa37.v29.number_rsv = "(37:29)"
psa37.v29.number = "(37:29)"
psa37.v29.text = "The righteous shall possess the land, and dwell upon it for ever."
psa37.v30.number_rsv = "(37:30)"
psa37.v30.number = "(37:30)"
psa37.v30.text = "The mouth of the righteous utters wisdom, and his tongue speaks justice."
psa37.v31.number_rsv = "(37:31)"
psa37.v31.number = "(37:31)"
psa37.v31.text = "The law of his God is in his heart; his steps do not slip."
psa37.v32.number_rsv = "(37:32)"
psa37.v32.number = "(37:32)"
psa37.v32.text = "The wicked watches the righteous, and seeks to slay him."
psa37.v33.number_rsv = "(37:33)"
psa37.v33.number = "(37:33)"
psa37.v33.text = "The LORD will not abandon him to his power, or let him be condemned when he is brought to trial."
psa37.v34.number_rsv = "(37:34)"
psa37.v34.number = "(37:34)"
psa37.v34.text = "Wait for the LORD, and keep to his way, and he will exalt you to possess the land; you will look on the destruction of the wicked."
psa37.v35.number_rsv = "(37:35)"
psa37.v35.number = "(37:35)"
psa37.v35.text = "I have seen a wicked man overbearing, and towering like a cedar of Lebanon."
psa37.v36.number_rsv = "(37:36)"
psa37.v36.number = "(37:36)"
psa37.v36.text = "Again I passed by, and, lo, he was no more; though I sought him, he could not be found."
psa37.v37.number_rsv = "(37:37)"
psa37.v37.number = "(37:37)"
psa37.v37.text = "Mark the blameless man, and behold the upright, for there is posterity for the man of peace."
psa37.v38.number_rsv = "(37:38)"
psa37.v38.number = "(37:38)"
psa37.v38.text = "But transgressors shall be altogether destroyed; the posterity of the wicked shall be cut off."
psa37.v39.number_rsv = "(37:39)"
psa37.v39.number = "(37:39)"
psa37.v39.text = "The salvation of the righteous is from the LORD; he is their refuge in the time of trouble."
psa37.v40.number_rsv = "(37:40)"
psa37.v40.number = "(37:40)"
psa37.v40.text = "The LORD helps them and delivers them; he delivers them from the wicked, and saves them, because they take refuge in him."

kathisma_6.title = "KATHISMA 6."

psa38.title = "Psalm 38"
psa38.version = properties_en_US_rsv.version.designation
psa38.text = ""
psa38.v0.number_rsv = "(38:0)"
psa38.v0.number = "(38:0)"
psa38.v0.text = "A Psalm of David, for the memorial offering."
psa38.v1.number_rsv = "(38:1)"
psa38.v1.number = "(38:1)"
psa38.v1.text = "O LORD, rebuke me not in thy anger, nor chasten me in thy wrath!"
psa38.v2.number_rsv = "(38:2)"
psa38.v2.number = "(38:2)"
psa38.v2.text = "For thy arrows have sunk into me, and thy hand has come down on me."
psa38.v3.number_rsv = "(38:3)"
psa38.v3.number = "(38:3)"
psa38.v3.text = "There is no soundness in my fleshbecause of thy indignation; there is no health in my bonesbecause of my sin."
psa38.v4.number_rsv = "(38:4)"
psa38.v4.number = "(38:4)"
psa38.v4.text = "For my iniquities have gone over my head; they weigh like a burden too heavy for me."
psa38.v5.number_rsv = "(38:5)"
psa38.v5.number = "(38:5)"
psa38.v5.text = "My wounds grow foul and festerbecause of my foolishness,"
psa38.v6.number_rsv = "(38:6)"
psa38.v6.number = "(38:6)"
psa38.v6.text = "I am utterly bowed down and prostrate; all the day I go about mourning."
psa38.v7.number_rsv = "(38:7)"
psa38.v7.number = "(38:7)"
psa38.v7.text = "For my loins are filled with burning, and there is no soundness in my flesh."
psa38.v8.number_rsv = "(38:8)"
psa38.v8.number = "(38:8)"
psa38.v8.text = "I am utterly spent and crushed; I groan because of the tumult of my heart."
psa38.v9.number_rsv = "(38:9)"
psa38.v9.number = "(38:9)"
psa38.v9.text = "Lord, all my longing is known to thee, my sighing is not hidden from thee."
psa38.v10.number_rsv = "(38:10)"
psa38.v10.number = "(38:10)"
psa38.v10.text = "My heart throbs, my strength fails me; and the light of my eyes -- it also has gone from me."
psa38.v11.number_rsv = "(38:11)"
psa38.v11.number = "(38:11)"
psa38.v11.text = "My friends and companions stand aloof from my plague, and my kinsmen stand afar off."
psa38.v12.number_rsv = "(38:12)"
psa38.v12.number = "(38:12)"
psa38.v12.text = "Those who seek my life lay their snares, those who seek my hurt speak of ruin, and meditate treachery all the day long."
psa38.v13.number_rsv = "(38:13)"
psa38.v13.number = "(38:13)"
psa38.v13.text = "But I am like a deaf man, I do not hear, like a dumb man who does not open his mouth."
psa38.v14.number_rsv = "(38:14)"
psa38.v14.number = "(38:14)"
psa38.v14.text = "Yea, I am like a man who does not hear, and in whose mouth are no rebukes."
psa38.v15.number_rsv = "(38:15)"
psa38.v15.number = "(38:15)"
psa38.v15.text = "But for thee, O LORD, do I wait; it is thou, O LORD my God, who wilt answer."
psa38.v16.number_rsv = "(38:16)"
psa38.v16.number = "(38:16)"
psa38.v16.text = "For I pray, \"Only let them not rejoice over me, who boast against me when my foot slips!\""
psa38.v17.number_rsv = "(38:17)"
psa38.v17.number = "(38:17)"
psa38.v17.text = "For I am ready to fall, and my pain is ever with me."
psa38.v18.number_rsv = "(38:18)"
psa38.v18.number = "(38:18)"
psa38.v18.text = "I confess my iniquity, I am sorry for my sin."
psa38.v19.number_rsv = "(38:19)"
psa38.v19.number = "(38:19)"
psa38.v19.text = "Those who are my foes without cause are mighty, and many are those who hate me wrongfully."
psa38.v20.number_rsv = "(38:20)"
psa38.v20.number = "(38:20)"
psa38.v20.text = "Those who render me evil for goodare my adversaries because I follow after good."
psa38.v21.number_rsv = "(38:21)"
psa38.v21.number = "(38:21)"
psa38.v21.text = "Do not forsake me, O LORD! O my God, be not far from me!"
psa38.v22.number_rsv = "(38:22)"
psa38.v22.number = "(38:22)"
psa38.v22.text = "Make haste to help me, O Lord, my salvation!"

psa39.title = "Psalm 39"
psa39.version = properties_en_US_rsv.version.designation
psa39.text = ""
psa39.v0.number_rsv = "(39:0)"
psa39.v0.number = "(39:0)"
psa39.v0.text = "To the choirmaster: to Jeduthun. A Psalm of David."
psa39.v1.number_rsv = "(39:1)"
psa39.v1.number = "(39:1)"
psa39.v1.text = "I said, \"I will guard my ways, that I may not sin with my tongue; I will bridle my mouth, so long as the wicked are in my presence.\""
psa39.v2.number_rsv = "(39:2)"
psa39.v2.number = "(39:2)"
psa39.v2.text = "I was dumb and silent, I held my peace to no avail; my distress grew worse,"
psa39.v3.number_rsv = "(39:3)"
psa39.v3.number = "(39:3)"
psa39.v3.text = "my heart became hot within me.As I mused, the fire burned; then I spoke with my tongue:"
psa39.v4.number_rsv = "(39:4)"
psa39.v4.number = "(39:4)"
psa39.v4.text = "\"LORD, let me know my end, and what is the measure of my days; let me know how fleeting my life is!"
psa39.v5.number_rsv = "(39:5)"
psa39.v5.number = "(39:5)"
psa39.v5.text = "Behold, thou hast made my days a few handbreadths, and my lifetime is as nothing in thy sight.Surely every man stands as a mere breath!"
psa39.v6.number_rsv = "(39:6)"
psa39.v6.number = "(39:6)"
psa39.v6.text = "Surely man goes about as a shadow! Surely for nought are they in turmoil; man heaps up, and knows not who will gather!"
psa39.v7.number_rsv = "(39:7)"
psa39.v7.number = "(39:7)"
psa39.v7.text = "\"And now, Lord, for what do I wait? My hope is in thee."
psa39.v8.number_rsv = "(39:8)"
psa39.v8.number = "(39:8)"
psa39.v8.text = "Deliver me from all my transgressions.Make me not the scorn of the fool!"
psa39.v9.number_rsv = "(39:9)"
psa39.v9.number = "(39:9)"
psa39.v9.text = "I am dumb, I do not open my mouth; for it is thou who hast done it."
psa39.v10.number_rsv = "(39:10)"
psa39.v10.number = "(39:10)"
psa39.v10.text = "Remove thy stroke from me; I am spent by the blows of thy hand."
psa39.v11.number_rsv = "(39:11)"
psa39.v11.number = "(39:11)"
psa39.v11.text = "When thou dost chasten manwith rebukes for sin, thou dost consume like a moth what is dear to him; surely every man is a mere breath!"
psa39.v12.number_rsv = "(39:12)"
psa39.v12.number = "(39:12)"
psa39.v12.text = "\"Hear my prayer, O LORD, and give ear to my cry; hold not thy peace at my tears! For I am thy passing guest, a sojourner, like all my fathers."
psa39.v13.number_rsv = "(39:13)"
psa39.v13.number = "(39:13)"
psa39.v13.text = "Look away from me, that I may know gladness, before I depart and be no more!\""

psa40.title = "Psalm 40"
psa40.version = properties_en_US_rsv.version.designation
psa40.text = ""
psa40.v0.number_rsv = "(40:0)"
psa40.v0.number = "(40:0)"
psa40.v0.text = "To the choirmaster. A Psalm of David."
psa40.v1.number_rsv = "(40:1)"
psa40.v1.number = "(40:1)"
psa40.v1.text = "I waited patiently for the LORD; he inclined to me and heard my cry."
psa40.v2.number_rsv = "(40:2)"
psa40.v2.number = "(40:2)"
psa40.v2.text = "He drew me up from the desolate pit, out of the miry bog, and set my feet upon a rock, making my steps secure."
psa40.v3.number_rsv = "(40:3)"
psa40.v3.number = "(40:3)"
psa40.v3.text = "He put a new song in my mouth, a song of praise to our God.Many will see and fear, and put their trust in the LORD."
psa40.v4.number_rsv = "(40:4)"
psa40.v4.number = "(40:4)"
psa40.v4.text = "Blessed is the man who makesthe LORD his trust, who does not turn to the proud, to those who go astray after false gods!"
psa40.v5.number_rsv = "(40:5)"
psa40.v5.number = "(40:5)"
psa40.v5.text = "Thou hast multiplied, O LORD my God, thy wondrous deeds and thy thoughts toward us; none can compare with thee! Were I to proclaim and tell of them, they would be more than can be numbered."
psa40.v6.number_rsv = "(40:6)"
psa40.v6.number = "(40:6)"
psa40.v6.text = "Sacrifice and offering thou dost not desire; but thou hast given me an open ear.Burnt offering and sin offeringthou hast not required."
psa40.v7.number_rsv = "(40:7)"
psa40.v7.number = "(40:7)"
psa40.v7.text = "Then I said, \"Lo, I come; in the roll of the book it is written of me;"
psa40.v8.number_rsv = "(40:8)"
psa40.v8.number = "(40:8)"
psa40.v8.text = "I delight to do thy will, O my God; thy law is within my heart.\""
psa40.v9.number_rsv = "(40:9)"
psa40.v9.number = "(40:9)"
psa40.v9.text = "I have told the glad news of deliverancein the great congregation; lo, I have not restrained my lips, as thou knowest, O LORD."
psa40.v10.number_rsv = "(40:10)"
psa40.v10.number = "(40:10)"
psa40.v10.text = "I have not hid thy saving help within my heart, I have spoken of thy faithfulness and thy salvation; I have not concealed thy steadfast love and thy faithfulnessfrom the great congregation."
psa40.v11.number_rsv = "(40:11)"
psa40.v11.number = "(40:11)"
psa40.v11.text = "Do not thou, O LORD, withholdthy mercy from me, let thy steadfast love and thy faithfulnessever preserve me!"
psa40.v12.number_rsv = "(40:12)"
psa40.v12.number = "(40:12)"
psa40.v12.text = "For evils have encompassed mewithout number; my iniquities have overtaken me, till I cannot see; they are more than the hairs of my head; my heart fails me."
psa40.v13.number_rsv = "(40:13)"
psa40.v13.number = "(40:13)"
psa40.v13.text = "Be pleased, O LORD, to deliver me! O LORD, make haste to help me!"
psa40.v14.number_rsv = "(40:14)"
psa40.v14.number = "(40:14)"
psa40.v14.text = "Let them be put to shame and confusion altogetherwho seek to snatch away my life; let them be turned back and brought to dishonorwho desire my hurt!"
psa40.v15.number_rsv = "(40:15)"
psa40.v15.number = "(40:15)"
psa40.v15.text = "Let them be appalled because of their shamewho say to me, \"Aha, Aha!\""
psa40.v16.number_rsv = "(40:16)"
psa40.v16.number = "(40:16)"
psa40.v16.text = "But may all who seek theerejoice and be glad in thee; may those who love thy salvationsay continually, \"Great is the LORD!\""
psa40.v17.number_rsv = "(40:17)"
psa40.v17.number = "(40:17)"
psa40.v17.text = "As for me, I am poor and needy; but the Lord takes thought for me.Thou art my help and my deliverer; do not tarry, O my God!"

psa41.title = "Psalm 41"
psa41.version = properties_en_US_rsv.version.designation
psa41.text = ""
psa41.v0.number_rsv = "(41:0)"
psa41.v0.number = "(41:0)"
psa41.v0.text = "To the choirmaster. A Psalm of David."
psa41.v1.number_rsv = "(41:1)"
psa41.v1.number = "(41:1)"
psa41.v1.text = "Blessed is he who considers the poor! The LORD delivers him in the day of trouble;"
psa41.v2.number_rsv = "(41:2)"
psa41.v2.number = "(41:2)"
psa41.v2.text = "the LORD protects him and keeps him alive; he is called blessed in the land; thou dost not give him up to the will of his enemies."
psa41.v3.number_rsv = "(41:3)"
psa41.v3.number = "(41:3)"
psa41.v3.text = "The LORD sustains him on his sickbed; in his illness thou healest all his infirmities."
psa41.v4.number_rsv = "(41:4)"
psa41.v4.number = "(41:4)"
psa41.v4.text = "As for me, I said, \"O LORD, be gracious to me; heal me, for I have sinned against thee!\""
psa41.v5.number_rsv = "(41:5)"
psa41.v5.number = "(41:5)"
psa41.v5.text = "My enemies say of me in malice:\"When will he die, and his name perish?\""
psa41.v6.number_rsv = "(41:6)"
psa41.v6.number = "(41:6)"
psa41.v6.text = "And when one comes to see me, he utters empty words, while his heart gathers mischief; when he goes out, he tells it abroad."
psa41.v7.number_rsv = "(41:7)"
psa41.v7.number = "(41:7)"
psa41.v7.text = "All who hate me whisper together about me; they imagine the worst for me."
psa41.v8.number_rsv = "(41:8)"
psa41.v8.number = "(41:8)"
psa41.v8.text = "They say, \"A deadly thing has fastened upon him; he will not rise again from where he lies.\""
psa41.v9.number_rsv = "(41:9)"
psa41.v9.number = "(41:9)"
psa41.v9.text = "Even my bosom friend in whom I trusted, who ate of my bread, has lifted his heel against me."
psa41.v10.number_rsv = "(41:10)"
psa41.v10.number = "(41:10)"
psa41.v10.text = "But do thou, O LORD, be gracious to me, and raise me up, that I may requite them!"
psa41.v11.number_rsv = "(41:11)"
psa41.v11.number = "(41:11)"
psa41.v11.text = "By this I know that thou art pleased with me, in that my enemy has not triumphed over me."
psa41.v12.number_rsv = "(41:12)"
psa41.v12.number = "(41:12)"
psa41.v12.text = "But thou hast upheld me because of my integrity, and set me in thy presence for ever."
psa41.v13.number_rsv = "(41:13)"
psa41.v13.number = "(41:13)"
psa41.v13.text = "Blessed be the LORD, the God of Israel, from everlasting to everlasting! Amen and Amen."

psa42.title = "Psalm 42"
psa42.version = properties_en_US_rsv.version.designation
psa42.text = ""
psa42.v0.number_rsv = "(42:0)"
psa42.v0.number = "(42:0)"
psa42.v0.text = "To the choirmaster. A Maskil of the Sons of Korah."
psa42.v1.number_rsv = "(42:1)"
psa42.v1.number = "(42:1)"
psa42.v1.text = "As a hart longsfor flowing streams, so longs my soulfor thee, O God."
psa42.v2.number_rsv = "(42:2)"
psa42.v2.number = "(42:2)"
psa42.v2.text = "My soul thirsts for God, for the living God.When shall I come and beholdthe face of God?"
psa42.v3.number_rsv = "(42:3)"
psa42.v3.number = "(42:3)"
psa42.v3.text = "My tears have been my foodday and night, while men say to me continually,\"Where is your God?\""
psa42.v4.number_rsv = "(42:4)"
psa42.v4.number = "(42:4)"
psa42.v4.text = "These things I remember, as I pour out my soul:how I went with the throng, and led them in procession to the house of God, with glad shouts and songs of thanksgiving, a multitude keeping festival."
psa42.v5.number_rsv = "(42:5)"
psa42.v5.number = "(42:5)"
psa42.v5.text = "Why are you cast down, O my soul, and why are you disquieted within me? Hope in God; for I shall again praise him, my help"
psa42.v6.number_rsv = "(42:6)"
psa42.v6.number = "(42:6)"
psa42.v6.text = "and my God.My soul is cast down within me, therefore I remember theefrom the land of Jordan and of Hermon, from Mount Mizar."
psa42.v7.number_rsv = "(42:7)"
psa42.v7.number = "(42:7)"
psa42.v7.text = "Deep calls to deepat the thunder of thy cataracts; all thy waves and thy billowshave gone over me."
psa42.v8.number_rsv = "(42:8)"
psa42.v8.number = "(42:8)"
psa42.v8.text = "By day the LORD commands his steadfast love; and at night his song is with me, a prayer to the God of my life."
psa42.v9.number_rsv = "(42:9)"
psa42.v9.number = "(42:9)"
psa42.v9.text = "I say to God, my rock:\"Why hast thou forgotten me? Why go I mourningbecause of the oppression of the enemy?\""
psa42.v10.number_rsv = "(42:10)"
psa42.v10.number = "(42:10)"
psa42.v10.text = "As with a deadly wound in my body, my adversaries taunt me, while they say to me continually,\"Where is your God?\""
psa42.v11.number_rsv = "(42:11)"
psa42.v11.number = "(42:11)"
psa42.v11.text = "Why are you cast down, O my soul, and why are you disquieted within me? Hope in God; for I shall again praise him, my help and my God."

psa43.title = "Psalm 43"
psa43.version = properties_en_US_rsv.version.designation
psa43.text = ""
psa43.v1.number_rsv = "(43:1)"
psa43.v1.number = "(43:1)"
psa43.v1.text = "Vindicate me, O God, and defend my causeagainst an ungodly people; from deceitful and unjust mendeliver me!"
psa43.v2.number_rsv = "(43:2)"
psa43.v2.number = "(43:2)"
psa43.v2.text = "For thou art the God in whom I take refuge; why hast thou cast me off? Why go I mourningbecause of the oppression of the enemy?"
psa43.v3.number_rsv = "(43:3)"
psa43.v3.number = "(43:3)"
psa43.v3.text = "Oh send out thy light and thy truth; let them lead me, let them bring me to thy holy hilland to thy dwelling!"
psa43.v4.number_rsv = "(43:4)"
psa43.v4.number = "(43:4)"
psa43.v4.text = "Then I will go to the altar of God, to God my exceeding joy; and I will praise thee with the lyre, O God, my God."
psa43.v5.number_rsv = "(43:5)"
psa43.v5.number = "(43:5)"
psa43.v5.text = "Why are you cast down, O my soul, and why are you disquieted within me? Hope in God; for I shall again praise him, my help and my God.R"

psa44.title = "Psalm 44"
psa44.version = properties_en_US_rsv.version.designation
psa44.text = ""
psa44.v0.number_rsv = "(44:0)"
psa44.v0.number = "(44:0)"
psa44.v0.text = "To the choirmaster. A Maskil of the Sons of Korah."
psa44.v1.number_rsv = "(44:1)"
psa44.v1.number = "(44:1)"
psa44.v1.text = "We have heard with our ears, O God, our fathers have told us, what deeds thou didst perform in their days, in the days of old:"
psa44.v2.number_rsv = "(44:2)"
psa44.v2.number = "(44:2)"
psa44.v2.text = "thou with thy own hand didst drive out the nations, but them thou didst plant; thou didst afflict the peoples, but them thou didst set free;"
psa44.v3.number_rsv = "(44:3)"
psa44.v3.number = "(44:3)"
psa44.v3.text = "for not by their own sword did they win the land, nor did their own arm give them victory; but thy right hand, and thy arm, and the light of thy countenance; for thou didst delight in them."
psa44.v4.number_rsv = "(44:4)"
psa44.v4.number = "(44:4)"
psa44.v4.text = "Thou art my King and my God, who ordainest victories for Jacob."
psa44.v5.number_rsv = "(44:5)"
psa44.v5.number = "(44:5)"
psa44.v5.text = "Through thee we push down our foes; through thy name we tread down our assailants."
psa44.v6.number_rsv = "(44:6)"
psa44.v6.number = "(44:6)"
psa44.v6.text = "For not in my bow do I trust, nor can my sword save me."
psa44.v7.number_rsv = "(44:7)"
psa44.v7.number = "(44:7)"
psa44.v7.text = "But thou hast saved us from our foes, and hast put to confusion those who hate us."
psa44.v8.number_rsv = "(44:8)"
psa44.v8.number = "(44:8)"
psa44.v8.text = "In God we have boasted continually, and we will give thanks to thy name for ever."
psa44.v9.number_rsv = "(44:9)"
psa44.v9.number = "(44:9)"
psa44.v9.text = "Yet thou hast cast us off and abased us, and hast not gone out with our armies."
psa44.v10.number_rsv = "(44:10)"
psa44.v10.number = "(44:10)"
psa44.v10.text = "Thou hast made us turn back from the foe; and our enemies have gotten spoil."
psa44.v11.number_rsv = "(44:11)"
psa44.v11.number = "(44:11)"
psa44.v11.text = "Thou hast made us like sheep for slaughter, and hast scattered us among the nations."
psa44.v12.number_rsv = "(44:12)"
psa44.v12.number = "(44:12)"
psa44.v12.text = "Thou hast sold thy people for a trifle, demanding no high price for them."
psa44.v13.number_rsv = "(44:13)"
psa44.v13.number = "(44:13)"
psa44.v13.text = "Thou hast made us the taunt of our neighbors, the derision and scorn of those about us."
psa44.v14.number_rsv = "(44:14)"
psa44.v14.number = "(44:14)"
psa44.v14.text = "Thou hast made us a byword among the nations, a laughingstock among the peoples."
psa44.v15.number_rsv = "(44:15)"
psa44.v15.number = "(44:15)"
psa44.v15.text = "All day long my disgrace is before me, and shame has covered my face,"
psa44.v16.number_rsv = "(44:16)"
psa44.v16.number = "(44:16)"
psa44.v16.text = "at the words of the taunters and revilers, at the sight of the enemy and the avenger."
psa44.v17.number_rsv = "(44:17)"
psa44.v17.number = "(44:17)"
psa44.v17.text = "All this has come upon us, though we have not forgotten thee, or been false to thy covenant."
psa44.v18.number_rsv = "(44:18)"
psa44.v18.number = "(44:18)"
psa44.v18.text = "Our heart has not turned back, nor have our steps departed from thy way,"
psa44.v19.number_rsv = "(44:19)"
psa44.v19.number = "(44:19)"
psa44.v19.text = "that thou shouldst have broken us in the place of jackals, and covered us with deep darkness."
psa44.v20.number_rsv = "(44:20)"
psa44.v20.number = "(44:20)"
psa44.v20.text = "If we had forgotten the name of our God, or spread forth our hands to a strange god,"
psa44.v21.number_rsv = "(44:21)"
psa44.v21.number = "(44:21)"
psa44.v21.text = "would not God discover this? For he knows the secrets of the heart."
psa44.v22.number_rsv = "(44:22)"
psa44.v22.number = "(44:22)"
psa44.v22.text = "Nay, for thy sake we are slain all the day long, and accounted as sheep for the slaughter."
psa44.v23.number_rsv = "(44:23)"
psa44.v23.number = "(44:23)"
psa44.v23.text = "Rouse thyself! Why sleepest thou, O Lord? Awake! Do not cast us off for ever!"
psa44.v24.number_rsv = "(44:24)"
psa44.v24.number = "(44:24)"
psa44.v24.text = "Why dost thou hide thy face? Why dost thou forget our affliction and oppression?"
psa44.v25.number_rsv = "(44:25)"
psa44.v25.number = "(44:25)"
psa44.v25.text = "For our soul is bowed down to the dust; our body cleaves to the ground."
psa44.v26.number_rsv = "(44:26)"
psa44.v26.number = "(44:26)"
psa44.v26.text = "Rise up, come to our help! Deliver us for the sake of thy steadfast love!"

psa45.title = "Psalm 45"
psa45.version = properties_en_US_rsv.version.designation
psa45.text = ""
psa45.v0.number_rsv = "(45:0)"
psa45.v0.number = "(45:0)"
psa45.v0.text = "To the choirmaster: according to Lilies. A Maskil of the Sons of Korah a love song."
psa45.v1.number_rsv = "(45:1)"
psa45.v1.number = "(45:1)"
psa45.v1.text = "My heart overflows with a goodly theme; I address my verses to the king; my tongue is like the pen of a ready scribe."
psa45.v2.number_rsv = "(45:2)"
psa45.v2.number = "(45:2)"
psa45.v2.text = "You are the fairest of the sons of men; grace is poured upon your lips; therefore God has blessed you for ever."
psa45.v3.number_rsv = "(45:3)"
psa45.v3.number = "(45:3)"
psa45.v3.text = "Gird your sword upon your thigh, O mighty one, in your glory and majesty!"
psa45.v4.number_rsv = "(45:4)"
psa45.v4.number = "(45:4)"
psa45.v4.text = "In your majesty ride forth victoriouslyfor the cause of truth and to defend the right; let your right hand teach you dread deeds!"
psa45.v5.number_rsv = "(45:5)"
psa45.v5.number = "(45:5)"
psa45.v5.text = "Your arrows are sharpin the heart of the king\"s enemies; the peoples fall under you."
psa45.v6.number_rsv = "(45:6)"
psa45.v6.number = "(45:6)"
psa45.v6.text = "Your divine throne endures for ever and ever.Your royal scepter is a scepter of equity;"
psa45.v7.number_rsv = "(45:7)"
psa45.v7.number = "(45:7)"
psa45.v7.text = "you love righteousness and hate wickedness.Therefore God, your God, has anointed youwith the oil of gladness above your fellows;"
psa45.v8.number_rsv = "(45:8)"
psa45.v8.number = "(45:8)"
psa45.v8.text = "your robes are all fragrant with myrrh and aloes and cassia.From ivory palaces stringed instruments make you glad;"
psa45.v9.number_rsv = "(45:9)"
psa45.v9.number = "(45:9)"
psa45.v9.text = "daughters of kings are among your ladies of honor; at your right hand stands the queen in gold of Ophir."
psa45.v10.number_rsv = "(45:10)"
psa45.v10.number = "(45:10)"
psa45.v10.text = "Hear, O daughter, consider, and incline your ear; forget your people and your father\"s house;"
psa45.v11.number_rsv = "(45:11)"
psa45.v11.number = "(45:11)"
psa45.v11.text = "and the king will desire your beauty.Since he is your lord, bow to him;"
psa45.v12.number_rsv = "(45:12)"
psa45.v12.number = "(45:12)"
psa45.v12.text = "the people of Tyre will sue your favor with gifts, the richest of the people"
psa45.v13.number_rsv = "(45:13)"
psa45.v13.number = "(45:13)"
psa45.v13.text = "with all kinds of wealth.The princess is decked in her chamber with gold-woven robes;"
psa45.v14.number_rsv = "(45:14)"
psa45.v14.number = "(45:14)"
psa45.v14.text = "in many-colored robes she is led to the king, with her virgin companions, her escort, in her train."
psa45.v15.number_rsv = "(45:15)"
psa45.v15.number = "(45:15)"
psa45.v15.text = "With joy and gladness they are led alongas they enter the palace of the king."
psa45.v16.number_rsv = "(45:16)"
psa45.v16.number = "(45:16)"
psa45.v16.text = "Instead of your fathers shall be your sons; you will make them princes in all the earth."
psa45.v17.number_rsv = "(45:17)"
psa45.v17.number = "(45:17)"
psa45.v17.text = "I will cause your name to be celebrated in all generations; therefore the peoples will praise you for ever and ever."

psa46.title = "Psalm 46"
psa46.version = properties_en_US_rsv.version.designation
psa46.text = ""
psa46.v0.number_rsv = "(46:0)"
psa46.v0.number = "(46:0)"
psa46.v0.text = "To the choirmaster. A Psalm of the Sons of Korah. According to Alamoth. A Song."
psa46.v1.number_rsv = "(46:1)"
psa46.v1.number = "(46:1)"
psa46.v1.text = "God is our refuge and strength, a very present help in trouble."
psa46.v2.number_rsv = "(46:2)"
psa46.v2.number = "(46:2)"
psa46.v2.text = "Therefore we will not fear though the earth should change, though the mountains shake in the heart of the sea;"
psa46.v3.number_rsv = "(46:3)"
psa46.v3.number = "(46:3)"
psa46.v3.text = "though its waters roar and foam, though the mountains tremble with its tumult."
psa46.v4.number_rsv = "(46:4)"
psa46.v4.number = "(46:4)"
psa46.v4.text = "There is a river whose streams make glad the city of God, the holy habitation of the Most High."
psa46.v5.number_rsv = "(46:5)"
psa46.v5.number = "(46:5)"
psa46.v5.text = "God is in the midst of her, she shall not be moved; God will help her right early."
psa46.v6.number_rsv = "(46:6)"
psa46.v6.number = "(46:6)"
psa46.v6.text = "The nations rage, the kingdoms totter; he utters his voice, the earth melts."
psa46.v7.number_rsv = "(46:7)"
psa46.v7.number = "(46:7)"
psa46.v7.text = "The LORD of hosts is with us; the God of Jacob is our refuge."
psa46.v8.number_rsv = "(46:8)"
psa46.v8.number = "(46:8)"
psa46.v8.text = "Come, behold the works of the LORD, how he has wrought desolations in the earth."
psa46.v9.number_rsv = "(46:9)"
psa46.v9.number = "(46:9)"
psa46.v9.text = "He makes wars cease to the end of the earth; he breaks the bow, and shatters the spear, he burns the chariots with fire!"
psa46.v10.number_rsv = "(46:10)"
psa46.v10.number = "(46:10)"
psa46.v10.text = "\"Be still, and know that I am God.I am exalted among the nations, I am exalted in the earth!\""
psa46.v11.number_rsv = "(46:11)"
psa46.v11.number = "(46:11)"
psa46.v11.text = "The LORD of hosts is with us; the God of Jacob is our refuge."

kathisma_7.title = "KATHISMA 7."

psa47.title = "Psalm 47"
psa47.version = properties_en_US_rsv.version.designation
psa47.text = ""
psa47.v0.number_rsv = "(47:0)"
psa47.v0.number = "(47:0)"
psa47.v0.text = "To the choirmaster. A Psalm of the Sons of Korah."
psa47.v1.number_rsv = "(47:1)"
psa47.v1.number = "(47:1)"
psa47.v1.text = "Clap your hands, all peoples! Shout to God with loud songs of joy!"
psa47.v2.number_rsv = "(47:2)"
psa47.v2.number = "(47:2)"
psa47.v2.text = "For the LORD, the Most High, is terrible, a great king over all the earth."
psa47.v3.number_rsv = "(47:3)"
psa47.v3.number = "(47:3)"
psa47.v3.text = "He subdued peoples under us, and nations under our feet."
psa47.v4.number_rsv = "(47:4)"
psa47.v4.number = "(47:4)"
psa47.v4.text = "He chose our heritage for us, the pride of Jacob whom he loves."
psa47.v5.number_rsv = "(47:5)"
psa47.v5.number = "(47:5)"
psa47.v5.text = "God has gone up with a shout, the LORD with the sound of a trumpet."
psa47.v6.number_rsv = "(47:6)"
psa47.v6.number = "(47:6)"
psa47.v6.text = "Sing praises to God, sing praises! Sing praises to our King, sing praises!"
psa47.v7.number_rsv = "(47:7)"
psa47.v7.number = "(47:7)"
psa47.v7.text = "For God is the king of all the earth; sing praises with a psalm!"
psa47.v8.number_rsv = "(47:8)"
psa47.v8.number = "(47:8)"
psa47.v8.text = "God reigns over the nations; God sits on his holy throne."
psa47.v9.number_rsv = "(47:9)"
psa47.v9.number = "(47:9)"
psa47.v9.text = "The princes of the peoples gatheras the people of the God of Abraham.For the shields of the earth belong to God; he is highly exalted!"

psa48.title = "Psalm 48"
psa48.version = properties_en_US_rsv.version.designation
psa48.text = ""
psa48.v0.number_rsv = "(48:0)"
psa48.v0.number = "(48:0)"
psa48.v0.text = "A Song. A Psalm of the Sons of Korah."
psa48.v1.number_rsv = "(48:1)"
psa48.v1.number = "(48:1)"
psa48.v1.text = "Great is the LORD and greatly to be praisedin the city of our God! His holy mountain,"
psa48.v2.number_rsv = "(48:2)"
psa48.v2.number = "(48:2)"
psa48.v2.text = "beautiful in elevation, is the joy of all the earth, Mount Zion, in the far north, the city of the great King."
psa48.v3.number_rsv = "(48:3)"
psa48.v3.number = "(48:3)"
psa48.v3.text = "Within her citadels Godhas shown himself a sure defense."
psa48.v4.number_rsv = "(48:4)"
psa48.v4.number = "(48:4)"
psa48.v4.text = "For lo, the kings assembled, they came on together."
psa48.v5.number_rsv = "(48:5)"
psa48.v5.number = "(48:5)"
psa48.v5.text = "As soon as they saw it, they were astounded, they were in panic, they took to flight;"
psa48.v6.number_rsv = "(48:6)"
psa48.v6.number = "(48:6)"
psa48.v6.text = "trembling took hold of them there, anguish as of a woman in travail."
psa48.v7.number_rsv = "(48:7)"
psa48.v7.number = "(48:7)"
psa48.v7.text = "By the east wind thou didst shatterthe ships of Tarshish."
psa48.v8.number_rsv = "(48:8)"
psa48.v8.number = "(48:8)"
psa48.v8.text = "As we have heard, so have we seenin the city of the LORD of hosts, in the city of our God, which God establishes for ever."
psa48.v9.number_rsv = "(48:9)"
psa48.v9.number = "(48:9)"
psa48.v9.text = "We have thought on thy steadfast love, O God, in the midst of thy temple."
psa48.v10.number_rsv = "(48:10)"
psa48.v10.number = "(48:10)"
psa48.v10.text = "As thy name, O God, so thy praise reaches to the ends of the earth.Thy right hand is filled with victory;"
psa48.v11.number_rsv = "(48:11)"
psa48.v11.number = "(48:11)"
psa48.v11.text = "let Mount Zion be glad! Let the daughters of Judah rejoicebecause of thy judgments!"
psa48.v12.number_rsv = "(48:12)"
psa48.v12.number = "(48:12)"
psa48.v12.text = "Walk about Zion, go round about her, number her towers,"
psa48.v13.number_rsv = "(48:13)"
psa48.v13.number = "(48:13)"
psa48.v13.text = "consider well her ramparts, go through her citadels; that you may tell the next generation"
psa48.v14.number_rsv = "(48:14)"
psa48.v14.number = "(48:14)"
psa48.v14.text = "that this is God, our God for ever and ever.He will be our guide for ever."

psa49.title = "Psalm 49"
psa49.version = properties_en_US_rsv.version.designation
psa49.text = ""
psa49.v0.number_rsv = "(49:0)"
psa49.v0.number = "(49:0)"
psa49.v0.text = "To the choirmaster. A Psalm of the Sons of Korah."
psa49.v1.number_rsv = "(49:1)"
psa49.v1.number = "(49:1)"
psa49.v1.text = "Hear this, all peoples! Give ear, all inhabitants of the world,"
psa49.v2.number_rsv = "(49:2)"
psa49.v2.number = "(49:2)"
psa49.v2.text = "both low and high, rich and poor together!"
psa49.v3.number_rsv = "(49:3)"
psa49.v3.number = "(49:3)"
psa49.v3.text = "My mouth shall speak wisdom; the meditation of my heart shall be understanding."
psa49.v4.number_rsv = "(49:4)"
psa49.v4.number = "(49:4)"
psa49.v4.text = "I will incline my ear to a proverb; I will solve my riddle to the music of the lyre."
psa49.v5.number_rsv = "(49:5)"
psa49.v5.number = "(49:5)"
psa49.v5.text = "Why should I fear in times of trouble, when the iniquity of my persecutors surrounds me,"
psa49.v6.number_rsv = "(49:6)"
psa49.v6.number = "(49:6)"
psa49.v6.text = "men who trust in their wealthand boast of the abundance of their riches?"
psa49.v7.number_rsv = "(49:7)"
psa49.v7.number = "(49:7)"
psa49.v7.text = "Truly no man can ransom himself, or give to God the price of his life,"
psa49.v8.number_rsv = "(49:8)"
psa49.v8.number = "(49:8)"
psa49.v8.text = "for the ransom of his life is costly, and can never suffice,"
psa49.v9.number_rsv = "(49:9)"
psa49.v9.number = "(49:9)"
psa49.v9.text = "that he should continue to live on for ever, and never see the Pit."
psa49.v10.number_rsv = "(49:10)"
psa49.v10.number = "(49:10)"
psa49.v10.text = "Yea, he shall see that even the wise die, the fool and the stupid alike must perishand leave their wealth to others."
psa49.v11.number_rsv = "(49:11)"
psa49.v11.number = "(49:11)"
psa49.v11.text = "Their graves are their homes for ever, their dwelling places to all generations, though they named lands their own."
psa49.v12.number_rsv = "(49:12)"
psa49.v12.number = "(49:12)"
psa49.v12.text = "Man cannot abide in his pomp, he is like the beasts that perish."
psa49.v13.number_rsv = "(49:13)"
psa49.v13.number = "(49:13)"
psa49.v13.text = "This is the fate of those who have foolish confidence, the end of those who are pleased with their portion."
psa49.v14.number_rsv = "(49:14)"
psa49.v14.number = "(49:14)"
psa49.v14.text = "Like sheep they are appointed for Sheol; Death shall be their shepherd; straight to the grave they descend, and their form shall waste away; Sheol shall be their home."
psa49.v15.number_rsv = "(49:15)"
psa49.v15.number = "(49:15)"
psa49.v15.text = "But God will ransom my soul from the power of Sheol, for he will receive me."
psa49.v16.number_rsv = "(49:16)"
psa49.v16.number = "(49:16)"
psa49.v16.text = "Be not afraid when one becomes rich, when the glory of his house increases."
psa49.v17.number_rsv = "(49:17)"
psa49.v17.number = "(49:17)"
psa49.v17.text = "For when he dies he will carry nothing away; his glory will not go down after him."
psa49.v18.number_rsv = "(49:18)"
psa49.v18.number = "(49:18)"
psa49.v18.text = "Though, while he lives, he counts himself happy, and though a man gets praise when he does well for himself,"
psa49.v19.number_rsv = "(49:19)"
psa49.v19.number = "(49:19)"
psa49.v19.text = "he will go to the generation of his fathers, who will never more see the light."
psa49.v20.number_rsv = "(49:20)"
psa49.v20.number = "(49:20)"
psa49.v20.text = "Man cannot abide in his pomp, he is like the beasts that perish."

psa50.title = "Psalm 50"
psa50.version = properties_en_US_rsv.version.designation
psa50.text = ""
psa50.v0.number_rsv = "(50:0)"
psa50.v0.number = "(50:0)"
psa50.v0.text = "A Psalm of Asaph."
psa50.v1.number_rsv = "(50:1)"
psa50.v1.number = "(50:1)"
psa50.v1.text = "The Mighty One, God the LORD, speaks and summons the earthfrom the rising of the sun to its setting."
psa50.v2.number_rsv = "(50:2)"
psa50.v2.number = "(50:2)"
psa50.v2.text = "Out of Zion, the perfection of beauty, God shines forth."
psa50.v3.number_rsv = "(50:3)"
psa50.v3.number = "(50:3)"
psa50.v3.text = "Our God comes, he does not keep silence, before him is a devouring fire, round about him a mighty tempest."
psa50.v4.number_rsv = "(50:4)"
psa50.v4.number = "(50:4)"
psa50.v4.text = "He calls to the heavens aboveand to the earth, that he may judge his people:"
psa50.v5.number_rsv = "(50:5)"
psa50.v5.number = "(50:5)"
psa50.v5.text = "\"Gather to me my faithful ones, who made a covenant with me by sacrifice!\""
psa50.v6.number_rsv = "(50:6)"
psa50.v6.number = "(50:6)"
psa50.v6.text = "The heavens declare his righteousness, for God himself is judge!"
psa50.v7.number_rsv = "(50:7)"
psa50.v7.number = "(50:7)"
psa50.v7.text = "\"Hear, O my people, and I will speak, O Israel, I will testify against you.I am God, your God."
psa50.v8.number_rsv = "(50:8)"
psa50.v8.number = "(50:8)"
psa50.v8.text = "I do not reprove you for your sacrifices; your burnt offerings are continually before me."
psa50.v9.number_rsv = "(50:9)"
psa50.v9.number = "(50:9)"
psa50.v9.text = "I will accept no bull from your house, nor he-goat from your folds."
psa50.v10.number_rsv = "(50:10)"
psa50.v10.number = "(50:10)"
psa50.v10.text = "For every beast of the forest is mine, the cattle on a thousand hills."
psa50.v11.number_rsv = "(50:11)"
psa50.v11.number = "(50:11)"
psa50.v11.text = "I know all the birds of the air, and all that moves in the field is mine."
psa50.v12.number_rsv = "(50:12)"
psa50.v12.number = "(50:12)"
psa50.v12.text = "\"If I were hungry, I would not tell you; for the world and all that is in it is mine."
psa50.v13.number_rsv = "(50:13)"
psa50.v13.number = "(50:13)"
psa50.v13.text = "Do I eat the flesh of bulls, or drink the blood of goats?"
psa50.v14.number_rsv = "(50:14)"
psa50.v14.number = "(50:14)"
psa50.v14.text = "Offer to God a sacrifice of thanksgiving, and pay your vows to the Most High;"
psa50.v15.number_rsv = "(50:15)"
psa50.v15.number = "(50:15)"
psa50.v15.text = "and call upon me in the day of trouble; I will deliver you, and you shall glorify me.\""
psa50.v16.number_rsv = "(50:16)"
psa50.v16.number = "(50:16)"
psa50.v16.text = "But to the wicked God says:\"What right have you to recite my statutes, or take my covenant on your lips?"
psa50.v17.number_rsv = "(50:17)"
psa50.v17.number = "(50:17)"
psa50.v17.text = "For you hate discipline, and you cast my words behind you."
psa50.v18.number_rsv = "(50:18)"
psa50.v18.number = "(50:18)"
psa50.v18.text = "If you see a thief, you are a friend of his; and you keep company with adulterers."
psa50.v19.number_rsv = "(50:19)"
psa50.v19.number = "(50:19)"
psa50.v19.text = "\"You give your mouth free rein for evil, and your tongue frames deceit."
psa50.v20.number_rsv = "(50:20)"
psa50.v20.number = "(50:20)"
psa50.v20.text = "You sit and speak against your brother; you slander your own mother\"s son."
psa50.v21.number_rsv = "(50:21)"
psa50.v21.number = "(50:21)"
psa50.v21.text = "These things you have done and I have been silent; you thought that I was one like yourself.But now I rebuke you, and lay the charge before you."
psa50.v22.number_rsv = "(50:22)"
psa50.v22.number = "(50:22)"
psa50.v22.text = "\"Mark this, then, you who forget God, lest I rend, and there be none to deliver!"
psa50.v23.number_rsv = "(50:23)"
psa50.v23.number = "(50:23)"
psa50.v23.text = "He who brings thanksgiving as his sacrifice honors me; to him who orders his way arightI will show the salvation of God!\""

psa51.title = "Psalm 51"
psa51.version = properties_en_US_rsv.version.designation
psa51.text = ""
psa51.v0.number_rsv = "(51:0)"
psa51.v0.number = "(51:0)"
psa51.v0.text = "To the choirmaster. A Psalm of David, when Nathan the prophet came to him, after he had gone in to Bathsheba."
psa51.v1.number_rsv = "(51:1)"
psa51.v1.number = "(51:1)"
psa51.v1.text = "Have mercy on me, O God, according to thy steadfast love; according to thy abundant mercy blot out my transgressions."
psa51.v2.number_rsv = "(51:2)"
psa51.v2.number = "(51:2)"
psa51.v2.text = "Wash me thoroughly from my iniquity, and cleanse me from my sin!"
psa51.v3.number_rsv = "(51:3)"
psa51.v3.number = "(51:3)"
psa51.v3.text = "For I know my transgressions, and my sin is ever before me."
psa51.v4.number_rsv = "(51:4)"
psa51.v4.number = "(51:4)"
psa51.v4.text = "Against thee, thee only, have I sinned, and done that which is evil in thy sight, so that thou art justified in thy sentenceand blameless in thy judgment."
psa51.v5.number_rsv = "(51:5)"
psa51.v5.number = "(51:5)"
psa51.v5.text = "Behold, I was brought forth in iniquity, and in sin did my mother conceive me."
psa51.v6.number_rsv = "(51:6)"
psa51.v6.number = "(51:6)"
psa51.v6.text = "Behold, thou desirest truth in the inward being; therefore teach me wisdom in my secret heart."
psa51.v7.number_rsv = "(51:7)"
psa51.v7.number = "(51:7)"
psa51.v7.text = "Purge me with hyssop, and I shall be clean; wash me, and I shall be whiter than snow."
psa51.v8.number_rsv = "(51:8)"
psa51.v8.number = "(51:8)"
psa51.v8.text = "Fill me with joy and gladness; let the bones which thou hast broken rejoice."
psa51.v9.number_rsv = "(51:9)"
psa51.v9.number = "(51:9)"
psa51.v9.text = "Hide thy face from my sins, and blot out all my iniquities."
psa51.v10.number_rsv = "(51:10)"
psa51.v10.number = "(51:10)"
psa51.v10.text = "Create in me a clean heart, O God, and put a new and right spirit within me."
psa51.v11.number_rsv = "(51:11)"
psa51.v11.number = "(51:11)"
psa51.v11.text = "Cast me not away from thy presence, and take not thy holy Spirit from me."
psa51.v12.number_rsv = "(51:12)"
psa51.v12.number = "(51:12)"
psa51.v12.text = "Restore to me the joy of thy salvation, and uphold me with a willing spirit."
psa51.v13.number_rsv = "(51:13)"
psa51.v13.number = "(51:13)"
psa51.v13.text = "Then I will teach transgressors thy ways, and sinners will return to thee."
psa51.v14.number_rsv = "(51:14)"
psa51.v14.number = "(51:14)"
psa51.v14.text = "Deliver me from bloodguiltiness, O God, thou God of my salvation, and my tongue will sing aloud of thy deliverance."
psa51.v15.number_rsv = "(51:15)"
psa51.v15.number = "(51:15)"
psa51.v15.text = "O Lord, open thou my lips, and my mouth shall show forth thy praise."
psa51.v16.number_rsv = "(51:16)"
psa51.v16.number = "(51:16)"
psa51.v16.text = "For thou hast no delight in sacrifice; were I to give a burnt offering, thou wouldst not be pleased."
psa51.v17.number_rsv = "(51:17)"
psa51.v17.number = "(51:17)"
psa51.v17.text = "The sacrifice acceptable to God is a broken spirit; a broken and contrite heart, O God, thou wilt not despise."
psa51.v18.number_rsv = "(51:18)"
psa51.v18.number = "(51:18)"
psa51.v18.text = "Do good to Zion in thy good pleasure; rebuild the walls of Jerusalem,"
psa51.v19.number_rsv = "(51:19)"
psa51.v19.number = "(51:19)"
psa51.v19.text = "then wilt thou delight in right sacrifices, in burnt offerings and whole burnt offerings; then bulls will be offered on thy altar."

psa52.title = "Psalm 52"
psa52.version = properties_en_US_rsv.version.designation
psa52.text = ""
psa52.v0.number_rsv = "(52:0)"
psa52.v0.number = "(52:0)"
psa52.v0.text = "To the choirmaster. A Maskil of David, when Doeg, the Edomite, came and told Saul, \"David has come to the house of Ahimelech.\""
psa52.v1.number_rsv = "(52:1)"
psa52.v1.number = "(52:1)"
psa52.v1.text = "Why do you boast, O mighty man, of mischief done against the godly? All the day"
psa52.v2.number_rsv = "(52:2)"
psa52.v2.number = "(52:2)"
psa52.v2.text = "you are plotting destruction.Your tongue is like a sharp razor, you worker of treachery."
psa52.v3.number_rsv = "(52:3)"
psa52.v3.number = "(52:3)"
psa52.v3.text = "You love evil more than good, and lying more than speaking the truth."
psa52.v4.number_rsv = "(52:4)"
psa52.v4.number = "(52:4)"
psa52.v4.text = "You love all words that devour, O deceitful tongue."
psa52.v5.number_rsv = "(52:5)"
psa52.v5.number = "(52:5)"
psa52.v5.text = "But God will break you down for ever; he will snatch and tear you from your tent; he will uproot you from the land of the living."
psa52.v6.number_rsv = "(52:6)"
psa52.v6.number = "(52:6)"
psa52.v6.text = "The righteous shall see, and fear, and shall laugh at him, saying,"
psa52.v7.number_rsv = "(52:7)"
psa52.v7.number = "(52:7)"
psa52.v7.text = "\"See the man who would not makeGod his refuge, but trusted in the abundance of his riches, and sought refuge in his wealth!\""
psa52.v8.number_rsv = "(52:8)"
psa52.v8.number = "(52:8)"
psa52.v8.text = "But I am like a green olive treein the house of God.I trust in the steadfast love of Godfor ever and ever."
psa52.v9.number_rsv = "(52:9)"
psa52.v9.number = "(52:9)"
psa52.v9.text = "I will thank thee for ever, because thou hast done it.I will proclaim thy name, for it is good, in the presence of the godly."

psa53.title = "Psalm 53"
psa53.version = properties_en_US_rsv.version.designation
psa53.text = ""
psa53.v0.number_rsv = "(53:0)"
psa53.v0.number = "(53:0)"
psa53.v0.text = "To the choirmaster: according to Mahalath. A Maskil of David."
psa53.v1.number_rsv = "(53:1)"
psa53.v1.number = "(53:1)"
psa53.v1.text = "The fool says in his heart,\"There is no God.\"They are corrupt, doing abominable iniquity; there is none that does good."
psa53.v2.number_rsv = "(53:2)"
psa53.v2.number = "(53:2)"
psa53.v2.text = "God looks down from heavenupon the sons of mento see if there are any that are wise, that seek after God."
psa53.v3.number_rsv = "(53:3)"
psa53.v3.number = "(53:3)"
psa53.v3.text = "They have all fallen away; they are all alike depraved; there is none that does good, no, not one."
psa53.v4.number_rsv = "(53:4)"
psa53.v4.number = "(53:4)"
psa53.v4.text = "Have those who work evil no understanding, who eat up my people as they eat bread, and do not call upon God?"
psa53.v5.number_rsv = "(53:5)"
psa53.v5.number = "(53:5)"
psa53.v5.text = "There they are, in great terror, in terror such as has not been! For God will scatter the bones of the ungodly; they will be put to shame, for God has rejected them."
psa53.v6.number_rsv = "(53:6)"
psa53.v6.number = "(53:6)"
psa53.v6.text = "O that deliverance for Israel would come from Zion! When God restores the fortunes of his people, Jacob will rejoice and Israel be glad."

psa54.title = "Psalm 54"
psa54.version = properties_en_US_rsv.version.designation
psa54.text = ""
psa54.v0.number_rsv = "(54:0)"
psa54.v0.number = "(54:0)"
psa54.v0.text = "To the choirmaster: with stringed instruments. A Maskil of David, when the Ziphites went and told Saul, \"David is in hiding among us.\""
psa54.v1.number_rsv = "(54:1)"
psa54.v1.number = "(54:1)"
psa54.v1.text = "Save me, O God, by thy name, and vindicate me by thy might."
psa54.v2.number_rsv = "(54:2)"
psa54.v2.number = "(54:2)"
psa54.v2.text = "Hear my prayer, O God; give ear to the words of my mouth."
psa54.v3.number_rsv = "(54:3)"
psa54.v3.number = "(54:3)"
psa54.v3.text = "For insolent men have risen against me, ruthless men seek my life; they do not set God before them."
psa54.v4.number_rsv = "(54:4)"
psa54.v4.number = "(54:4)"
psa54.v4.text = "Behold, God is my helper; the Lord is the upholder of my life."
psa54.v5.number_rsv = "(54:5)"
psa54.v5.number = "(54:5)"
psa54.v5.text = "He will requite my enemies with evil; in thy faithfulness put an end to them."
psa54.v6.number_rsv = "(54:6)"
psa54.v6.number = "(54:6)"
psa54.v6.text = "With a freewill offering I will sacrifice to thee; I will give thanks to thy name, O LORD, for it is good."
psa54.v7.number_rsv = "(54:7)"
psa54.v7.number = "(54:7)"
psa54.v7.text = "For thou hast delivered me from every trouble, and my eye has looked in triumph on my enemies.R"

psa55.title = "Psalm 55"
psa55.version = properties_en_US_rsv.version.designation
psa55.text = ""
psa55.v0.number_rsv = "(55:0)"
psa55.v0.number = "(55:0)"
psa55.v0.text = "To the choirmaster: with stringed instruments. A Maskil of David."
psa55.v1.number_rsv = "(55:1)"
psa55.v1.number = "(55:1)"
psa55.v1.text = "Give ear to my prayer, O God; and hide not thyself from my supplication!"
psa55.v2.number_rsv = "(55:2)"
psa55.v2.number = "(55:2)"
psa55.v2.text = "Attend to me, and answer me; I am overcome by my trouble.I am distraught"
psa55.v3.number_rsv = "(55:3)"
psa55.v3.number = "(55:3)"
psa55.v3.text = "by the noise of the enemy, because of the oppression of the wicked.For they bring trouble upon me, and in anger they cherish enmity against me."
psa55.v4.number_rsv = "(55:4)"
psa55.v4.number = "(55:4)"
psa55.v4.text = "My heart is in anguish within me, the terrors of death have fallen upon me."
psa55.v5.number_rsv = "(55:5)"
psa55.v5.number = "(55:5)"
psa55.v5.text = "Fear and trembling come upon me, and horror overwhelms me."
psa55.v6.number_rsv = "(55:6)"
psa55.v6.number = "(55:6)"
psa55.v6.text = "And I say, \"O that I had wings like a dove! I would fly away and be at rest;"
psa55.v7.number_rsv = "(55:7)"
psa55.v7.number = "(55:7)"
psa55.v7.text = "yea, I would wander afar, I would lodge in the wilderness,"
psa55.v8.number_rsv = "(55:8)"
psa55.v8.number = "(55:8)"
psa55.v8.text = "I would haste to find me a shelterfrom the raging wind and tempest.\""
psa55.v9.number_rsv = "(55:9)"
psa55.v9.number = "(55:9)"
psa55.v9.text = "Destroy their plans, O Lord, confuse their tongues; for I see violence and strife in the city."
psa55.v10.number_rsv = "(55:10)"
psa55.v10.number = "(55:10)"
psa55.v10.text = "Day and night they go around iton its walls; and mischief and trouble are within it,"
psa55.v11.number_rsv = "(55:11)"
psa55.v11.number = "(55:11)"
psa55.v11.text = "ruin is in its midst; oppression and frauddo not depart from its market place."
psa55.v12.number_rsv = "(55:12)"
psa55.v12.number = "(55:12)"
psa55.v12.text = "It is not an enemy who taunts me --then I could bear it; it is not an adversary who deals insolently with me --then I could hide from him."
psa55.v13.number_rsv = "(55:13)"
psa55.v13.number = "(55:13)"
psa55.v13.text = "But it is you, my equal, my companion, my familiar friend."
psa55.v14.number_rsv = "(55:14)"
psa55.v14.number = "(55:14)"
psa55.v14.text = "We used to hold sweet converse together; within God\"s house we walked in fellowship."
psa55.v15.number_rsv = "(55:15)"
psa55.v15.number = "(55:15)"
psa55.v15.text = "Let death come upon them; let them go down to Sheol alive; let them go away in terror into their graves."
psa55.v16.number_rsv = "(55:16)"
psa55.v16.number = "(55:16)"
psa55.v16.text = "But I call upon God; and the LORD will save me."
psa55.v17.number_rsv = "(55:17)"
psa55.v17.number = "(55:17)"
psa55.v17.text = "Evening and morning and at noonI utter my complaint and moan, and he will hear my voice."
psa55.v18.number_rsv = "(55:18)"
psa55.v18.number = "(55:18)"
psa55.v18.text = "He will deliver my soul in safetyfrom the battle that I wage, for many are arrayed against me."
psa55.v19.number_rsv = "(55:19)"
psa55.v19.number = "(55:19)"
psa55.v19.text = "God will give ear, and humble them, he who is enthroned from of old; because they keep no law, and do not fear God."
psa55.v20.number_rsv = "(55:20)"
psa55.v20.number = "(55:20)"
psa55.v20.text = "My companion stretched out his hand against his friends, he violated his covenant."
psa55.v21.number_rsv = "(55:21)"
psa55.v21.number = "(55:21)"
psa55.v21.text = "His speech was smoother than butter, yet war was in his heart; his words were softer than oil, yet they were drawn swords."
psa55.v22.number_rsv = "(55:22)"
psa55.v22.number = "(55:22)"
psa55.v22.text = "Cast your burden on the LORD, and he will sustain you; he will never permitthe righteous to be moved."
psa55.v23.number_rsv = "(55:23)"
psa55.v23.number = "(55:23)"
psa55.v23.text = "But thou, O God, wilt cast them downinto the lowest pit; men of blood and treacheryshall not live out half their days.But I will trust in thee."

kathisma_8.title = "KATHISMA 8."

psa56.title = "Psalm 56"
psa56.version = properties_en_US_rsv.version.designation
psa56.text = ""
psa56.v0.number_rsv = "(56:0)"
psa56.v0.number = "(56:0)"
psa56.v0.text = "To the choirmaster: according to The Dove on Far-off Terebinths. A Miktam of David, when the Philistines seized him in Gath."
psa56.v1.number_rsv = "(56:1)"
psa56.v1.number = "(56:1)"
psa56.v1.text = "Be gracious to me, O God, for men trample upon me; all day long foemen oppress me;"
psa56.v2.number_rsv = "(56:2)"
psa56.v2.number = "(56:2)"
psa56.v2.text = "my enemies trample upon me all day long, for many fight against me proudly."
psa56.v3.number_rsv = "(56:3)"
psa56.v3.number = "(56:3)"
psa56.v3.text = "When I am afraid, I put my trust in thee."
psa56.v4.number_rsv = "(56:4)"
psa56.v4.number = "(56:4)"
psa56.v4.text = "In God, whose word I praise, in God I trust without a fear.What can flesh do to me?"
psa56.v5.number_rsv = "(56:5)"
psa56.v5.number = "(56:5)"
psa56.v5.text = "All day long they seek to injure my cause; all their thoughts are against me for evil."
psa56.v6.number_rsv = "(56:6)"
psa56.v6.number = "(56:6)"
psa56.v6.text = "They band themselves together, they lurk, they watch my steps. As they have waited for my life," 
psa56.v7.number_rsv = "(56:7)"
psa56.v7.number = "(56:7)"
psa56.v7.text = "so recompense them for their crime; in wrath cast down the peoples, O God!"
psa56.v8.number_rsv = "(56:8)"
psa56.v8.number = "(56:8)"
psa56.v8.text = "Thou hast kept count of my tossings; put thou my tears in thy bottle! Are they not in thy book?"
psa56.v9.number_rsv = "(56:9)"
psa56.v9.number = "(56:9)"
psa56.v9.text = "Then my enemies will be turned backin the day when I call.This I know, that God is for me."
psa56.v10.number_rsv = "(56:10)"
psa56.v10.number = "(56:10)"
psa56.v10.text = "In God, whose word I praise, in the LORD, whose word I praise,"
psa56.v11.number_rsv = "(56:11)"
psa56.v11.number = "(56:11)"
psa56.v11.text = "in God I trust without a fear.What can man do to me?"
psa56.v12.number_rsv = "(56:12)"
psa56.v12.number = "(56:12)"
psa56.v12.text = "My vows to thee I must perform, O God; I will render thank offerings to thee."
psa56.v13.number_rsv = "(56:13)"
psa56.v13.number = "(56:13)"
psa56.v13.text = "For thou hast delivered my soul from death, yea, my feet from falling, that I may walk before Godin the light of life."

psa57.title = "Psalm 57"
psa57.version = properties_en_US_rsv.version.designation
psa57.text = ""
psa57.v0.number_rsv = "(57:0)"
psa57.v0.number = "(57:0)"
psa57.v0.text = "To the choirmaster: according to Do Not Destroy. A Miktam of David, when he fled from Saul, in the cave."
psa57.v1.number_rsv = "(57:1)"
psa57.v1.number = "(57:1)"
psa57.v1.text = "Be merdiful to me, O God, be merciful to me, for in thee my soul takes refuge; in the shadow of thy wings I will take refuge, till the storms of destruction pass by."
psa57.v2.number_rsv = "(57:2)"
psa57.v2.number = "(57:2)"
psa57.v2.text = "I cry to God Most High, to God who fulfils his purpose for me."
psa57.v3.number_rsv = "(57:3)"
psa57.v3.number = "(57:3)"
psa57.v3.text = "He will send from heaven and save me, he will put to shame those who trample upon me. God will send forth his steadfast love and his faithfulness!"
psa57.v4.number_rsv = "(57:4)"
psa57.v4.number = "(57:4)"
psa57.v4.text = "I lie in the midst of lionsthat greedily devour the sons of men; their teeth are spears and arrows, their tongues sharp swords."
psa57.v5.number_rsv = "(57:5)"
psa57.v5.number = "(57:5)"
psa57.v5.text = "Be exalted, O God, above the heavens! Let thy glory be over all the earth!"
psa57.v6.number_rsv = "(57:6)"
psa57.v6.number = "(57:6)"
psa57.v6.text = "They set a net for my steps; my soul was bowed down.They dug a pit in my way, but they have fallen into it themselves."
psa57.v7.number_rsv = "(57:7)"
psa57.v7.number = "(57:7)"
psa57.v7.text = "My heart is steadfast, O God, my heart is steadfast! I will sing and make melody!"
psa57.v8.number_rsv = "(57:8)"
psa57.v8.number = "(57:8)"
psa57.v8.text = "Awake, my soul! Awake, O harp and lyre! I will awake the dawn!"
psa57.v9.number_rsv = "(57:9)"
psa57.v9.number = "(57:9)"
psa57.v9.text = "I will give thanks to thee, O Lord, among the peoples; I will sing praises to thee among the nations."
psa57.v10.number_rsv = "(57:10)"
psa57.v10.number = "(57:10)"
psa57.v10.text = "For thy steadfast love is great to the heavens, thy faithfulness to the clouds."
psa57.v11.number_rsv = "(57:11)"
psa57.v11.number = "(57:11)"
psa57.v11.text = "Be exalted, O God, above the heavens! Let thy glory be over all the earth!"

psa58.title = "Psalm 58"
psa58.version = properties_en_US_rsv.version.designation
psa58.text = ""
psa58.v0.number_rsv = "(58:0)"
psa58.v0.number = "(58:0)"
psa58.v0.text = "To the choirmaster: according to Do Not Destroy. A Miktam of David."
psa58.v1.number_rsv = "(58:1)"
psa58.v1.number = "(58:1)"
psa58.v1.text = "Do you indeed decree what is right, you gods? Do you judge the sons of men uprightly?"
psa58.v2.number_rsv = "(58:2)"
psa58.v2.number = "(58:2)"
psa58.v2.text = "Nay, in your hearts you devise wrongs; your hands deal out violence on earth."
psa58.v3.number_rsv = "(58:3)"
psa58.v3.number = "(58:3)"
psa58.v3.text = "The wicked go astray from the womb, they err from their birth, speaking lies."
psa58.v4.number_rsv = "(58:4)"
psa58.v4.number = "(58:4)"
psa58.v4.text = "They have venom like the venom of a serpent, like the deaf adder that stops its ear,"
psa58.v5.number_rsv = "(58:5)"
psa58.v5.number = "(58:5)"
psa58.v5.text = "so that it does not hear the voice of charmersor of the cunning enchanter."
psa58.v6.number_rsv = "(58:6)"
psa58.v6.number = "(58:6)"
psa58.v6.text = "O God, break the teeth in their mouths; tear out the fangs of the young lions, O LORD!"
psa58.v7.number_rsv = "(58:7)"
psa58.v7.number = "(58:7)"
psa58.v7.text = "Let them vanish like water that runs away; like grass let them be trodden down and wither."
psa58.v8.number_rsv = "(58:8)"
psa58.v8.number = "(58:8)"
psa58.v8.text = "Let them be like the snail which dissolves into slime, like the untimely birth that never sees the sun."
psa58.v9.number_rsv = "(58:9)"
psa58.v9.number = "(58:9)"
psa58.v9.text = "Sooner than your pots can feel the heat of thorns, whether green or ablaze, may he sweep them away!"
psa58.v10.number_rsv = "(58:10)"
psa58.v10.number = "(58:10)"
psa58.v10.text = "The righteous will rejoice when he sees the vengeance; he will bathe his feet in the blood of the wicked."
psa58.v11.number_rsv = "(58:11)"
psa58.v11.number = "(58:11)"
psa58.v11.text = "Men will say, \"Surely there is a reward for the righteous; surely there is a God who judges on earth.\""

psa59.title = "Psalm 59"
psa59.version = properties_en_US_rsv.version.designation
psa59.text = ""
psa59.v0.number_rsv = "(59:0)"
psa59.v0.number = "(59:0)"
psa59.v0.text = "To the choirmaster: according to Do Not Destroy. A Miktam of David, when Saul sent men to watch his house in order to kill him."
psa59.v1.number_rsv = "(59:1)"
psa59.v1.number = "(59:1)"
psa59.v1.text = "Deliver me from my enemies, O my God, protect me from those who rise up against me,"
psa59.v2.number_rsv = "(59:2)"
psa59.v2.number = "(59:2)"
psa59.v2.text = "deliver me from those who work evil, and save me from bloodthirsty men."
psa59.v3.number_rsv = "(59:3)"
psa59.v3.number = "(59:3)"
psa59.v3.text = "For, lo, they lie in wait for my life; fierce men band themselves against me.For no transgression or sin of mine, O LORD,"
psa59.v4.number_rsv = "(59:4)"
psa59.v4.number = "(59:4)"
psa59.v4.text = "for no fault of mine, they run and make ready.Rouse thyself, come to my help, and see!"
psa59.v5.number_rsv = "(59:5)"
psa59.v5.number = "(59:5)"
psa59.v5.text = "Thou, LORD God of hosts, art God of Israel.Awake to punish all the nations; spare none of those who treacherously plot evil."
psa59.v6.number_rsv = "(59:6)"
psa59.v6.number = "(59:6)"
psa59.v6.text = "Each evening they come back, howling like dogsand prowling about the city."
psa59.v7.number_rsv = "(59:7)"
psa59.v7.number = "(59:7)"
psa59.v7.text = "There they are, bellowing with their mouths, and snarling with their lips --for \"Who,\" they think, \"will hear us?\""
psa59.v8.number_rsv = "(59:8)"
psa59.v8.number = "(59:8)"
psa59.v8.text = "But thou, O LORD, dost laugh at them; thou dost hold all the nations in derision."
psa59.v9.number_rsv = "(59:9)"
psa59.v9.number = "(59:9)"
psa59.v9.text = "O my Strength, I will sing praises to thee; for thou, O God, art my fortress."
psa59.v10.number_rsv = "(59:10)"
psa59.v10.number = "(59:10)"
psa59.v10.text = "My God in his steadfast love will meet me; my God will let me look in triumph on my enemies."
psa59.v11.number_rsv = "(59:11)"
psa59.v11.number = "(59:11)"
psa59.v11.text = "Slay them not, lest my people forget; make them totter by thy power, and bring them down, O Lord, our shield!"
psa59.v12.number_rsv = "(59:12)"
psa59.v12.number = "(59:12)"
psa59.v12.text = "For the sin of their mouths, the words of their lips, let them be trapped in their pride.For the cursing and lies which they utter,"
psa59.v13.number_rsv = "(59:13)"
psa59.v13.number = "(59:13)"
psa59.v13.text = "consume them in wrath, consume them till they are no more, that men may know that God rules over Jacobto the ends of the earth."
psa59.v14.number_rsv = "(59:14)"
psa59.v14.number = "(59:14)"
psa59.v14.text = "Each evening they come back, howling like dogsand prowling about the city."
psa59.v15.number_rsv = "(59:15)"
psa59.v15.number = "(59:15)"
psa59.v15.text = "They roam about for food, and growl if they do not get their fill."
psa59.v16.number_rsv = "(59:16)"
psa59.v16.number = "(59:16)"
psa59.v16.text = "But I will sing of thy might; I will sing aloud of thy steadfast love in the morning.For thou hast been to me a fortressand a refuge in the day of my distress."
psa59.v17.number_rsv = "(59:17)"
psa59.v17.number = "(59:17)"
psa59.v17.text = "O my Strength, I will sing praises to thee, for thou, O God, art my fortress, the God who shows me steadfast love."

psa60.title = "Psalm 60"
psa60.version = properties_en_US_rsv.version.designation
psa60.text = ""
psa60.v0.number_rsv = "(60:0)"
psa60.v0.number = "(60:0)"
psa60.v0.text = "To the choirmaster: according to Shushan Eduth. A Miktam of David; for instruction; when he strove with Aram-naharaim and with Aram-zobah, and when Joab on his return killed twelve thousand of Edom in the Valley of Salt."
psa60.v1.number_rsv = "(60:1)"
psa60.v1.number = "(60:1)"
psa60.v1.text = "O God, thou hast rejected us, broken our defenses; thou hast been angry; oh, restore us."
psa60.v2.number_rsv = "(60:2)"
psa60.v2.number = "(60:2)"
psa60.v2.text = "Thou hast made the land to quake, thou hast rent it open; repair its breaches, for it totters."
psa60.v3.number_rsv = "(60:3)"
psa60.v3.number = "(60:3)"
psa60.v3.text = "Thou hast made thy people suffer hard things; thou hast given us wine to drink that made us reel."
psa60.v4.number_rsv = "(60:4)"
psa60.v4.number = "(60:4)"
psa60.v4.text = "Thou hast set up a banner for those who fear thee, to rally to it from the bow."
psa60.v5.number_rsv = "(60:5)"
psa60.v5.number = "(60:5)"
psa60.v5.text = "That thy beloved may be delivered, give victory by thy right hand and answer us!"
psa60.v6.number_rsv = "(60:6)"
psa60.v6.number = "(60:6)"
psa60.v6.text = "God has spoken in his sanctuary:\"With exultation I will divide up Shechemand portion out the Vale of Succoth."
psa60.v7.number_rsv = "(60:7)"
psa60.v7.number = "(60:7)"
psa60.v7.text = "Gilead is mine; Manas\"seh is mine; E\"phraim is my helmet; Judah is my scepter."
psa60.v8.number_rsv = "(60:8)"
psa60.v8.number = "(60:8)"
psa60.v8.text = "Moab is my washbasin; upon Edom I cast my shoe; over Philistia I shout in triumph.\""
psa60.v9.number_rsv = "(60:9)"
psa60.v9.number = "(60:9)"
psa60.v9.text = "Who will bring me to the fortified city? Who will lead me to Edom?"
psa60.v10.number_rsv = "(60:10)"
psa60.v10.number = "(60:10)"
psa60.v10.text = "Hast thou not rejected us, O God? Thou dost not go forth, O God, with our armies."
psa60.v11.number_rsv = "(60:11)"
psa60.v11.number = "(60:11)"
psa60.v11.text = "O grant us help against the foe, for vain is the help of man!"
psa60.v12.number_rsv = "(60:12)"
psa60.v12.number = "(60:12)"
psa60.v12.text = "With God we shall do valiantly; it is he who will tread down our foes."

psa61.title = "Psalm 61"
psa61.version = properties_en_US_rsv.version.designation
psa61.text = ""
psa61.v0.number_rsv = "(61:0)"
psa61.v0.number = "(61:0)"
psa61.v0.text = "To the choirmaster: with stringed instruments. A Psalm of David."
psa61.v1.number_rsv = "(61:1)"
psa61.v1.number = "(61:1)"
psa61.v1.text = "Hear my cry, O God, listen to my prayer;"
psa61.v2.number_rsv = "(61:2)"
psa61.v2.number = "(61:2)"
psa61.v2.text = "from the end of the earth I call to thee, when my heart is faint.Lead thou meto the rock that is higher than I;"
psa61.v3.number_rsv = "(61:3)"
psa61.v3.number = "(61:3)"
psa61.v3.text = "for thou art my refuge, a strong tower against the enemy."
psa61.v4.number_rsv = "(61:4)"
psa61.v4.number = "(61:4)"
psa61.v4.text = "Let me dwell in thy tent for ever! Oh to be safe under the shelter of thy wings!"
psa61.v5.number_rsv = "(61:5)"
psa61.v5.number = "(61:5)"
psa61.v5.text = "For thou, O God, hast heard my vows, thou hast given me the heritage of those who fear thy name."
psa61.v6.number_rsv = "(61:6)"
psa61.v6.number = "(61:6)"
psa61.v6.text = "Prolong the life of the king; may his years endure to all generations!"
psa61.v7.number_rsv = "(61:7)"
psa61.v7.number = "(61:7)"
psa61.v7.text = "May he be enthroned for ever before God; bid steadfast love and faithfulness watch over him!"
psa61.v8.number_rsv = "(61:8)"
psa61.v8.number = "(61:8)"
psa61.v8.text = "So will I ever sing praises to thy name, as I pay my vows day after day."

psa62.title = "Psalm 62"
psa62.version = properties_en_US_rsv.version.designation
psa62.text = ""
psa62.v0.number_rsv = "(62:0)"
psa62.v0.number = "(62:0)"
psa62.v0.text = "To the choirmaster: according to Jeduthun. A Psalm of David."
psa62.v1.number_rsv = "(62:1)"
psa62.v1.number = "(62:1)"
psa62.v1.text = "For God alone my soul waits in silence; from him comes my salvation."
psa62.v2.number_rsv = "(62:2)"
psa62.v2.number = "(62:2)"
psa62.v2.text = "He only is my rock and my salvation, my fortress; I shall not be greatly moved."
psa62.v3.number_rsv = "(62:3)"
psa62.v3.number = "(62:3)"
psa62.v3.text = "How long will you set upon a manto shatter him, all of you, like a leaning wall, a tottering fence?"
psa62.v4.number_rsv = "(62:4)"
psa62.v4.number = "(62:4)"
psa62.v4.text = "They only plan to thrust him down from his eminence.They take pleasure in falsehood.They bless with their mouths, but inwardly they curse."
psa62.v5.number_rsv = "(62:5)"
psa62.v5.number = "(62:5)"
psa62.v5.text = "For God alone my soul waits in silence, for my hope is from him."
psa62.v6.number_rsv = "(62:6)"
psa62.v6.number = "(62:6)"
psa62.v6.text = "He only is my rock and my salvation, my fortress; I shall not be shaken."
psa62.v7.number_rsv = "(62:7)"
psa62.v7.number = "(62:7)"
psa62.v7.text = "On God rests my deliverance and my honor; my mighty rock, my refuge is God."
psa62.v8.number_rsv = "(62:8)"
psa62.v8.number = "(62:8)"
psa62.v8.text = "Trust in him at all times, O people; pour out your heart before him; God is a refuge for us."
psa62.v9.number_rsv = "(62:9)"
psa62.v9.number = "(62:9)"
psa62.v9.text = "Men of low estate are but a breath, men of high estate are a delusion; in the balances they go up; they are together lighter than a breath."
psa62.v10.number_rsv = "(62:10)"
psa62.v10.number = "(62:10)"
psa62.v10.text = "Put no confidence in extortion, set no vain hopes on robbery; if riches increase, set not your heart on them."
psa62.v11.number_rsv = "(62:11)"
psa62.v11.number = "(62:11)"
psa62.v11.text = "Once God has spoken; twice have I heard this:that power belongs to God;"
psa62.v12.number_rsv = "(62:12)"
psa62.v12.number = "(62:12)"
psa62.v12.text = "and that to thee, O Lord, belongs steadfast love.For thou dost requite a manaccording to his work."

psa63.title = "Psalm 63"
psa63.version = properties_en_US_rsv.version.designation
psa63.text = ""
psa63.v0.number_rsv = "(63:0)"
psa63.v0.number = "(63:0)"
psa63.v0.text = "A Psalm of David, when he was in the Wilderness of Judah."
psa63.v1.number_rsv = "(63:1)"
psa63.v1.number = "(63:1)"
psa63.v1.text = "O God, thou art my God, I seek thee, my soul thirsts for thee; my flesh faints for thee, as in a dry and weary land where no water is."
psa63.v2.number_rsv = "(63:2)"
psa63.v2.number = "(63:2)"
psa63.v2.text = "So I have looked upon thee in the sanctuary, beholding thy power and glory."
psa63.v3.number_rsv = "(63:3)"
psa63.v3.number = "(63:3)"
psa63.v3.text = "Because thy steadfast love is better than life, my lips will praise thee."
psa63.v4.number_rsv = "(63:4)"
psa63.v4.number = "(63:4)"
psa63.v4.text = "So I will bless thee as long as I live; I will lift up my hands and call on thy name."
psa63.v5.number_rsv = "(63:5)"
psa63.v5.number = "(63:5)"
psa63.v5.text = "My soul is feasted as with marrow and fat, and my mouth praises thee with joyful lips,"
psa63.v6.number_rsv = "(63:6)"
psa63.v6.number = "(63:6)"
psa63.v6.text = "when I think of thee upon my bed, and meditate on thee in the watches of the night;"
psa63.v7.number_rsv = "(63:7)"
psa63.v7.number = "(63:7)"
psa63.v7.text = "for thou hast been my help, and in the shadow of thy wings I sing for joy."
psa63.v8.number_rsv = "(63:8)"
psa63.v8.number = "(63:8)"
psa63.v8.text = "My soul clings to thee; thy right hand upholds me."
psa63.v9.number_rsv = "(63:9)"
psa63.v9.number = "(63:9)"
psa63.v9.text = "But those who seek to destroy my lifeshall go down into the depths of the earth;"
psa63.v10.number_rsv = "(63:10)"
psa63.v10.number = "(63:10)"
psa63.v10.text = "they shall be given over to the power of the sword, they shall be prey for jackals."
psa63.v11.number_rsv = "(63:11)"
psa63.v11.number = "(63:11)"
psa63.v11.text = "But the king shall rejoice in God; all who swear by him shall glory; for the mouths of liars will be stopped."

psa64.title = "Psalm 64"
psa64.version = properties_en_US_rsv.version.designation
psa64.text = ""
psa64.v0.number_rsv = "(64:0)"
psa64.v0.number = "(64:0)"
psa64.v0.text = "To the choirmaster. A Psalm of David."
psa64.v1.number_rsv = "(64:1)"
psa64.v1.number = "(64:1)"
psa64.v1.text = "Hear my voice, O God, in my complaint; preserve my life from dread of the enemy,"
psa64.v2.number_rsv = "(64:2)"
psa64.v2.number = "(64:2)"
psa64.v2.text = "hide me from the secret plots of the wicked, from the scheming of evildoers,"
psa64.v3.number_rsv = "(64:3)"
psa64.v3.number = "(64:3)"
psa64.v3.text = "who whet their tongues like swords, who aim bitter words like arrows,"
psa64.v4.number_rsv = "(64:4)"
psa64.v4.number = "(64:4)"
psa64.v4.text = "shooting from ambush at the blameless, shooting at him suddenly and without fear."
psa64.v5.number_rsv = "(64:5)"
psa64.v5.number = "(64:5)"
psa64.v5.text = "They hold fast to their evil purpose; they talk of laying snares secretly, thinking, \"Who can see us?"
psa64.v6.number_rsv = "(64:6)"
psa64.v6.number = "(64:6)"
psa64.v6.text = "Who can search out our crimes? We have thought out a cunningly conceived plot.\"For the inward mind and heart of a man are deep!"
psa64.v7.number_rsv = "(64:7)"
psa64.v7.number = "(64:7)"
psa64.v7.text = "But God will shoot his arrow at them; they will be wounded suddenly."
psa64.v8.number_rsv = "(64:8)"
psa64.v8.number = "(64:8)"
psa64.v8.text = "Because of their tongue he will bring them to ruin; all who see them will wag their heads."
psa64.v9.number_rsv = "(64:9)"
psa64.v9.number = "(64:9)"
psa64.v9.text = "Then all men will fear; they will tell what God has wrought, and ponder what he has done."
psa64.v10.number_rsv = "(64:10)"
psa64.v10.number = "(64:10)"
psa64.v10.text = "Let the righteous rejoice in the LORD, and take refuge in him! Let all the upright in heart glory!"

kathisma_9.title = "KATHISMA 9."

psa65.title = "Psalm 65"
psa65.version = properties_en_US_rsv.version.designation
psa65.text = ""
psa65.v0.number_rsv = "(65:0)"
psa65.v0.number = "(65:0)"
psa65.v0.text = "To the choirmaster. A Psalm of David. A Song."
psa65.v1.number_rsv = "(65:1)"
psa65.v1.number = "(65:1)"
psa65.v1.text = "Praise is due to thee, O God, in Zion; and to thee shall vows be performed,"
psa65.v2.number_rsv = "(65:2)"
psa65.v2.number = "(65:2)"
psa65.v2.text = "O thou who hearest prayer! To thee shall all flesh come"
psa65.v3.number_rsv = "(65:3)"
psa65.v3.number = "(65:3)"
psa65.v3.text = "on account of sins.When our transgressions prevail over us, thou dost forgive them."
psa65.v4.number_rsv = "(65:4)"
psa65.v4.number = "(65:4)"
psa65.v4.text = "Blessed is he whom thou dost choose and bring near, to dwell in thy courts! We shall be satisfied with the goodness of thy house, thy holy temple!"
psa65.v5.number_rsv = "(65:5)"
psa65.v5.number = "(65:5)"
psa65.v5.text = "By dread deeds thou dost answer us with deliverance, O God of our salvation, who art the hope of all the ends of the earth, and of the farthest seas;"
psa65.v6.number_rsv = "(65:6)"
psa65.v6.number = "(65:6)"
psa65.v6.text = "who by thy strength hast established the mountains, being girded with might;"
psa65.v7.number_rsv = "(65:7)"
psa65.v7.number = "(65:7)"
psa65.v7.text = "who dost still the roaring of the seas, the roaring of their waves, the tumult of the peoples;"
psa65.v8.number_rsv = "(65:8)"
psa65.v8.number = "(65:8)"
psa65.v8.text = "so that those who dwell at earth\"s farthest boundsare afraid at thy signs; thou makest the outgoings of the morning and the eveningto shout for joy."
psa65.v9.number_rsv = "(65:9)"
psa65.v9.number = "(65:9)"
psa65.v9.text = "Thou visitest the earth and waterest it, thou greatly enrichest it; the river of God is full of water; thou providest their grain, for so thou hast prepared it."
psa65.v10.number_rsv = "(65:10)"
psa65.v10.number = "(65:10)"
psa65.v10.text = "Thou waterest its furrows abundantly, settling its ridges, softening it with showers, and blessing its growth."
psa65.v11.number_rsv = "(65:11)"
psa65.v11.number = "(65:11)"
psa65.v11.text = "Thou crownest the year with thy bounty; the tracks of thy chariot drip with fatness."
psa65.v12.number_rsv = "(65:12)"
psa65.v12.number = "(65:12)"
psa65.v12.text = "The pastures of the wilderness drip, the hills gird themselves with joy,"
psa65.v13.number_rsv = "(65:13)"
psa65.v13.number = "(65:13)"
psa65.v13.text = "the meadows clothe themselves with flocks, the valleys deck themselves with grain, they shout and sing together for joy."

psa66.title = "Psalm 66"
psa66.version = properties_en_US_rsv.version.designation
psa66.text = ""
psa66.v0.number_rsv = "(66:0)"
psa66.v0.number = "(66:0)"
psa66.v0.text = "To the choirmaster. A Song. A Psalm."
psa66.v1.number_rsv = "(66:1)"
psa66.v1.number = "(66:1)"
psa66.v1.text = "Make a joyful noise to God, all the earth;"
psa66.v2.number_rsv = "(66:2)"
psa66.v2.number = "(66:2)"
psa66.v2.text = "sing the glory of his name; give to him glorious praise!"
psa66.v3.number_rsv = "(66:3)"
psa66.v3.number = "(66:3)"
psa66.v3.text = "Say to God, \"How terrible are thy deeds! So great is thy power that thy enemies cringe before thee."
psa66.v4.number_rsv = "(66:4)"
psa66.v4.number = "(66:4)"
psa66.v4.text = "All the earth worships thee; they sing praises to thee, sing praises to thy name.\""
psa66.v5.number_rsv = "(66:5)"
psa66.v5.number = "(66:5)"
psa66.v5.text = "Come and see what God has done:he is terrible in his deeds among men."
psa66.v6.number_rsv = "(66:6)"
psa66.v6.number = "(66:6)"
psa66.v6.text = "He turned the sea into dry land; men passed through the river on foot.There did we rejoice in him,"
psa66.v7.number_rsv = "(66:7)"
psa66.v7.number = "(66:7)"
psa66.v7.text = "who rules by his might for ever, whose eyes keep watch on the nations --let not the rebellious exalt themselves."
psa66.v8.number_rsv = "(66:8)"
psa66.v8.number = "(66:8)"
psa66.v8.text = "Bless our God, O peoples, let the sound of his praise be heard,"
psa66.v9.number_rsv = "(66:9)"
psa66.v9.number = "(66:9)"
psa66.v9.text = "who has kept us among the living, and has not let our feet slip."
psa66.v10.number_rsv = "(66:10)"
psa66.v10.number = "(66:10)"
psa66.v10.text = "For thou, O God, hast tested us; thou hast tried us as silver is tried."
psa66.v11.number_rsv = "(66:11)"
psa66.v11.number = "(66:11)"
psa66.v11.text = "Thou didst bring us into the net; thou didst lay affliction on our loins;"
psa66.v12.number_rsv = "(66:12)"
psa66.v12.number = "(66:12)"
psa66.v12.text = "thou didst let men ride over our heads; we went through fire and through water; yet thou hast brought us forth to a spacious place."
psa66.v13.number_rsv = "(66:13)"
psa66.v13.number = "(66:13)"
psa66.v13.text = "I will come into thy house with burnt offerings; I will pay thee my vows,"
psa66.v14.number_rsv = "(66:14)"
psa66.v14.number = "(66:14)"
psa66.v14.text = "that which my lips utteredand my mouth promised when I was in trouble."
psa66.v15.number_rsv = "(66:15)"
psa66.v15.number = "(66:15)"
psa66.v15.text = "I will offer to thee burnt offerings of fatlings, with the smoke of the sacrifice of rams; I will make an offering of bulls and goats."
psa66.v16.number_rsv = "(66:16)"
psa66.v16.number = "(66:16)"
psa66.v16.text = "Come and hear, all you who fear God, and I will tell what he has done for me."
psa66.v17.number_rsv = "(66:17)"
psa66.v17.number = "(66:17)"
psa66.v17.text = "I cried aloud to him, and he was extolled with my tongue."
psa66.v18.number_rsv = "(66:18)"
psa66.v18.number = "(66:18)"
psa66.v18.text = "If I had cherished iniquity in my heart, the Lord would not have listened."
psa66.v19.number_rsv = "(66:19)"
psa66.v19.number = "(66:19)"
psa66.v19.text = "But truly God has listened; he has given heed to the voice of my prayer."
psa66.v20.number_rsv = "(66:20)"
psa66.v20.number = "(66:20)"
psa66.v20.text = "Blessed be God, because he has not rejected my prayeror removed his steadfast love from me! R"

psa67.title = "Psalm 67"
psa67.version = properties_en_US_rsv.version.designation
psa67.text = ""
psa67.v0.number_rsv = "(67:0)"
psa67.v0.number = "(67:0)"
psa67.v0.text = "To the choirmaster: with stringed instruments. A Psalm. A Song."
psa67.v1.number_rsv = "(67:1)"
psa67.v1.number = "(67:1)"
psa67.v1.text = "May God be gracious to us and bless usand make his face to shine upon us,"
psa67.v2.number_rsv = "(67:2)"
psa67.v2.number = "(67:2)"
psa67.v2.text = "that thy way may be known upon earth, thy saving power among all nations."
psa67.v3.number_rsv = "(67:3)"
psa67.v3.number = "(67:3)"
psa67.v3.text = "Let the peoples praise thee, O God; let all the peoples praise thee!"
psa67.v4.number_rsv = "(67:4)"
psa67.v4.number = "(67:4)"
psa67.v4.text = "Let the nations be glad and sing for joy, for thou dost judge the peoples with equityand guide the nations upon earth."
psa67.v5.number_rsv = "(67:5)"
psa67.v5.number = "(67:5)"
psa67.v5.text = "Let the peoples praise thee, O God; let all the peoples praise thee!"
psa67.v6.number_rsv = "(67:6)"
psa67.v6.number = "(67:6)"
psa67.v6.text = "The earth has yielded its increase; God, our God, has blessed us."
psa67.v7.number_rsv = "(67:7)"
psa67.v7.number = "(67:7)"
psa67.v7.text = "God has blessed us; let all the ends of the earth fear him!"

psa68.title = "Psalm 68"
psa68.version = properties_en_US_rsv.version.designation
psa68.text = ""
psa68.v0.number_rsv = "(68:0)"
psa68.v0.number = "(68:0)"
psa68.v0.text = "To the choirmaster. A Psalm of David. A Song."
psa68.v1.number_rsv = "(68:1)"
psa68.v1.number = "(68:1)"
psa68.v1.text = "Let God arise, let his enemies be scattered; let those who hate him flee before him!"
psa68.v2.number_rsv = "(68:2)"
psa68.v2.number = "(68:2)"
psa68.v2.text = "As smoke is driven away, so drive them away; as wax melts before fire, let the wicked perish before God!"
psa68.v3.number_rsv = "(68:3)"
psa68.v3.number = "(68:3)"
psa68.v3.text = "But let the righteous be joyful; let them exult before God; let them be jubilant with joy!"
psa68.v4.number_rsv = "(68:4)"
psa68.v4.number = "(68:4)"
psa68.v4.text = "Sing to God, sing praises to his name; lift up a song to him who rides upon the clouds; his name is the LORD, exult before him!"
psa68.v5.number_rsv = "(68:5)"
psa68.v5.number = "(68:5)"
psa68.v5.text = "Father of the fatherless and protector of widowsis God in his holy habitation."
psa68.v6.number_rsv = "(68:6)"
psa68.v6.number = "(68:6)"
psa68.v6.text = "God gives the desolate a home to dwell in; he leads out the prisoners to prosperity; but the rebellious dwell in a parched land."
psa68.v7.number_rsv = "(68:7)"
psa68.v7.number = "(68:7)"
psa68.v7.text = "O God, when thou didst go forth before thy people, when thou didst march through the wilderness,"
psa68.v8.number_rsv = "(68:8)"
psa68.v8.number = "(68:8)"
psa68.v8.text = "the earth quaked, the heavens poured down rain, at the presence of God; yon Sinai quaked at the presence of God, the God of Israel."
psa68.v9.number_rsv = "(68:9)"
psa68.v9.number = "(68:9)"
psa68.v9.text = "Rain in abundance, O God, thou didst shed abroad; thou didst restore thy heritage as it languished;"
psa68.v10.number_rsv = "(68:10)"
psa68.v10.number = "(68:10)"
psa68.v10.text = "thy flock found a dwelling in it; in thy goodness, O God, thou didst provide for the needy."
psa68.v11.number_rsv = "(68:11)"
psa68.v11.number = "(68:11)"
psa68.v11.text = "The Lord gives the command; great is the host of those who bore the tidings:"
psa68.v12.number_rsv = "(68:12)"
psa68.v12.number = "(68:12)"
psa68.v12.text = "\"The kings of the armies, they flee, they flee!\"The women at home divide the spoil,"
psa68.v13.number_rsv = "(68:13)"
psa68.v13.number = "(68:13)"
psa68.v13.text = "though they stay among the sheepfolds --the wings of a dove covered with silver, its pinions with green gold."
psa68.v14.number_rsv = "(68:14)"
psa68.v14.number = "(68:14)"
psa68.v14.text = "When the Almighty scattered kings there, snow fell on Zalmon."
psa68.v15.number_rsv = "(68:15)"
psa68.v15.number = "(68:15)"
psa68.v15.text = "O mighty mountain, mountain of Bashan; O many-peaked mountain, mountain of Bashan!"
psa68.v16.number_rsv = "(68:16)"
psa68.v16.number = "(68:16)"
psa68.v16.text = "Why look you with envy, O many-peaked mountain, at the mount which God desired for his abode, yea, where the LORD will dwell for ever?"
psa68.v17.number_rsv = "(68:17)"
psa68.v17.number = "(68:17)"
psa68.v17.text = "With mighty chariotry, twice ten thousand, thousands upon thousands, the Lord came from Sinai into the holy place."
psa68.v18.number_rsv = "(68:18)"
psa68.v18.number = "(68:18)"
psa68.v18.text = "Thou didst ascend the high mount, leading captives in thy train, and receiving gifts among men, even among the rebellious, that the LORD God may dwell there."
psa68.v19.number_rsv = "(68:19)"
psa68.v19.number = "(68:19)"
psa68.v19.text = "Blessed be the Lord, who daily bears us up; God is our salvation."
psa68.v20.number_rsv = "(68:20)"
psa68.v20.number = "(68:20)"
psa68.v20.text = "Our God is a God of salvation; and to GOD, the Lord, belongs escape from death."
psa68.v21.number_rsv = "(68:21)"
psa68.v21.number = "(68:21)"
psa68.v21.text = "But God will shatter the heads of his enemies, the hairy crown of him who walks in his guilty ways."
psa68.v22.number_rsv = "(68:22)"
psa68.v22.number = "(68:22)"
psa68.v22.text = "The Lord said,\"I will bring them back from Bashan, I will bring them back from the depths of the sea,"
psa68.v23.number_rsv = "(68:23)"
psa68.v23.number = "(68:23)"
psa68.v23.text = "that you may bathe your feet in blood, that the tongues of your dogs may have their portion from the foe.\""
psa68.v24.number_rsv = "(68:24)"
psa68.v24.number = "(68:24)"
psa68.v24.text = "Thy solemn processions are seen, O God, the processions of my God, my King, into the sanctuary --"
psa68.v25.number_rsv = "(68:25)"
psa68.v25.number = "(68:25)"
psa68.v25.text = "the singers in front, the minstrels last, between them maidens playing timbrels:"
psa68.v26.number_rsv = "(68:26)"
psa68.v26.number = "(68:26)"
psa68.v26.text = "\"Bless God in the great congregation, the LORD, O you who are of Israel\"s fountain!\""
psa68.v27.number_rsv = "(68:27)"
psa68.v27.number = "(68:27)"
psa68.v27.text = "There is Benjamin, the least of them, in the lead, the princes of Judah in their throng, the princes of Zeb\"ulun, the princes of Naph\"tali."
psa68.v28.number_rsv = "(68:28)"
psa68.v28.number = "(68:28)"
psa68.v28.text = "Summon thy might, O God; show thy strength, O God, thou who hast wrought for us."
psa68.v29.number_rsv = "(68:29)"
psa68.v29.number = "(68:29)"
psa68.v29.text = "Because of thy temple at Jerusalemkings bear gifts to thee."
psa68.v30.number_rsv = "(68:30)"
psa68.v30.number = "(68:30)"
psa68.v30.text = "Rebuke the beasts that dwell among the reeds, the herd of bulls with the calves of the peoples.Trample under foot those who lust after tribute; scatter the peoples who delight in war."
psa68.v31.number_rsv = "(68:31)"
psa68.v31.number = "(68:31)"
psa68.v31.text = "Let bronze be brought from Egypt; let Ethiopia hasten to stretch out her hands to God."
psa68.v32.number_rsv = "(68:32)"
psa68.v32.number = "(68:32)"
psa68.v32.text = "Sing to God, O kingdoms of the earth; sing praises to the Lord,"
psa68.v33.number_rsv = "(68:33)"
psa68.v33.number = "(68:33)"
psa68.v33.text = "to him who rides in the heavens, the ancient heavens; lo, he sends forth his voice, his mighty voice."
psa68.v34.number_rsv = "(68:34)"
psa68.v34.number = "(68:34)"
psa68.v34.text = "Ascribe power to God, whose majesty is over Israel, and his power is in the skies."
psa68.v35.number_rsv = "(68:35)"
psa68.v35.number = "(68:35)"
psa68.v35.text = "Terrible is God in his sanctuary, the God of Israel, he gives power and strength to his people.Blessed be God!"

psa69.title = "Psalm 69"
psa69.version = properties_en_US_rsv.version.designation
psa69.text = ""
psa69.v0.number_rsv = "(69:0)"
psa69.v0.number = "(69:0)"
psa69.v0.text = "To the choirmaster: according to Lilies. A Psalm of David."
psa69.v1.number_rsv = "(69:1)"
psa69.v1.number = "(69:1)"
psa69.v1.text = "Save me, O God! For the waters have come up to my neck."
psa69.v2.number_rsv = "(69:2)"
psa69.v2.number = "(69:2)"
psa69.v2.text = "I sink in deep mire, where there is no foothold; I have come into deep waters, and the flood sweeps over me."
psa69.v3.number_rsv = "(69:3)"
psa69.v3.number = "(69:3)"
psa69.v3.text = "I am weary with my crying; my throat is parched.My eyes grow dimwith waiting for my God."
psa69.v4.number_rsv = "(69:4)"
psa69.v4.number = "(69:4)"
psa69.v4.text = "More in number than the hairs of my headare those who hate me without cause; mighty are those who would destroy me, those who attack me with lies.What I did not stealmust I now restore?"
psa69.v5.number_rsv = "(69:5)"
psa69.v5.number = "(69:5)"
psa69.v5.text = "O God, thou knowest my folly; the wrongs I have done are not hidden from thee."
psa69.v6.number_rsv = "(69:6)"
psa69.v6.number = "(69:6)"
psa69.v6.text = "Let not those who hope in thee be put to shame through me, O Lord GOD of hosts; let not those who seek thee be brought to dishonor through me, O God of Israel."
psa69.v7.number_rsv = "(69:7)"
psa69.v7.number = "(69:7)"
psa69.v7.text = "For it is for thy sake that I have borne reproach, that shame has covered my face."
psa69.v8.number_rsv = "(69:8)"
psa69.v8.number = "(69:8)"
psa69.v8.text = "I have become a stranger to my brethren, an alien to my mother\"s sons."
psa69.v9.number_rsv = "(69:9)"
psa69.v9.number = "(69:9)"
psa69.v9.text = "For zeal for thy house has consumed me, and the insults of those who insult thee have fallen on me."
psa69.v10.number_rsv = "(69:10)"
psa69.v10.number = "(69:10)"
psa69.v10.text = "When I humbled my soul with fasting, it became my reproach."
psa69.v11.number_rsv = "(69:11)"
psa69.v11.number = "(69:11)"
psa69.v11.text = "When I made sackcloth my clothing, I became a byword to them."
psa69.v12.number_rsv = "(69:12)"
psa69.v12.number = "(69:12)"
psa69.v12.text = "I am the talk of those who sit in the gate, and the drunkards make songs about me."
psa69.v13.number_rsv = "(69:13)"
psa69.v13.number = "(69:13)"
psa69.v13.text = "But as for me, my prayer is to thee, O LORD.At an acceptable time, O God, in the abundance of thy steadfast love answer me.With thy faithful help"
psa69.v14.number_rsv = "(69:14)"
psa69.v14.number = "(69:14)"
psa69.v14.text = "rescue mefrom sinking in the mire; let me be delivered from my enemiesand from the deep waters."
psa69.v15.number_rsv = "(69:15)"
psa69.v15.number = "(69:15)"
psa69.v15.text = "Let not the flood sweep over me, or the deep swallow me up, or the pit close its mouth over me."
psa69.v16.number_rsv = "(69:16)"
psa69.v16.number = "(69:16)"
psa69.v16.text = "Answer me, O LORD, for thy steadfast love is good; according to thy abundant mercy, turn to me."
psa69.v17.number_rsv = "(69:17)"
psa69.v17.number = "(69:17)"
psa69.v17.text = "Hide not thy face from thy servant; for I am in distress, make haste to answer me."
psa69.v18.number_rsv = "(69:18)"
psa69.v18.number = "(69:18)"
psa69.v18.text = "Draw near to me, redeem me, set me free because of my enemies!"
psa69.v19.number_rsv = "(69:19)"
psa69.v19.number = "(69:19)"
psa69.v19.text = "Thou knowest my reproach, and my shame and my dishonor; my foes are all known to thee."
psa69.v20.number_rsv = "(69:20)"
psa69.v20.number = "(69:20)"
psa69.v20.text = "Insults have broken my heart, so that I am in despair.I looked for pity, but there was none; and for comforters, but I found none."
psa69.v21.number_rsv = "(69:21)"
psa69.v21.number = "(69:21)"
psa69.v21.text = "They gave me poison for food, and for my thirst they gave me vinegar to drink."
psa69.v22.number_rsv = "(69:22)"
psa69.v22.number = "(69:22)"
psa69.v22.text = "Let their own table before them become a snare; let their sacrificial feasts be a trap."
psa69.v23.number_rsv = "(69:23)"
psa69.v23.number = "(69:23)"
psa69.v23.text = "Let their eyes be darkened, so that they cannot see; and make their loins tremble continually."
psa69.v24.number_rsv = "(69:24)"
psa69.v24.number = "(69:24)"
psa69.v24.text = "Pour out thy indignation upon them, and let thy burning anger overtake them."
psa69.v25.number_rsv = "(69:25)"
psa69.v25.number = "(69:25)"
psa69.v25.text = "May their camp be a desolation, let no one dwell in their tents."
psa69.v26.number_rsv = "(69:26)"
psa69.v26.number = "(69:26)"
psa69.v26.text = "For they persecute him whom thou hast smitten, and him whom thou hast wounded, they afflict still more."
psa69.v27.number_rsv = "(69:27)"
psa69.v27.number = "(69:27)"
psa69.v27.text = "Add to them punishment upon punishment; may they have no acquittal from thee."
psa69.v28.number_rsv = "(69:28)"
psa69.v28.number = "(69:28)"
psa69.v28.text = "Let them be blotted out of the book of the living; let them not be enrolled among the righteous."
psa69.v29.number_rsv = "(69:29)"
psa69.v29.number = "(69:29)"
psa69.v29.text = "But I am afflicted and in pain; let thy salvation, O God, set me on high!"
psa69.v30.number_rsv = "(69:30)"
psa69.v30.number = "(69:30)"
psa69.v30.text = "I will praise the name of God with a song; I will magnify him with thanksgiving."
psa69.v31.number_rsv = "(69:31)"
psa69.v31.number = "(69:31)"
psa69.v31.text = "This will please the LORD more than an oxor a bull with horns and hoofs."
psa69.v32.number_rsv = "(69:32)"
psa69.v32.number = "(69:32)"
psa69.v32.text = "Let the oppressed see it and be glad; you who seek God, let your hearts revive."
psa69.v33.number_rsv = "(69:33)"
psa69.v33.number = "(69:33)"
psa69.v33.text = "For the LORD hears the needy, and does not despise his own that are in bonds."
psa69.v34.number_rsv = "(69:34)"
psa69.v34.number = "(69:34)"
psa69.v34.text = "Let heaven and earth praise him, the seas and everything that moves therein."
psa69.v35.number_rsv = "(69:35)"
psa69.v35.number = "(69:35)"
psa69.v35.text = "For God will save Zionand rebuild the cities of Judah; and his servants shall dwell there and possess it;"
psa69.v36.number_rsv = "(69:36)"
psa69.v36.number = "(69:36)"
psa69.v36.text = "the children of his servants shall inherit it, and those who love his name shall dwell in it."

psa70.title = "Psalm 70"
psa70.version = properties_en_US_rsv.version.designation
psa70.text = ""
psa70.v0.number_rsv = "(70:0)"
psa70.v0.number = "(70:0)"
psa70.v0.text = "To the choirmaster. A Psalm of David, for the memorial offering."
psa70.v1.number_rsv = "(70:1)"
psa70.v1.number = "(70:1)"
psa70.v1.text = "Be pleased, O God, to deliver me! O LORD, make haste to help me!"
psa70.v2.number_rsv = "(70:2)"
psa70.v2.number = "(70:2)"
psa70.v2.text = "Let them be put to shame and confusionwho seek my life! Let them be turned back and brought to dishonorwho desire my hurt!"
psa70.v3.number_rsv = "(70:3)"
psa70.v3.number = "(70:3)"
psa70.v3.text = "Let them be appalled because of their shamewho say, \"Aha, Aha!\""
psa70.v4.number_rsv = "(70:4)"
psa70.v4.number = "(70:4)"
psa70.v4.text = "May all who seek theerejoice and be glad in thee! May those who love thy salvationsay evermore, \"God is great!\""
psa70.v5.number_rsv = "(70:5)"
psa70.v5.number = "(70:5)"
psa70.v5.text = "But I am poor and needy; hasten to me, O God! Thou art my help and my deliverer; O LORD, do not tarry!"

kathisma_10.title = "KATHISMA 10."

psa71.title = "Psalm 71"
psa71.version = properties_en_US_rsv.version.designation
psa71.text = ""
psa71.v1.number_rsv = "(71:1)"
psa71.v1.number = "(71:1)"
psa71.v1.text = "In thee, O LORD, do I take refuge; let me never be put to shame!"
psa71.v2.number_rsv = "(71:2)"
psa71.v2.number = "(71:2)"
psa71.v2.text = "In thy righteousness deliver me and rescue me; incline thy ear to me, and save me!"
psa71.v3.number_rsv = "(71:3)"
psa71.v3.number = "(71:3)"
psa71.v3.text = "Be thou to me a rock of refuge, a strong fortress, to save me, for thou art my rock and my fortress."
psa71.v4.number_rsv = "(71:4)"
psa71.v4.number = "(71:4)"
psa71.v4.text = "Rescue me, O my God, from the hand of the wicked, from the grasp of the unjust and cruel man."
psa71.v5.number_rsv = "(71:5)"
psa71.v5.number = "(71:5)"
psa71.v5.text = "For thou, O Lord, art my hope, my trust, O LORD, from my youth."
psa71.v6.number_rsv = "(71:6)"
psa71.v6.number = "(71:6)"
psa71.v6.text = "Upon thee I have leaned from my birth; thou art he who took me from my mother\"s womb.My praise is continually of thee."
psa71.v7.number_rsv = "(71:7)"
psa71.v7.number = "(71:7)"
psa71.v7.text = "I have been as a portent to many; but thou art my strong refuge."
psa71.v8.number_rsv = "(71:8)"
psa71.v8.number = "(71:8)"
psa71.v8.text = "My mouth is filled with thy praise, and with thy glory all the day."
psa71.v9.number_rsv = "(71:9)"
psa71.v9.number = "(71:9)"
psa71.v9.text = "Do not cast me off in the time of old age; forsake me not when my strength is spent."
psa71.v10.number_rsv = "(71:10)"
psa71.v10.number = "(71:10)"
psa71.v10.text = "For my enemies speak concerning me, those who watch for my life consult together,"
psa71.v11.number_rsv = "(71:11)"
psa71.v11.number = "(71:11)"
psa71.v11.text = "and say, \"God has forsaken him; pursue and seize him, for there is none to deliver him.\""
psa71.v12.number_rsv = "(71:12)"
psa71.v12.number = "(71:12)"
psa71.v12.text = "O God, be not far from me; O my God, make haste to help me!"
psa71.v13.number_rsv = "(71:13)"
psa71.v13.number = "(71:13)"
psa71.v13.text = "May my accusers be put to shame and consumed; with scorn and disgrace may they be coveredwho seek my hurt."
psa71.v14.number_rsv = "(71:14)"
psa71.v14.number = "(71:14)"
psa71.v14.text = "But I will hope continually, and will praise thee yet more and more."
psa71.v15.number_rsv = "(71:15)"
psa71.v15.number = "(71:15)"
psa71.v15.text = "My mouth will tell of thy righteous acts, of thy deeds of salvation all the day, for their number is past my knowledge."
psa71.v16.number_rsv = "(71:16)"
psa71.v16.number = "(71:16)"
psa71.v16.text = "With the mighty deeds of the Lord GOD I will come, I will praise thy righteousness, thine alone."
psa71.v17.number_rsv = "(71:17)"
psa71.v17.number = "(71:17)"
psa71.v17.text = "O God, from my youth thou hast taught me, and I still proclaim thy wondrous deeds."
psa71.v18.number_rsv = "(71:18)"
psa71.v18.number = "(71:18)"
psa71.v18.text = "So even to old age and gray hairs, O God, do not forsake me, till I proclaim thy mightto all the generations to come.Thy power"
psa71.v19.number_rsv = "(71:19)"
psa71.v19.number = "(71:19)"
psa71.v19.text = "and thy righteousness, O God, reach the high heavens.Thou who hast done great things, O God, who is like thee?"
psa71.v20.number_rsv = "(71:20)"
psa71.v20.number = "(71:20)"
psa71.v20.text = "Thou who hast made me see many sore troubleswilt revive me again; from the depths of the earththou wilt bring me up again."
psa71.v21.number_rsv = "(71:21)"
psa71.v21.number = "(71:21)"
psa71.v21.text = "Thou wilt increase my honor, and comfort me again."
psa71.v22.number_rsv = "(71:22)"
psa71.v22.number = "(71:22)"
psa71.v22.text = "I will also praise thee with the harpfor thy faithfulness, O my God; I will sing praises to thee with the lyre, O Holy One of Israel."
psa71.v23.number_rsv = "(71:23)"
psa71.v23.number = "(71:23)"
psa71.v23.text = "My lips will shout for joy, when I sing praises to thee; my soul also, which thou hast rescued."
psa71.v24.number_rsv = "(71:24)"
psa71.v24.number = "(71:24)"
psa71.v24.text = "And my tongue will talk of thy righteous helpall the day long, for they have been put to shame and disgracedwho sought to do me hurt."

psa72.title = "Psalm 72"
psa72.version = properties_en_US_rsv.version.designation
psa72.text = ""
psa72.v0.number_rsv = "(72:0)"
psa72.v0.number = "(72:0)"
psa72.v0.text = "A Psalm of Solomon."
psa72.v1.number_rsv = "(72:1)"
psa72.v1.number = "(72:1)"
psa72.v1.text = "Give the king thy justice, O God, and thy righteousness to the royal son!"
psa72.v2.number_rsv = "(72:2)"
psa72.v2.number = "(72:2)"
psa72.v2.text = "May he judge thy people with righteousness, and thy poor with justice!"
psa72.v3.number_rsv = "(72:3)"
psa72.v3.number = "(72:3)"
psa72.v3.text = "Let the mountains bear prosperity for the people, and the hills, in righteousness!"
psa72.v4.number_rsv = "(72:4)"
psa72.v4.number = "(72:4)"
psa72.v4.text = "May he defend the cause of the poor of the people, give deliverance to the needy, and crush the oppressor!"
psa72.v5.number_rsv = "(72:5)"
psa72.v5.number = "(72:5)"
psa72.v5.text = "May he live while the sun endures, and as long as the moon, throughout all generations!"
psa72.v6.number_rsv = "(72:6)"
psa72.v6.number = "(72:6)"
psa72.v6.text = "May he be like rain that falls on the mown grass, like showers that water the earth!"
psa72.v7.number_rsv = "(72:7)"
psa72.v7.number = "(72:7)"
psa72.v7.text = "In his days may righteousness flourish, and peace abound, till the moon be no more!"
psa72.v8.number_rsv = "(72:8)"
psa72.v8.number = "(72:8)"
psa72.v8.text = "May he have dominion from sea to sea, and from the River to the ends of the earth!"
psa72.v9.number_rsv = "(72:9)"
psa72.v9.number = "(72:9)"
psa72.v9.text = "May his foes bow down before him, and his enemies lick the dust!"
psa72.v10.number_rsv = "(72:10)"
psa72.v10.number = "(72:10)"
psa72.v10.text = "May the kings of Tarshish and of the islesrender him tribute, may the kings of Sheba and Sebabring gifts!"
psa72.v11.number_rsv = "(72:11)"
psa72.v11.number = "(72:11)"
psa72.v11.text = "May all kings fall down before him, all nations serve him!"
psa72.v12.number_rsv = "(72:12)"
psa72.v12.number = "(72:12)"
psa72.v12.text = "For he delivers the needy when he calls, the poor and him who has no helper."
psa72.v13.number_rsv = "(72:13)"
psa72.v13.number = "(72:13)"
psa72.v13.text = "He has pity on the weak and the needy, and saves the lives of the needy."
psa72.v14.number_rsv = "(72:14)"
psa72.v14.number = "(72:14)"
psa72.v14.text = "From oppression and violence he redeems their life; and precious is their blood in his sight."
psa72.v15.number_rsv = "(72:15)"
psa72.v15.number = "(72:15)"
psa72.v15.text = "Long may he live, may gold of Sheba be given to him! May prayer be made for him continually, and blessings invoked for him all the day!"
psa72.v16.number_rsv = "(72:16)"
psa72.v16.number = "(72:16)"
psa72.v16.text = "May there be abundance of grain in the land; on the tops of the mountains may it wave; may its fruit be like Lebanon; and may men blossom forth from the citieslike the grass of the field!"
psa72.v17.number_rsv = "(72:17)"
psa72.v17.number = "(72:17)"
psa72.v17.text = "May his name endure for ever, his fame continue as long as the sun! May men bless themselves by him, all nations call him blessed!"
psa72.v18.number_rsv = "(72:18)"
psa72.v18.number = "(72:18)"
psa72.v18.text = "Blessed be the LORD, the God of Israel, who alone does wondrous things."
psa72.v19.number_rsv = "(72:19)"
psa72.v19.number = "(72:19)"
psa72.v19.text = "Blessed be his glorious name for ever; may his glory fill the whole earth! Amen and Amen!"
psa72.v20.number_rsv = "(72:20)"
psa72.v20.number = "(72:20)"
psa72.v20.text = "The prayers of David, the son of Jesse, are ended."

psa73.title = "Psalm 73"
psa73.version = properties_en_US_rsv.version.designation
psa73.text = ""
psa73.v0.number_rsv = "(73:0)"
psa73.v0.number = "(73:0)"
psa73.v0.text = "A Psalm of Asaph."
psa73.v1.number_rsv = "(73:1)"
psa73.v1.number = "(73:1)"
psa73.v1.text = "Truly God is good to the upright, to those who are pure in heart."
psa73.v2.number_rsv = "(73:2)"
psa73.v2.number = "(73:2)"
psa73.v2.text = "But as for me, my feet had almost stumbled, my steps had well nigh slipped."
psa73.v3.number_rsv = "(73:3)"
psa73.v3.number = "(73:3)"
psa73.v3.text = "For I was envious of the arrogant, when I saw the prosperity of the wicked."
psa73.v4.number_rsv = "(73:4)"
psa73.v4.number = "(73:4)"
psa73.v4.text = "For they have no pangs; their bodies are sound and sleek."
psa73.v5.number_rsv = "(73:5)"
psa73.v5.number = "(73:5)"
psa73.v5.text = "They are not in trouble as other men are; they are not stricken like other men."
psa73.v6.number_rsv = "(73:6)"
psa73.v6.number = "(73:6)"
psa73.v6.text = "Therefore pride is their necklace; violence covers them as a garment."
psa73.v7.number_rsv = "(73:7)"
psa73.v7.number = "(73:7)"
psa73.v7.text = "Their eyes swell out with fatness, their hearts overflow with follies."
psa73.v8.number_rsv = "(73:8)"
psa73.v8.number = "(73:8)"
psa73.v8.text = "They scoff and speak with malice; loftily they threaten oppression."
psa73.v9.number_rsv = "(73:9)"
psa73.v9.number = "(73:9)"
psa73.v9.text = "They set their mouths against the heavens, and their tongue struts through the earth."
psa73.v10.number_rsv = "(73:10)"
psa73.v10.number = "(73:10)"
psa73.v10.text = "Therefore the people turn and praise them; and find no fault in them."
psa73.v11.number_rsv = "(73:11)"
psa73.v11.number = "(73:11)"
psa73.v11.text = "And they say, \"How can God know? Is there knowledge in the Most High?\""
psa73.v12.number_rsv = "(73:12)"
psa73.v12.number = "(73:12)"
psa73.v12.text = "Behold, these are the wicked; always at ease, they increase in riches."
psa73.v13.number_rsv = "(73:13)"
psa73.v13.number = "(73:13)"
psa73.v13.text = "All in vain have I kept my heart cleanand washed my hands in innocence."
psa73.v14.number_rsv = "(73:14)"
psa73.v14.number = "(73:14)"
psa73.v14.text = "For all the day long I have been stricken, and chastened every morning."
psa73.v15.number_rsv = "(73:15)"
psa73.v15.number = "(73:15)"
psa73.v15.text = "If I had said, \"I will speak thus,\"I would have been untrue to the generation of thy children."
psa73.v16.number_rsv = "(73:16)"
psa73.v16.number = "(73:16)"
psa73.v16.text = "But when I thought how to understand this, it seemed to me a wearisome task,"
psa73.v17.number_rsv = "(73:17)"
psa73.v17.number = "(73:17)"
psa73.v17.text = "until I went into the sanctuary of God; then I perceived their end."
psa73.v18.number_rsv = "(73:18)"
psa73.v18.number = "(73:18)"
psa73.v18.text = "Truly thou dost set them in slippery places; thou dost make them fall to ruin."
psa73.v19.number_rsv = "(73:19)"
psa73.v19.number = "(73:19)"
psa73.v19.text = "How they are destroyed in a moment, swept away utterly by terrors!"
psa73.v20.number_rsv = "(73:20)"
psa73.v20.number = "(73:20)"
psa73.v20.text = "They are like a dream when one awakes, on awaking you despise their phantoms."
psa73.v21.number_rsv = "(73:21)"
psa73.v21.number = "(73:21)"
psa73.v21.text = "When my soul was embittered, when I was pricked in heart,"
psa73.v22.number_rsv = "(73:22)"
psa73.v22.number = "(73:22)"
psa73.v22.text = "I was stupid and ignorant, I was like a beast toward thee."
psa73.v23.number_rsv = "(73:23)"
psa73.v23.number = "(73:23)"
psa73.v23.text = "Nevertheless I am continually with thee; thou dost hold my right hand."
psa73.v24.number_rsv = "(73:24)"
psa73.v24.number = "(73:24)"
psa73.v24.text = "Thou dost guide me with thy counsel, and afterward thou wilt receive me to glory."
psa73.v25.number_rsv = "(73:25)"
psa73.v25.number = "(73:25)"
psa73.v25.text = "Whom have I in heaven but thee? And there is nothing upon earth that I desire besides thee."
psa73.v26.number_rsv = "(73:26)"
psa73.v26.number = "(73:26)"
psa73.v26.text = "My flesh and my heart may fail, but God is the strength of my heart and my portion for ever."
psa73.v27.number_rsv = "(73:27)"
psa73.v27.number = "(73:27)"
psa73.v27.text = "For lo, those who are far from thee shall perish; thou dost put an end to those who are false to thee."
psa73.v28.number_rsv = "(73:28)"
psa73.v28.number = "(73:28)"
psa73.v28.text = "But for me it is good to be near God; I have made the Lord GOD my refuge, that I may tell of all thy works.R"

psa74.title = "Psalm 74"
psa74.version = properties_en_US_rsv.version.designation
psa74.text = ""
psa74.v0.number_rsv = "(74:0)"
psa74.v0.number = "(74:0)"
psa74.v0.text = "A Maskil of Asaph."
psa74.v1.number_rsv = "(74:1)"
psa74.v1.number = "(74:1)"
psa74.v1.text = "O God, why dost thou cast us off for ever? Why does thy anger smoke against the sheep of thy pasture?"
psa74.v2.number_rsv = "(74:2)"
psa74.v2.number = "(74:2)"
psa74.v2.text = "Remember thy congregation, which thou hast gotten of old, which thou hast redeemed to be the tribe of thy heritage! Remember Mount Zion, where thou hast dwelt."
psa74.v3.number_rsv = "(74:3)"
psa74.v3.number = "(74:3)"
psa74.v3.text = "Direct thy steps to the perpetual ruins; the enemy has destroyed everything in the sanctuary!"
psa74.v4.number_rsv = "(74:4)"
psa74.v4.number = "(74:4)"
psa74.v4.text = "Thy foes have roared in the midst of thy holy place; they set up their own signs for signs."
psa74.v5.number_rsv = "(74:5)"
psa74.v5.number = "(74:5)"
psa74.v5.text = "At the upper entrance they hackedthe wooden trellis with axes."
psa74.v6.number_rsv = "(74:6)"
psa74.v6.number = "(74:6)"
psa74.v6.text = "And then all its carved woodthey broke down with hatchets and hammers."
psa74.v7.number_rsv = "(74:7)"
psa74.v7.number = "(74:7)"
psa74.v7.text = "They set thy sanctuary on fire; to the ground they desecrated the dwelling place of thy name."
psa74.v8.number_rsv = "(74:8)"
psa74.v8.number = "(74:8)"
psa74.v8.text = "They said to themselves, \"We will utterly subdue them\"; they burned all the meeting places of God in the land."
psa74.v9.number_rsv = "(74:9)"
psa74.v9.number = "(74:9)"
psa74.v9.text = "We do not see our signs; there is no longer any prophet, and there is none among us who knows how long."
psa74.v10.number_rsv = "(74:10)"
psa74.v10.number = "(74:10)"
psa74.v10.text = "How long, O God, is the foe to scoff? Is the enemy to revile thy name for ever?"
psa74.v11.number_rsv = "(74:11)"
psa74.v11.number = "(74:11)"
psa74.v11.text = "Why dost thou hold back thy hand, why dost thou keep thy right hand in thy bosom?"
psa74.v12.number_rsv = "(74:12)"
psa74.v12.number = "(74:12)"
psa74.v12.text = "Yet God my King is from of old, working salvation in the midst of the earth."
psa74.v13.number_rsv = "(74:13)"
psa74.v13.number = "(74:13)"
psa74.v13.text = "Thou didst divide the sea by thy might; thou didst break the heads of the dragons on the waters."
psa74.v14.number_rsv = "(74:14)"
psa74.v14.number = "(74:14)"
psa74.v14.text = "Thou didst crush the heads of Leviathan, thou didst give him as food for the creatures of the wilderness."
psa74.v15.number_rsv = "(74:15)"
psa74.v15.number = "(74:15)"
psa74.v15.text = "Thou didst cleave open springs and brooks; thou didst dry up ever-flowing streams."
psa74.v16.number_rsv = "(74:16)"
psa74.v16.number = "(74:16)"
psa74.v16.text = "Thine is the day, thine also the night; thou hast established the luminaries and the sun."
psa74.v17.number_rsv = "(74:17)"
psa74.v17.number = "(74:17)"
psa74.v17.text = "Thou hast fixed all the bounds of the earth; thou hast made summer and winter."
psa74.v18.number_rsv = "(74:18)"
psa74.v18.number = "(74:18)"
psa74.v18.text = "Remember this, O LORD, how the enemy scoffs, and an impious people reviles thy name."
psa74.v19.number_rsv = "(74:19)"
psa74.v19.number = "(74:19)"
psa74.v19.text = "Do not deliver the soul of thy dove to the wild beasts; do not forget the life of thy poor for ever."
psa74.v20.number_rsv = "(74:20)"
psa74.v20.number = "(74:20)"
psa74.v20.text = "Have regard for thy covenant; for the dark places of the land are full of the habitations ofviolence."
psa74.v21.number_rsv = "(74:21)"
psa74.v21.number = "(74:21)"
psa74.v21.text = "Let not the downtrodden be put to shame; let the poor and needy praise thy name."
psa74.v22.number_rsv = "(74:22)"
psa74.v22.number = "(74:22)"
psa74.v22.text = "Arise, O God, plead thy cause; remember how the impious scoff at thee all the day!"
psa74.v23.number_rsv = "(74:23)"
psa74.v23.number = "(74:23)"
psa74.v23.text = "Do not forget the clamor of thy foes, the uproar of thy adversaries which goes up continually!"

psa75.title = "Psalm 75"
psa75.version = properties_en_US_rsv.version.designation
psa75.text = ""
psa75.v0.number_rsv = "(75:0)"
psa75.v0.number = "(75:0)"
psa75.v0.text = "To the choirmaster: according to Do Not Destroy. A Psalm of Asaph. A Song."
psa75.v1.number_rsv = "(75:1)"
psa75.v1.number = "(75:1)"
psa75.v1.text = "We give thanks to thee, O God; we give thanks; we call on thy name and recount thy wondrous deeds."
psa75.v2.number_rsv = "(75:2)"
psa75.v2.number = "(75:2)"
psa75.v2.text = "At the set time which I appointI will judge with equity."
psa75.v3.number_rsv = "(75:3)"
psa75.v3.number = "(75:3)"
psa75.v3.text = "When the earth totters, and all its inhabitants, it is I who keep steady its pillars."
psa75.v4.number_rsv = "(75:4)"
psa75.v4.number = "(75:4)"
psa75.v4.text = "I say to the boastful, \"Do not boast,\"and to the wicked, \"Do not lift up your horn;"
psa75.v5.number_rsv = "(75:5)"
psa75.v5.number = "(75:5)"
psa75.v5.text = "do not lift up your horn on high, or speak with insolent neck.\""
psa75.v6.number_rsv = "(75:6)"
psa75.v6.number = "(75:6)"
psa75.v6.text = "For not from the east or from the westand not from the wilderness comes lifting up;"
psa75.v7.number_rsv = "(75:7)"
psa75.v7.number = "(75:7)"
psa75.v7.text = "but it is God who executes judgment, putting down one and lifting up another."
psa75.v8.number_rsv = "(75:8)"
psa75.v8.number = "(75:8)"
psa75.v8.text = "For in the hand of the LORD there is a cup, with foaming wine, well mixed; and he will pour a draught from it, and all the wicked of the earthshall drain it down to the dregs."
psa75.v9.number_rsv = "(75:9)"
psa75.v9.number = "(75:9)"
psa75.v9.text = "But I will rejoice for ever, I will sing praises to the God of Jacob."
psa75.v10.number_rsv = "(75:10)"
psa75.v10.number = "(75:10)"
psa75.v10.text = "All the horns of the wicked he will cut off, but the horns of the righteous shall be exalted."

psa76.title = "Psalm 76"
psa76.version = properties_en_US_rsv.version.designation
psa76.text = ""
psa76.v0.number_rsv = "(76:0)"
psa76.v0.number = "(76:0)"
psa76.v0.text = "To the choirmaster: with stringed instruments. A Psalm of Asaph. A Song."
psa76.v1.number_rsv = "(76:1)"
psa76.v1.number = "(76:1)"
psa76.v1.text = "In Judah God is known, his name is great in Israel."
psa76.v2.number_rsv = "(76:2)"
psa76.v2.number = "(76:2)"
psa76.v2.text = "His abode has been established in Salem, his dwelling place in Zion."
psa76.v3.number_rsv = "(76:3)"
psa76.v3.number = "(76:3)"
psa76.v3.text = "There he broke the flashing arrows, the shield, the sword, and the weapons of war."
psa76.v4.number_rsv = "(76:4)"
psa76.v4.number = "(76:4)"
psa76.v4.text = "Glorious art thou, more majesticthan the everlasting mountains."
psa76.v5.number_rsv = "(76:5)"
psa76.v5.number = "(76:5)"
psa76.v5.text = "The stouthearted were stripped of their spoil; they sank into sleep; all the men of warwere unable to use their hands."
psa76.v6.number_rsv = "(76:6)"
psa76.v6.number = "(76:6)"
psa76.v6.text = "At thy rebuke, O God of Jacob, both rider and horse lay stunned."
psa76.v7.number_rsv = "(76:7)"
psa76.v7.number = "(76:7)"
psa76.v7.text = "But thou, terrible art thou! Who can stand before theewhen once thy anger is roused?"
psa76.v8.number_rsv = "(76:8)"
psa76.v8.number = "(76:8)"
psa76.v8.text = "From the heavens thou didst utter judgment; the earth feared and was still,"
psa76.v9.number_rsv = "(76:9)"
psa76.v9.number = "(76:9)"
psa76.v9.text = "when God arose to establish judgmentto save all the oppressed of the earth."
psa76.v10.number_rsv = "(76:10)"
psa76.v10.number = "(76:10)"
psa76.v10.text = "Surely the wrath of men shall praise thee; the residue of wrath thou wilt gird upon thee."
psa76.v11.number_rsv = "(76:11)"
psa76.v11.number = "(76:11)"
psa76.v11.text = "Make your vows to the LORD your God, and perform them; let all around him bring giftsto him who is to be feared,"
psa76.v12.number_rsv = "(76:12)"
psa76.v12.number = "(76:12)"
psa76.v12.text = "who cuts off the spirit of princes, who is terrible to the kings of the earth."

psa77.title = "Psalm 77"
psa77.version = properties_en_US_rsv.version.designation
psa77.text = ""
psa77.v0.number_rsv = "(77:0)"
psa77.v0.number = "(77:0)"
psa77.v0.text = "To the choirmaster: according to Jeduthun. A Psalm of Asaph."
psa77.v1.number_rsv = "(77:1)"
psa77.v1.number = "(77:1)"
psa77.v1.text = "I cry aloud to God, aloud to God, that he may hear me."
psa77.v2.number_rsv = "(77:2)"
psa77.v2.number = "(77:2)"
psa77.v2.text = "In the day of my trouble I seek the Lord; in the night my hand is stretched out without wearying; my soul refuses to be comforted."
psa77.v3.number_rsv = "(77:3)"
psa77.v3.number = "(77:3)"
psa77.v3.text = "I think of God, and I moan; I meditate, and my spirit faints."
psa77.v4.number_rsv = "(77:4)"
psa77.v4.number = "(77:4)"
psa77.v4.text = "Thou dost hold my eyelids from closing; I am so troubled that I cannot speak."
psa77.v5.number_rsv = "(77:5)"
psa77.v5.number = "(77:5)"
psa77.v5.text = "I consider the days of old, I remember the years long ago."
psa77.v6.number_rsv = "(77:6)"
psa77.v6.number = "(77:6)"
psa77.v6.text = "I commune with my heart in the night; I meditate and search my spirit:"
psa77.v7.number_rsv = "(77:7)"
psa77.v7.number = "(77:7)"
psa77.v7.text = "\"Will the Lord spurn for ever, and never again be favorable?"
psa77.v8.number_rsv = "(77:8)"
psa77.v8.number = "(77:8)"
psa77.v8.text = "Has his steadfast love for ever ceased? Are his promises at an end for all time?"
psa77.v9.number_rsv = "(77:9)"
psa77.v9.number = "(77:9)"
psa77.v9.text = "Has God forgotten to be gracious? Has he in anger shut up his compassion?\""
psa77.v10.number_rsv = "(77:10)"
psa77.v10.number = "(77:10)"
psa77.v10.text = "And I say, \"It is my griefthat the right hand of the Most High has changed.\""
psa77.v11.number_rsv = "(77:11)"
psa77.v11.number = "(77:11)"
psa77.v11.text = "I will call to mind the deeds of the LORD; yea, I will remember thy wonders of old."
psa77.v12.number_rsv = "(77:12)"
psa77.v12.number = "(77:12)"
psa77.v12.text = "I will meditate on all thy work, and muse on thy mighty deeds."
psa77.v13.number_rsv = "(77:13)"
psa77.v13.number = "(77:13)"
psa77.v13.text = "Thy way, O God, is holy.What god is great like our God?"
psa77.v14.number_rsv = "(77:14)"
psa77.v14.number = "(77:14)"
psa77.v14.text = "Thou art the God who workest wonders, who hast manifested thy might among the peoples."
psa77.v15.number_rsv = "(77:15)"
psa77.v15.number = "(77:15)"
psa77.v15.text = "Thou didst with thy arm redeem thy people, the sons of Jacob and Joseph."
psa77.v16.number_rsv = "(77:16)"
psa77.v16.number = "(77:16)"
psa77.v16.text = "When the waters saw thee, O God, when the waters saw thee, they were afraid, yea, the deep trembled."
psa77.v17.number_rsv = "(77:17)"
psa77.v17.number = "(77:17)"
psa77.v17.text = "The clouds poured out water; the skies gave forth thunder; thy arrows flashed on every side."
psa77.v18.number_rsv = "(77:18)"
psa77.v18.number = "(77:18)"
psa77.v18.text = "The crash of thy thunder was in the whirlwind; thy lightnings lighted up the world; the earth trembled and shook."
psa77.v19.number_rsv = "(77:19)"
psa77.v19.number = "(77:19)"
psa77.v19.text = "Thy way was through the sea, thy path through the great waters; yet thy footprints were unseen."
psa77.v20.number_rsv = "(77:20)"
psa77.v20.number = "(77:20)"
psa77.v20.text = "Thou didst lead thy people like a flockby the hand of Moses and Aaron."

kathisma_11.title = "KATHISMA 11."

psa78.title = "Psalm 78"
psa78.version = properties_en_US_rsv.version.designation
psa78.text = ""
psa78.v0.number_rsv = "(78:0)"
psa78.v0.number = "(78:0)"
psa78.v0.text = "A Maskil of Asaph."
psa78.v1.number_rsv = "(78:1)"
psa78.v1.number = "(78:1)"
psa78.v1.text = "Give ear, O my people, to my teaching; incline your ears to the words of my mouth!"
psa78.v2.number_rsv = "(78:2)"
psa78.v2.number = "(78:2)"
psa78.v2.text = "I will open my mouth in a parable; I will utter dark sayings from of old,"
psa78.v3.number_rsv = "(78:3)"
psa78.v3.number = "(78:3)"
psa78.v3.text = "things that we have heard and known, that our fathers have told us."
psa78.v4.number_rsv = "(78:4)"
psa78.v4.number = "(78:4)"
psa78.v4.text = "We will not hide them from their children, but tell to the coming generationthe glorious deeds of the LORD, and his might, and the wonders which he has wrought."
psa78.v5.number_rsv = "(78:5)"
psa78.v5.number = "(78:5)"
psa78.v5.text = "He established a testimony in Jacob, and appointed a law in Israel, which he commanded our fathersto teach to their children;"
psa78.v6.number_rsv = "(78:6)"
psa78.v6.number = "(78:6)"
psa78.v6.text = "that the next generation might know them, the children yet unborn, and arise and tell them to their children,"
psa78.v7.number_rsv = "(78:7)"
psa78.v7.number = "(78:7)"
psa78.v7.text = "so that they should set their hope in God, and not forget the works of God, but keep his commandments;"
psa78.v8.number_rsv = "(78:8)"
psa78.v8.number = "(78:8)"
psa78.v8.text = "and that they should not be like their fathers, a stubborn and rebellious generation, a generation whose heart was not steadfast, whose spirit was not faithful to God."
psa78.v9.number_rsv = "(78:9)"
psa78.v9.number = "(78:9)"
psa78.v9.text = "The E\"phraimites, armed with the bow, turned back on the day of battle."
psa78.v10.number_rsv = "(78:10)"
psa78.v10.number = "(78:10)"
psa78.v10.text = "They did not keep God\"s covenant, but refused to walk according to his law."
psa78.v11.number_rsv = "(78:11)"
psa78.v11.number = "(78:11)"
psa78.v11.text = "They forgot what he had done, and the miracles that he had shown them."
psa78.v12.number_rsv = "(78:12)"
psa78.v12.number = "(78:12)"
psa78.v12.text = "In the sight of their fathers he wrought marvelsin the land of Egypt, in the fields of Zo\"an."
psa78.v13.number_rsv = "(78:13)"
psa78.v13.number = "(78:13)"
psa78.v13.text = "He divided the sea and let them pass through it, and made the waters stand like a heap."
psa78.v14.number_rsv = "(78:14)"
psa78.v14.number = "(78:14)"
psa78.v14.text = "In the daytime he led them with a cloud, and all the night with a fiery light."
psa78.v15.number_rsv = "(78:15)"
psa78.v15.number = "(78:15)"
psa78.v15.text = "He cleft rocks in the wilderness, and gave them drink abundantly as from the deep."
psa78.v16.number_rsv = "(78:16)"
psa78.v16.number = "(78:16)"
psa78.v16.text = "He made streams come out of the rock, and caused waters to flow down like rivers."
psa78.v17.number_rsv = "(78:17)"
psa78.v17.number = "(78:17)"
psa78.v17.text = "Yet they sinned still more against him, rebelling against the Most High in the desert."
psa78.v18.number_rsv = "(78:18)"
psa78.v18.number = "(78:18)"
psa78.v18.text = "They tested God in their heartby demanding the food they craved."
psa78.v19.number_rsv = "(78:19)"
psa78.v19.number = "(78:19)"
psa78.v19.text = "They spoke against God, saying,\"Can God spread a table in the wilderness?"
psa78.v20.number_rsv = "(78:20)"
psa78.v20.number = "(78:20)"
psa78.v20.text = "He smote the rock so that water gushed outand streams overflowed.Can he also give bread, or provide meat for his people?\""
psa78.v21.number_rsv = "(78:21)"
psa78.v21.number = "(78:21)"
psa78.v21.text = "Therefore, when the LORD heard, he was full of wrath; a fire was kindled against Jacob, his anger mounted against Israel;"
psa78.v22.number_rsv = "(78:22)"
psa78.v22.number = "(78:22)"
psa78.v22.text = "because they had no faith in God, and did not trust his saving power."
psa78.v23.number_rsv = "(78:23)"
psa78.v23.number = "(78:23)"
psa78.v23.text = "Yet he commanded the skies above, and opened the doors of heaven;"
psa78.v24.number_rsv = "(78:24)"
psa78.v24.number = "(78:24)"
psa78.v24.text = "and he rained down upon them manna to eat, and gave them the grain of heaven."
psa78.v25.number_rsv = "(78:25)"
psa78.v25.number = "(78:25)"
psa78.v25.text = "Man ate of the bread of the angels; he sent them food in abundance."
psa78.v26.number_rsv = "(78:26)"
psa78.v26.number = "(78:26)"
psa78.v26.text = "He caused the east wind to blow in the heavens, and by his power he led out the south wind;"
psa78.v27.number_rsv = "(78:27)"
psa78.v27.number = "(78:27)"
psa78.v27.text = "he rained flesh upon them like dust, winged birds like the sand of the seas;"
psa78.v28.number_rsv = "(78:28)"
psa78.v28.number = "(78:28)"
psa78.v28.text = "he let them fall in the midst of their camp, all around their habitations."
psa78.v29.number_rsv = "(78:29)"
psa78.v29.number = "(78:29)"
psa78.v29.text = "And they ate and were well filled, for he gave them what they craved."
psa78.v30.number_rsv = "(78:30)"
psa78.v30.number = "(78:30)"
psa78.v30.text = "But before they had sated their craving, while the food was still in their mouths,"
psa78.v31.number_rsv = "(78:31)"
psa78.v31.number = "(78:31)"
psa78.v31.text = "the anger of God rose against themand he slew the strongest of them, and laid low the picked men of Israel."
psa78.v32.number_rsv = "(78:32)"
psa78.v32.number = "(78:32)"
psa78.v32.text = "In spite of all this they still sinned; despite his wonders they did not believe."
psa78.v33.number_rsv = "(78:33)"
psa78.v33.number = "(78:33)"
psa78.v33.text = "So he made their days vanish like a breath, and their years in terror."
psa78.v34.number_rsv = "(78:34)"
psa78.v34.number = "(78:34)"
psa78.v34.text = "When he slew them, they sought for him; they repented and sought God earnestly."
psa78.v35.number_rsv = "(78:35)"
psa78.v35.number = "(78:35)"
psa78.v35.text = "They remembered that God was their rock, the Most High God their redeemer."
psa78.v36.number_rsv = "(78:36)"
psa78.v36.number = "(78:36)"
psa78.v36.text = "But they flattered him with their mouths; they lied to him with their tongues."
psa78.v37.number_rsv = "(78:37)"
psa78.v37.number = "(78:37)"
psa78.v37.text = "Their heart was not steadfast toward him; they were not true to his covenant."
psa78.v38.number_rsv = "(78:38)"
psa78.v38.number = "(78:38)"
psa78.v38.text = "Yet he, being compassionate, forgave their iniquity, and did not destroy them; he restrained his anger often, and did not stir up all his wrath."
psa78.v39.number_rsv = "(78:39)"
psa78.v39.number = "(78:39)"
psa78.v39.text = "He remembered that they were but flesh, a wind that passes and comes not again."
psa78.v40.number_rsv = "(78:40)"
psa78.v40.number = "(78:40)"
psa78.v40.text = "How often they rebelled against him in the wildernessand grieved him in the desert!"
psa78.v41.number_rsv = "(78:41)"
psa78.v41.number = "(78:41)"
psa78.v41.text = "They tested him again and again, and provoked the Holy One of Israel."
psa78.v42.number_rsv = "(78:42)"
psa78.v42.number = "(78:42)"
psa78.v42.text = "They did not keep in mind his power, or the day when he redeemed them from the foe;"
psa78.v43.number_rsv = "(78:43)"
psa78.v43.number = "(78:43)"
psa78.v43.text = "when he wrought his signs in Egypt, and his miracles in the fields of Zo\"an."
psa78.v44.number_rsv = "(78:44)"
psa78.v44.number = "(78:44)"
psa78.v44.text = "He turned their rivers to blood, so that they could not drink of their streams."
psa78.v45.number_rsv = "(78:45)"
psa78.v45.number = "(78:45)"
psa78.v45.text = "He sent among them swarms of flies, which devoured them, and frogs, which destroyed them."
psa78.v46.number_rsv = "(78:46)"
psa78.v46.number = "(78:46)"
psa78.v46.text = "He gave their crops to the caterpillar, and the fruit of their labor to the locust."
psa78.v47.number_rsv = "(78:47)"
psa78.v47.number = "(78:47)"
psa78.v47.text = "He destroyed their vines with hail, and their sycamores with frost."
psa78.v48.number_rsv = "(78:48)"
psa78.v48.number = "(78:48)"
psa78.v48.text = "He gave over their cattle to the hail, and their flocks to thunderbolts."
psa78.v49.number_rsv = "(78:49)"
psa78.v49.number = "(78:49)"
psa78.v49.text = "He let loose on them his fierce anger, wrath, indignation, and distress, a company of destroying angels."
psa78.v50.number_rsv = "(78:50)"
psa78.v50.number = "(78:50)"
psa78.v50.text = "He made a path for his anger; he did not spare them from death, but gave their lives over to the plague."
psa78.v51.number_rsv = "(78:51)"
psa78.v51.number = "(78:51)"
psa78.v51.text = "He smote all the first-born in Egypt, the first issue of their strength in the tents of Ham."
psa78.v52.number_rsv = "(78:52)"
psa78.v52.number = "(78:52)"
psa78.v52.text = "Then he led forth his people like sheep, and guided them in the wilderness like a flock."
psa78.v53.number_rsv = "(78:53)"
psa78.v53.number = "(78:53)"
psa78.v53.text = "He led them in safety, so that they were not afraid; but the sea overwhelmed their enemies."
psa78.v54.number_rsv = "(78:54)"
psa78.v54.number = "(78:54)"
psa78.v54.text = "And he brought them to his holy land, to the mountain which his right hand had won."
psa78.v55.number_rsv = "(78:55)"
psa78.v55.number = "(78:55)"
psa78.v55.text = "He drove out nations before them; he apportioned them for a possessionand settled the tribes of Israel in their tents."
psa78.v56.number_rsv = "(78:56)"
psa78.v56.number = "(78:56)"
psa78.v56.text = "Yet they tested and rebelled against the Most High God, and did not observe his testimonies,"
psa78.v57.number_rsv = "(78:57)"
psa78.v57.number = "(78:57)"
psa78.v57.text = "but turned away and acted treacherously like their fathers; they twisted like a deceitful bow."
psa78.v58.number_rsv = "(78:58)"
psa78.v58.number = "(78:58)"
psa78.v58.text = "For they provoked him to anger with their high places; they moved him to jealousy with their graven images."
psa78.v59.number_rsv = "(78:59)"
psa78.v59.number = "(78:59)"
psa78.v59.text = "When God heard, he was full of wrath, and he utterly rejected Israel."
psa78.v60.number_rsv = "(78:60)"
psa78.v60.number = "(78:60)"
psa78.v60.text = "He forsook his dwelling at Shiloh, the tent where he dwelt among men,"
psa78.v61.number_rsv = "(78:61)"
psa78.v61.number = "(78:61)"
psa78.v61.text = "and delivered his power to captivity, his glory to the hand of the foe."
psa78.v62.number_rsv = "(78:62)"
psa78.v62.number = "(78:62)"
psa78.v62.text = "He gave his people over to the sword, and vented his wrath on his heritage."
psa78.v63.number_rsv = "(78:63)"
psa78.v63.number = "(78:63)"
psa78.v63.text = "Fire devoured their young men, and their maidens had no marriage song."
psa78.v64.number_rsv = "(78:64)"
psa78.v64.number = "(78:64)"
psa78.v64.text = "Their priests fell by the sword, and their widows made no lamentation."
psa78.v65.number_rsv = "(78:65)"
psa78.v65.number = "(78:65)"
psa78.v65.text = "Then the Lord awoke as from sleep, like a strong man shouting because of wine."
psa78.v66.number_rsv = "(78:66)"
psa78.v66.number = "(78:66)"
psa78.v66.text = "And he put his adversaries to rout; he put them to everlasting shame."
psa78.v67.number_rsv = "(78:67)"
psa78.v67.number = "(78:67)"
psa78.v67.text = "He rejected the tent of Joseph, he did not choose the tribe of E\"phraim;"
psa78.v68.number_rsv = "(78:68)"
psa78.v68.number = "(78:68)"
psa78.v68.text = "but he chose the tribe of Judah, Mount Zion, which he loves."
psa78.v69.number_rsv = "(78:69)"
psa78.v69.number = "(78:69)"
psa78.v69.text = "He built his sanctuary like the high heavens, like the earth, which he has founded for ever."
psa78.v70.number_rsv = "(78:70)"
psa78.v70.number = "(78:70)"
psa78.v70.text = "He chose David his servant, and took him from the sheepfolds;"
psa78.v71.number_rsv = "(78:71)"
psa78.v71.number = "(78:71)"
psa78.v71.text = "from tending the ewes that had young he brought himto be the shepherd of Jacob his people, of Israel his inheritance."
psa78.v72.number_rsv = "(78:72)"
psa78.v72.number = "(78:72)"
psa78.v72.text = "With upright heart he tended them, and guided them with skilful hand."

psa79.title = "Psalm 79"
psa79.version = properties_en_US_rsv.version.designation
psa79.text = ""
psa79.v0.number_rsv = "(79:0)"
psa79.v0.number = "(79:0)"
psa79.v0.text = "A Psalm of Asaph."
psa79.v1.number_rsv = "(79:1)"
psa79.v1.number = "(79:1)"
psa79.v1.text = "O God, the heathen have come into thy inheritance; they have defiled thy holy temple; they have laid Jerusalem in ruins."
psa79.v2.number_rsv = "(79:2)"
psa79.v2.number = "(79:2)"
psa79.v2.text = "They have given the bodies of thy servantsto the birds of the air for food, the flesh of thy saints to the beasts of the earth."
psa79.v3.number_rsv = "(79:3)"
psa79.v3.number = "(79:3)"
psa79.v3.text = "They have poured out their blood like waterround about Jerusalem, and there was none to bury them."
psa79.v4.number_rsv = "(79:4)"
psa79.v4.number = "(79:4)"
psa79.v4.text = "We have become a taunt to our neighbors, mocked and derided by those round about us."
psa79.v5.number_rsv = "(79:5)"
psa79.v5.number = "(79:5)"
psa79.v5.text = "How long, O LORD? Wilt thou be angry for ever? Will thy jealous wrath burn like fire?"
psa79.v6.number_rsv = "(79:6)"
psa79.v6.number = "(79:6)"
psa79.v6.text = "Pour out thy anger on the nationsthat do not know thee, and on the kingdomsthat do not call on thy name!"
psa79.v7.number_rsv = "(79:7)"
psa79.v7.number = "(79:7)"
psa79.v7.text = "For they have devoured Jacob, and laid waste his habitation."
psa79.v8.number_rsv = "(79:8)"
psa79.v8.number = "(79:8)"
psa79.v8.text = "Do not remember against us the iniquities of our forefathers; let thy compassion come speedily to meet us, for we are brought very low."
psa79.v9.number_rsv = "(79:9)"
psa79.v9.number = "(79:9)"
psa79.v9.text = "Help us, O God of our salvation, for the glory of thy name; deliver us, and forgive our sins, for thy name\"s sake!"
psa79.v10.number_rsv = "(79:10)"
psa79.v10.number = "(79:10)"
psa79.v10.text = "Why should the nations say,\"Where is their God?\"Let the avenging of the outpoured blood of thy servantsbe known among the nations before our eyes!"
psa79.v11.number_rsv = "(79:11)"
psa79.v11.number = "(79:11)"
psa79.v11.text = "Let the groans of the prisoners come before thee; according to thy great power preserve those doomed to die!"
psa79.v12.number_rsv = "(79:12)"
psa79.v12.number = "(79:12)"
psa79.v12.text = "Return sevenfold into the bosom of our neighborsthe taunts with which they have taunted thee, O Lord!"
psa79.v13.number_rsv = "(79:13)"
psa79.v13.number = "(79:13)"
psa79.v13.text = "Then we thy people, the flock of thy pasture, will give thanks to thee for ever; from generation to generation we will recount thy praise."

psa80.title = "Psalm 80"
psa80.version = properties_en_US_rsv.version.designation
psa80.text = ""
psa80.v0.number_rsv = "(80:0)"
psa80.v0.number = "(80:0)"
psa80.v0.text = "To the choirmaster: according to Lilies. A Testimony of Asaph. A Psalm."
psa80.v1.number_rsv = "(80:1)"
psa80.v1.number = "(80:1)"
psa80.v1.text = "Give ear, O Shepherd of Israel, thou who leadest Joseph like a flock! Thou who art enthroned upon the cherubim, shine forth"
psa80.v2.number_rsv = "(80:2)"
psa80.v2.number = "(80:2)"
psa80.v2.text = "before E\"phraim and Benjamin and Manas\"seh! Stir up thy might, and come to save us!"
psa80.v3.number_rsv = "(80:3)"
psa80.v3.number = "(80:3)"
psa80.v3.text = "Restore us, O God; let thy face shine, that we may be saved!"
psa80.v4.number_rsv = "(80:4)"
psa80.v4.number = "(80:4)"
psa80.v4.text = "O LORD God of hosts, how long wilt thou be angry with thy people\"s prayers?"
psa80.v5.number_rsv = "(80:5)"
psa80.v5.number = "(80:5)"
psa80.v5.text = "Thou hast fed them with the bread of tears, and given them tears to drink in full measure."
psa80.v6.number_rsv = "(80:6)"
psa80.v6.number = "(80:6)"
psa80.v6.text = "Thou dost make us the scorn of our neighbors; and our enemies laugh among themselves."
psa80.v7.number_rsv = "(80:7)"
psa80.v7.number = "(80:7)"
psa80.v7.text = "Restore us, O God of hosts; let thy face shine, that we may be saved!"
psa80.v8.number_rsv = "(80:8)"
psa80.v8.number = "(80:8)"
psa80.v8.text = "Thou didst bring a vine out of Egypt; thou didst drive out the nations and plant it."
psa80.v9.number_rsv = "(80:9)"
psa80.v9.number = "(80:9)"
psa80.v9.text = "Thou didst clear the ground for it; it took deep root and filled the land."
psa80.v10.number_rsv = "(80:10)"
psa80.v10.number = "(80:10)"
psa80.v10.text = "The mountains were covered with its shade, the mighty cedars with its branches;"
psa80.v11.number_rsv = "(80:11)"
psa80.v11.number = "(80:11)"
psa80.v11.text = "it sent out its branches to the sea, and its shoots to the River."
psa80.v12.number_rsv = "(80:12)"
psa80.v12.number = "(80:12)"
psa80.v12.text = "Why then hast thou broken down its walls, so that all who pass along the way pluck its fruit?"
psa80.v13.number_rsv = "(80:13)"
psa80.v13.number = "(80:13)"
psa80.v13.text = "The boar from the forest ravages it, and all that move in the field feed on it."
psa80.v14.number_rsv = "(80:14)"
psa80.v14.number = "(80:14)"
psa80.v14.text = "Turn again, O God of hosts! Look down from heaven, and see; have regard for this vine,"
psa80.v15.number_rsv = "(80:15)"
psa80.v15.number = "(80:15)"
psa80.v15.text = "the stock which thy right hand planted."
psa80.v16.number_rsv = "(80:16)"
psa80.v16.number = "(80:16)"
psa80.v16.text = "They have burned it with fire, they have cut it down; may they perish at the rebuke of thy countenance!"
psa80.v17.number_rsv = "(80:17)"
psa80.v17.number = "(80:17)"
psa80.v17.text = "But let thy hand be upon the man of thy right hand, the son of man whom thou hast made strong for thyself!"
psa80.v18.number_rsv = "(80:18)"
psa80.v18.number = "(80:18)"
psa80.v18.text = "Then we will never turn back from thee; give us life, and we will call on thy name!"
psa80.v19.number_rsv = "(80:19)"
psa80.v19.number = "(80:19)"
psa80.v19.text = "Restore us, O LORD God of hosts! let thy face shine, that we may be saved! R"

psa81.title = "Psalm 81"
psa81.version = properties_en_US_rsv.version.designation
psa81.text = ""
psa81.v0.number_rsv = "(81:0)"
psa81.v0.number = "(81:0)"
psa81.v0.text = "To the choirmaster: according to The Gittith. A Psalm of Asaph."
psa81.v1.number_rsv = "(81:1)"
psa81.v1.number = "(81:1)"
psa81.v1.text = "Sing aloud to God our strength; shout for joy to the God of Jacob!"
psa81.v2.number_rsv = "(81:2)"
psa81.v2.number = "(81:2)"
psa81.v2.text = "Raise a song, sound the timbrel, the sweet lyre with the harp."
psa81.v3.number_rsv = "(81:3)"
psa81.v3.number = "(81:3)"
psa81.v3.text = "Blow the trumpet at the new moon, at the full moon, on our feast day."
psa81.v4.number_rsv = "(81:4)"
psa81.v4.number = "(81:4)"
psa81.v4.text = "For it is a statute for Israel, an ordinance of the God of Jacob."
psa81.v5.number_rsv = "(81:5)"
psa81.v5.number = "(81:5)"
psa81.v5.text = "He made it a decree in Joseph, when he went out over the land of Egypt.I hear a voice I had not known:"
psa81.v6.number_rsv = "(81:6)"
psa81.v6.number = "(81:6)"
psa81.v6.text = "\"I relieved your shoulder of the burden; your hands were freed from the basket."
psa81.v7.number_rsv = "(81:7)"
psa81.v7.number = "(81:7)"
psa81.v7.text = "In distress you called, and I delivered you; I answered you in the secret place of thunder; I tested you at the waters of Mer\"ibah."
psa81.v8.number_rsv = "(81:8)"
psa81.v8.number = "(81:8)"
psa81.v8.text = "Hear, O my people, while I admonish you! O Israel, if you would but listen to me!"
psa81.v9.number_rsv = "(81:9)"
psa81.v9.number = "(81:9)"
psa81.v9.text = "There shall be no strange god among you; you shall not bow down to a foreign god."
psa81.v10.number_rsv = "(81:10)"
psa81.v10.number = "(81:10)"
psa81.v10.text = "I am the LORD your God, who brought you up out of the land of Egypt.Open your mouth wide, and I will fill it."
psa81.v11.number_rsv = "(81:11)"
psa81.v11.number = "(81:11)"
psa81.v11.text = "\"But my people did not listen to my voice; Israel would have none of me."
psa81.v12.number_rsv = "(81:12)"
psa81.v12.number = "(81:12)"
psa81.v12.text = "So I gave them over to their stubborn hearts, to follow their own counsels."
psa81.v13.number_rsv = "(81:13)"
psa81.v13.number = "(81:13)"
psa81.v13.text = "O that my people would listen to me, that Israel would walk in my ways!"
psa81.v14.number_rsv = "(81:14)"
psa81.v14.number = "(81:14)"
psa81.v14.text = "I would soon subdue their enemies, and turn my hand against their foes."
psa81.v15.number_rsv = "(81:15)"
psa81.v15.number = "(81:15)"
psa81.v15.text = "Those who hate the LORD would cringe toward him, and their fate would last for ever."
psa81.v16.number_rsv = "(81:16)"
psa81.v16.number = "(81:16)"
psa81.v16.text = "I would feed you with the finest of the wheat, and with honey from the rock I would satisfy you.\""

psa82.title = "Psalm 82"
psa82.version = properties_en_US_rsv.version.designation
psa82.text = ""
psa82.v0.number_rsv = "(82:0)"
psa82.v0.number = "(82:0)"
psa82.v0.text = "A Psalm of Asaph."
psa82.v1.number_rsv = "(82:1)"
psa82.v1.number = "(82:1)"
psa82.v1.text = "God has taken his place in the divine council; in the midst of the gods he holds judgment:"
psa82.v2.number_rsv = "(82:2)"
psa82.v2.number = "(82:2)"
psa82.v2.text = "\"How long will you judge unjustlyand show partiality to the wicked?"
psa82.v3.number_rsv = "(82:3)"
psa82.v3.number = "(82:3)"
psa82.v3.text = "Give justice to the weak and the fatherless; maintain the right of the afflicted and the destitute."
psa82.v4.number_rsv = "(82:4)"
psa82.v4.number = "(82:4)"
psa82.v4.text = "Rescue the weak and the needy; deliver them from the hand of the wicked.\""
psa82.v5.number_rsv = "(82:5)"
psa82.v5.number = "(82:5)"
psa82.v5.text = "They have neither knowledge nor understanding, they walk about in darkness; all the foundations of the earth are shaken."
psa82.v6.number_rsv = "(82:6)"
psa82.v6.number = "(82:6)"
psa82.v6.text = "I say, \"You are gods, sons of the Most High, all of you;"
psa82.v7.number_rsv = "(82:7)"
psa82.v7.number = "(82:7)"
psa82.v7.text = "nevertheless, you shall die like men, and fall like any prince.\""
psa82.v8.number_rsv = "(82:8)"
psa82.v8.number = "(82:8)"
psa82.v8.text = "Arise, O God, judge the earth; for to thee belong all the nations!"

psa83.title = "Psalm 83"
psa83.version = properties_en_US_rsv.version.designation
psa83.text = ""
psa83.v0.number_rsv = "(83:0)"
psa83.v0.number = "(83:0)"
psa83.v0.text = "A Song. A Psalm of Asaph."
psa83.v1.number_rsv = "(83:1)"
psa83.v1.number = "(83:1)"
psa83.v1.text = "O God, do not keep silence; do not hold thy peace or be still, O God!"
psa83.v2.number_rsv = "(83:2)"
psa83.v2.number = "(83:2)"
psa83.v2.text = "For lo, thy enemies are in tumult; those who hate thee have raised their heads."
psa83.v3.number_rsv = "(83:3)"
psa83.v3.number = "(83:3)"
psa83.v3.text = "They lay crafty plans against thy people; they consult together against thy protected ones."
psa83.v4.number_rsv = "(83:4)"
psa83.v4.number = "(83:4)"
psa83.v4.text = "They say, \"Come, let us wipe them out as a nation; let the name of Israel be remembered no more!\""
psa83.v5.number_rsv = "(83:5)"
psa83.v5.number = "(83:5)"
psa83.v5.text = "Yea, they conspire with one accord; against thee they make a covenant --"
psa83.v6.number_rsv = "(83:6)"
psa83.v6.number = "(83:6)"
psa83.v6.text = "the tents of Edom and the Ish\"maelites, Moab and the Hagrites,"
psa83.v7.number_rsv = "(83:7)"
psa83.v7.number = "(83:7)"
psa83.v7.text = "Gebal and Ammon and Am\"alek, Philistia with the inhabitants of Tyre;"
psa83.v8.number_rsv = "(83:8)"
psa83.v8.number = "(83:8)"
psa83.v8.text = "Assyria also has joined them; they are the strong arm of the children of Lot."
psa83.v9.number_rsv = "(83:9)"
psa83.v9.number = "(83:9)"
psa83.v9.text = "Do to them as thou didst to Mid\"ian, as to Sis\"era and Jabin at the river Kishon,"
psa83.v10.number_rsv = "(83:10)"
psa83.v10.number = "(83:10)"
psa83.v10.text = "who were destroyed at En-dor, who became dung for the ground."
psa83.v11.number_rsv = "(83:11)"
psa83.v11.number = "(83:11)"
psa83.v11.text = "Make their nobles like Oreb and Zeeb, all their princes like Zebah and Zalmun\"na,"
psa83.v12.number_rsv = "(83:12)"
psa83.v12.number = "(83:12)"
psa83.v12.text = "who said, \"Let us take possession for ourselvesof the pastures of God.\""
psa83.v13.number_rsv = "(83:13)"
psa83.v13.number = "(83:13)"
psa83.v13.text = "O my God, make them like whirling dust, like chaff before the wind."
psa83.v14.number_rsv = "(83:14)"
psa83.v14.number = "(83:14)"
psa83.v14.text = "As fire consumes the forest, as the flame sets the mountains ablaze,"
psa83.v15.number_rsv = "(83:15)"
psa83.v15.number = "(83:15)"
psa83.v15.text = "so do thou pursue them with thy tempestand terrify them with thy hurricane!"
psa83.v16.number_rsv = "(83:16)"
psa83.v16.number = "(83:16)"
psa83.v16.text = "Fill their faces with shame, that they may seek thy name, O LORD."
psa83.v17.number_rsv = "(83:17)"
psa83.v17.number = "(83:17)"
psa83.v17.text = "Let them be put to shame and dismayed for ever; let them perish in disgrace."
psa83.v18.number_rsv = "(83:18)"
psa83.v18.number = "(83:18)"
psa83.v18.text = "Let them know that thou alone, whose name is the LORD, art the Most High over all the earth."

psa84.title = "Psalm 84"
psa84.version = properties_en_US_rsv.version.designation
psa84.text = ""
psa84.v0.number_rsv = "(84:0)"
psa84.v0.number = "(84:0)"
psa84.v0.text = "To the choirmaster: according to The Gittith. A Psalm of the Sons of Korah."
psa84.v1.number_rsv = "(84:1)"
psa84.v1.number = "(84:1)"
psa84.v1.text = "How lovely is thy dwelling place, O LORD of hosts!"
psa84.v2.number_rsv = "(84:2)"
psa84.v2.number = "(84:2)"
psa84.v2.text = "My soul longs, yea, faintsfor the courts of the LORD; my heart and flesh sing for joyto the living God."
psa84.v3.number_rsv = "(84:3)"
psa84.v3.number = "(84:3)"
psa84.v3.text = "Even the sparrow finds a home, and the swallow a nest for herself, where she may lay her young, at thy altars, O LORD of hosts, my King and my God."
psa84.v4.number_rsv = "(84:4)"
psa84.v4.number = "(84:4)"
psa84.v4.text = "Blessed are those who dwell in thy house, ever singing thy praise!"
psa84.v5.number_rsv = "(84:5)"
psa84.v5.number = "(84:5)"
psa84.v5.text = "Blessed are the men whose strength is in thee, in whose heart are the highways to Zion."
psa84.v6.number_rsv = "(84:6)"
psa84.v6.number = "(84:6)"
psa84.v6.text = "As they go through the valley of Bacathey make it a place of springs; the early rain also covers it with pools."
psa84.v7.number_rsv = "(84:7)"
psa84.v7.number = "(84:7)"
psa84.v7.text = "They go from strength to strength; the God of gods will be seen in Zion."
psa84.v8.number_rsv = "(84:8)"
psa84.v8.number = "(84:8)"
psa84.v8.text = "O LORD God of hosts, hear my prayer; give ear, O God of Jacob!"
psa84.v9.number_rsv = "(84:9)"
psa84.v9.number = "(84:9)"
psa84.v9.text = "Behold our shield, O God; look upon the face of thine anointed!"
psa84.v10.number_rsv = "(84:10)"
psa84.v10.number = "(84:10)"
psa84.v10.text = "For a day in thy courts is betterthan a thousand elsewhere.I would rather be a doorkeeper in the house of my Godthan dwell in the tents of wickedness."
psa84.v11.number_rsv = "(84:11)"
psa84.v11.number = "(84:11)"
psa84.v11.text = "For the LORD God is a sun and shield; he bestows favor and honor.No good thing does the LORD withholdfrom those who walk uprightly."
psa84.v12.number_rsv = "(84:12)"
psa84.v12.number = "(84:12)"
psa84.v12.text = "O LORD of hosts, blessed is the man who trusts in thee!"

psa85.title = "Psalm 85"
psa85.version = properties_en_US_rsv.version.designation
psa85.text = ""
psa85.v0.number_rsv = "(85:0)"
psa85.v0.number = "(85:0)"
psa85.v0.text = "To the choirmaster. A Psalm of the Sons of Korah."
psa85.v1.number_rsv = "(85:1)"
psa85.v1.number = "(85:1)"
psa85.v1.text = "LORD, thou wast favorable to thy land; thou didst restore the fortunes of Jacob."
psa85.v2.number_rsv = "(85:2)"
psa85.v2.number = "(85:2)"
psa85.v2.text = "Thou didst forgive the iniquity of thy people; thou didst pardon all their sin."
psa85.v3.number_rsv = "(85:3)"
psa85.v3.number = "(85:3)"
psa85.v3.text = "Thou didst withdraw all thy wrath; thou didst turn from thy hot anger."
psa85.v4.number_rsv = "(85:4)"
psa85.v4.number = "(85:4)"
psa85.v4.text = "Restore us again, O God of our salvation, and put away thy indignation toward us!"
psa85.v5.number_rsv = "(85:5)"
psa85.v5.number = "(85:5)"
psa85.v5.text = "Wilt thou be angry with us for ever? Wilt thou prolong thy anger to all generations?"
psa85.v6.number_rsv = "(85:6)"
psa85.v6.number = "(85:6)"
psa85.v6.text = "Wilt thou not revive us again, that thy people may rejoice in thee?"
psa85.v7.number_rsv = "(85:7)"
psa85.v7.number = "(85:7)"
psa85.v7.text = "Show us thy steadfast love, O LORD, and grant us thy salvation."
psa85.v8.number_rsv = "(85:8)"
psa85.v8.number = "(85:8)"
psa85.v8.text = "Let me hear what God the LORD will speak, for he will speak peace to his people, to his saints, to those who turn to him in their hearts."
psa85.v9.number_rsv = "(85:9)"
psa85.v9.number = "(85:9)"
psa85.v9.text = "Surely his salvation is at hand for those who fear him, that glory may dwell in our land."
psa85.v10.number_rsv = "(85:10)"
psa85.v10.number = "(85:10)"
psa85.v10.text = "Steadfast love and faithfulness will meet; righteousness and peace will kiss each other."
psa85.v11.number_rsv = "(85:11)"
psa85.v11.number = "(85:11)"
psa85.v11.text = "Faithfulness will spring up from the ground, and righteousness will look down from the sky."
psa85.v12.number_rsv = "(85:12)"
psa85.v12.number = "(85:12)"
psa85.v12.text = "Yea, the LORD will give what is good, and our land will yield its increase."
psa85.v13.number_rsv = "(85:13)"
psa85.v13.number = "(85:13)"
psa85.v13.text = "Righteousness will go before him, and make his footsteps a way."

kathisma_12.title = "KATHISMA 12."

psa86.title = "Psalm 86"
psa86.version = properties_en_US_rsv.version.designation
psa86.text = ""
psa86.v0.number_rsv = "(86:0)"
psa86.v0.number = "(86:0)"
psa86.v0.text = "A Prayer of David."
psa86.v1.number_rsv = "(86:1)"
psa86.v1.number = "(86:1)"
psa86.v1.text = "Incline thy ear, O LORD, and answer me, for I am poor and needy."
psa86.v2.number_rsv = "(86:2)"
psa86.v2.number = "(86:2)"
psa86.v2.text = "Preserve my life, for I am godly; save thy servant who trusts in thee.Thou art my God;"
psa86.v3.number_rsv = "(86:3)"
psa86.v3.number = "(86:3)"
psa86.v3.text = "be gracious to me, O Lord, for to thee do I cry all the day."
psa86.v4.number_rsv = "(86:4)"
psa86.v4.number = "(86:4)"
psa86.v4.text = "Gladden the soul of thy servant, for to thee, O Lord, do I lift up my soul."
psa86.v5.number_rsv = "(86:5)"
psa86.v5.number = "(86:5)"
psa86.v5.text = "For thou, O Lord, art good and forgiving, abounding in steadfast love to all who call on thee."
psa86.v6.number_rsv = "(86:6)"
psa86.v6.number = "(86:6)"
psa86.v6.text = "Give ear, O LORD, to my prayer; hearken to my cry of supplication."
psa86.v7.number_rsv = "(86:7)"
psa86.v7.number = "(86:7)"
psa86.v7.text = "In the day of my trouble I call on thee, for thou dost answer me."
psa86.v8.number_rsv = "(86:8)"
psa86.v8.number = "(86:8)"
psa86.v8.text = "There is none like thee among the gods, O Lord, nor are there any works like thine."
psa86.v9.number_rsv = "(86:9)"
psa86.v9.number = "(86:9)"
psa86.v9.text = "All the nations thou hast made shall comeand bow down before thee, O Lord, and shall glorify thy name."
psa86.v10.number_rsv = "(86:10)"
psa86.v10.number = "(86:10)"
psa86.v10.text = "For thou art great and doest wondrous things, thou alone art God."
psa86.v11.number_rsv = "(86:11)"
psa86.v11.number = "(86:11)"
psa86.v11.text = "Teach me thy way, O LORD, that I may walk in thy truth; unite my heart to fear thy name."
psa86.v12.number_rsv = "(86:12)"
psa86.v12.number = "(86:12)"
psa86.v12.text = "I give thanks to thee, O Lord my God, with my whole heart, and I will glorify thy name for ever."
psa86.v13.number_rsv = "(86:13)"
psa86.v13.number = "(86:13)"
psa86.v13.text = "For great is thy steadfast love toward me; thou hast delivered my soul from the depths of Sheol."
psa86.v14.number_rsv = "(86:14)"
psa86.v14.number = "(86:14)"
psa86.v14.text = "O God, insolent men have risen up against me; a band of ruthless men seek my life, and they do not set thee before them."
psa86.v15.number_rsv = "(86:15)"
psa86.v15.number = "(86:15)"
psa86.v15.text = "But thou, O Lord, art a God merciful and gracious, slow to anger and abounding in steadfast love and faithfulness."
psa86.v16.number_rsv = "(86:16)"
psa86.v16.number = "(86:16)"
psa86.v16.text = "Turn to me and take pity on me; give thy strength to thy servant, and save the son of thy handmaid."
psa86.v17.number_rsv = "(86:17)"
psa86.v17.number = "(86:17)"
psa86.v17.text = "Show me a sign of thy favor, that those who hate me may see and be put to shamebecause thou, LORD, hast helped me and comforted me."

psa87.title = "Psalm 87"
psa87.version = properties_en_US_rsv.version.designation
psa87.text = ""
psa87.v0.number_rsv = "(87:0)"
psa87.v0.number = "(87:0)"
psa87.v0.text = "A Psalm of the Sons of Korah. A Song."
psa87.v1.number_rsv = "(87:1)"
psa87.v1.number = "(87:1)"
psa87.v1.text = "On the holy mount stands the city he founded;"
psa87.v2.number_rsv = "(87:2)"
psa87.v2.number = "(87:2)"
psa87.v2.text = "the LORD loves the gates of Zionmore than all the dwelling places of Jacob."
psa87.v3.number_rsv = "(87:3)"
psa87.v3.number = "(87:3)"
psa87.v3.text = "Glorious things are spoken of you, O city of God."
psa87.v4.number_rsv = "(87:4)"
psa87.v4.number = "(87:4)"
psa87.v4.text = "Among those who know me I mention Rahab and Babylon; behold, Philistia and Tyre, with Ethiopia --\"This one was born there,\" they say."
psa87.v5.number_rsv = "(87:5)"
psa87.v5.number = "(87:5)"
psa87.v5.text = "And of Zion it shall be said,\"This one and that one were born in her\"; for the Most High himself will establish her."
psa87.v6.number_rsv = "(87:6)"
psa87.v6.number = "(87:6)"
psa87.v6.text = "The LORD records as he registers the peoples,\"This one was born there.\""
psa87.v7.number_rsv = "(87:7)"
psa87.v7.number = "(87:7)"
psa87.v7.text = "Singers and dancers alike say,\"All my springs are in you.\""

psa88.title = "Psalm 88"
psa88.version = properties_en_US_rsv.version.designation
psa88.text = ""
psa88.v0.number_rsv = "(88:0)"
psa88.v0.number = "(88:0)"
psa88.v0.text = "A Song. A Psalm of the Sons of Korah. To the choirmaster: according to Mahalath Leannoth. A Maskil of Heman the Ezrahite."
psa88.v1.number_rsv = "(88:1)"
psa88.v1.number = "(88:1)"
psa88.v1.text = "O LORD, my God, I call for help by day; I cry out in the night before thee."
psa88.v2.number_rsv = "(88:2)"
psa88.v2.number = "(88:2)"
psa88.v2.text = "Let my prayer come before thee, incline thy ear to my cry!"
psa88.v3.number_rsv = "(88:3)"
psa88.v3.number = "(88:3)"
psa88.v3.text = "For my soul is full of troubles, and my life draws near to Sheol."
psa88.v4.number_rsv = "(88:4)"
psa88.v4.number = "(88:4)"
psa88.v4.text = "I am reckoned among those who go down to the Pit; I am a man who has no strength,"
psa88.v5.number_rsv = "(88:5)"
psa88.v5.number = "(88:5)"
psa88.v5.text = "like one forsaken among the dead, like the slain that lie in the grave, like those whom thou dost remember no more, for they are cut off from thy hand."
psa88.v6.number_rsv = "(88:6)"
psa88.v6.number = "(88:6)"
psa88.v6.text = "Thou hast put me in the depths of the Pit, in the regions dark and deep."
psa88.v7.number_rsv = "(88:7)"
psa88.v7.number = "(88:7)"
psa88.v7.text = "Thy wrath lies heavy upon me, and thou dost overwhelm me with all thy waves."
psa88.v8.number_rsv = "(88:8)"
psa88.v8.number = "(88:8)"
psa88.v8.text = "Thou hast caused my companions to shun me; thou hast made me a thing of horror to them.I am shut in so that I cannot escape;"
psa88.v9.number_rsv = "(88:9)"
psa88.v9.number = "(88:9)"
psa88.v9.text = "my eye grows dim through sorrow.Every day I call upon thee, O LORD; I spread out my hands to thee."
psa88.v10.number_rsv = "(88:10)"
psa88.v10.number = "(88:10)"
psa88.v10.text = "Dost thou work wonders for the dead? Do the shades rise up to praise thee?"
psa88.v11.number_rsv = "(88:11)"
psa88.v11.number = "(88:11)"
psa88.v11.text = "Is thy steadfast love declared in the grave, or thy faithfulness in Abaddon?"
psa88.v12.number_rsv = "(88:12)"
psa88.v12.number = "(88:12)"
psa88.v12.text = "Are thy wonders known in the darkness, or thy saving help in the land of forgetfulness?"
psa88.v13.number_rsv = "(88:13)"
psa88.v13.number = "(88:13)"
psa88.v13.text = "But I, O LORD, cry to thee; in the morning my prayer comes before thee."
psa88.v14.number_rsv = "(88:14)"
psa88.v14.number = "(88:14)"
psa88.v14.text = "O LORD, why dost thou cast me off? Why dost thou hide thy face from me?"
psa88.v15.number_rsv = "(88:15)"
psa88.v15.number = "(88:15)"
psa88.v15.text = "Afflicted and close to death from my youth up, I suffer thy terrors; I am helpless."
psa88.v16.number_rsv = "(88:16)"
psa88.v16.number = "(88:16)"
psa88.v16.text = "Thy wrath has swept over me; thy dread assaults destroy me."
psa88.v17.number_rsv = "(88:17)"
psa88.v17.number = "(88:17)"
psa88.v17.text = "They surround me like a flood all day long; they close in upon me together."
psa88.v18.number_rsv = "(88:18)"
psa88.v18.number = "(88:18)"
psa88.v18.text = "Thou hast caused lover and friend to shun me; my companions are in darkness."

psa89.title = "Psalm 89"
psa89.version = properties_en_US_rsv.version.designation
psa89.text = ""
psa89.v0.number_rsv = "(89:0)"
psa89.v0.number = "(89:0)"
psa89.v0.text = "A Maskil of Ethan the Ezrahite."
psa89.v1.number_rsv = "(89:1)"
psa89.v1.number = "(89:1)"
psa89.v1.text = "I will sing of thy steadfast love, O LORD, for ever; with my mouth I will proclaim thy faithfulness to all generations."
psa89.v2.number_rsv = "(89:2)"
psa89.v2.number = "(89:2)"
psa89.v2.text = "For thy steadfast love was established for ever, thy faithfulness is firm as the heavens."
psa89.v3.number_rsv = "(89:3)"
psa89.v3.number = "(89:3)"
psa89.v3.text = "Thou hast said, \"I have made a covenant with my chosen one, I have sworn to David my servant:"
psa89.v4.number_rsv = "(89:4)"
psa89.v4.number = "(89:4)"
psa89.v4.text = "`I will establish your descendants for ever, and build your throne for all generations.\"\""
psa89.v5.number_rsv = "(89:5)"
psa89.v5.number = "(89:5)"
psa89.v5.text = "Let the heavens praise thy wonders, O LORD, thy faithfulness in the assembly of the holy ones!"
psa89.v6.number_rsv = "(89:6)"
psa89.v6.number = "(89:6)"
psa89.v6.text = "For who in the skies can be compared to the LORD? Who among the heavenly beings is like the LORD,"
psa89.v7.number_rsv = "(89:7)"
psa89.v7.number = "(89:7)"
psa89.v7.text = "a God feared in the council of the holy ones, great and terrible above all that are round about him?"
psa89.v8.number_rsv = "(89:8)"
psa89.v8.number = "(89:8)"
psa89.v8.text = "O LORD God of hosts, who is mighty as thou art, O LORD, with thy faithfulness round about thee?"
psa89.v9.number_rsv = "(89:9)"
psa89.v9.number = "(89:9)"
psa89.v9.text = "Thou dost rule the raging of the sea; when its waves rise, thou stillest them."
psa89.v10.number_rsv = "(89:10)"
psa89.v10.number = "(89:10)"
psa89.v10.text = "Thou didst crush Rahab like a carcass, thou didst scatter thy enemies with thy mighty arm."
psa89.v11.number_rsv = "(89:11)"
psa89.v11.number = "(89:11)"
psa89.v11.text = "The heavens are thine, the earth also is thine; the world and all that is in it, thou hast founded them."
psa89.v12.number_rsv = "(89:12)"
psa89.v12.number = "(89:12)"
psa89.v12.text = "The north and the south, thou hast created them; Tabor and Hermon joyously praise thy name."
psa89.v13.number_rsv = "(89:13)"
psa89.v13.number = "(89:13)"
psa89.v13.text = "Thou hast a mighty arm; strong is thy hand, high thy right hand."
psa89.v14.number_rsv = "(89:14)"
psa89.v14.number = "(89:14)"
psa89.v14.text = "Righteousness and justice are the foundation of thy throne; steadfast love and faithfulness go before thee."
psa89.v15.number_rsv = "(89:15)"
psa89.v15.number = "(89:15)"
psa89.v15.text = "Blessed are the people who know the festal shout, who walk, O LORD, in the light of thy countenance,"
psa89.v16.number_rsv = "(89:16)"
psa89.v16.number = "(89:16)"
psa89.v16.text = "who exult in thy name all the day, and extol thy righteousness."
psa89.v17.number_rsv = "(89:17)"
psa89.v17.number = "(89:17)"
psa89.v17.text = "For thou art the glory of their strength; by thy favor our horn is exalted."
psa89.v18.number_rsv = "(89:18)"
psa89.v18.number = "(89:18)"
psa89.v18.text = "For our shield belongs to the LORD, our king to the Holy One of Israel."
psa89.v19.number_rsv = "(89:19)"
psa89.v19.number = "(89:19)"
psa89.v19.text = "Of old thou didst speak in a visionto thy faithful one, and say:\"I have set the crown upon one who is mighty, I have exalted one chosen from the people."
psa89.v20.number_rsv = "(89:20)"
psa89.v20.number = "(89:20)"
psa89.v20.text = "I have found David, my servant; with my holy oil I have anointed him;"
psa89.v21.number_rsv = "(89:21)"
psa89.v21.number = "(89:21)"
psa89.v21.text = "so that my hand shall ever abide with him, my arm also shall strengthen him."
psa89.v22.number_rsv = "(89:22)"
psa89.v22.number = "(89:22)"
psa89.v22.text = "The enemy shall not outwit him, the wicked shall not humble him."
psa89.v23.number_rsv = "(89:23)"
psa89.v23.number = "(89:23)"
psa89.v23.text = "I will crush his foes before himand strike down those who hate him."
psa89.v24.number_rsv = "(89:24)"
psa89.v24.number = "(89:24)"
psa89.v24.text = "My faithfulness and my steadfast love shall be with him, and in my name shall his horn be exalted."
psa89.v25.number_rsv = "(89:25)"
psa89.v25.number = "(89:25)"
psa89.v25.text = "I will set his hand on the seaand his right hand on the rivers."
psa89.v26.number_rsv = "(89:26)"
psa89.v26.number = "(89:26)"
psa89.v26.text = "He shall cry to me, `Thou art my Father, my God, and the Rock of my salvation.\""
psa89.v27.number_rsv = "(89:27)"
psa89.v27.number = "(89:27)"
psa89.v27.text = "And I will make him the first-born, the highest of the kings of the earth."
psa89.v28.number_rsv = "(89:28)"
psa89.v28.number = "(89:28)"
psa89.v28.text = "My steadfast love I will keep for him for ever, and my covenant will stand firm for him."
psa89.v29.number_rsv = "(89:29)"
psa89.v29.number = "(89:29)"
psa89.v29.text = "I will establish his line for everand his throne as the days of the heavens."
psa89.v30.number_rsv = "(89:30)"
psa89.v30.number = "(89:30)"
psa89.v30.text = "If his children forsake my lawand do not walk according to my ordinances,"
psa89.v31.number_rsv = "(89:31)"
psa89.v31.number = "(89:31)"
psa89.v31.text = "if they violate my statutesand do not keep my commandments,"
psa89.v32.number_rsv = "(89:32)"
psa89.v32.number = "(89:32)"
psa89.v32.text = "then I will punish their transgression with the rodand their iniquity with scourges;"
psa89.v33.number_rsv = "(89:33)"
psa89.v33.number = "(89:33)"
psa89.v33.text = "but I will not remove from him my steadfast love, or be false to my faithfulness."
psa89.v34.number_rsv = "(89:34)"
psa89.v34.number = "(89:34)"
psa89.v34.text = "I will not violate my covenant, or alter the word that went forth from my lips."
psa89.v35.number_rsv = "(89:35)"
psa89.v35.number = "(89:35)"
psa89.v35.text = "Once for all I have sworn by my holiness; I will not lie to David."
psa89.v36.number_rsv = "(89:36)"
psa89.v36.number = "(89:36)"
psa89.v36.text = "His line shall endure for ever, his throne as long as the sun before me."
psa89.v37.number_rsv = "(89:37)"
psa89.v37.number = "(89:37)"
psa89.v37.text = "Like the moon it shall be established for ever; it shall stand firm while the skies endure.\""
psa89.v38.number_rsv = "(89:38)"
psa89.v38.number = "(89:38)"
psa89.v38.text = "But now thou hast cast off and rejected, thou art full of wrath against thy anointed."
psa89.v39.number_rsv = "(89:39)"
psa89.v39.number = "(89:39)"
psa89.v39.text = "Thou hast renounced the covenant with thy servant; thou hast defiled his crown in the dust."
psa89.v40.number_rsv = "(89:40)"
psa89.v40.number = "(89:40)"
psa89.v40.text = "Thou hast breached all his walls; thou hast laid his strongholds in ruins."
psa89.v41.number_rsv = "(89:41)"
psa89.v41.number = "(89:41)"
psa89.v41.text = "All that pass by despoil him; he has become the scorn of his neighbors."
psa89.v42.number_rsv = "(89:42)"
psa89.v42.number = "(89:42)"
psa89.v42.text = "Thou hast exalted the right hand of his foes; thou hast made all his enemies rejoice."
psa89.v43.number_rsv = "(89:43)"
psa89.v43.number = "(89:43)"
psa89.v43.text = "Yea, thou hast turned back the edge of his sword, and thou hast not made him stand in battle."
psa89.v44.number_rsv = "(89:44)"
psa89.v44.number = "(89:44)"
psa89.v44.text = "Thou hast removed the scepter from his hand, and cast his throne to the ground."
psa89.v45.number_rsv = "(89:45)"
psa89.v45.number = "(89:45)"
psa89.v45.text = "Thou hast cut short the days of his youth; thou hast covered him with shame."
psa89.v46.number_rsv = "(89:46)"
psa89.v46.number = "(89:46)"
psa89.v46.text = "How long, O LORD? Wilt thou hide thyself for ever? How long will thy wrath burn like fire?"
psa89.v47.number_rsv = "(89:47)"
psa89.v47.number = "(89:47)"
psa89.v47.text = "Remember, O Lord, what the measure of life is, for what vanity thou hast created all the sons of men!"
psa89.v48.number_rsv = "(89:48)"
psa89.v48.number = "(89:48)"
psa89.v48.text = "What man can live and never see death? Who can deliver his soul from the power of Sheol?"
psa89.v49.number_rsv = "(89:49)"
psa89.v49.number = "(89:49)"
psa89.v49.text = "Lord, where is thy steadfast love of old, which by thy faithfulness thou didst swear to David?"
psa89.v50.number_rsv = "(89:50)"
psa89.v50.number = "(89:50)"
psa89.v50.text = "Remember, O Lord, how thy servant is scorned; how I bear in my bosom the insults of the peoples,"
psa89.v51.number_rsv = "(89:51)"
psa89.v51.number = "(89:51)"
psa89.v51.text = "with which thy enemies taunt, O LORD, with which they mock the footsteps of thy anointed."
psa89.v52.number_rsv = "(89:52)"
psa89.v52.number = "(89:52)"
psa89.v52.text = "Blessed be the LORD for ever! Amen and Amen."

psa90.title = "Psalm 90"
psa90.version = properties_en_US_rsv.version.designation
psa90.text = ""
psa90.v0.number_rsv = "(90:0)"
psa90.v0.number = "(90:0)"
psa90.v0.text = "A Prayer of Moses, the man of God."
psa90.v1.number_rsv = "(90:1)"
psa90.v1.number = "(90:1)"
psa90.v1.text = "LORD, thou hast been our dwelling placein all generations."
psa90.v2.number_rsv = "(90:2)"
psa90.v2.number = "(90:2)"
psa90.v2.text = "Before the mountains were brought forth, or ever thou hadst formed the earth and the world, from everlasting to everlasting thou art God."
psa90.v3.number_rsv = "(90:3)"
psa90.v3.number = "(90:3)"
psa90.v3.text = "Thou turnest man back to the dust, and sayest, \"Turn back, O children of men!\""
psa90.v4.number_rsv = "(90:4)"
psa90.v4.number = "(90:4)"
psa90.v4.text = "For a thousand years in thy sightare but as yesterday when it is past, or as a watch in the night."
psa90.v5.number_rsv = "(90:5)"
psa90.v5.number = "(90:5)"
psa90.v5.text = "Thou dost sweep men away; they are like a dream, like grass which is renewed in the morning:"
psa90.v6.number_rsv = "(90:6)"
psa90.v6.number = "(90:6)"
psa90.v6.text = "in the morning it flourishes and is renewed; in the evening it fades and withers."
psa90.v7.number_rsv = "(90:7)"
psa90.v7.number = "(90:7)"
psa90.v7.text = "For we are consumed by thy anger; by thy wrath we are overwhelmed."
psa90.v8.number_rsv = "(90:8)"
psa90.v8.number = "(90:8)"
psa90.v8.text = "Thou hast set our iniquities before thee, our secret sins in the light of thy countenance."
psa90.v9.number_rsv = "(90:9)"
psa90.v9.number = "(90:9)"
psa90.v9.text = "For all our days pass away under thy wrath, our years come to an end like a sigh."
psa90.v10.number_rsv = "(90:10)"
psa90.v10.number = "(90:10)"
psa90.v10.text = "The years of our life are threescore and ten, or even by reason of strength fourscore; yet their span is but toil and trouble; they are soon gone, and we fly away."
psa90.v11.number_rsv = "(90:11)"
psa90.v11.number = "(90:11)"
psa90.v11.text = "Who considers the power of thy anger, and thy wrath according to the fear of thee?"
psa90.v12.number_rsv = "(90:12)"
psa90.v12.number = "(90:12)"
psa90.v12.text = "So teach us to number our daysthat we may get a heart of wisdom."
psa90.v13.number_rsv = "(90:13)"
psa90.v13.number = "(90:13)"
psa90.v13.text = "Return, O LORD! How long? Have pity on thy servants!"
psa90.v14.number_rsv = "(90:14)"
psa90.v14.number = "(90:14)"
psa90.v14.text = "Satisfy us in the morning with thy steadfast love, that we may rejoice and be glad all our days."
psa90.v15.number_rsv = "(90:15)"
psa90.v15.number = "(90:15)"
psa90.v15.text = "Make us glad as many days as thou hast afflicted us, and as many years as we have seen evil."
psa90.v16.number_rsv = "(90:16)"
psa90.v16.number = "(90:16)"
psa90.v16.text = "Let thy work be manifest to thy servants, and thy glorious power to their children."
psa90.v17.number_rsv = "(90:17)"
psa90.v17.number = "(90:17)"
psa90.v17.text = "Let the favor of the Lord our God be upon us, and establish thou the work of our hands upon us, yea, the work of our hands establish thou it.R"

psa91.title = "Psalm 91"
psa91.version = properties_en_US_rsv.version.designation
psa91.text = ""
psa91.v1.number_rsv = "(91:1)"
psa91.v1.number = "(91:1)"
psa91.v1.text = "He who dwells in the shelter of the Most High, who abides in the shadow of the Almighty,"
psa91.v2.number_rsv = "(91:2)"
psa91.v2.number = "(91:2)"
psa91.v2.text = "will say to the LORD, \"My refuge and my fortress; my God, in whom I trust.\""
psa91.v3.number_rsv = "(91:3)"
psa91.v3.number = "(91:3)"
psa91.v3.text = "For he will deliver you from the snare of the fowlerand from the deadly pestilence;"
psa91.v4.number_rsv = "(91:4)"
psa91.v4.number = "(91:4)"
psa91.v4.text = "he will cover you with his pinions, and under his wings you will find refuge; his faithfulness is a shield and buckler."
psa91.v5.number_rsv = "(91:5)"
psa91.v5.number = "(91:5)"
psa91.v5.text = "You will not fear the terror of the night, nor the arrow that flies by day,"
psa91.v6.number_rsv = "(91:6)"
psa91.v6.number = "(91:6)"
psa91.v6.text = "nor the pestilence that stalks in darkness, nor the destruction that wastes at noonday."
psa91.v7.number_rsv = "(91:7)"
psa91.v7.number = "(91:7)"
psa91.v7.text = "A thousand may fall at your side, ten thousand at your right hand; but it will not come near you."
psa91.v8.number_rsv = "(91:8)"
psa91.v8.number = "(91:8)"
psa91.v8.text = "You will only look with your eyesand see the recompense of the wicked."
psa91.v9.number_rsv = "(91:9)"
psa91.v9.number = "(91:9)"
psa91.v9.text = "Because you have made the LORD your refuge, the Most High your habitation,"
psa91.v10.number_rsv = "(91:10)"
psa91.v10.number = "(91:10)"
psa91.v10.text = "no evil shall befall you, no scourge come near your tent."
psa91.v11.number_rsv = "(91:11)"
psa91.v11.number = "(91:11)"
psa91.v11.text = "For he will give his angels charge of youto guard you in all your ways."
psa91.v12.number_rsv = "(91:12)"
psa91.v12.number = "(91:12)"
psa91.v12.text = "On their hands they will bear you up, lest you dash your foot against a stone."
psa91.v13.number_rsv = "(91:13)"
psa91.v13.number = "(91:13)"
psa91.v13.text = "You will tread on the lion and the adder, the young lion and the serpent you will trample under foot."
psa91.v14.number_rsv = "(91:14)"
psa91.v14.number = "(91:14)"
psa91.v14.text = "Because he cleaves to me in love, I will deliver him; I will protect him, because he knows my name."
psa91.v15.number_rsv = "(91:15)"
psa91.v15.number = "(91:15)"
psa91.v15.text = "When he calls to me, I will answer him; I will be with him in trouble, I will rescue him and honor him."
psa91.v16.number_rsv = "(91:16)"
psa91.v16.number = "(91:16)"
psa91.v16.text = "With long life I will satisfy him, and show him my salvation."

kathisma_13.title = "KATHISMA 13."

psa92.title = "Psalm 92"
psa92.version = properties_en_US_rsv.version.designation
psa92.text = ""
psa92.v0.number_rsv = "(92:0)"
psa92.v0.number = "(92:0)"
psa92.v0.text = "A Psalm. A Song for the Sabbath."
psa92.v1.number_rsv = "(92:1)"
psa92.v1.number = "(92:1)"
psa92.v1.text = "It is good to give thanks to the LORD, to sing praises to thy name, O Most High;"
psa92.v2.number_rsv = "(92:2)"
psa92.v2.number = "(92:2)"
psa92.v2.text = "to declare thy steadfast love in the morning, and thy faithfulness by night,"
psa92.v3.number_rsv = "(92:3)"
psa92.v3.number = "(92:3)"
psa92.v3.text = "to the music of the lute and the harp, to the melody of the lyre."
psa92.v4.number_rsv = "(92:4)"
psa92.v4.number = "(92:4)"
psa92.v4.text = "For thou, O LORD, hast made me glad by thy work; at the works of thy hands I sing for joy."
psa92.v5.number_rsv = "(92:5)"
psa92.v5.number = "(92:5)"
psa92.v5.text = "How great are thy works, O LORD! Thy thoughts are very deep!"
psa92.v6.number_rsv = "(92:6)"
psa92.v6.number = "(92:6)"
psa92.v6.text = "The dull man cannot know, the stupid cannot understand this:"
psa92.v7.number_rsv = "(92:7)"
psa92.v7.number = "(92:7)"
psa92.v7.text = "that, though the wicked sprout like grassand all evildoers flourish, they are doomed to destruction for ever,"
psa92.v8.number_rsv = "(92:8)"
psa92.v8.number = "(92:8)"
psa92.v8.text = "but thou, O LORD, art on high for ever."
psa92.v9.number_rsv = "(92:9)"
psa92.v9.number = "(92:9)"
psa92.v9.text = "For, lo, thy enemies, O LORD, for, lo, thy enemies shall perish; all evildoers shall be scattered."
psa92.v10.number_rsv = "(92:10)"
psa92.v10.number = "(92:10)"
psa92.v10.text = "But thou hast exalted my horn like that of the wild ox; thou hast poured over me fresh oil."
psa92.v11.number_rsv = "(92:11)"
psa92.v11.number = "(92:11)"
psa92.v11.text = "My eyes have seen the downfall of my enemies, my ears have heard the doom of my evil assailants."
psa92.v12.number_rsv = "(92:12)"
psa92.v12.number = "(92:12)"
psa92.v12.text = "The righteous flourish like the palm tree, and grow like a cedar in Lebanon."
psa92.v13.number_rsv = "(92:13)"
psa92.v13.number = "(92:13)"
psa92.v13.text = "They are planted in the house of the LORD, they flourish in the courts of our God."
psa92.v14.number_rsv = "(92:14)"
psa92.v14.number = "(92:14)"
psa92.v14.text = "They still bring forth fruit in old age, they are ever full of sap and green,"
psa92.v15.number_rsv = "(92:15)"
psa92.v15.number = "(92:15)"
psa92.v15.text = "to show that the LORD is upright; he is my rock, and there is no unrighteousness in him."

psa93.title = "Psalm 93"
psa93.version = properties_en_US_rsv.version.designation
psa93.text = ""
psa93.v1.number_rsv = "(93:1)"
psa93.v1.number = "(93:1)"
psa93.v1.text = "The LORD reigns; he is robed in majesty; the LORD is robed, he is girded with strength.Yea, the world is established; it shall never be moved;"
psa93.v2.number_rsv = "(93:2)"
psa93.v2.number = "(93:2)"
psa93.v2.text = "thy throne is established from of old; thou art from everlasting."
psa93.v3.number_rsv = "(93:3)"
psa93.v3.number = "(93:3)"
psa93.v3.text = "The floods have lifted up, O LORD, the floods have lifted up their voice, the floods lift up their roaring."
psa93.v4.number_rsv = "(93:4)"
psa93.v4.number = "(93:4)"
psa93.v4.text = "Mightier than the thunders of many waters, mightier than the waves of the sea, the LORD on high is mighty!"
psa93.v5.number_rsv = "(93:5)"
psa93.v5.number = "(93:5)"
psa93.v5.text = "Thy decrees are very sure; holiness befits thy house, O LORD, for evermore."

psa94.title = "Psalm 94"
psa94.version = properties_en_US_rsv.version.designation
psa94.text = ""
psa94.v1.number_rsv = "(94:1)"
psa94.v1.number = "(94:1)"
psa94.v1.text = "O LORD, thou God of vengeance, thou God of vengeance, shine forth!"
psa94.v2.number_rsv = "(94:2)"
psa94.v2.number = "(94:2)"
psa94.v2.text = "Rise up, O judge of the earth; render to the proud their deserts!"
psa94.v3.number_rsv = "(94:3)"
psa94.v3.number = "(94:3)"
psa94.v3.text = "O LORD, how long shall the wicked, how long shall the wicked exult?"
psa94.v4.number_rsv = "(94:4)"
psa94.v4.number = "(94:4)"
psa94.v4.text = "They pour out their arrogant words, they boast, all the evildoers."
psa94.v5.number_rsv = "(94:5)"
psa94.v5.number = "(94:5)"
psa94.v5.text = "They crush thy people, O LORD, and afflict thy heritage."
psa94.v6.number_rsv = "(94:6)"
psa94.v6.number = "(94:6)"
psa94.v6.text = "They slay the widow and the sojourner, and murder the fatherless;"
psa94.v7.number_rsv = "(94:7)"
psa94.v7.number = "(94:7)"
psa94.v7.text = "and they say, \"The LORD does not see; the God of Jacob does not perceive.\""
psa94.v8.number_rsv = "(94:8)"
psa94.v8.number = "(94:8)"
psa94.v8.text = "Understand, O dullest of the people! Fools, when will you be wise?"
psa94.v9.number_rsv = "(94:9)"
psa94.v9.number = "(94:9)"
psa94.v9.text = "He who planted the ear, does he not hear? He who formed the eye, does he not see?"
psa94.v10.number_rsv = "(94:10)"
psa94.v10.number = "(94:10)"
psa94.v10.text = "He who chastens the nations, does he not chastise? He who teaches men knowledge,"
psa94.v11.number_rsv = "(94:11)"
psa94.v11.number = "(94:11)"
psa94.v11.text = "the LORD, knows the thoughts of man, that they are but a breath."
psa94.v12.number_rsv = "(94:12)"
psa94.v12.number = "(94:12)"
psa94.v12.text = "Blessed is the man whom thou dost chasten, O LORD, and whom thou dost teach out of thy law"
psa94.v13.number_rsv = "(94:13)"
psa94.v13.number = "(94:13)"
psa94.v13.text = "to give him respite from days of trouble, until a pit is dug for the wicked."
psa94.v14.number_rsv = "(94:14)"
psa94.v14.number = "(94:14)"
psa94.v14.text = "For the LORD will not forsake his people; he will not abandon his heritage;"
psa94.v15.number_rsv = "(94:15)"
psa94.v15.number = "(94:15)"
psa94.v15.text = "for justice will return to the righteous, and all the upright in heart will follow it."
psa94.v16.number_rsv = "(94:16)"
psa94.v16.number = "(94:16)"
psa94.v16.text = "Who rises up for me against the wicked? Who stands up for me against evildoers?"
psa94.v17.number_rsv = "(94:17)"
psa94.v17.number = "(94:17)"
psa94.v17.text = "If the LORD had not been my help, my soul would soon have dwelt in the land of silence."
psa94.v18.number_rsv = "(94:18)"
psa94.v18.number = "(94:18)"
psa94.v18.text = "When I thought, \"My foot slips,\"thy steadfast love, O LORD, held me up."
psa94.v19.number_rsv = "(94:19)"
psa94.v19.number = "(94:19)"
psa94.v19.text = "When the cares of my heart are many, thy consolations cheer my soul."
psa94.v20.number_rsv = "(94:20)"
psa94.v20.number = "(94:20)"
psa94.v20.text = "Can wicked rulers be allied with thee, who frame mischief by statute?"
psa94.v21.number_rsv = "(94:21)"
psa94.v21.number = "(94:21)"
psa94.v21.text = "They band together against the life of the righteous, and condemn the innocent to death."
psa94.v22.number_rsv = "(94:22)"
psa94.v22.number = "(94:22)"
psa94.v22.text = "But the LORD has become my stronghold, and my God the rock of my refuge."
psa94.v23.number_rsv = "(94:23)"
psa94.v23.number = "(94:23)"
psa94.v23.text = "He will bring back on them their iniquityand wipe them out for their wickedness; the LORD our God will wipe them out."

psa95.title = "Psalm 95"
psa95.version = properties_en_US_rsv.version.designation
psa95.text = ""
psa95.v1.number_rsv = "(95:1)"
psa95.v1.number = "(95:1)"
psa95.v1.text = "O come, let us sing to the LORD; let us make a joyful noise to the rock of our salvation!"
psa95.v2.number_rsv = "(95:2)"
psa95.v2.number = "(95:2)"
psa95.v2.text = "Let us come into his presence with thanksgiving; let us make a joyful noise to him with songs of praise!"
psa95.v3.number_rsv = "(95:3)"
psa95.v3.number = "(95:3)"
psa95.v3.text = "For the LORD is a great God, and a great King above all gods."
psa95.v4.number_rsv = "(95:4)"
psa95.v4.number = "(95:4)"
psa95.v4.text = "In his hand are the depths of the earth; the heights of the mountains are his also."
psa95.v5.number_rsv = "(95:5)"
psa95.v5.number = "(95:5)"
psa95.v5.text = "The sea is his, for he made it; for his hands formed the dry land."
psa95.v6.number_rsv = "(95:6)"
psa95.v6.number = "(95:6)"
psa95.v6.text = "O come, let us worship and bow down, let us kneel before the LORD, our Maker!"
psa95.v7.number_rsv = "(95:7)"
psa95.v7.number = "(95:7)"
psa95.v7.text = "For he is our God, and we are the people of his pasture, and the sheep of his hand.O that today you would hearken to his voice!"
psa95.v8.number_rsv = "(95:8)"
psa95.v8.number = "(95:8)"
psa95.v8.text = "Harden not your hearts, as at Mer\"ibah, as on the day at Massah in the wilderness,"
psa95.v9.number_rsv = "(95:9)"
psa95.v9.number = "(95:9)"
psa95.v9.text = "when your fathers tested me, and put me to the proof, though they had seen my work."
psa95.v10.number_rsv = "(95:10)"
psa95.v10.number = "(95:10)"
psa95.v10.text = "For forty years I loathed that generationand said, \"They are a people who err in heart, and they do not regard my ways.\""
psa95.v11.number_rsv = "(95:11)"
psa95.v11.number = "(95:11)"
psa95.v11.text = "Therefore I swore in my angerthat they should not enter my rest."

psa96.title = "Psalm 96"
psa96.version = properties_en_US_rsv.version.designation
psa96.text = ""
psa96.v1.number_rsv = "(96:1)"
psa96.v1.number = "(96:1)"
psa96.v1.text = "O sing to the LORD a new song; sing to the LORD, all the earth!"
psa96.v2.number_rsv = "(96:2)"
psa96.v2.number = "(96:2)"
psa96.v2.text = "Sing to the LORD, bless his name; tell of his salvation from day to day."
psa96.v3.number_rsv = "(96:3)"
psa96.v3.number = "(96:3)"
psa96.v3.text = "Declare his glory among the nations, his marvelous works among all the peoples!"
psa96.v4.number_rsv = "(96:4)"
psa96.v4.number = "(96:4)"
psa96.v4.text = "For great is the LORD, and greatly to be praised; he is to be feared above all gods."
psa96.v5.number_rsv = "(96:5)"
psa96.v5.number = "(96:5)"
psa96.v5.text = "For all the gods of the peoples are idols; but the LORD made the heavens."
psa96.v6.number_rsv = "(96:6)"
psa96.v6.number = "(96:6)"
psa96.v6.text = "Honor and majesty are before him; strength and beauty are in his sanctuary."
psa96.v7.number_rsv = "(96:7)"
psa96.v7.number = "(96:7)"
psa96.v7.text = "Ascribe to the LORD, O families of the peoples, ascribe to the LORD glory and strength!"
psa96.v8.number_rsv = "(96:8)"
psa96.v8.number = "(96:8)"
psa96.v8.text = "Ascribe to the LORD the glory due his name; bring an offering, and come into his courts!"
psa96.v9.number_rsv = "(96:9)"
psa96.v9.number = "(96:9)"
psa96.v9.text = "Worship the LORD in holy array; tremble before him, all the earth!"
psa96.v10.number_rsv = "(96:10)"
psa96.v10.number = "(96:10)"
psa96.v10.text = "Say among the nations, \"The LORD reigns! Yea, the world is established, it shall never be moved; he will judge the peoples with equity.\""
psa96.v11.number_rsv = "(96:11)"
psa96.v11.number = "(96:11)"
psa96.v11.text = "Let the heavens be glad, and let the earth rejoice; let the sea roar, and all that fills it;"
psa96.v12.number_rsv = "(96:12)"
psa96.v12.number = "(96:12)"
psa96.v12.text = "let the field exult, and everything in it! Then shall all the trees of the wood sing for joy"
psa96.v13.number_rsv = "(96:13)"
psa96.v13.number = "(96:13)"
psa96.v13.text = "before the LORD, for he comes, for he comes to judge the earth.He will judge the world with righteousness, and the peoples with his truth."

psa97.title = "Psalm 97"
psa97.version = properties_en_US_rsv.version.designation
psa97.text = ""
psa97.v1.number_rsv = "(97:1)"
psa97.v1.number = "(97:1)"
psa97.v1.text = "The LORD reigns; let the earth rejoice; let the many coastlands be glad!"
psa97.v2.number_rsv = "(97:2)"
psa97.v2.number = "(97:2)"
psa97.v2.text = "Clouds and thick darkness are round about him; righteousness and justice are the foundation of his throne."
psa97.v3.number_rsv = "(97:3)"
psa97.v3.number = "(97:3)"
psa97.v3.text = "Fire goes before him, and burns up his adversaries round about."
psa97.v4.number_rsv = "(97:4)"
psa97.v4.number = "(97:4)"
psa97.v4.text = "His lightnings lighten the world; the earth sees and trembles."
psa97.v5.number_rsv = "(97:5)"
psa97.v5.number = "(97:5)"
psa97.v5.text = "The mountains melt like wax before the LORD, before the Lord of all the earth."
psa97.v6.number_rsv = "(97:6)"
psa97.v6.number = "(97:6)"
psa97.v6.text = "The heavens proclaim his righteousness; and all the peoples behold his glory."
psa97.v7.number_rsv = "(97:7)"
psa97.v7.number = "(97:7)"
psa97.v7.text = "All worshipers of images are put to shame, who make their boast in worthless idols; all gods bow down before him."
psa97.v8.number_rsv = "(97:8)"
psa97.v8.number = "(97:8)"
psa97.v8.text = "Zion hears and is glad, and the daughters of Judah rejoice, because of thy judgments, O God."
psa97.v9.number_rsv = "(97:9)"
psa97.v9.number = "(97:9)"
psa97.v9.text = "For thou, O LORD, art most high over all the earth; thou art exalted far above all gods."
psa97.v10.number_rsv = "(97:10)"
psa97.v10.number = "(97:10)"
psa97.v10.text = "The LORD loves those who hate evil; he preserves the lives of his saints; he delivers them from the hand of the wicked."
psa97.v11.number_rsv = "(97:11)"
psa97.v11.number = "(97:11)"
psa97.v11.text = "Light dawns for the righteous, and joy for the upright in heart."
psa97.v12.number_rsv = "(97:12)"
psa97.v12.number = "(97:12)"
psa97.v12.text = "Rejoice in the LORD, O you righteous, and give thanks to his holy name!"

psa98.title = "Psalm 98"
psa98.version = properties_en_US_rsv.version.designation
psa98.text = ""
psa98.v0.number_rsv = "(98:0)"
psa98.v0.number = "(98:0)"
psa98.v0.text = "A Psalm."
psa98.v1.number_rsv = "(98:1)"
psa98.v1.number = "(98:1)"
psa98.v1.text = "O sing to the LORD a new song, for he has done marvelous things! His right hand and his holy armhave gotten him victory."
psa98.v2.number_rsv = "(98:2)"
psa98.v2.number = "(98:2)"
psa98.v2.text = "The LORD has made known his victory, he has revealed his vindication in the sight of the nations."
psa98.v3.number_rsv = "(98:3)"
psa98.v3.number = "(98:3)"
psa98.v3.text = "He has remembered his steadfast love and faithfulnessto the house of Israel.All the ends of the earth have seenthe victory of our God."
psa98.v4.number_rsv = "(98:4)"
psa98.v4.number = "(98:4)"
psa98.v4.text = "Make a joyful noise to the LORD, all the earth; break forth into joyous song and sing praises!"
psa98.v5.number_rsv = "(98:5)"
psa98.v5.number = "(98:5)"
psa98.v5.text = "Sing praises to the LORD with the lyre, with the lyre and the sound of melody!"
psa98.v6.number_rsv = "(98:6)"
psa98.v6.number = "(98:6)"
psa98.v6.text = "With trumpets and the sound of the hornmake a joyful noise before the King, the LORD!"
psa98.v7.number_rsv = "(98:7)"
psa98.v7.number = "(98:7)"
psa98.v7.text = "Let the sea roar, and all that fills it; the world and those who dwell in it!"
psa98.v8.number_rsv = "(98:8)"
psa98.v8.number = "(98:8)"
psa98.v8.text = "Let the floods clap their hands; let the hills sing for joy together"
psa98.v9.number_rsv = "(98:9)"
psa98.v9.number = "(98:9)"
psa98.v9.text = "before the LORD, for he comesto judge the earth.He will judge the world with righteousness, and the peoples with equity."

psa99.title = "Psalm 99"
psa99.version = properties_en_US_rsv.version.designation
psa99.text = ""
psa99.v1.number_rsv = "(99:1)"
psa99.v1.number = "(99:1)"
psa99.v1.text = "The LORD reigns; let the peoples tremble! He sits enthroned upon the cherubim; let the earth quake!"
psa99.v2.number_rsv = "(99:2)"
psa99.v2.number = "(99:2)"
psa99.v2.text = "The LORD is great in Zion; he is exalted over all the peoples."
psa99.v3.number_rsv = "(99:3)"
psa99.v3.number = "(99:3)"
psa99.v3.text = "Let them praise thy great and terrible name! Holy is he!"
psa99.v4.number_rsv = "(99:4)"
psa99.v4.number = "(99:4)"
psa99.v4.text = "Mighty King, lover of justice, thou hast established equity; thou hast executed justiceand righteousness in Jacob."
psa99.v5.number_rsv = "(99:5)"
psa99.v5.number = "(99:5)"
psa99.v5.text = "Extol the LORD our God; worship at his footstool! Holy is he!"
psa99.v6.number_rsv = "(99:6)"
psa99.v6.number = "(99:6)"
psa99.v6.text = "Moses and Aaron were among his priests, Samuel also was among those who called on his name.They cried to the LORD, and he answered them."
psa99.v7.number_rsv = "(99:7)"
psa99.v7.number = "(99:7)"
psa99.v7.text = "He spoke to them in the pillar of cloud; they kept his testimonies, and the statutes that he gave them."
psa99.v8.number_rsv = "(99:8)"
psa99.v8.number = "(99:8)"
psa99.v8.text = "O LORD our God, thou didst answer them; thou wast a forgiving God to them, but an avenger of their wrongdoings."
psa99.v9.number_rsv = "(99:9)"
psa99.v9.number = "(99:9)"
psa99.v9.text = "Extol the LORD our God, and worship at his holy mountain; for the LORD our God is holy!"

psa100.title = "Psalm 100"
psa100.version = properties_en_US_rsv.version.designation
psa100.text = ""
psa100.v0.number_rsv = "(100:0)"
psa100.v0.number = "(100:0)"
psa100.v0.text = "A Psalm for the thank offering."
psa100.v1.number_rsv = "(100:1)"
psa100.v1.number = "(100:1)"
psa100.v1.text = "Make a joyful noise to the LORD, all the lands!"
psa100.v2.number_rsv = "(100:2)"
psa100.v2.number = "(100:2)"
psa100.v2.text = "Serve the LORD with gladness! Come into his presence with singing!"
psa100.v3.number_rsv = "(100:3)"
psa100.v3.number = "(100:3)"
psa100.v3.text = "Know that the LORD is God! It is he that made us, and we are his; we are his people, and the sheep of his pasture."
psa100.v4.number_rsv = "(100:4)"
psa100.v4.number = "(100:4)"
psa100.v4.text = "Enter his gates with thanksgiving, and his courts with praise! Give thanks to him, bless his name!"
psa100.v5.number_rsv = "(100:5)"
psa100.v5.number = "(100:5)"
psa100.v5.text = "For the LORD is good; his steadfast love endures for ever, and his faithfulness to all generations."

psa101.title = "Psalm 101"
psa101.version = properties_en_US_rsv.version.designation
psa101.text = ""
psa101.v0.number_rsv = "(101:0)"
psa101.v0.number = "(101:0)"
psa101.v0.text = "A Psalm of David."
psa101.v1.number_rsv = "(101:1)"
psa101.v1.number = "(101:1)"
psa101.v1.text = "I will sing of loyalty and of justice; to thee, O LORD, I will sing."
psa101.v2.number_rsv = "(101:2)"
psa101.v2.number = "(101:2)"
psa101.v2.text = "I will give heed to the way that is blameless.Oh when wilt thou come to me? I will walk with integrity of heartwithin my house;"
psa101.v3.number_rsv = "(101:3)"
psa101.v3.number = "(101:3)"
psa101.v3.text = "I will not set before my eyesanything that is base.I hate the work of those who fall away; it shall not cleave to me."
psa101.v4.number_rsv = "(101:4)"
psa101.v4.number = "(101:4)"
psa101.v4.text = "Perverseness of heart shall be far from me; I will know nothing of evil."
psa101.v5.number_rsv = "(101:5)"
psa101.v5.number = "(101:5)"
psa101.v5.text = "Him who slanders his neighbor secretlyI will destroy.The man of haughty looks and arrogant heartI will not endure."
psa101.v6.number_rsv = "(101:6)"
psa101.v6.number = "(101:6)"
psa101.v6.text = "I will look with favor on the faithful in the land, that they may dwell with me; he who walks in the way that is blamelessshall minister to me."
psa101.v7.number_rsv = "(101:7)"
psa101.v7.number = "(101:7)"
psa101.v7.text = "No man who practices deceitshall dwell in my house; no man who utters liesshall continue in my presence."
psa101.v8.number_rsv = "(101:8)"
psa101.v8.number = "(101:8)"
psa101.v8.text = "Morning by morning I will destroyall the wicked in the land, cutting off all the evildoersfrom the city of the LORD."

kathisma_14.title = "KATHISMA 14."

psa102.title = "Psalm 102"
psa102.version = properties_en_US_rsv.version.designation
psa102.text = ""
psa102.v0.number_rsv = "(102:0)"
psa102.v0.number = "(102:0)"
psa102.v0.text = "A prayer of one afflicted, when he is faint and pours out his complaintbefore the LORD."
psa102.v1.number_rsv = "(102:1)"
psa102.v1.number = "(102:1)"
psa102.v1.text = "Hear my prayer, O LORD; let my cry come to thee!"
psa102.v2.number_rsv = "(102:2)"
psa102.v2.number = "(102:2)"
psa102.v2.text = "Do not hide thy face from mein the day of my distress! Incline thy ear to me; answer me speedily in the day when I call!"
psa102.v3.number_rsv = "(102:3)"
psa102.v3.number = "(102:3)"
psa102.v3.text = "For my days pass away like smoke, and my bones burn like a furnace."
psa102.v4.number_rsv = "(102:4)"
psa102.v4.number = "(102:4)"
psa102.v4.text = "My heart is smitten like grass, and withered; I forget to eat my bread."
psa102.v5.number_rsv = "(102:5)"
psa102.v5.number = "(102:5)"
psa102.v5.text = "Because of my loud groaningmy bones cleave to my flesh."
psa102.v6.number_rsv = "(102:6)"
psa102.v6.number = "(102:6)"
psa102.v6.text = "I am like a vulture of the wilderness, like an owl of the waste places;"
psa102.v7.number_rsv = "(102:7)"
psa102.v7.number = "(102:7)"
psa102.v7.text = "I lie awake, I am like a lonely bird on the housetop."
psa102.v8.number_rsv = "(102:8)"
psa102.v8.number = "(102:8)"
psa102.v8.text = "All the day my enemies taunt me, those who deride me use my name for a curse."
psa102.v9.number_rsv = "(102:9)"
psa102.v9.number = "(102:9)"
psa102.v9.text = "For I eat ashes like bread, and mingle tears with my drink,"
psa102.v10.number_rsv = "(102:10)"
psa102.v10.number = "(102:10)"
psa102.v10.text = "because of thy indignation and anger; for thou hast taken me up and thrown me away."
psa102.v11.number_rsv = "(102:11)"
psa102.v11.number = "(102:11)"
psa102.v11.text = "My days are like an evening shadow; I wither away like grass."
psa102.v12.number_rsv = "(102:12)"
psa102.v12.number = "(102:12)"
psa102.v12.text = "But thou, O LORD, art enthroned for ever; thy name endures to all generations."
psa102.v13.number_rsv = "(102:13)"
psa102.v13.number = "(102:13)"
psa102.v13.text = "Thou wilt arise and have pity on Zion; it is the time to favor her; the appointed time has come."
psa102.v14.number_rsv = "(102:14)"
psa102.v14.number = "(102:14)"
psa102.v14.text = "For thy servants hold her stones dear, and have pity on her dust."
psa102.v15.number_rsv = "(102:15)"
psa102.v15.number = "(102:15)"
psa102.v15.text = "The nations will fear the name of the LORD, and all the kings of the earth thy glory."
psa102.v16.number_rsv = "(102:16)"
psa102.v16.number = "(102:16)"
psa102.v16.text = "For the LORD will build up Zion, he will appear in his glory;"
psa102.v17.number_rsv = "(102:17)"
psa102.v17.number = "(102:17)"
psa102.v17.text = "he will regard the prayer of the destitute, and will not despise their supplication."
psa102.v18.number_rsv = "(102:18)"
psa102.v18.number = "(102:18)"
psa102.v18.text = "Let this be recorded for a generation to come, so that a people yet unborn may praise the LORD:"
psa102.v19.number_rsv = "(102:19)"
psa102.v19.number = "(102:19)"
psa102.v19.text = "that he looked down from his holy height, from heaven the LORD looked at the earth,"
psa102.v20.number_rsv = "(102:20)"
psa102.v20.number = "(102:20)"
psa102.v20.text = "to hear the groans of the prisoners, to set free those who were doomed to die;"
psa102.v21.number_rsv = "(102:21)"
psa102.v21.number = "(102:21)"
psa102.v21.text = "that men may declare in Zion the name of the LORD, and in Jerusalem his praise,"
psa102.v22.number_rsv = "(102:22)"
psa102.v22.number = "(102:22)"
psa102.v22.text = "when peoples gather together, and kingdoms, to worship the LORD."
psa102.v23.number_rsv = "(102:23)"
psa102.v23.number = "(102:23)"
psa102.v23.text = "He has broken my strength in mid-course; he has shortened my days."
psa102.v24.number_rsv = "(102:24)"
psa102.v24.number = "(102:24)"
psa102.v24.text = "\"O my God,\" I say, \"take me not hencein the midst of my days, thou whose years endurethroughout all generations!\""
psa102.v25.number_rsv = "(102:25)"
psa102.v25.number = "(102:25)"
psa102.v25.text = "Of old thou didst lay the foundation of the earth, and the heavens are the work of thy hands."
psa102.v26.number_rsv = "(102:26)"
psa102.v26.number = "(102:26)"
psa102.v26.text = "They will perish, but thou dost endure; they will all wear out like a garment.Thou changest them like raiment, and they pass away;"
psa102.v27.number_rsv = "(102:27)"
psa102.v27.number = "(102:27)"
psa102.v27.text = "but thou art the same, and thy years have no end."
psa102.v28.number_rsv = "(102:28)"
psa102.v28.number = "(102:28)"
psa102.v28.text = "The children of thy servants shall dwell secure; their posterity shall be established before thee."

psa103.title = "Psalm 103"
psa103.version = properties_en_US_rsv.version.designation
psa103.text = ""
psa103.v0.number_rsv = "(103:0)"
psa103.v0.number = "(103:0)"
psa103.v0.text = "A Psalm of David."
psa103.v1.number_rsv = "(103:1)"
psa103.v1.number = "(103:1)"
psa103.v1.text = "Bless the LORD, O my soul; and all that is within me, bless his holy name!"
psa103.v2.number_rsv = "(103:2)"
psa103.v2.number = "(103:2)"
psa103.v2.text = "Bless the LORD, O my soul, and forget not all his benefits,"
psa103.v3.number_rsv = "(103:3)"
psa103.v3.number = "(103:3)"
psa103.v3.text = "who forgives all your iniquity, who heals all your diseases,"
psa103.v4.number_rsv = "(103:4)"
psa103.v4.number = "(103:4)"
psa103.v4.text = "who redeems your life from the Pit, who crowns you with steadfast love and mercy,"
psa103.v5.number_rsv = "(103:5)"
psa103.v5.number = "(103:5)"
psa103.v5.text = "who satisfies you with good as long as you liveso that your youth is renewed like the eagle\"s."
psa103.v6.number_rsv = "(103:6)"
psa103.v6.number = "(103:6)"
psa103.v6.text = "The LORD works vindicationand justice for all who are oppressed."
psa103.v7.number_rsv = "(103:7)"
psa103.v7.number = "(103:7)"
psa103.v7.text = "He made known his ways to Moses, his acts to the people of Israel."
psa103.v8.number_rsv = "(103:8)"
psa103.v8.number = "(103:8)"
psa103.v8.text = "The LORD is merciful and gracious, slow to anger and abounding in steadfast love."
psa103.v9.number_rsv = "(103:9)"
psa103.v9.number = "(103:9)"
psa103.v9.text = "He will not always chide, nor will he keep his anger for ever."
psa103.v10.number_rsv = "(103:10)"
psa103.v10.number = "(103:10)"
psa103.v10.text = "He does not deal with us according to our sins, nor requite us according to our iniquities."
psa103.v11.number_rsv = "(103:11)"
psa103.v11.number = "(103:11)"
psa103.v11.text = "For as the heavens are high above the earth, so great is his steadfast love toward those who fear him;"
psa103.v12.number_rsv = "(103:12)"
psa103.v12.number = "(103:12)"
psa103.v12.text = "as far as the east is from the west, so far does he remove our transgressions from us."
psa103.v13.number_rsv = "(103:13)"
psa103.v13.number = "(103:13)"
psa103.v13.text = "As a father pities his children, so the LORD pities those who fear him."
psa103.v14.number_rsv = "(103:14)"
psa103.v14.number = "(103:14)"
psa103.v14.text = "For he knows our frame; he remembers that we are dust."
psa103.v15.number_rsv = "(103:15)"
psa103.v15.number = "(103:15)"
psa103.v15.text = "As for man, his days are like grass; he flourishes like a flower of the field;"
psa103.v16.number_rsv = "(103:16)"
psa103.v16.number = "(103:16)"
psa103.v16.text = "for the wind passes over it, and it is gone, and its place knows it no more."
psa103.v17.number_rsv = "(103:17)"
psa103.v17.number = "(103:17)"
psa103.v17.text = "But the steadfast love of the LORD is from everlasting to everlastingupon those who fear him, and his righteousness to children\"s children,"
psa103.v18.number_rsv = "(103:18)"
psa103.v18.number = "(103:18)"
psa103.v18.text = "to those who keep his covenantand remember to do his commandments."
psa103.v19.number_rsv = "(103:19)"
psa103.v19.number = "(103:19)"
psa103.v19.text = "The LORD has established his throne in the heavens, and his kingdom rules over all."
psa103.v20.number_rsv = "(103:20)"
psa103.v20.number = "(103:20)"
psa103.v20.text = "Bless the LORD, O you his angels, you mighty ones who do his word, hearkening to the voice of his word!"
psa103.v21.number_rsv = "(103:21)"
psa103.v21.number = "(103:21)"
psa103.v21.text = "Bless the LORD, all his hosts, his ministers that do his will!"
psa103.v22.number_rsv = "(103:22)"
psa103.v22.number = "(103:22)"
psa103.v22.text = "Bless the LORD, all his works, in all places of his dominion.Bless the LORD, O my soul!"

psa104.title = "Psalm 104"
psa104.version = properties_en_US_rsv.version.designation
psa104.text = ""
psa104.v1.number_rsv = "(104:1)"
psa104.v1.number = "(104:1)"
psa104.v1.text = "Bless the LORD, O my soul! O LORD my God, thou art very great! Thou art clothed with honor and majesty,"
psa104.v2.number_rsv = "(104:2)"
psa104.v2.number = "(104:2)"
psa104.v2.text = "who coverest thyself with light as with a garment, who hast stretched out the heavens like a tent,"
psa104.v3.number_rsv = "(104:3)"
psa104.v3.number = "(104:3)"
psa104.v3.text = "who hast laid the beams of thy chambers on the waters, who makest the clouds thy chariot, who ridest on the wings of the wind,"
psa104.v4.number_rsv = "(104:4)"
psa104.v4.number = "(104:4)"
psa104.v4.text = "who makest the winds thy messengers, fire and flame thy ministers."
psa104.v5.number_rsv = "(104:5)"
psa104.v5.number = "(104:5)"
psa104.v5.text = "Thou didst set the earth on its foundations, so that it should never be shaken."
psa104.v6.number_rsv = "(104:6)"
psa104.v6.number = "(104:6)"
psa104.v6.text = "Thou didst cover it with the deep as with a garment; the waters stood above the mountains."
psa104.v7.number_rsv = "(104:7)"
psa104.v7.number = "(104:7)"
psa104.v7.text = "At thy rebuke they fled; at the sound of thy thunder they took to flight."
psa104.v8.number_rsv = "(104:8)"
psa104.v8.number = "(104:8)"
psa104.v8.text = "The mountains rose, the valleys sank downto the place which thou didst appoint for them."
psa104.v9.number_rsv = "(104:9)"
psa104.v9.number = "(104:9)"
psa104.v9.text = "Thou didst set a bound which they should not pass, so that they might not again cover the earth."
psa104.v10.number_rsv = "(104:10)"
psa104.v10.number = "(104:10)"
psa104.v10.text = "Thou makest springs gush forth in the valleys; they flow between the hills,"
psa104.v11.number_rsv = "(104:11)"
psa104.v11.number = "(104:11)"
psa104.v11.text = "they give drink to every beast of the field; the wild asses quench their thirst."
psa104.v12.number_rsv = "(104:12)"
psa104.v12.number = "(104:12)"
psa104.v12.text = "By them the birds of the air have their habitation; they sing among the branches."
psa104.v13.number_rsv = "(104:13)"
psa104.v13.number = "(104:13)"
psa104.v13.text = "From thy lofty abode thou waterest the mountains; the earth is satisfied with the fruit of thy work."
psa104.v14.number_rsv = "(104:14)"
psa104.v14.number = "(104:14)"
psa104.v14.text = "Thou dost cause the grass to grow for the cattle, and plants for man to cultivate, that he may bring forth food from the earth,"
psa104.v15.number_rsv = "(104:15)"
psa104.v15.number = "(104:15)"
psa104.v15.text = "and wine to gladden the heart of man, oil to make his face shine, and bread to strengthen man\"s heart."
psa104.v16.number_rsv = "(104:16)"
psa104.v16.number = "(104:16)"
psa104.v16.text = "The trees of the LORD are watered abundantly, the cedars of Lebanon which he planted."
psa104.v17.number_rsv = "(104:17)"
psa104.v17.number = "(104:17)"
psa104.v17.text = "In them the birds build their nests; the stork has her home in the fir trees."
psa104.v18.number_rsv = "(104:18)"
psa104.v18.number = "(104:18)"
psa104.v18.text = "The high mountains are for the wild goats; the rocks are a refuge for the badgers."
psa104.v19.number_rsv = "(104:19)"
psa104.v19.number = "(104:19)"
psa104.v19.text = "Thou hast made the moon to mark the seasons; the sun knows its time for setting."
psa104.v20.number_rsv = "(104:20)"
psa104.v20.number = "(104:20)"
psa104.v20.text = "Thou makest darkness, and it is night, when all the beasts of the forest creep forth."
psa104.v21.number_rsv = "(104:21)"
psa104.v21.number = "(104:21)"
psa104.v21.text = "The young lions roar for their prey, seeking their food from God."
psa104.v22.number_rsv = "(104:22)"
psa104.v22.number = "(104:22)"
psa104.v22.text = "When the sun rises, they get them awayand lie down in their dens."
psa104.v23.number_rsv = "(104:23)"
psa104.v23.number = "(104:23)"
psa104.v23.text = "Man goes forth to his workand to his labor until the evening."
psa104.v24.number_rsv = "(104:24)"
psa104.v24.number = "(104:24)"
psa104.v24.text = "O LORD, how manifold are thy works! In wisdom hast thou made them all; the earth is full of thy creatures."
psa104.v25.number_rsv = "(104:25)"
psa104.v25.number = "(104:25)"
psa104.v25.text = "Yonder is the sea, great and wide, which teems with things innumerable, living things both small and great."
psa104.v26.number_rsv = "(104:26)"
psa104.v26.number = "(104:26)"
psa104.v26.text = "There go the ships, and Leviathan which thou didst form to sport in it."
psa104.v27.number_rsv = "(104:27)"
psa104.v27.number = "(104:27)"
psa104.v27.text = "These all look to thee, to give them their food in due season."
psa104.v28.number_rsv = "(104:28)"
psa104.v28.number = "(104:28)"
psa104.v28.text = "When thou givest to them, they gather it up; when thou openest thy hand, they are filled with good things."
psa104.v29.number_rsv = "(104:29)"
psa104.v29.number = "(104:29)"
psa104.v29.text = "When thou hidest thy face, they are dismayed; when thou takest away their breath, they dieand return to their dust."
psa104.v30.number_rsv = "(104:30)"
psa104.v30.number = "(104:30)"
psa104.v30.text = "When thou sendest forth thy Spirit, they are created; and thou renewest the face of the ground."
psa104.v31.number_rsv = "(104:31)"
psa104.v31.number = "(104:31)"
psa104.v31.text = "May the glory of the LORD endure for ever, may the LORD rejoice in his works,"
psa104.v32.number_rsv = "(104:32)"
psa104.v32.number = "(104:32)"
psa104.v32.text = "who looks on the earth and it trembles, who touches the mountains and they smoke!"
psa104.v33.number_rsv = "(104:33)"
psa104.v33.number = "(104:33)"
psa104.v33.text = "I will sing to the LORD as long as I live; I will sing praise to my God while I have being."
psa104.v34.number_rsv = "(104:34)"
psa104.v34.number = "(104:34)"
psa104.v34.text = "May my meditation be pleasing to him, for I rejoice in the LORD."
psa104.v35.number_rsv = "(104:35)"
psa104.v35.number = "(104:35)"
psa104.v35.text = "Let sinners be consumed from the earth, and let the wicked be no more! Bless the LORD, O my soul! Praise the LORD!"

psa105.title = "Psalm 105"
psa105.version = properties_en_US_rsv.version.designation
psa105.text = ""
psa105.v1.number_rsv = "(105:1)"
psa105.v1.number = "(105:1)"
psa105.v1.text = "O give thanks to the LORD, call on his name, make known his deeds among the peoples!"
psa105.v2.number_rsv = "(105:2)"
psa105.v2.number = "(105:2)"
psa105.v2.text = "Sing to him, sing praises to him, tell of all his wonderful works!"
psa105.v3.number_rsv = "(105:3)"
psa105.v3.number = "(105:3)"
psa105.v3.text = "Glory in his holy name; let the hearts of those who seek the LORD rejoice!"
psa105.v4.number_rsv = "(105:4)"
psa105.v4.number = "(105:4)"
psa105.v4.text = "Seek the LORD and his strength, seek his presence continually!"
psa105.v5.number_rsv = "(105:5)"
psa105.v5.number = "(105:5)"
psa105.v5.text = "Remember the wonderful works that he has done, his miracles, and the judgments he uttered,"
psa105.v6.number_rsv = "(105:6)"
psa105.v6.number = "(105:6)"
psa105.v6.text = "O offspring of Abraham his servant, sons of Jacob, his chosen ones!"
psa105.v7.number_rsv = "(105:7)"
psa105.v7.number = "(105:7)"
psa105.v7.text = "He is the LORD our God; his judgments are in all the earth."
psa105.v8.number_rsv = "(105:8)"
psa105.v8.number = "(105:8)"
psa105.v8.text = "He is mindful of his covenant for ever, of the word that he commanded, for a thousand generations,"
psa105.v9.number_rsv = "(105:9)"
psa105.v9.number = "(105:9)"
psa105.v9.text = "the covenant which he made with Abraham, his sworn promise to Isaac,"
psa105.v10.number_rsv = "(105:10)"
psa105.v10.number = "(105:10)"
psa105.v10.text = "which he confirmed to Jacob as a statute, to Israel as an everlasting covenant,"
psa105.v11.number_rsv = "(105:11)"
psa105.v11.number = "(105:11)"
psa105.v11.text = "saying, \"To you I will give the land of Canaanas your portion for an inheritance.\""
psa105.v12.number_rsv = "(105:12)"
psa105.v12.number = "(105:12)"
psa105.v12.text = "When they were few in number, of little account, and sojourners in it,"
psa105.v13.number_rsv = "(105:13)"
psa105.v13.number = "(105:13)"
psa105.v13.text = "wandering from nation to nation, from one kingdom to another people,"
psa105.v14.number_rsv = "(105:14)"
psa105.v14.number = "(105:14)"
psa105.v14.text = "he allowed no one to oppress them; he rebuked kings on their account,"
psa105.v15.number_rsv = "(105:15)"
psa105.v15.number = "(105:15)"
psa105.v15.text = "saying, \"Touch not my anointed ones, do my prophets no harm!\""
psa105.v16.number_rsv = "(105:16)"
psa105.v16.number = "(105:16)"
psa105.v16.text = "When he summoned a famine on the land, and broke every staff of bread,"
psa105.v17.number_rsv = "(105:17)"
psa105.v17.number = "(105:17)"
psa105.v17.text = "he had sent a man ahead of them, Joseph, who was sold as a slave."
psa105.v18.number_rsv = "(105:18)"
psa105.v18.number = "(105:18)"
psa105.v18.text = "His feet were hurt with fetters, his neck was put in a collar of iron;"
psa105.v19.number_rsv = "(105:19)"
psa105.v19.number = "(105:19)"
psa105.v19.text = "until what he had said came to passthe word of the LORD tested him."
psa105.v20.number_rsv = "(105:20)"
psa105.v20.number = "(105:20)"
psa105.v20.text = "The king sent and released him, the ruler of the peoples set him free;"
psa105.v21.number_rsv = "(105:21)"
psa105.v21.number = "(105:21)"
psa105.v21.text = "he made him lord of his house, and ruler of all his possessions,"
psa105.v22.number_rsv = "(105:22)"
psa105.v22.number = "(105:22)"
psa105.v22.text = "to instruct his princes at his pleasure, and to teach his elders wisdom."
psa105.v23.number_rsv = "(105:23)"
psa105.v23.number = "(105:23)"
psa105.v23.text = "Then Israel came to Egypt; Jacob sojourned in the land of Ham."
psa105.v24.number_rsv = "(105:24)"
psa105.v24.number = "(105:24)"
psa105.v24.text = "And the LORD made his people very fruitful, and made them stronger than their foes."
psa105.v25.number_rsv = "(105:25)"
psa105.v25.number = "(105:25)"
psa105.v25.text = "He turned their hearts to hate his people, to deal craftily with his servants."
psa105.v26.number_rsv = "(105:26)"
psa105.v26.number = "(105:26)"
psa105.v26.text = "He sent Moses his servant, and Aaron whom he had chosen."
psa105.v27.number_rsv = "(105:27)"
psa105.v27.number = "(105:27)"
psa105.v27.text = "They wrought his signs among them, and miracles in the land of Ham."
psa105.v28.number_rsv = "(105:28)"
psa105.v28.number = "(105:28)"
psa105.v28.text = "He sent darkness, and made the land dark; they rebelled against his words."
psa105.v29.number_rsv = "(105:29)"
psa105.v29.number = "(105:29)"
psa105.v29.text = "He turned their waters into blood, and caused their fish to die."
psa105.v30.number_rsv = "(105:30)"
psa105.v30.number = "(105:30)"
psa105.v30.text = "Their land swarmed with frogs, even in the chambers of their kings."
psa105.v31.number_rsv = "(105:31)"
psa105.v31.number = "(105:31)"
psa105.v31.text = "He spoke, and there came swarms of flies, and gnats throughout their country."
psa105.v32.number_rsv = "(105:32)"
psa105.v32.number = "(105:32)"
psa105.v32.text = "He gave them hail for rain, and lightning that flashed through their land."
psa105.v33.number_rsv = "(105:33)"
psa105.v33.number = "(105:33)"
psa105.v33.text = "He smote their vines and fig trees, and shattered the trees of their country."
psa105.v34.number_rsv = "(105:34)"
psa105.v34.number = "(105:34)"
psa105.v34.text = "He spoke, and the locusts came, and young locusts without number;"
psa105.v35.number_rsv = "(105:35)"
psa105.v35.number = "(105:35)"
psa105.v35.text = "which devoured all the vegetation in their land, and ate up the fruit of their ground."
psa105.v36.number_rsv = "(105:36)"
psa105.v36.number = "(105:36)"
psa105.v36.text = "He smote all the first-born in their land, the first issue of all their strength."
psa105.v37.number_rsv = "(105:37)"
psa105.v37.number = "(105:37)"
psa105.v37.text = "Then he led forth Israel with silver and gold, and there was none among his tribes who stumbled."
psa105.v38.number_rsv = "(105:38)"
psa105.v38.number = "(105:38)"
psa105.v38.text = "Egypt was glad when they departed, for dread of them had fallen upon it."
psa105.v39.number_rsv = "(105:39)"
psa105.v39.number = "(105:39)"
psa105.v39.text = "He spread a cloud for a covering, and fire to give light by night."
psa105.v40.number_rsv = "(105:40)"
psa105.v40.number = "(105:40)"
psa105.v40.text = "They asked, and he brought quails, and gave them bread from heaven in abundance."
psa105.v41.number_rsv = "(105:41)"
psa105.v41.number = "(105:41)"
psa105.v41.text = "He opened the rock, and water gushed forth; it flowed through the desert like a river."
psa105.v42.number_rsv = "(105:42)"
psa105.v42.number = "(105:42)"
psa105.v42.text = "For he remembered his holy promise, and Abraham his servant."
psa105.v43.number_rsv = "(105:43)"
psa105.v43.number = "(105:43)"
psa105.v43.text = "So he led forth his people with joy, his chosen ones with singing."
psa105.v44.number_rsv = "(105:44)"
psa105.v44.number = "(105:44)"
psa105.v44.text = "And he gave them the lands of the nations; and they took possession of the fruit of the peoples\" toil,"
psa105.v45.number_rsv = "(105:45)"
psa105.v45.number = "(105:45)"
psa105.v45.text = "to the end that they should keep his statutes, and observe his laws.Praise the LORD!"

kathisma_15.title = "KATHISMA 15."

psa106.title = "Psalm 106"
psa106.version = properties_en_US_rsv.version.designation
psa106.text = ""
psa106.v1.number_rsv = "(106:1)"
psa106.v1.number = "(106:1)"
psa106.v1.text = "Praise the LORD! O give thanks to the LORD, for he is good; for his steadfast love endures for ever!"
psa106.v2.number_rsv = "(106:2)"
psa106.v2.number = "(106:2)"
psa106.v2.text = "Who can utter the mighty doings of the LORD, or show forth all his praise?"
psa106.v3.number_rsv = "(106:3)"
psa106.v3.number = "(106:3)"
psa106.v3.text = "Blessed are they who observe justice, who do righteousness at all times!"
psa106.v4.number_rsv = "(106:4)"
psa106.v4.number = "(106:4)"
psa106.v4.text = "Remember me, O LORD, when thou showest favor to thy people; help me when thou deliverest them;"
psa106.v5.number_rsv = "(106:5)"
psa106.v5.number = "(106:5)"
psa106.v5.text = "that I may see the prosperity of thy chosen ones, that I may rejoice in the gladness of thy nation, that I may glory with thy heritage."
psa106.v6.number_rsv = "(106:6)"
psa106.v6.number = "(106:6)"
psa106.v6.text = "Both we and our fathers have sinned; we have committed iniquity, we have done wickedly."
psa106.v7.number_rsv = "(106:7)"
psa106.v7.number = "(106:7)"
psa106.v7.text = "Our fathers, when they were in Egypt, did not consider thy wonderful works; they did not remember the abundance of thy steadfast love, but rebelled against the Most High at the Red Sea."
psa106.v8.number_rsv = "(106:8)"
psa106.v8.number = "(106:8)"
psa106.v8.text = "Yet he saved them for his name\"s sake, that he might make known his mighty power."
psa106.v9.number_rsv = "(106:9)"
psa106.v9.number = "(106:9)"
psa106.v9.text = "He rebuked the Red Sea, and it became dry; and he led them through the deep as through a desert."
psa106.v10.number_rsv = "(106:10)"
psa106.v10.number = "(106:10)"
psa106.v10.text = "So he saved them from the hand of the foe, and delivered them from the power of the enemy."
psa106.v11.number_rsv = "(106:11)"
psa106.v11.number = "(106:11)"
psa106.v11.text = "And the waters covered their adversaries; not one of them was left."
psa106.v12.number_rsv = "(106:12)"
psa106.v12.number = "(106:12)"
psa106.v12.text = "Then they believed his words; they sang his praise."
psa106.v13.number_rsv = "(106:13)"
psa106.v13.number = "(106:13)"
psa106.v13.text = "But they soon forgot his works; they did not wait for his counsel."
psa106.v14.number_rsv = "(106:14)"
psa106.v14.number = "(106:14)"
psa106.v14.text = "But they had a wanton craving in the wilderness, and put God to the test in the desert;"
psa106.v15.number_rsv = "(106:15)"
psa106.v15.number = "(106:15)"
psa106.v15.text = "he gave them what they asked, but sent a wasting disease among them."
psa106.v16.number_rsv = "(106:16)"
psa106.v16.number = "(106:16)"
psa106.v16.text = "When men in the camp were jealous of Mosesand Aaron, the holy one of the LORD,"
psa106.v17.number_rsv = "(106:17)"
psa106.v17.number = "(106:17)"
psa106.v17.text = "the earth opened and swallowed up Dathan, and covered the company of Abi\"ram."
psa106.v18.number_rsv = "(106:18)"
psa106.v18.number = "(106:18)"
psa106.v18.text = "Fire also broke out in their company; the flame burned up the wicked."
psa106.v19.number_rsv = "(106:19)"
psa106.v19.number = "(106:19)"
psa106.v19.text = "They made a calf in Horeband worshiped a molten image."
psa106.v20.number_rsv = "(106:20)"
psa106.v20.number = "(106:20)"
psa106.v20.text = "They exchanged the glory of Godfor the image of an ox that eats grass."
psa106.v21.number_rsv = "(106:21)"
psa106.v21.number = "(106:21)"
psa106.v21.text = "They forgot God, their Savior, who had done great things in Egypt,"
psa106.v22.number_rsv = "(106:22)"
psa106.v22.number = "(106:22)"
psa106.v22.text = "wondrous works in the land of Ham, and terrible things by the Red Sea."
psa106.v23.number_rsv = "(106:23)"
psa106.v23.number = "(106:23)"
psa106.v23.text = "Therefore he said he would destroy them --had not Moses, his chosen one, stood in the breach before him, to turn away his wrath from destroying them."
psa106.v24.number_rsv = "(106:24)"
psa106.v24.number = "(106:24)"
psa106.v24.text = "Then they despised the pleasant land, having no faith in his promise."
psa106.v25.number_rsv = "(106:25)"
psa106.v25.number = "(106:25)"
psa106.v25.text = "They murmured in their tents, and did not obey the voice of the LORD."
psa106.v26.number_rsv = "(106:26)"
psa106.v26.number = "(106:26)"
psa106.v26.text = "Therefore he raised his hand and swore to themthat he would make them fall in the wilderness,"
psa106.v27.number_rsv = "(106:27)"
psa106.v27.number = "(106:27)"
psa106.v27.text = "and would disperse their descendants among the nations, scattering them over the lands."
psa106.v28.number_rsv = "(106:28)"
psa106.v28.number = "(106:28)"
psa106.v28.text = "Then they attached themselves to the Ba\"al of Pe\"or, and ate sacrifices offered to the dead;"
psa106.v29.number_rsv = "(106:29)"
psa106.v29.number = "(106:29)"
psa106.v29.text = "they provoked the LORD to anger with their doings, and a plague broke out among them."
psa106.v30.number_rsv = "(106:30)"
psa106.v30.number = "(106:30)"
psa106.v30.text = "Then Phin\"ehas stood up and interposed, and the plague was stayed."
psa106.v31.number_rsv = "(106:31)"
psa106.v31.number = "(106:31)"
psa106.v31.text = "And that has been reckoned to him as righteousnessfrom generation to generation for ever."
psa106.v32.number_rsv = "(106:32)"
psa106.v32.number = "(106:32)"
psa106.v32.text = "They angered him at the waters of Mer\"ibah, and it went ill with Moses on their account;"
psa106.v33.number_rsv = "(106:33)"
psa106.v33.number = "(106:33)"
psa106.v33.text = "for they made his spirit bitter, and he spoke words that were rash."
psa106.v34.number_rsv = "(106:34)"
psa106.v34.number = "(106:34)"
psa106.v34.text = "They did not destroy the peoples, as the LORD commanded them,"
psa106.v35.number_rsv = "(106:35)"
psa106.v35.number = "(106:35)"
psa106.v35.text = "but they mingled with the nationsand learned to do as they did."
psa106.v36.number_rsv = "(106:36)"
psa106.v36.number = "(106:36)"
psa106.v36.text = "They served their idols, which became a snare to them."
psa106.v37.number_rsv = "(106:37)"
psa106.v37.number = "(106:37)"
psa106.v37.text = "They sacrificed their sonsand their daughters to the demons;"
psa106.v38.number_rsv = "(106:38)"
psa106.v38.number = "(106:38)"
psa106.v38.text = "they poured out innocent blood, the blood of their sons and daughters, whom they sacrificed to the idols of Canaan; and the land was polluted with blood."
psa106.v39.number_rsv = "(106:39)"
psa106.v39.number = "(106:39)"
psa106.v39.text = "Thus they became unclean by their acts, and played the harlot in their doings."
psa106.v40.number_rsv = "(106:40)"
psa106.v40.number = "(106:40)"
psa106.v40.text = "Then the anger of the LORD was kindled against his people, and he abhorred his heritage;"
psa106.v41.number_rsv = "(106:41)"
psa106.v41.number = "(106:41)"
psa106.v41.text = "he gave them into the hand of the nations, so that those who hated them ruled over them."
psa106.v42.number_rsv = "(106:42)"
psa106.v42.number = "(106:42)"
psa106.v42.text = "Their enemies oppressed them, and they were brought into subjection under their power."
psa106.v43.number_rsv = "(106:43)"
psa106.v43.number = "(106:43)"
psa106.v43.text = "Many times he delivered them, but they were rebellious in their purposes, and were brought low through their iniquity."
psa106.v44.number_rsv = "(106:44)"
psa106.v44.number = "(106:44)"
psa106.v44.text = "Nevertheless he regarded their distress, when he heard their cry."
psa106.v45.number_rsv = "(106:45)"
psa106.v45.number = "(106:45)"
psa106.v45.text = "He remembered for their sake his covenant, and relented according to the abundance of his steadfast love."
psa106.v46.number_rsv = "(106:46)"
psa106.v46.number = "(106:46)"
psa106.v46.text = "He caused them to be pitiedby all those who held them captive."
psa106.v47.number_rsv = "(106:47)"
psa106.v47.number = "(106:47)"
psa106.v47.text = "Save us, O LORD our God, and gather us from among the nations, that we may give thanks to thy holy nameand glory in thy praise."
psa106.v48.number_rsv = "(106:48)"
psa106.v48.number = "(106:48)"
psa106.v48.text = "Blessed be the LORD, the God of Israel, from everlasting to everlasting! And let all the people say, \"Amen!\"Praise the LORD!"

psa107.title = "Psalm 107"
psa107.version = properties_en_US_rsv.version.designation
psa107.text = ""
psa107.v1.number_rsv = "(107:1)"
psa107.v1.number = "(107:1)"
psa107.v1.text = "O give thanks to the LORD, for he is good; for his steadfast love endures for ever!"
psa107.v2.number_rsv = "(107:2)"
psa107.v2.number = "(107:2)"
psa107.v2.text = "Let the redeemed of the LORD say so, whom he has redeemed from trouble"
psa107.v3.number_rsv = "(107:3)"
psa107.v3.number = "(107:3)"
psa107.v3.text = "and gathered in from the lands, from the east and from the west, from the north and from the south."
psa107.v4.number_rsv = "(107:4)"
psa107.v4.number = "(107:4)"
psa107.v4.text = "Some wandered in desert wastes, finding no way to a city to dwell in;"
psa107.v5.number_rsv = "(107:5)"
psa107.v5.number = "(107:5)"
psa107.v5.text = "hungry and thirsty, their soul fainted within them."
psa107.v6.number_rsv = "(107:6)"
psa107.v6.number = "(107:6)"
psa107.v6.text = "Then they cried to the LORD in their trouble, and he delivered them from their distress;"
psa107.v7.number_rsv = "(107:7)"
psa107.v7.number = "(107:7)"
psa107.v7.text = "he led them by a straight way, till they reached a city to dwell in."
psa107.v8.number_rsv = "(107:8)"
psa107.v8.number = "(107:8)"
psa107.v8.text = "Let them thank the LORD for his steadfast love, for his wonderful works to the sons of men!"
psa107.v9.number_rsv = "(107:9)"
psa107.v9.number = "(107:9)"
psa107.v9.text = "For he satisfies him who is thirsty, and the hungry he fills with good things."
psa107.v10.number_rsv = "(107:10)"
psa107.v10.number = "(107:10)"
psa107.v10.text = "Some sat in darkness and in gloom, prisoners in affliction and in irons,"
psa107.v11.number_rsv = "(107:11)"
psa107.v11.number = "(107:11)"
psa107.v11.text = "for they had rebelled against the words of God, and spurned the counsel of the Most High."
psa107.v12.number_rsv = "(107:12)"
psa107.v12.number = "(107:12)"
psa107.v12.text = "Their hearts were bowed down with hard labor; they fell down, with none to help."
psa107.v13.number_rsv = "(107:13)"
psa107.v13.number = "(107:13)"
psa107.v13.text = "Then they cried to the LORD in their trouble, and he delivered them from their distress;"
psa107.v14.number_rsv = "(107:14)"
psa107.v14.number = "(107:14)"
psa107.v14.text = "he brought them out of darkness and gloom, and broke their bonds asunder."
psa107.v15.number_rsv = "(107:15)"
psa107.v15.number = "(107:15)"
psa107.v15.text = "Let them thank the LORD for his steadfast love, for his wonderful works to the sons of men!"
psa107.v16.number_rsv = "(107:16)"
psa107.v16.number = "(107:16)"
psa107.v16.text = "For he shatters the doors of bronze, and cuts in two the bars of iron."
psa107.v17.number_rsv = "(107:17)"
psa107.v17.number = "(107:17)"
psa107.v17.text = "Some were sick through their sinful ways, and because of their iniquities suffered affliction;"
psa107.v18.number_rsv = "(107:18)"
psa107.v18.number = "(107:18)"
psa107.v18.text = "they loathed any kind of food, and they drew near to the gates of death."
psa107.v19.number_rsv = "(107:19)"
psa107.v19.number = "(107:19)"
psa107.v19.text = "Then they cried to the LORD in their trouble, and he delivered them from their distress;"
psa107.v20.number_rsv = "(107:20)"
psa107.v20.number = "(107:20)"
psa107.v20.text = "he sent forth his word, and healed them, and delivered them from destruction."
psa107.v21.number_rsv = "(107:21)"
psa107.v21.number = "(107:21)"
psa107.v21.text = "Let them thank the LORD for his steadfast love, for his wonderful works to the sons of men!"
psa107.v22.number_rsv = "(107:22)"
psa107.v22.number = "(107:22)"
psa107.v22.text = "And let them offer sacrifices of thanksgiving, and tell of his deeds in songs of joy!"
psa107.v23.number_rsv = "(107:23)"
psa107.v23.number = "(107:23)"
psa107.v23.text = "Some went down to the sea in ships, doing business on the great waters;"
psa107.v24.number_rsv = "(107:24)"
psa107.v24.number = "(107:24)"
psa107.v24.text = "they saw the deeds of the LORD, his wondrous works in the deep."
psa107.v25.number_rsv = "(107:25)"
psa107.v25.number = "(107:25)"
psa107.v25.text = "For he commanded, and raised the stormy wind, which lifted up the waves of the sea."
psa107.v26.number_rsv = "(107:26)"
psa107.v26.number = "(107:26)"
psa107.v26.text = "They mounted up to heaven, they went down to the depths; their courage melted away in their evil plight;"
psa107.v27.number_rsv = "(107:27)"
psa107.v27.number = "(107:27)"
psa107.v27.text = "they reeled and staggered like drunken men, and were at their wits\" end."
psa107.v28.number_rsv = "(107:28)"
psa107.v28.number = "(107:28)"
psa107.v28.text = "Then they cried to the LORD in their trouble, and he delivered them from their distress;"
psa107.v29.number_rsv = "(107:29)"
psa107.v29.number = "(107:29)"
psa107.v29.text = "he made the storm be still, and the waves of the sea were hushed."
psa107.v30.number_rsv = "(107:30)"
psa107.v30.number = "(107:30)"
psa107.v30.text = "Then they were glad because they had quiet, and he brought them to their desired haven."
psa107.v31.number_rsv = "(107:31)"
psa107.v31.number = "(107:31)"
psa107.v31.text = "Let them thank the LORD for his steadfast love, for his wonderful works to the sons of men!"
psa107.v32.number_rsv = "(107:32)"
psa107.v32.number = "(107:32)"
psa107.v32.text = "Let them extol him in the congregation of the people, and praise him in the assembly of the elders."
psa107.v33.number_rsv = "(107:33)"
psa107.v33.number = "(107:33)"
psa107.v33.text = "He turns rivers into a desert, springs of water into thirsty ground,"
psa107.v34.number_rsv = "(107:34)"
psa107.v34.number = "(107:34)"
psa107.v34.text = "a fruitful land into a salty waste, because of the wickedness of its inhabitants."
psa107.v35.number_rsv = "(107:35)"
psa107.v35.number = "(107:35)"
psa107.v35.text = "He turns a desert into pools of water, a parched land into springs of water."
psa107.v36.number_rsv = "(107:36)"
psa107.v36.number = "(107:36)"
psa107.v36.text = "And there he lets the hungry dwell, and they establish a city to live in;"
psa107.v37.number_rsv = "(107:37)"
psa107.v37.number = "(107:37)"
psa107.v37.text = "they sow fields, and plant vineyards, and get a fruitful yield."
psa107.v38.number_rsv = "(107:38)"
psa107.v38.number = "(107:38)"
psa107.v38.text = "By his blessing they multiply greatly; and he does not let their cattle decrease."
psa107.v39.number_rsv = "(107:39)"
psa107.v39.number = "(107:39)"
psa107.v39.text = "When they are diminished and brought lowthrough oppression, trouble, and sorrow,"
psa107.v40.number_rsv = "(107:40)"
psa107.v40.number = "(107:40)"
psa107.v40.text = "he pours contempt upon princesand makes them wander in trackless wastes;"
psa107.v41.number_rsv = "(107:41)"
psa107.v41.number = "(107:41)"
psa107.v41.text = "but he raises up the needy out of affliction, and makes their families like flocks."
psa107.v42.number_rsv = "(107:42)"
psa107.v42.number = "(107:42)"
psa107.v42.text = "The upright see it and are glad; and all wickedness stops its mouth."
psa107.v43.number_rsv = "(107:43)"
psa107.v43.number = "(107:43)"
psa107.v43.text = "Whoever is wise, let him give heed to these things; let men consider the steadfast love of the LORD."

psa108.title = "Psalm 108"
psa108.version = properties_en_US_rsv.version.designation
psa108.text = ""
psa108.v0.number_rsv = "(108:0)"
psa108.v0.number = "(108:0)"
psa108.v0.text = "A Song. A Psalm of David."
psa108.v1.number_rsv = "(108:1)"
psa108.v1.number = "(108:1)"
psa108.v1.text = "My heart is steadfast, O God, my heart is steadfast! I will sing and make melody! Awake, my soul!"
psa108.v2.number_rsv = "(108:2)"
psa108.v2.number = "(108:2)"
psa108.v2.text = "Awake, O harp and lyre! I will awake the dawn!"
psa108.v3.number_rsv = "(108:3)"
psa108.v3.number = "(108:3)"
psa108.v3.text = "I will give thanks to thee, O LORD, among the peoples, I will sing praises to thee among the nations."
psa108.v4.number_rsv = "(108:4)"
psa108.v4.number = "(108:4)"
psa108.v4.text = "For thy steadfast love is great above the heavens, thy faithfulness reaches to the clouds."
psa108.v5.number_rsv = "(108:5)"
psa108.v5.number = "(108:5)"
psa108.v5.text = "Be exalted, O God, above the heavens! Let thy glory be over all the earth!"
psa108.v6.number_rsv = "(108:6)"
psa108.v6.number = "(108:6)"
psa108.v6.text = "That thy beloved may be delivered, give help by thy right hand, and answer me!"
psa108.v7.number_rsv = "(108:7)"
psa108.v7.number = "(108:7)"
psa108.v7.text = "God has promised in his sanctuary:\"With exultation I will divide up Shechem, and portion out the Vale of Succoth."
psa108.v8.number_rsv = "(108:8)"
psa108.v8.number = "(108:8)"
psa108.v8.text = "Gilead is mine; Manas\"seh is mine; E\"phraim is my helmet; Judah my scepter."
psa108.v9.number_rsv = "(108:9)"
psa108.v9.number = "(108:9)"
psa108.v9.text = "Moab is my washbasin; upon Edom I cast my shoe; over Philistia I shout in triumph.\""
psa108.v10.number_rsv = "(108:10)"
psa108.v10.number = "(108:10)"
psa108.v10.text = "Who will bring me to the fortified city? Who will lead me to Edom?"
psa108.v11.number_rsv = "(108:11)"
psa108.v11.number = "(108:11)"
psa108.v11.text = "Hast thou not rejected us, O God? Thou dost not go forth, O God, with our armies."
psa108.v12.number_rsv = "(108:12)"
psa108.v12.number = "(108:12)"
psa108.v12.text = "O grant us help against the foe, for vain is the help of man!"
psa108.v13.number_rsv = "(108:13)"
psa108.v13.number = "(108:13)"
psa108.v13.text = "With God we shall do valiantly; it is he who will tread down our foes.R"

psa109.title = "Psalm 109"
psa109.version = properties_en_US_rsv.version.designation
psa109.text = ""
psa109.v0.number_rsv = "(109:0)"
psa109.v0.number = "(109:0)"
psa109.v0.text = "To the choirmaster. A Psalm of David."
psa109.v1.number_rsv = "(109:1)"
psa109.v1.number = "(109:1)"
psa109.v1.text = "Be not silent, O God of my praise!"
psa109.v2.number_rsv = "(109:2)"
psa109.v2.number = "(109:2)"
psa109.v2.text = "For wicked and deceitful mouths are opened against me, speaking against me with lying tongues."
psa109.v3.number_rsv = "(109:3)"
psa109.v3.number = "(109:3)"
psa109.v3.text = "They beset me with words of hate, and attack me without cause."
psa109.v4.number_rsv = "(109:4)"
psa109.v4.number = "(109:4)"
psa109.v4.text = "In return for my love they accuse me, even as I make prayer for them."
psa109.v5.number_rsv = "(109:5)"
psa109.v5.number = "(109:5)"
psa109.v5.text = "So they reward me evil for good, and hatred for my love."
psa109.v6.number_rsv = "(109:6)"
psa109.v6.number = "(109:6)"
psa109.v6.text = "Appoint a wicked man against him; let an accuser bring him to trial."
psa109.v7.number_rsv = "(109:7)"
psa109.v7.number = "(109:7)"
psa109.v7.text = "When he is tried, let him come forth guilty; let his prayer be counted as sin!"
psa109.v8.number_rsv = "(109:8)"
psa109.v8.number = "(109:8)"
psa109.v8.text = "May his days be few; may another seize his goods!"
psa109.v9.number_rsv = "(109:9)"
psa109.v9.number = "(109:9)"
psa109.v9.text = "May his children be fatherless, and his wife a widow!"
psa109.v10.number_rsv = "(109:10)"
psa109.v10.number = "(109:10)"
psa109.v10.text = "May his children wander about and beg; may they be driven out of the ruins they inhabit!"
psa109.v11.number_rsv = "(109:11)"
psa109.v11.number = "(109:11)"
psa109.v11.text = "May the creditor seize all that he has; may strangers plunder the fruits of his toil!"
psa109.v12.number_rsv = "(109:12)"
psa109.v12.number = "(109:12)"
psa109.v12.text = "Let there be none to extend kindness to him, nor any to pity his fatherless children!"
psa109.v13.number_rsv = "(109:13)"
psa109.v13.number = "(109:13)"
psa109.v13.text = "May his posterity be cut off; may his name be blotted out in the second generation!"
psa109.v14.number_rsv = "(109:14)"
psa109.v14.number = "(109:14)"
psa109.v14.text = "May the iniquity of his fathers be remembered before the LORD, and let not the sin of his mother be blotted out!"
psa109.v15.number_rsv = "(109:15)"
psa109.v15.number = "(109:15)"
psa109.v15.text = "Let them be before the LORD continually; and may his memory be cut off from the earth!"
psa109.v16.number_rsv = "(109:16)"
psa109.v16.number = "(109:16)"
psa109.v16.text = "For he did not remember to show kindness, but pursued the poor and needyand the brokenhearted to their death."
psa109.v17.number_rsv = "(109:17)"
psa109.v17.number = "(109:17)"
psa109.v17.text = "He loved to curse; let curses come on him! He did not like blessing; may it be far from him!"
psa109.v18.number_rsv = "(109:18)"
psa109.v18.number = "(109:18)"
psa109.v18.text = "He clothed himself with cursing as his coat, may it soak into his body like water, like oil into his bones!"
psa109.v19.number_rsv = "(109:19)"
psa109.v19.number = "(109:19)"
psa109.v19.text = "May it be like a garment which he wraps round him, like a belt with which he daily girds himself!"
psa109.v20.number_rsv = "(109:20)"
psa109.v20.number = "(109:20)"
psa109.v20.text = "May this be the reward of my accusers from the LORD, of those who speak evil against my life!"
psa109.v21.number_rsv = "(109:21)"
psa109.v21.number = "(109:21)"
psa109.v21.text = "But thou, O GOD my Lord, deal on my behalf for thy name\"s sake; because thy steadfast love is good, deliver me!"
psa109.v22.number_rsv = "(109:22)"
psa109.v22.number = "(109:22)"
psa109.v22.text = "For I am poor and needy, and my heart is stricken within me."
psa109.v23.number_rsv = "(109:23)"
psa109.v23.number = "(109:23)"
psa109.v23.text = "I am gone, like a shadow at evening; I am shaken off like a locust."
psa109.v24.number_rsv = "(109:24)"
psa109.v24.number = "(109:24)"
psa109.v24.text = "My knees are weak through fasting; my body has become gaunt."
psa109.v25.number_rsv = "(109:25)"
psa109.v25.number = "(109:25)"
psa109.v25.text = "I am an object of scorn to my accusers; when they see me, they wag their heads."
psa109.v26.number_rsv = "(109:26)"
psa109.v26.number = "(109:26)"
psa109.v26.text = "Help me, O LORD my God! Save me according to thy steadfast love!"
psa109.v27.number_rsv = "(109:27)"
psa109.v27.number = "(109:27)"
psa109.v27.text = "Let them know that this is thy hand; thou, O LORD, hast done it!"
psa109.v28.number_rsv = "(109:28)"
psa109.v28.number = "(109:28)"
psa109.v28.text = "Let them curse, but do thou bless! Let my assailants be put to shame; may thy servant be glad!"
psa109.v29.number_rsv = "(109:29)"
psa109.v29.number = "(109:29)"
psa109.v29.text = "May my accusers be clothed with dishonor; may they be wrapped in their own shame as in a mantle!"
psa109.v30.number_rsv = "(109:30)"
psa109.v30.number = "(109:30)"
psa109.v30.text = "With my mouth I will give great thanks to the LORD; I will praise him in the midst of the throng."
psa109.v31.number_rsv = "(109:31)"
psa109.v31.number = "(109:31)"
psa109.v31.text = "For he stands at the right hand of the needy, to save him from those who condemn him to death."

kathisma_16.title = "KATHISMA 16."

psa110.title = "Psalm 110"
psa110.version = properties_en_US_rsv.version.designation
psa110.text = ""
psa110.v0.number_rsv = "(110:0)"
psa110.v0.number = "(110:0)"
psa110.v0.text = "A Psalm of David."
psa110.v1.number_rsv = "(110:1)"
psa110.v1.number = "(110:1)"
psa110.v1.text = "The LORD says to my lord:\"Sit at my right hand, till I make your enemies your footstool.\""
psa110.v2.number_rsv = "(110:2)"
psa110.v2.number = "(110:2)"
psa110.v2.text = "The LORD sends forth from Zionyour mighty scepter.Rule in the midst of your foes!"
psa110.v3.number_rsv = "(110:3)"
psa110.v3.number = "(110:3)"
psa110.v3.text = "Your people will offer themselves freelyon the day you lead your hostupon the holy mountains.From the womb of the morninglike dew your youth will come to you."
psa110.v4.number_rsv = "(110:4)"
psa110.v4.number = "(110:4)"
psa110.v4.text = "The LORD has swornand will not change his mind,\"You are a priest for everafter the order of Melchiz\"edek.\""
psa110.v5.number_rsv = "(110:5)"
psa110.v5.number = "(110:5)"
psa110.v5.text = "The Lord is at your right hand; he will shatter kings on the day of his wrath."
psa110.v6.number_rsv = "(110:6)"
psa110.v6.number = "(110:6)"
psa110.v6.text = "He will execute judgment among the nations, filling them with corpses; he will shatter chiefsover the wide earth."
psa110.v7.number_rsv = "(110:7)"
psa110.v7.number = "(110:7)"
psa110.v7.text = "He will drink from the brook by the way; therefore he will lift up his head."

psa111.title = "Psalm 111"
psa111.version = properties_en_US_rsv.version.designation
psa111.text = ""
psa111.v1.number_rsv = "(111:1)"
psa111.v1.number = "(111:1)"
psa111.v1.text = "Prasie the LORD.I will give thanks to the LORD with my whole heart, in the company of the upright, in the congregation."
psa111.v2.number_rsv = "(111:2)"
psa111.v2.number = "(111:2)"
psa111.v2.text = "Great are the works of the LORD, studied by all who have pleasure in them."
psa111.v3.number_rsv = "(111:3)"
psa111.v3.number = "(111:3)"
psa111.v3.text = "Full of honor and majesty is his work, and his righteousness endures for ever."
psa111.v4.number_rsv = "(111:4)"
psa111.v4.number = "(111:4)"
psa111.v4.text = "He has caused his wonderful works to be remembered; the LORD is gracious and merciful."
psa111.v5.number_rsv = "(111:5)"
psa111.v5.number = "(111:5)"
psa111.v5.text = "He provides food for those who fear him; he is ever mindful of his covenant."
psa111.v6.number_rsv = "(111:6)"
psa111.v6.number = "(111:6)"
psa111.v6.text = "He has shown his people the power of his works, in giving them the heritage of the nations."
psa111.v7.number_rsv = "(111:7)"
psa111.v7.number = "(111:7)"
psa111.v7.text = "The works of his hands are faithful and just; all his precepts are trustworthy,"
psa111.v8.number_rsv = "(111:8)"
psa111.v8.number = "(111:8)"
psa111.v8.text = "they are established for ever and ever, to be performed with faithfulness and uprightness."
psa111.v9.number_rsv = "(111:9)"
psa111.v9.number = "(111:9)"
psa111.v9.text = "He sent redemption to his people; he has commanded his covenant for ever.Holy and terrible is his name!"
psa111.v10.number_rsv = "(111:10)"
psa111.v10.number = "(111:10)"
psa111.v10.text = "The fear of the LORD is the beginning of wisdom; a good understanding have all those who practice it.His praise endures for ever!"

psa112.title = "Psalm 112"
psa112.version = properties_en_US_rsv.version.designation
psa112.text = ""
psa112.v1.number_rsv = "(112:1)"
psa112.v1.number = "(112:1)"
psa112.v1.text = "Praise the LORD.Blessed is the man who fears the LORD, who greatly delights in his commandments!"
psa112.v2.number_rsv = "(112:2)"
psa112.v2.number = "(112:2)"
psa112.v2.text = "His descendants will be mighty in the land; the generation of the upright will be blessed."
psa112.v3.number_rsv = "(112:3)"
psa112.v3.number = "(112:3)"
psa112.v3.text = "Wealth and riches are in his house; and his righteousness endures for ever."
psa112.v4.number_rsv = "(112:4)"
psa112.v4.number = "(112:4)"
psa112.v4.text = "Light rises in the darkness for the upright; the LORD is gracious, merciful, and righteous."
psa112.v5.number_rsv = "(112:5)"
psa112.v5.number = "(112:5)"
psa112.v5.text = "It is well with the man who deals generously and lends, who conducts his affairs with justice."
psa112.v6.number_rsv = "(112:6)"
psa112.v6.number = "(112:6)"
psa112.v6.text = "For the righteous will never be moved; he will be remembered for ever."
psa112.v7.number_rsv = "(112:7)"
psa112.v7.number = "(112:7)"
psa112.v7.text = "He is not afraid of evil tidings; his heart is firm, trusting in the LORD."
psa112.v8.number_rsv = "(112:8)"
psa112.v8.number = "(112:8)"
psa112.v8.text = "His heart is steady, he will not be afraid, until he sees his desire on his adversaries."
psa112.v9.number_rsv = "(112:9)"
psa112.v9.number = "(112:9)"
psa112.v9.text = "He has distributed freely, he has given to the poor; his righteousness endures for ever; his horn is exalted in honor."
psa112.v10.number_rsv = "(112:10)"
psa112.v10.number = "(112:10)"
psa112.v10.text = "The wicked man sees it and is angry; he gnashes his teeth and melts away; the desire of the wicked man comes to nought."

psa113.title = "Psalm 113"
psa113.version = properties_en_US_rsv.version.designation
psa113.text = ""
psa113.v1.number_rsv = "(113:1)"
psa113.v1.number = "(113:1)"
psa113.v1.text = "Praise the LORD! Praise, O servants of the LORD, praise the name of the LORD!"
psa113.v2.number_rsv = "(113:2)"
psa113.v2.number = "(113:2)"
psa113.v2.text = "Blessed be the name of the LORDfrom this time forth and for evermore!"
psa113.v3.number_rsv = "(113:3)"
psa113.v3.number = "(113:3)"
psa113.v3.text = "From the rising of the sun to its settingthe name of the LORD is to be praised!"
psa113.v4.number_rsv = "(113:4)"
psa113.v4.number = "(113:4)"
psa113.v4.text = "The LORD is high above all nations, and his glory above the heavens!"
psa113.v5.number_rsv = "(113:5)"
psa113.v5.number = "(113:5)"
psa113.v5.text = "Who is like the LORD our God, who is seated on high,"
psa113.v6.number_rsv = "(113:6)"
psa113.v6.number = "(113:6)"
psa113.v6.text = "who looks far downupon the heavens and the earth?"
psa113.v7.number_rsv = "(113:7)"
psa113.v7.number = "(113:7)"
psa113.v7.text = "He raises the poor from the dust, and lifts the needy from the ash heap,"
psa113.v8.number_rsv = "(113:8)"
psa113.v8.number = "(113:8)"
psa113.v8.text = "to make them sit with princes, with the princes of his people."
psa113.v9.number_rsv = "(113:9)"
psa113.v9.number = "(113:9)"
psa113.v9.text = "He gives the barren woman a home, making her the joyous mother of children.Praise the LORD!"

psa114.title = "Psalm 114"
psa114.version = properties_en_US_rsv.version.designation
psa114.text = ""
psa114.v1.number_rsv = "(114:1)"
psa114.v1.number = "(114:1)"
psa114.v1.text = "When Israel went forth from Egypt, the house of Jacob from a people of strange language,"
psa114.v2.number_rsv = "(114:2)"
psa114.v2.number = "(114:2)"
psa114.v2.text = "Judah became his sanctuary, Israel his dominion."
psa114.v3.number_rsv = "(114:3)"
psa114.v3.number = "(114:3)"
psa114.v3.text = "The sea looked and fled, Jordan turned back."
psa114.v4.number_rsv = "(114:4)"
psa114.v4.number = "(114:4)"
psa114.v4.text = "The mountains skipped like rams, the hills like lambs."
psa114.v5.number_rsv = "(114:5)"
psa114.v5.number = "(114:5)"
psa114.v5.text = "What ails you, O sea, that you flee? O Jordan, that you turn back?"
psa114.v6.number_rsv = "(114:6)"
psa114.v6.number = "(114:6)"
psa114.v6.text = "O mountains, that you skip like rams? O hills, like lambs?"
psa114.v7.number_rsv = "(114:7)"
psa114.v7.number = "(114:7)"
psa114.v7.text = "Tremble, O earth, at the presence of the LORD, at the presence of the God of Jacob,"
psa114.v8.number_rsv = "(114:8)"
psa114.v8.number = "(114:8)"
psa114.v8.text = "who turns the rock into a pool of water, the flint into a spring of water."

psa115.title = "Psalm 115"
psa115.version = properties_en_US_rsv.version.designation
psa115.text = ""
psa115.v1.number_rsv = "(115:1)"
psa115.v1.number = "(115:1)"
psa115.v1.text = "Not to us, O LORD, not to us, but to thy name give glory, for the sake of thy steadfast love and thy faithfulness!"
psa115.v2.number_rsv = "(115:2)"
psa115.v2.number = "(115:2)"
psa115.v2.text = "Why should the nations say,\"Where is their God?\""
psa115.v3.number_rsv = "(115:3)"
psa115.v3.number = "(115:3)"
psa115.v3.text = "Our God is in the heavens; he does whatever he pleases."
psa115.v4.number_rsv = "(115:4)"
psa115.v4.number = "(115:4)"
psa115.v4.text = "Their idols are silver and gold, the work of men\"s hands."
psa115.v5.number_rsv = "(115:5)"
psa115.v5.number = "(115:5)"
psa115.v5.text = "They have mouths, but do not speak; eyes, but do not see."
psa115.v6.number_rsv = "(115:6)"
psa115.v6.number = "(115:6)"
psa115.v6.text = "They have ears, but do not hear; noses, but do not smell."
psa115.v7.number_rsv = "(115:7)"
psa115.v7.number = "(115:7)"
psa115.v7.text = "They have hands, but do not feel; feet, but do not walk; and they do not make a sound in their throat."
psa115.v8.number_rsv = "(115:8)"
psa115.v8.number = "(115:8)"
psa115.v8.text = "Those who make them are like them; so are all who trust in them."
psa115.v9.number_rsv = "(115:9)"
psa115.v9.number = "(115:9)"
psa115.v9.text = "O Israel, trust in the LORD! He is their help and their shield."
psa115.v10.number_rsv = "(115:10)"
psa115.v10.number = "(115:10)"
psa115.v10.text = "O house of Aaron, put your trust in the LORD! He is their help and their shield."
psa115.v11.number_rsv = "(115:11)"
psa115.v11.number = "(115:11)"
psa115.v11.text = "You who fear the LORD, trust in the LORD! He is their help and their shield."
psa115.v12.number_rsv = "(115:12)"
psa115.v12.number = "(115:12)"
psa115.v12.text = "The LORD has been mindful of us; he will bless us; he will bless the house of Israel; he will bless the house of Aaron;"
psa115.v13.number_rsv = "(115:13)"
psa115.v13.number = "(115:13)"
psa115.v13.text = "he will bless those who fear the LORD, both small and great."
psa115.v14.number_rsv = "(115:14)"
psa115.v14.number = "(115:14)"
psa115.v14.text = "May the LORD give you increase, you and your children!"
psa115.v15.number_rsv = "(115:15)"
psa115.v15.number = "(115:15)"
psa115.v15.text = "May you be blessed by the LORD, who made heaven and earth!"
psa115.v16.number_rsv = "(115:16)"
psa115.v16.number = "(115:16)"
psa115.v16.text = "The heavens are the LORD\"s heavens, but the earth he has given to the sons of men."
psa115.v17.number_rsv = "(115:17)"
psa115.v17.number = "(115:17)"
psa115.v17.text = "The dead do not praise the LORD, nor do any that go down into silence."
psa115.v18.number_rsv = "(115:18)"
psa115.v18.number = "(115:18)"
psa115.v18.text = "But we will bless the LORDfrom this time forth and for evermore.Praise the LORD!"

psa116.title = "Psalm 116"
psa116.version = properties_en_US_rsv.version.designation
psa116.text = ""
psa116.v1.number_rsv = "(116:1)"
psa116.v1.number = "(116:1)"
psa116.v1.text = "I love the LORD, because he has heardmy voice and my supplications."
psa116.v2.number_rsv = "(116:2)"
psa116.v2.number = "(116:2)"
psa116.v2.text = "Because he inclined his ear to me, therefore I will call on him as long as I live."
psa116.v3.number_rsv = "(116:3)"
psa116.v3.number = "(116:3)"
psa116.v3.text = "The snares of death encompassed me; the pangs of Sheol laid hold on me; I suffered distress and anguish."
psa116.v4.number_rsv = "(116:4)"
psa116.v4.number = "(116:4)"
psa116.v4.text = "Then I called on the name of the LORD:\"O LORD, I beseech thee, save my life!\""
psa116.v5.number_rsv = "(116:5)"
psa116.v5.number = "(116:5)"
psa116.v5.text = "Gracious is the LORD, and righteous; our God is merciful."
psa116.v6.number_rsv = "(116:6)"
psa116.v6.number = "(116:6)"
psa116.v6.text = "The LORD preserves the simple; when I was brought low, he saved me."
psa116.v7.number_rsv = "(116:7)"
psa116.v7.number = "(116:7)"
psa116.v7.text = "Return, O my soul, to your rest; for the LORD has dealt bountifully with you."
psa116.v8.number_rsv = "(116:8)"
psa116.v8.number = "(116:8)"
psa116.v8.text = "For thou hast delivered my soul from death, my eyes from tears, my feet from stumbling;"
psa116.v9.number_rsv = "(116:9)"
psa116.v9.number = "(116:9)"
psa116.v9.text = "I walk before the LORDin the land of the living."
psa116.v10.number_rsv = "(116:10)"
psa116.v10.number = "(116:10)"
psa116.v10.text = "I kept my faith, even when I said,\"I am greatly afflicted\";"
psa116.v11.number_rsv = "(116:11)"
psa116.v11.number = "(116:11)"
psa116.v11.text = "I said in my consternation,\"Men are all a vain hope.\""
psa116.v12.number_rsv = "(116:12)"
psa116.v12.number = "(116:12)"
psa116.v12.text = "What shall I render to the LORDfor all his bounty to me?"
psa116.v13.number_rsv = "(116:13)"
psa116.v13.number = "(116:13)"
psa116.v13.text = "I will lift up the cup of salvationand call on the name of the LORD,"
psa116.v14.number_rsv = "(116:14)"
psa116.v14.number = "(116:14)"
psa116.v14.text = "I will pay my vows to the LORDin the presence of all his people."
psa116.v15.number_rsv = "(116:15)"
psa116.v15.number = "(116:15)"
psa116.v15.text = "Precious in the sight of the LORDis the death of his saints."
psa116.v16.number_rsv = "(116:16)"
psa116.v16.number = "(116:16)"
psa116.v16.text = "O LORD, I am thy servant; I am thy servant, the son of thy handmaid.Thou hast loosed my bonds."
psa116.v17.number_rsv = "(116:17)"
psa116.v17.number = "(116:17)"
psa116.v17.text = "I will offer to thee the sacrifice of thanksgivingand call on the name of the LORD."
psa116.v18.number_rsv = "(116:18)"
psa116.v18.number = "(116:18)"
psa116.v18.text = "I will pay my vows to the LORDin the presence of all his people,"
psa116.v19.number_rsv = "(116:19)"
psa116.v19.number = "(116:19)"
psa116.v19.text = "in the courts of the house of the LORD, in your midst, O Jerusalem.Praise the LORD!"

psa117.title = "Psalm 117"
psa117.version = properties_en_US_rsv.version.designation
psa117.text = ""
psa117.v1.number_rsv = "(117:1)"
psa117.v1.number = "(117:1)"
psa117.v1.text = "Praise the LORD, all nations! Extol him, all peoples!"
psa117.v2.number_rsv = "(117:2)"
psa117.v2.number = "(117:2)"
psa117.v2.text = "For great is his steadfast love toward us; and the faithfulness of the LORD endures for ever.Praise the LORD!"

psa118.title = "Psalm 118"
psa118.version = properties_en_US_rsv.version.designation
psa118.text = ""
psa118.v1.number_rsv = "(118:1)"
psa118.v1.number = "(118:1)"
psa118.v1.text = "O give thanks to the LORD, for he is good; his steadfast love endures for ever!"
psa118.v2.number_rsv = "(118:2)"
psa118.v2.number = "(118:2)"
psa118.v2.text = "Let Israel say,\"His steadfast love endures for ever.\""
psa118.v3.number_rsv = "(118:3)"
psa118.v3.number = "(118:3)"
psa118.v3.text = "Let the house of Aaron say,\"His steadfast love endures for ever.\""
psa118.v4.number_rsv = "(118:4)"
psa118.v4.number = "(118:4)"
psa118.v4.text = "Let those who fear the LORD say,\"His steadfast love endures for ever.\""
psa118.v5.number_rsv = "(118:5)"
psa118.v5.number = "(118:5)"
psa118.v5.text = "Out of my distress I called on the LORD; the LORD answered me and set me free."
psa118.v6.number_rsv = "(118:6)"
psa118.v6.number = "(118:6)"
psa118.v6.text = "With the LORD on my side I do not fear.What can man do to me?"
psa118.v7.number_rsv = "(118:7)"
psa118.v7.number = "(118:7)"
psa118.v7.text = "The LORD is on my side to help me; I shall look in triumph on those who hate me."
psa118.v8.number_rsv = "(118:8)"
psa118.v8.number = "(118:8)"
psa118.v8.text = "It is better to take refuge in the LORDthan to put confidence in man."
psa118.v9.number_rsv = "(118:9)"
psa118.v9.number = "(118:9)"
psa118.v9.text = "It is better to take refuge in the LORDthan to put confidence in princes."
psa118.v10.number_rsv = "(118:10)"
psa118.v10.number = "(118:10)"
psa118.v10.text = "All nations surrounded me; in the name of the LORD I cut them off!"
psa118.v11.number_rsv = "(118:11)"
psa118.v11.number = "(118:11)"
psa118.v11.text = "They surrounded me, surrounded me on every side; in the name of the LORD I cut them off!"
psa118.v12.number_rsv = "(118:12)"
psa118.v12.number = "(118:12)"
psa118.v12.text = "They surrounded me like bees, they blazed like a fire of thorns; in the name of the LORD I cut them off!"
psa118.v13.number_rsv = "(118:13)"
psa118.v13.number = "(118:13)"
psa118.v13.text = "I was pushed hard, so that I was falling, but the LORD helped me."
psa118.v14.number_rsv = "(118:14)"
psa118.v14.number = "(118:14)"
psa118.v14.text = "The LORD is my strength and my song; he has become my salvation."
psa118.v15.number_rsv = "(118:15)"
psa118.v15.number = "(118:15)"
psa118.v15.text = "Hark, glad songs of victoryin the tents of the righteous:\"The right hand of the LORD does valiantly,"
psa118.v16.number_rsv = "(118:16)"
psa118.v16.number = "(118:16)"
psa118.v16.text = "the right hand of the LORD is exalted, the right hand of the LORD does valiantly!\""
psa118.v17.number_rsv = "(118:17)"
psa118.v17.number = "(118:17)"
psa118.v17.text = "I shall not die, but I shall live, and recount the deeds of the LORD."
psa118.v18.number_rsv = "(118:18)"
psa118.v18.number = "(118:18)"
psa118.v18.text = "The LORD has chastened me sorely, but he has not given me over to death."
psa118.v19.number_rsv = "(118:19)"
psa118.v19.number = "(118:19)"
psa118.v19.text = "Open to me the gates of righteousness, that I may enter through themand give thanks to the LORD."
psa118.v20.number_rsv = "(118:20)"
psa118.v20.number = "(118:20)"
psa118.v20.text = "This is the gate of the LORD; the righteous shall enter through it."
psa118.v21.number_rsv = "(118:21)"
psa118.v21.number = "(118:21)"
psa118.v21.text = "I thank thee that thou hast answered meand hast become my salvation."
psa118.v22.number_rsv = "(118:22)"
psa118.v22.number = "(118:22)"
psa118.v22.text = "The stone which the builders rejectedhas become the head of the corner."
psa118.v23.number_rsv = "(118:23)"
psa118.v23.number = "(118:23)"
psa118.v23.text = "This is the LORD\"s doing; it is marvelous in our eyes."
psa118.v24.number_rsv = "(118:24)"
psa118.v24.number = "(118:24)"
psa118.v24.text = "This is the day which the LORD has made; let us rejoice and be glad in it."
psa118.v25.number_rsv = "(118:25)"
psa118.v25.number = "(118:25)"
psa118.v25.text = "Save us, we beseech thee, O LORD! O LORD, we beseech thee, give us success!"
psa118.v26.number_rsv = "(118:26)"
psa118.v26.number = "(118:26)"
psa118.v26.text = "Blessed be he who enters in the name of the LORD! We bless you from the house of the LORD."
psa118.v27.number_rsv = "(118:27)"
psa118.v27.number = "(118:27)"
psa118.v27.text = "The LORD is God, and he has given us light.Bind the festal procession with branches, up to the horns of the altar!"
psa118.v28.number_rsv = "(118:28)"
psa118.v28.number = "(118:28)"
psa118.v28.text = "Thou art my God, and I will give thanks to thee; thou art my God, I will extol thee."
psa118.v29.number_rsv = "(118:29)"
psa118.v29.number = "(118:29)"
psa118.v29.text = "O give thanks to the LORD, for he is good; for his steadfast love endures for ever! R"

kathisma_17.title = "KATHISMA 17."

psa119.title = "Psalm 119"
psa119.version = properties_en_US_rsv.version.designation
psa119.text = ""
psa119.v1.number_rsv = "(119:1)"
psa119.v1.number = "(119:1)"
psa119.v1.text = "Blessed are those whose way is blameless, who walk in the law of the LORD!"
psa119.v2.number_rsv = "(119:2)"
psa119.v2.number = "(119:2)"
psa119.v2.text = "Blessed are those who keep his testimonies, who seek him with their whole heart,"
psa119.v3.number_rsv = "(119:3)"
psa119.v3.number = "(119:3)"
psa119.v3.text = "who also do no wrong, but walk in his ways!"
psa119.v4.number_rsv = "(119:4)"
psa119.v4.number = "(119:4)"
psa119.v4.text = "Thou hast commanded thy preceptsto be kept diligently."
psa119.v5.number_rsv = "(119:5)"
psa119.v5.number = "(119:5)"
psa119.v5.text = "O that my ways may be steadfastin keeping thy statutes!"
psa119.v6.number_rsv = "(119:6)"
psa119.v6.number = "(119:6)"
psa119.v6.text = "Then I shall not be put to shame, having my eyes fixed on all thy commandments."
psa119.v7.number_rsv = "(119:7)"
psa119.v7.number = "(119:7)"
psa119.v7.text = "I will praise thee with an upright heart, when I learn thy righteous ordinances."
psa119.v8.number_rsv = "(119:8)"
psa119.v8.number = "(119:8)"
psa119.v8.text = "I will observe thy statutes; O forsake me not utterly!"
psa119.v9.number_rsv = "(119:9)"
psa119.v9.number = "(119:9)"
psa119.v9.text = "How can a young man keep his way pure? By guarding it according to thy word."
psa119.v10.number_rsv = "(119:10)"
psa119.v10.number = "(119:10)"
psa119.v10.text = "With my whole heart I seek thee; let me not wander from thy commandments!"
psa119.v11.number_rsv = "(119:11)"
psa119.v11.number = "(119:11)"
psa119.v11.text = "I have laid up thy word in my heart, that I might not sin against thee."
psa119.v12.number_rsv = "(119:12)"
psa119.v12.number = "(119:12)"
psa119.v12.text = "Blessed be thou, O LORD; teach me thy statutes!"
psa119.v13.number_rsv = "(119:13)"
psa119.v13.number = "(119:13)"
psa119.v13.text = "With my lips I declareall the ordinances of thy mouth."
psa119.v14.number_rsv = "(119:14)"
psa119.v14.number = "(119:14)"
psa119.v14.text = "In the way of thy testimonies I delightas much as in all riches."
psa119.v15.number_rsv = "(119:15)"
psa119.v15.number = "(119:15)"
psa119.v15.text = "I will meditate on thy precepts, and fix my eyes on thy ways."
psa119.v16.number_rsv = "(119:16)"
psa119.v16.number = "(119:16)"
psa119.v16.text = "I will delight in thy statutes; I will not forget thy word."
psa119.v17.number_rsv = "(119:17)"
psa119.v17.number = "(119:17)"
psa119.v17.text = "Deal bountifully with thy servant, that I may live and observe thy word."
psa119.v18.number_rsv = "(119:18)"
psa119.v18.number = "(119:18)"
psa119.v18.text = "Open my eyes, that I may beholdwondrous things out of thy law."
psa119.v19.number_rsv = "(119:19)"
psa119.v19.number = "(119:19)"
psa119.v19.text = "I am a sojourner on earth; hide not thy commandments from me!"
psa119.v20.number_rsv = "(119:20)"
psa119.v20.number = "(119:20)"
psa119.v20.text = "My soul is consumed with longingfor thy ordinances at all times."
psa119.v21.number_rsv = "(119:21)"
psa119.v21.number = "(119:21)"
psa119.v21.text = "Thou dost rebuke the insolent, accursed ones, who wander from thy commandments;"
psa119.v22.number_rsv = "(119:22)"
psa119.v22.number = "(119:22)"
psa119.v22.text = "take away from me their scorn and contempt, for I have kept thy testimonies."
psa119.v23.number_rsv = "(119:23)"
psa119.v23.number = "(119:23)"
psa119.v23.text = "Even though princes sit plotting against me, thy servant will meditate on thy statutes."
psa119.v24.number_rsv = "(119:24)"
psa119.v24.number = "(119:24)"
psa119.v24.text = "Thy testimonies are my delight, they are my counselors."
psa119.v25.number_rsv = "(119:25)"
psa119.v25.number = "(119:25)"
psa119.v25.text = "My soul cleaves to the dust; revive me according to thy word!"
psa119.v26.number_rsv = "(119:26)"
psa119.v26.number = "(119:26)"
psa119.v26.text = "When I told of my ways, thou didst answer me; teach me thy statutes!"
psa119.v27.number_rsv = "(119:27)"
psa119.v27.number = "(119:27)"
psa119.v27.text = "Make me understand the way of thy precepts, and I will meditate on thy wondrous works."
psa119.v28.number_rsv = "(119:28)"
psa119.v28.number = "(119:28)"
psa119.v28.text = "My soul melts away for sorrow; strengthen me according to thy word!"
psa119.v29.number_rsv = "(119:29)"
psa119.v29.number = "(119:29)"
psa119.v29.text = "Put false ways far from me; and graciously teach me thy law!"
psa119.v30.number_rsv = "(119:30)"
psa119.v30.number = "(119:30)"
psa119.v30.text = "I have chosen the way of faithfulness, I set thy ordinances before me."
psa119.v31.number_rsv = "(119:31)"
psa119.v31.number = "(119:31)"
psa119.v31.text = "I cleave to thy testimonies, O LORD; let me not be put to shame!"
psa119.v32.number_rsv = "(119:32)"
psa119.v32.number = "(119:32)"
psa119.v32.text = "I will run in the way of thy commandmentswhen thou enlargest my understanding!"
psa119.v33.number_rsv = "(119:33)"
psa119.v33.number = "(119:33)"
psa119.v33.text = "Teach me, O LORD, the way of thy statutes; and I will keep it to the end."
psa119.v34.number_rsv = "(119:34)"
psa119.v34.number = "(119:34)"
psa119.v34.text = "Give me understanding, that I may keep thy lawand observe it with my whole heart."
psa119.v35.number_rsv = "(119:35)"
psa119.v35.number = "(119:35)"
psa119.v35.text = "Lead me in the path of thy commandments, for I delight in it."
psa119.v36.number_rsv = "(119:36)"
psa119.v36.number = "(119:36)"
psa119.v36.text = "Incline my heart to thy testimonies, and not to gain!"
psa119.v37.number_rsv = "(119:37)"
psa119.v37.number = "(119:37)"
psa119.v37.text = "Turn my eyes from looking at vanities; and give me life in thy ways."
psa119.v38.number_rsv = "(119:38)"
psa119.v38.number = "(119:38)"
psa119.v38.text = "Confirm to thy servant thy promise, which is for those who fear thee."
psa119.v39.number_rsv = "(119:39)"
psa119.v39.number = "(119:39)"
psa119.v39.text = "Turn away the reproach which I dread; for thy ordinances are good."
psa119.v40.number_rsv = "(119:40)"
psa119.v40.number = "(119:40)"
psa119.v40.text = "Behold, I long for thy precepts; in thy righteousness give me life!"
psa119.v41.number_rsv = "(119:41)"
psa119.v41.number = "(119:41)"
psa119.v41.text = "Let thy steadfast love come to me, O LORD, thy salvation according to thy promise;"
psa119.v42.number_rsv = "(119:42)"
psa119.v42.number = "(119:42)"
psa119.v42.text = "then shall I have an answer for those who taunt me, for I trust in thy word."
psa119.v43.number_rsv = "(119:43)"
psa119.v43.number = "(119:43)"
psa119.v43.text = "And take not the word of truth utterly out of my mouth, for my hope is in thy ordinances."
psa119.v44.number_rsv = "(119:44)"
psa119.v44.number = "(119:44)"
psa119.v44.text = "I will keep thy law continually, for ever and ever;"
psa119.v45.number_rsv = "(119:45)"
psa119.v45.number = "(119:45)"
psa119.v45.text = "and I shall walk at liberty, for I have sought thy precepts."
psa119.v46.number_rsv = "(119:46)"
psa119.v46.number = "(119:46)"
psa119.v46.text = "I will also speak of thy testimonies before kings, and shall not be put to shame;"
psa119.v47.number_rsv = "(119:47)"
psa119.v47.number = "(119:47)"
psa119.v47.text = "for I find my delight in thy commandments, which I love."
psa119.v48.number_rsv = "(119:48)"
psa119.v48.number = "(119:48)"
psa119.v48.text = "I revere thy commandments, which I love, and I will meditate on thy statutes."
psa119.v49.number_rsv = "(119:49)"
psa119.v49.number = "(119:49)"
psa119.v49.text = "Remember thy word to thy servant, in which thou hast made me hope."
psa119.v50.number_rsv = "(119:50)"
psa119.v50.number = "(119:50)"
psa119.v50.text = "This is my comfort in my afflictionthat thy promise gives me life."
psa119.v51.number_rsv = "(119:51)"
psa119.v51.number = "(119:51)"
psa119.v51.text = "Godless men utterly deride me, but I do not turn away from thy law."
psa119.v52.number_rsv = "(119:52)"
psa119.v52.number = "(119:52)"
psa119.v52.text = "When I think of thy ordinances from of old, I take comfort, O LORD."
psa119.v53.number_rsv = "(119:53)"
psa119.v53.number = "(119:53)"
psa119.v53.text = "Hot indignation seizes me because of the wicked, who forsake thy law."
psa119.v54.number_rsv = "(119:54)"
psa119.v54.number = "(119:54)"
psa119.v54.text = "Thy statutes have been my songsin the house of my pilgrimage."
psa119.v55.number_rsv = "(119:55)"
psa119.v55.number = "(119:55)"
psa119.v55.text = "I remember thy name in the night, O LORD, and keep thy law."
psa119.v56.number_rsv = "(119:56)"
psa119.v56.number = "(119:56)"
psa119.v56.text = "This blessing has fallen to me, that I have kept thy precepts."
psa119.v57.number_rsv = "(119:57)"
psa119.v57.number = "(119:57)"
psa119.v57.text = "The LORD is my portion; I promise to keep thy words."
psa119.v58.number_rsv = "(119:58)"
psa119.v58.number = "(119:58)"
psa119.v58.text = "I entreat thy favor with all my heart; be gracious to me according to thy promise."
psa119.v59.number_rsv = "(119:59)"
psa119.v59.number = "(119:59)"
psa119.v59.text = "When I think of thy ways, I turn my feet to thy testimonies;"
psa119.v60.number_rsv = "(119:60)"
psa119.v60.number = "(119:60)"
psa119.v60.text = "I hasten and do not delayto keep thy commandments."
psa119.v61.number_rsv = "(119:61)"
psa119.v61.number = "(119:61)"
psa119.v61.text = "Though the cords of the wicked ensnare me, I do not forget thy law."
psa119.v62.number_rsv = "(119:62)"
psa119.v62.number = "(119:62)"
psa119.v62.text = "At midnight I rise to praise thee, because of thy righteous ordinances."
psa119.v63.number_rsv = "(119:63)"
psa119.v63.number = "(119:63)"
psa119.v63.text = "I am a companion of all who fear thee, of those who keep thy precepts."
psa119.v64.number_rsv = "(119:64)"
psa119.v64.number = "(119:64)"
psa119.v64.text = "The earth, O LORD, is full of thy steadfast love; teach me thy statutes!"
psa119.v65.number_rsv = "(119:65)"
psa119.v65.number = "(119:65)"
psa119.v65.text = "Thou hast dealt well with thy servant, O LORD, according to thy word."
psa119.v66.number_rsv = "(119:66)"
psa119.v66.number = "(119:66)"
psa119.v66.text = "Teach me good judgment and knowledge, for I believe in thy commandments."
psa119.v67.number_rsv = "(119:67)"
psa119.v67.number = "(119:67)"
psa119.v67.text = "Before I was afflicted I went astray; but now I keep thy word."
psa119.v68.number_rsv = "(119:68)"
psa119.v68.number = "(119:68)"
psa119.v68.text = "Thou art good and doest good; teach me thy statutes."
psa119.v69.number_rsv = "(119:69)"
psa119.v69.number = "(119:69)"
psa119.v69.text = "The godless besmear me with lies, but with my whole heart I keep thy precepts;"
psa119.v70.number_rsv = "(119:70)"
psa119.v70.number = "(119:70)"
psa119.v70.text = "their heart is gross like fat, but I delight in thy law."
psa119.v71.number_rsv = "(119:71)"
psa119.v71.number = "(119:71)"
psa119.v71.text = "It is good for me that I was afflicted, that I might learn thy statutes."
psa119.v72.number_rsv = "(119:72)"
psa119.v72.number = "(119:72)"
psa119.v72.text = "The law of thy mouth is better to methan thousands of gold and silver pieces."
psa119.v73.number_rsv = "(119:73)"
psa119.v73.number = "(119:73)"
psa119.v73.text = "Thy hands have made and fashioned me; give me understanding that I may learn thy commandments."
psa119.v74.number_rsv = "(119:74)"
psa119.v74.number = "(119:74)"
psa119.v74.text = "Those who fear thee shall see me and rejoice, because I have hoped in thy word."
psa119.v75.number_rsv = "(119:75)"
psa119.v75.number = "(119:75)"
psa119.v75.text = "I know, O LORD, that thy judgments are right, and that in faithfulness thou hast afflicted me."
psa119.v76.number_rsv = "(119:76)"
psa119.v76.number = "(119:76)"
psa119.v76.text = "Let thy steadfast love be ready to comfort meaccording to thy promise to thy servant."
psa119.v77.number_rsv = "(119:77)"
psa119.v77.number = "(119:77)"
psa119.v77.text = "Let thy mercy come to me, that I may live; for thy law is my delight."
psa119.v78.number_rsv = "(119:78)"
psa119.v78.number = "(119:78)"
psa119.v78.text = "Let the godless be put to shame, because they have subverted me with guile; as for me, I will meditate on thy precepts."
psa119.v79.number_rsv = "(119:79)"
psa119.v79.number = "(119:79)"
psa119.v79.text = "Let those who fear thee turn to me, that they may know thy testimonies."
psa119.v80.number_rsv = "(119:80)"
psa119.v80.number = "(119:80)"
psa119.v80.text = "May my heart be blameless in thy statutes, that I may not be put to shame!"
psa119.v81.number_rsv = "(119:81)"
psa119.v81.number = "(119:81)"
psa119.v81.text = "My soul languishes for thy salvation; I hope in thy word."
psa119.v82.number_rsv = "(119:82)"
psa119.v82.number = "(119:82)"
psa119.v82.text = "My eyes fail with watching for thy promise; I ask, \"When wilt thou comfort me?\""
psa119.v83.number_rsv = "(119:83)"
psa119.v83.number = "(119:83)"
psa119.v83.text = "For I have become like a wineskin in the smoke, yet I have not forgotten thy statutes."
psa119.v84.number_rsv = "(119:84)"
psa119.v84.number = "(119:84)"
psa119.v84.text = "How long must thy servant endure? When wilt thou judge those who persecute me?"
psa119.v85.number_rsv = "(119:85)"
psa119.v85.number = "(119:85)"
psa119.v85.text = "Godless men have dug pitfalls for me, men who do not conform to thy law."
psa119.v86.number_rsv = "(119:86)"
psa119.v86.number = "(119:86)"
psa119.v86.text = "All thy commandments are sure; they persecute me with falsehood; help me!"
psa119.v87.number_rsv = "(119:87)"
psa119.v87.number = "(119:87)"
psa119.v87.text = "They have almost made an end of me on earth; but I have not forsaken thy precepts."
psa119.v88.number_rsv = "(119:88)"
psa119.v88.number = "(119:88)"
psa119.v88.text = "In thy steadfast love spare my life, that I may keep the testimonies of thy mouth."
psa119.v89.number_rsv = "(119:89)"
psa119.v89.number = "(119:89)"
psa119.v89.text = "For ever, O LORD, thy wordis firmly fixed in the heavens."
psa119.v90.number_rsv = "(119:90)"
psa119.v90.number = "(119:90)"
psa119.v90.text = "Thy faithfulness endures to all generations; thou hast established the earth, and it stands fast."
psa119.v91.number_rsv = "(119:91)"
psa119.v91.number = "(119:91)"
psa119.v91.text = "By thy appointment they stand this day; for all things are thy servants."
psa119.v92.number_rsv = "(119:92)"
psa119.v92.number = "(119:92)"
psa119.v92.text = "If thy law had not been my delight, I should have perished in my affliction."
psa119.v93.number_rsv = "(119:93)"
psa119.v93.number = "(119:93)"
psa119.v93.text = "I will never forget thy precepts; for by them thou hast given me life."
psa119.v94.number_rsv = "(119:94)"
psa119.v94.number = "(119:94)"
psa119.v94.text = "I am thine, save me; for I have sought thy precepts."
psa119.v95.number_rsv = "(119:95)"
psa119.v95.number = "(119:95)"
psa119.v95.text = "The wicked lie in wait to destroy me; but I consider thy testimonies."
psa119.v96.number_rsv = "(119:96)"
psa119.v96.number = "(119:96)"
psa119.v96.text = "I have seen a limit to all perfection, but thy commandment is exceedingly broad."
psa119.v97.number_rsv = "(119:97)"
psa119.v97.number = "(119:97)"
psa119.v97.text = "Oh, how I love thy law! It is my meditation all the day."
psa119.v98.number_rsv = "(119:98)"
psa119.v98.number = "(119:98)"
psa119.v98.text = "Thy commandment makes me wiser than my enemies, for it is ever with me."
psa119.v99.number_rsv = "(119:99)"
psa119.v99.number = "(119:99)"
psa119.v99.text = "I have more understanding than all my teachers, for thy testimonies are my meditation."
psa119.v100.number_rsv = "(119:100)"
psa119.v100.number = "(119:100)"
psa119.v100.text = "I understand more than the aged, for I keep thy precepts."
psa119.v101.number_rsv = "(119:101)"
psa119.v101.number = "(119:101)"
psa119.v101.text = "I hold back my feet from every evil way, in order to keep thy word."
psa119.v102.number_rsv = "(119:102)"
psa119.v102.number = "(119:102)"
psa119.v102.text = "I do not turn aside from thy ordinances, for thou hast taught me."
psa119.v103.number_rsv = "(119:103)"
psa119.v103.number = "(119:103)"
psa119.v103.text = "How sweet are thy words to my taste, sweeter than honey to my mouth!"
psa119.v104.number_rsv = "(119:104)"
psa119.v104.number = "(119:104)"
psa119.v104.text = "Through thy precepts I get understanding; therefore I hate every false way."
psa119.v105.number_rsv = "(119:105)"
psa119.v105.number = "(119:105)"
psa119.v105.text = "Thy word is a lamp to my feetand a light to my path."
psa119.v106.number_rsv = "(119:106)"
psa119.v106.number = "(119:106)"
psa119.v106.text = "I have sworn an oath and confirmed it, to observe thy righteous ordinances."
psa119.v107.number_rsv = "(119:107)"
psa119.v107.number = "(119:107)"
psa119.v107.text = "I am sorely afflicted; give me life, O LORD, according to thy word!"
psa119.v108.number_rsv = "(119:108)"
psa119.v108.number = "(119:108)"
psa119.v108.text = "Accept my offerings of praise, O LORD, and teach me thy ordinances."
psa119.v109.number_rsv = "(119:109)"
psa119.v109.number = "(119:109)"
psa119.v109.text = "I hold my life in my hand continually, but I do not forget thy law."
psa119.v110.number_rsv = "(119:110)"
psa119.v110.number = "(119:110)"
psa119.v110.text = "The wicked have laid a snare for me, but I do not stray from thy precepts."
psa119.v111.number_rsv = "(119:111)"
psa119.v111.number = "(119:111)"
psa119.v111.text = "Thy testimonies are my heritage for ever; yea, they are the joy of my heart."
psa119.v112.number_rsv = "(119:112)"
psa119.v112.number = "(119:112)"
psa119.v112.text = "I incline my heart to perform thy statutesfor ever, to the end."
psa119.v113.number_rsv = "(119:113)"
psa119.v113.number = "(119:113)"
psa119.v113.text = "I hate double-minded men, but I love thy law."
psa119.v114.number_rsv = "(119:114)"
psa119.v114.number = "(119:114)"
psa119.v114.text = "Thou art my hiding place and my shield; I hope in thy word."
psa119.v115.number_rsv = "(119:115)"
psa119.v115.number = "(119:115)"
psa119.v115.text = "Depart from me, you evildoers, that I may keep the commandments of my God."
psa119.v116.number_rsv = "(119:116)"
psa119.v116.number = "(119:116)"
psa119.v116.text = "Uphold me according to thy promise, that I may live, and let me not be put to shame in my hope!"
psa119.v117.number_rsv = "(119:117)"
psa119.v117.number = "(119:117)"
psa119.v117.text = "Hold me up, that I may be safeand have regard for thy statutes continually!"
psa119.v118.number_rsv = "(119:118)"
psa119.v118.number = "(119:118)"
psa119.v118.text = "Thou dost spurn all who go astray from thy statutes; yea, their cunning is in vain."
psa119.v119.number_rsv = "(119:119)"
psa119.v119.number = "(119:119)"
psa119.v119.text = "All the wicked of the earth thou dost count as dross; therefore I love thy testimonies."
psa119.v120.number_rsv = "(119:120)"
psa119.v120.number = "(119:120)"
psa119.v120.text = "My flesh trembles for fear of thee, and I am afraid of thy judgments."
psa119.v121.number_rsv = "(119:121)"
psa119.v121.number = "(119:121)"
psa119.v121.text = "I have done what is just and right; do not leave me to my oppressors."
psa119.v122.number_rsv = "(119:122)"
psa119.v122.number = "(119:122)"
psa119.v122.text = "Be surety for thy servant for good; let not the godless oppress me."
psa119.v123.number_rsv = "(119:123)"
psa119.v123.number = "(119:123)"
psa119.v123.text = "My eyes fail with watching for thy salvation, and for the fulfilment of thy righteous promise."
psa119.v124.number_rsv = "(119:124)"
psa119.v124.number = "(119:124)"
psa119.v124.text = "Deal with thy servant according to thy steadfast love, and teach me thy statutes."
psa119.v125.number_rsv = "(119:125)"
psa119.v125.number = "(119:125)"
psa119.v125.text = "I am thy servant; give me understanding, that I may know thy testimonies!"
psa119.v126.number_rsv = "(119:126)"
psa119.v126.number = "(119:126)"
psa119.v126.text = "It is time for the LORD to act, for thy law has been broken."
psa119.v127.number_rsv = "(119:127)"
psa119.v127.number = "(119:127)"
psa119.v127.text = "Therefore I love thy commandmentsabove gold, above fine gold."
psa119.v128.number_rsv = "(119:128)"
psa119.v128.number = "(119:128)"
psa119.v128.text = "Therefore I direct my steps by all thy precepts; I hate every false way."
psa119.v129.number_rsv = "(119:129)"
psa119.v129.number = "(119:129)"
psa119.v129.text = "Thy testimonies are wonderful; therefore my soul keeps them."
psa119.v130.number_rsv = "(119:130)"
psa119.v130.number = "(119:130)"
psa119.v130.text = "The unfolding of thy words gives light; it imparts understanding to the simple."
psa119.v131.number_rsv = "(119:131)"
psa119.v131.number = "(119:131)"
psa119.v131.text = "With open mouth I pant, because I long for thy commandments."
psa119.v132.number_rsv = "(119:132)"
psa119.v132.number = "(119:132)"
psa119.v132.text = "Turn to me and be gracious to me, as is thy wont toward those who love thy name."
psa119.v133.number_rsv = "(119:133)"
psa119.v133.number = "(119:133)"
psa119.v133.text = "Keep steady my steps according to thy promise, and let no iniquity get dominion over me."
psa119.v134.number_rsv = "(119:134)"
psa119.v134.number = "(119:134)"
psa119.v134.text = "Redeem me from man\"s oppression, that I may keep thy precepts."
psa119.v135.number_rsv = "(119:135)"
psa119.v135.number = "(119:135)"
psa119.v135.text = "Make thy face shine upon thy servant, and teach me thy statutes."
psa119.v136.number_rsv = "(119:136)"
psa119.v136.number = "(119:136)"
psa119.v136.text = "My eyes shed streams of tears, because men do not keep thy law."
psa119.v137.number_rsv = "(119:137)"
psa119.v137.number = "(119:137)"
psa119.v137.text = "Righteous art thou, O LORD, and right are thy judgments."
psa119.v138.number_rsv = "(119:138)"
psa119.v138.number = "(119:138)"
psa119.v138.text = "Thou hast appointed thy testimonies in righteousnessand in all faithfulness."
psa119.v139.number_rsv = "(119:139)"
psa119.v139.number = "(119:139)"
psa119.v139.text = "My zeal consumes me, because my foes forget thy words."
psa119.v140.number_rsv = "(119:140)"
psa119.v140.number = "(119:140)"
psa119.v140.text = "Thy promise is well tried, and thy servant loves it."
psa119.v141.number_rsv = "(119:141)"
psa119.v141.number = "(119:141)"
psa119.v141.text = "I am small and despised, yet I do not forget thy precepts."
psa119.v142.number_rsv = "(119:142)"
psa119.v142.number = "(119:142)"
psa119.v142.text = "Thy righteousness is righteous for ever, and thy law is true."
psa119.v143.number_rsv = "(119:143)"
psa119.v143.number = "(119:143)"
psa119.v143.text = "Trouble and anguish have come upon me, but thy commandments are my delight."
psa119.v144.number_rsv = "(119:144)"
psa119.v144.number = "(119:144)"
psa119.v144.text = "Thy testimonies are righteous for ever; give me understanding that I may live."
psa119.v145.number_rsv = "(119:145)"
psa119.v145.number = "(119:145)"
psa119.v145.text = "With my whole heart I cry; answer me, O LORD! I will keep thy statutes."
psa119.v146.number_rsv = "(119:146)"
psa119.v146.number = "(119:146)"
psa119.v146.text = "I cry to thee; save me, that I may observe thy testimonies."
psa119.v147.number_rsv = "(119:147)"
psa119.v147.number = "(119:147)"
psa119.v147.text = "I rise before dawn and cry for help; I hope in thy words."
psa119.v148.number_rsv = "(119:148)"
psa119.v148.number = "(119:148)"
psa119.v148.text = "My eyes are awake before the watches of the night, that I may meditate upon thy promise."
psa119.v149.number_rsv = "(119:149)"
psa119.v149.number = "(119:149)"
psa119.v149.text = "Hear my voice in thy steadfast love; O LORD, in thy justice preserve my life."
psa119.v150.number_rsv = "(119:150)"
psa119.v150.number = "(119:150)"
psa119.v150.text = "They draw near who persecute me with evil purpose; they are far from thy law."
psa119.v151.number_rsv = "(119:151)"
psa119.v151.number = "(119:151)"
psa119.v151.text = "But thou art near, O LORD, and all thy commandments are true."
psa119.v152.number_rsv = "(119:152)"
psa119.v152.number = "(119:152)"
psa119.v152.text = "Long have I known from thy testimoniesthat thou hast founded them for ever."
psa119.v153.number_rsv = "(119:153)"
psa119.v153.number = "(119:153)"
psa119.v153.text = "Look on my affliction and deliver me, for I do not forget thy law."
psa119.v154.number_rsv = "(119:154)"
psa119.v154.number = "(119:154)"
psa119.v154.text = "Plead my cause and redeem me; give me life according to thy promise!"
psa119.v155.number_rsv = "(119:155)"
psa119.v155.number = "(119:155)"
psa119.v155.text = "Salvation is far from the wicked, for they do not seek thy statutes."
psa119.v156.number_rsv = "(119:156)"
psa119.v156.number = "(119:156)"
psa119.v156.text = "Great is thy mercy, O LORD; give me life according to thy justice."
psa119.v157.number_rsv = "(119:157)"
psa119.v157.number = "(119:157)"
psa119.v157.text = "Many are my persecutors and my adversaries, but I do not swerve from thy testimonies."
psa119.v158.number_rsv = "(119:158)"
psa119.v158.number = "(119:158)"
psa119.v158.text = "I look at the faithless with disgust, because they do not keep thy commands."
psa119.v159.number_rsv = "(119:159)"
psa119.v159.number = "(119:159)"
psa119.v159.text = "Consider how I love thy precepts! Preserve my life according to thy steadfast love."
psa119.v160.number_rsv = "(119:160)"
psa119.v160.number = "(119:160)"
psa119.v160.text = "The sum of thy word is truth; and every one of thy righteous ordinances endures for ever."
psa119.v161.number_rsv = "(119:161)"
psa119.v161.number = "(119:161)"
psa119.v161.text = "Princes persecute me without cause, but my heart stands in awe of thy words."
psa119.v162.number_rsv = "(119:162)"
psa119.v162.number = "(119:162)"
psa119.v162.text = "I rejoice at thy wordlike one who finds great spoil."
psa119.v163.number_rsv = "(119:163)"
psa119.v163.number = "(119:163)"
psa119.v163.text = "I hate and abhor falsehood, but I love thy law."
psa119.v164.number_rsv = "(119:164)"
psa119.v164.number = "(119:164)"
psa119.v164.text = "Seven times a day I praise theefor thy righteous ordinances."
psa119.v165.number_rsv = "(119:165)"
psa119.v165.number = "(119:165)"
psa119.v165.text = "Great peace have those who love thy law; nothing can make them stumble."
psa119.v166.number_rsv = "(119:166)"
psa119.v166.number = "(119:166)"
psa119.v166.text = "I hope for thy salvation, O LORD, and I do thy commandments."
psa119.v167.number_rsv = "(119:167)"
psa119.v167.number = "(119:167)"
psa119.v167.text = "My soul keeps thy testimonies; I love them exceedingly."
psa119.v168.number_rsv = "(119:168)"
psa119.v168.number = "(119:168)"
psa119.v168.text = "I keep thy precepts and testimonies, for all my ways are before thee."
psa119.v169.number_rsv = "(119:169)"
psa119.v169.number = "(119:169)"
psa119.v169.text = "Let my cry come before thee, O LORD; give me understanding according to thy word!"
psa119.v170.number_rsv = "(119:170)"
psa119.v170.number = "(119:170)"
psa119.v170.text = "Let my supplication come before thee; deliver me according to thy word."
psa119.v171.number_rsv = "(119:171)"
psa119.v171.number = "(119:171)"
psa119.v171.text = "My lips will pour forth praisethat thou dost teach me thy statutes."
psa119.v172.number_rsv = "(119:172)"
psa119.v172.number = "(119:172)"
psa119.v172.text = "My tongue will sing of thy word, for all thy commandments are right."
psa119.v173.number_rsv = "(119:173)"
psa119.v173.number = "(119:173)"
psa119.v173.text = "Let thy hand be ready to help me, for I have chosen thy precepts."
psa119.v174.number_rsv = "(119:174)"
psa119.v174.number = "(119:174)"
psa119.v174.text = "I long for thy salvation, O LORD, and thy law is my delight."
psa119.v175.number_rsv = "(119:175)"
psa119.v175.number = "(119:175)"
psa119.v175.text = "Let me live, that I may praise thee, and let thy ordinances help me."
psa119.v176.number_rsv = "(119:176)"
psa119.v176.number = "(119:176)"
psa119.v176.text = "I have gone astray like a lost sheep; seek thy servant, for I do not forget thy commandments."

kathisma_18.title = "KATHISMA 18."

psa120.title = "Psalm 120"
psa120.version = properties_en_US_rsv.version.designation
psa120.text = ""
psa120.v0.number_rsv = "(120:0)"
psa120.v0.number = "(120:0)"
psa120.v0.text = "A Song of Ascents."
psa120.v1.number_rsv = "(120:1)"
psa120.v1.number = "(120:1)"
psa120.v1.text = "In my distress I cry to the LORD, that he may answer me:"
psa120.v2.number_rsv = "(120:2)"
psa120.v2.number = "(120:2)"
psa120.v2.text = "\"Deliver me, O LORD, from lying lips, from a deceitful tongue.\""
psa120.v3.number_rsv = "(120:3)"
psa120.v3.number = "(120:3)"
psa120.v3.text = "What shall be given to you? And what more shall be done to you, you deceitful tongue?"
psa120.v4.number_rsv = "(120:4)"
psa120.v4.number = "(120:4)"
psa120.v4.text = "A warrior\"s sharp arrows, with glowing coals of the broom tree!"
psa120.v5.number_rsv = "(120:5)"
psa120.v5.number = "(120:5)"
psa120.v5.text = "Woe is me, that I sojourn in Meshech, that I dwell among the tents of Kedar!"
psa120.v6.number_rsv = "(120:6)"
psa120.v6.number = "(120:6)"
psa120.v6.text = "Too long have I had my dwellingamong those who hate peace."
psa120.v7.number_rsv = "(120:7)"
psa120.v7.number = "(120:7)"
psa120.v7.text = "I am for peace; but when I speak, they are for war!"

psa121.title = "Psalm 121"
psa121.version = properties_en_US_rsv.version.designation
psa121.text = ""
psa121.v0.number_rsv = "(121:0)"
psa121.v0.number = "(121:0)"
psa121.v0.text = "A Song of Ascents."
psa121.v1.number_rsv = "(121:1)"
psa121.v1.number = "(121:1)"
psa121.v1.text = "I lift up my eyes to the hills.From whence does my help come?"
psa121.v2.number_rsv = "(121:2)"
psa121.v2.number = "(121:2)"
psa121.v2.text = "My help comes from the LORD, who made heaven and earth."
psa121.v3.number_rsv = "(121:3)"
psa121.v3.number = "(121:3)"
psa121.v3.text = "He will not let your foot be moved, he who keeps you will not slumber."
psa121.v4.number_rsv = "(121:4)"
psa121.v4.number = "(121:4)"
psa121.v4.text = "Behold, he who keeps Israelwill neither slumber nor sleep."
psa121.v5.number_rsv = "(121:5)"
psa121.v5.number = "(121:5)"
psa121.v5.text = "The LORD is your keeper; the LORD is your shadeon your right hand."
psa121.v6.number_rsv = "(121:6)"
psa121.v6.number = "(121:6)"
psa121.v6.text = "The sun shall not smite you by day, nor the moon by night."
psa121.v7.number_rsv = "(121:7)"
psa121.v7.number = "(121:7)"
psa121.v7.text = "The LORD will keep you from all evil; he will keep your life."
psa121.v8.number_rsv = "(121:8)"
psa121.v8.number = "(121:8)"
psa121.v8.text = "The LORD will keepyour going out and your coming infrom this time forth and for evermore."

psa122.title = "Psalm 122"
psa122.version = properties_en_US_rsv.version.designation
psa122.text = ""
psa122.v0.number_rsv = "(122:0)"
psa122.v0.number = "(122:0)"
psa122.v0.text = "A Song of Ascents. Of David."
psa122.v1.number_rsv = "(122:1)"
psa122.v1.number = "(122:1)"
psa122.v1.text = "I was glad when they said to me,\"Let us go to the house of the LORD!\""
psa122.v2.number_rsv = "(122:2)"
psa122.v2.number = "(122:2)"
psa122.v2.text = "Our feet have been standingwithin your gates, O Jerusalem!"
psa122.v3.number_rsv = "(122:3)"
psa122.v3.number = "(122:3)"
psa122.v3.text = "Jerusalem, built as a citywhich is bound firmly together,"
psa122.v4.number_rsv = "(122:4)"
psa122.v4.number = "(122:4)"
psa122.v4.text = "to which the tribes go up, the tribes of the LORD, as was decreed for Israel, to give thanks to the name of the LORD."
psa122.v5.number_rsv = "(122:5)"
psa122.v5.number = "(122:5)"
psa122.v5.text = "There thrones for judgment were set, the thrones of the house of David."
psa122.v6.number_rsv = "(122:6)"
psa122.v6.number = "(122:6)"
psa122.v6.text = "Pray for the peace of Jerusalem!\"May they prosper who love you!"
psa122.v7.number_rsv = "(122:7)"
psa122.v7.number = "(122:7)"
psa122.v7.text = "Peace be within your walls, and security within your towers!\""
psa122.v8.number_rsv = "(122:8)"
psa122.v8.number = "(122:8)"
psa122.v8.text = "For my brethren and companions\" sakeI will say, \"Peace be within you!\""
psa122.v9.number_rsv = "(122:9)"
psa122.v9.number = "(122:9)"
psa122.v9.text = "For the sake of the house of the LORD our God, I will seek your good."

psa123.title = "Psalm 123"
psa123.version = properties_en_US_rsv.version.designation
psa123.text = ""
psa123.v0.number_rsv = "(123:0)"
psa123.v0.number = "(123:0)"
psa123.v0.text = "A Song of Ascents."
psa123.v1.number_rsv = "(123:1)"
psa123.v1.number = "(123:1)"
psa123.v1.text = "To thee I lift up my eyes, O thou who art enthroned in the heavens!"
psa123.v2.number_rsv = "(123:2)"
psa123.v2.number = "(123:2)"
psa123.v2.text = "Behold, as the eyes of servantslook to the hand of their master, as the eyes of a maidto the hand of her mistress, so our eyes look to the LORD our God, till he have mercy upon us."
psa123.v3.number_rsv = "(123:3)"
psa123.v3.number = "(123:3)"
psa123.v3.text = "Have mercy upon us, O LORD, have mercy upon us, for we have had more than enough of contempt."
psa123.v4.number_rsv = "(123:4)"
psa123.v4.number = "(123:4)"
psa123.v4.text = "Too long our soul has been satedwith the scorn of those who are at ease, the contempt of the proud."

psa124.title = "Psalm 124"
psa124.version = properties_en_US_rsv.version.designation
psa124.text = ""
psa124.v0.number_rsv = "(124:0)"
psa124.v0.number = "(124:0)"
psa124.v0.text = "A Song of Ascents. Of David."
psa124.v1.number_rsv = "(124:1)"
psa124.v1.number = "(124:1)"
psa124.v1.text = "If it had not been the LORD who was on our side, let Israel now say --"
psa124.v2.number_rsv = "(124:2)"
psa124.v2.number = "(124:2)"
psa124.v2.text = "if it had not been the LORD who was on our side, when men rose up against us,"
psa124.v3.number_rsv = "(124:3)"
psa124.v3.number = "(124:3)"
psa124.v3.text = "then they would have swallowed us up alive, when their anger was kindled against us;"
psa124.v4.number_rsv = "(124:4)"
psa124.v4.number = "(124:4)"
psa124.v4.text = "then the flood would have swept us away, the torrent would have gone over us;"
psa124.v5.number_rsv = "(124:5)"
psa124.v5.number = "(124:5)"
psa124.v5.text = "then over us would have gonethe raging waters."
psa124.v6.number_rsv = "(124:6)"
psa124.v6.number = "(124:6)"
psa124.v6.text = "Blessed be the LORD, who has not given usas prey to their teeth!"
psa124.v7.number_rsv = "(124:7)"
psa124.v7.number = "(124:7)"
psa124.v7.text = "We have escaped as a birdfrom the snare of the fowlers; the snare is broken, and we have escaped!"
psa124.v8.number_rsv = "(124:8)"
psa124.v8.number = "(124:8)"
psa124.v8.text = "Our help is in the name of the LORD, who made heaven and earth."

psa125.title = "Psalm 126"
psa125.version = properties_en_US_rsv.version.designation
psa125.text = ""
psa125.v0.number_rsv = "(125:0)"
psa125.v0.number = "(125:0)"
psa125.v0.text = "A Song of Ascents."
psa125.v1.number_rsv = "(125:1)"
psa125.v1.number = "(125:1)"
psa125.v1.text = "Those who trust in the LORD are like Mount Zion, which cannot be moved, but abides for ever."
psa125.v2.number_rsv = "(125:2)"
psa125.v2.number = "(125:2)"
psa125.v2.text = "As the mountains are round about Jerusalem, so the LORD is round about his people, from this time forth and for evermore."
psa125.v3.number_rsv = "(125:3)"
psa125.v3.number = "(125:3)"
psa125.v3.text = "For the scepter of wickedness shall not restupon the land allotted to the righteous, lest the righteous put forththeir hands to do wrong."
psa125.v4.number_rsv = "(125:4)"
psa125.v4.number = "(125:4)"
psa125.v4.text = "Do good, O LORD, to those who are good, and to those who are upright in their hearts!"
psa125.v5.number_rsv = "(125:5)"
psa125.v5.number = "(125:5)"
psa125.v5.text = "But those who turn aside upon their crooked waysthe LORD will lead away with evildoers! Peace be in Israel!"

psa126.title = "Psalm 126"
psa126.version = properties_en_US_rsv.version.designation
psa126.text = ""
psa126.v0.number_rsv = "(126:0)"
psa126.v0.number = "(126:0)"
psa126.v0.text = "A Song of Ascents."
psa126.v1.number_rsv = "(126:1)"
psa126.v1.number = "(126:1)"
psa126.v1.text = "When the LORD restored the fortunes of Zion, we were like those who dream."
psa126.v2.number_rsv = "(126:2)"
psa126.v2.number = "(126:2)"
psa126.v2.text = "Then our mouth was filled with laughter, and our tongue with shouts of joy; then they said among the nations,\"The LORD has done great things for them.\""
psa126.v3.number_rsv = "(126:3)"
psa126.v3.number = "(126:3)"
psa126.v3.text = "The LORD has done great things for us; we are glad."
psa126.v4.number_rsv = "(126:4)"
psa126.v4.number = "(126:4)"
psa126.v4.text = "Restore our fortunes, O LORD, like the watercourses in the Negeb!"
psa126.v5.number_rsv = "(126:5)"
psa126.v5.number = "(126:5)"
psa126.v5.text = "May those who sow in tearsreap with shouts of joy!"
psa126.v6.number_rsv = "(126:6)"
psa126.v6.number = "(126:6)"
psa126.v6.text = "He that goes forth weeping, bearing the seed for sowing, shall come home with shouts of joy, bringing his sheaves with him."

psa127.title = "Psalm 127"
psa127.version = properties_en_US_rsv.version.designation
psa127.text = ""
psa127.v0.number_rsv = "(127:0)"
psa127.v0.number = "(127:0)"
psa127.v0.text = "A Song of Ascents. Of Solomon."
psa127.v1.number_rsv = "(127:1)"
psa127.v1.number = "(127:1)"
psa127.v1.text = "Unless the LORD builds the house, those who build it labor in vain.Unless the LORD watches over the city, the watchman stays awake in vain."
psa127.v2.number_rsv = "(127:2)"
psa127.v2.number = "(127:2)"
psa127.v2.text = "It is in vain that you rise up earlyand go late to rest, eating the bread of anxious toil; for he gives to his beloved sleep."
psa127.v3.number_rsv = "(127:3)"
psa127.v3.number = "(127:3)"
psa127.v3.text = "Lo, sons are a heritage from the LORD, the fruit of the womb a reward."
psa127.v4.number_rsv = "(127:4)"
psa127.v4.number = "(127:4)"
psa127.v4.text = "Like arrows in the hand of a warriorare the sons of one\"s youth."
psa127.v5.number_rsv = "(127:5)"
psa127.v5.number = "(127:5)"
psa127.v5.text = "Happy is the man who hashis quiver full of them! He shall not be put to shamewhen he speaks with his enemies in the gate."

psa128.title = "Psalm 128"
psa128.version = properties_en_US_rsv.version.designation
psa128.text = ""
psa128.v0.number_rsv = "(128:0)"
psa128.v0.number = "(128:0)"
psa128.v0.text = "A Song of Ascents."
psa128.v1.number_rsv = "(128:1)"
psa128.v1.number = "(128:1)"
psa128.v1.text = "Blessed is every one who fears the LORD, who walks in his ways!"
psa128.v2.number_rsv = "(128:2)"
psa128.v2.number = "(128:2)"
psa128.v2.text = "You shall eat the fruit of the labor of your hands; you shall be happy, and it shall be well with you."
psa128.v3.number_rsv = "(128:3)"
psa128.v3.number = "(128:3)"
psa128.v3.text = "Your wife will be like a fruitful vinewithin your house; your children will be like olive shootsaround your table."
psa128.v4.number_rsv = "(128:4)"
psa128.v4.number = "(128:4)"
psa128.v4.text = "Lo, thus shall the man be blessedwho fears the LORD."
psa128.v5.number_rsv = "(128:5)"
psa128.v5.number = "(128:5)"
psa128.v5.text = "The LORD bless you from Zion! May you see the prosperity of Jerusalemall the days of your life!"
psa128.v6.number_rsv = "(128:6)"
psa128.v6.number = "(128:6)"
psa128.v6.text = "May you see your children\"s children! Peace be upon Israel!"

psa129.title = "Psalm 129"
psa129.version = properties_en_US_rsv.version.designation
psa129.text = ""
psa129.v0.number_rsv = "(129:0)"
psa129.v0.number = "(129:0)"
psa129.v0.text = "A Song of Ascents."
psa129.v1.number_rsv = "(129:1)"
psa129.v1.number = "(129:1)"
psa129.v1.text = "\"Sorely have they afflicted me from my youth,\"let Israel now say --"
psa129.v2.number_rsv = "(129:2)"
psa129.v2.number = "(129:2)"
psa129.v2.text = "\"Sorely have they afflicted me from my youth, yet they have not prevailed against me."
psa129.v3.number_rsv = "(129:3)"
psa129.v3.number = "(129:3)"
psa129.v3.text = "The plowers plowed upon my back; they made long their furrows.\""
psa129.v4.number_rsv = "(129:4)"
psa129.v4.number = "(129:4)"
psa129.v4.text = "The LORD is righteous; he has cut the cords of the wicked."
psa129.v5.number_rsv = "(129:5)"
psa129.v5.number = "(129:5)"
psa129.v5.text = "May all who hate Zionbe put to shame and turned backward!"
psa129.v6.number_rsv = "(129:6)"
psa129.v6.number = "(129:6)"
psa129.v6.text = "Let them be like the grass on the housetops, which withers before it grows up,"
psa129.v7.number_rsv = "(129:7)"
psa129.v7.number = "(129:7)"
psa129.v7.text = "with which the reaper does not fill his handor the binder of sheaves his bosom,"
psa129.v8.number_rsv = "(129:8)"
psa129.v8.number = "(129:8)"
psa129.v8.text = "while those who pass by do not say,\"The blessing of the LORD be upon you! We bless you in the name of the LORD!\""

psa130.title = "Psalm 130"
psa130.version = properties_en_US_rsv.version.designation
psa130.text = ""
psa130.v0.number_rsv = "(130:0)"
psa130.v0.number = "(130:0)"
psa130.v0.text = "A Song of Ascents."
psa130.v1.number_rsv = "(130:1)"
psa130.v1.number = "(130:1)"
psa130.v1.text = "Out of the depths I cry to thee, O LORD!"
psa130.v2.number_rsv = "(130:2)"
psa130.v2.number = "(130:2)"
psa130.v2.text = "Lord, hear my voice! Let thy ears be attentiveto the voice of my supplications!"
psa130.v3.number_rsv = "(130:3)"
psa130.v3.number = "(130:3)"
psa130.v3.text = "If thou, O LORD, shouldst mark iniquities, Lord, who could stand?"
psa130.v4.number_rsv = "(130:4)"
psa130.v4.number = "(130:4)"
psa130.v4.text = "But there is forgiveness with thee, that thou mayest be feared."
psa130.v5.number_rsv = "(130:5)"
psa130.v5.number = "(130:5)"
psa130.v5.text = "I wait for the LORD, my soul waits, and in his word I hope;"
psa130.v6.number_rsv = "(130:6)"
psa130.v6.number = "(130:6)"
psa130.v6.text = "my soul waits for the LORDmore than watchmen for the morning, more than watchmen for the morning."
psa130.v7.number_rsv = "(130:7)"
psa130.v7.number = "(130:7)"
psa130.v7.text = "O Israel, hope in the LORD! For with the LORD there is steadfast love, and with him is plenteous redemption."
psa130.v8.number_rsv = "(130:8)"
psa130.v8.number = "(130:8)"
psa130.v8.text = "And he will redeem Israelfrom all his iniquities."

psa131.title = "Psalm 131"
psa131.version = properties_en_US_rsv.version.designation
psa131.text = ""
psa131.v0.number_rsv = "(131:0)"
psa131.v0.number = "(131:0)"
psa131.v0.text = "A Song of Ascents. Of David."
psa131.v1.number_rsv = "(131:1)"
psa131.v1.number = "(131:1)"
psa131.v1.text = "O LORD, my heart is not lifted up, my eyes are not raised too high; I do not occupy myself with thingstoo great and too marvelous for me."
psa131.v2.number_rsv = "(131:2)"
psa131.v2.number = "(131:2)"
psa131.v2.text = "But I have calmed and quieted my soul, like a child quieted at its mother\"s breast; like a child that is quieted is my soul."
psa131.v3.number_rsv = "(131:3)"
psa131.v3.number = "(131:3)"
psa131.v3.text = "O Israel, hope in the LORDfrom this time forth and for evermore."

psa132.title = "Psalm 132"
psa132.version = properties_en_US_rsv.version.designation
psa132.text = ""
psa132.v0.number_rsv = "(132:0)"
psa132.v0.number = "(132:0)"
psa132.v0.text = "A Song of Ascents."
psa132.v1.number_rsv = "(132:1)"
psa132.v1.number = "(132:1)"
psa132.v1.text = "Remember, O LORD, in David\"s favor, all the hardships he endured;"
psa132.v2.number_rsv = "(132:2)"
psa132.v2.number = "(132:2)"
psa132.v2.text = "how he swore to the LORDand vowed to the Mighty One of Jacob,"
psa132.v3.number_rsv = "(132:3)"
psa132.v3.number = "(132:3)"
psa132.v3.text = "\"I will not enter my houseor get into my bed;"
psa132.v4.number_rsv = "(132:4)"
psa132.v4.number = "(132:4)"
psa132.v4.text = "I will not give sleep to my eyesor slumber to my eyelids,"
psa132.v5.number_rsv = "(132:5)"
psa132.v5.number = "(132:5)"
psa132.v5.text = "until I find a place for the LORD, a dwelling place for the Mighty One of Jacob.\""
psa132.v6.number_rsv = "(132:6)"
psa132.v6.number = "(132:6)"
psa132.v6.text = "Lo, we heard of it in Eph\"rathah, we found it in the fields of Ja\"ar."
psa132.v7.number_rsv = "(132:7)"
psa132.v7.number = "(132:7)"
psa132.v7.text = "\"Let us go to his dwelling place; let us worship at his footstool!\""
psa132.v8.number_rsv = "(132:8)"
psa132.v8.number = "(132:8)"
psa132.v8.text = "Arise, O LORD, and go to thy resting place, thou and the ark of thy might."
psa132.v9.number_rsv = "(132:9)"
psa132.v9.number = "(132:9)"
psa132.v9.text = "Let thy priests be clothed with righteousness, and let thy saints shout for joy."
psa132.v10.number_rsv = "(132:10)"
psa132.v10.number = "(132:10)"
psa132.v10.text = "For thy servant David\"s sakedo not turn away the face of thy anointed one."
psa132.v11.number_rsv = "(132:11)"
psa132.v11.number = "(132:11)"
psa132.v11.text = "The LORD swore to David a sure oathfrom which he will not turn back:\"One of the sons of your bodyI will set on your throne."
psa132.v12.number_rsv = "(132:12)"
psa132.v12.number = "(132:12)"
psa132.v12.text = "If your sons keep my covenantand my testimonies which I shall teach them, their sons also for evershall sit upon your throne.\""
psa132.v13.number_rsv = "(132:13)"
psa132.v13.number = "(132:13)"
psa132.v13.text = "For the LORD has chosen Zion; he has desired it for his habitation:"
psa132.v14.number_rsv = "(132:14)"
psa132.v14.number = "(132:14)"
psa132.v14.text = "\"This is my resting place for ever; here I will dwell, for I have desired it."
psa132.v15.number_rsv = "(132:15)"
psa132.v15.number = "(132:15)"
psa132.v15.text = "I will abundantly bless her provisions; I will satisfy her poor with bread."
psa132.v16.number_rsv = "(132:16)"
psa132.v16.number = "(132:16)"
psa132.v16.text = "Her priests I will clothe with salvation, and her saints will shout for joy."
psa132.v17.number_rsv = "(132:17)"
psa132.v17.number = "(132:17)"
psa132.v17.text = "There I will make a horn to sprout for David; I have prepared a lamp for my anointed."
psa132.v18.number_rsv = "(132:18)"
psa132.v18.number = "(132:18)"
psa132.v18.text = "His enemies I will clothe with shame, but upon himself his crown will shed its luster.\""

psa133.title = "Psalm 133"
psa133.version = properties_en_US_rsv.version.designation
psa133.text = ""
psa133.v0.number_rsv = "(133:0)"
psa133.v0.number = "(133:0)"
psa133.v0.text = "A Song of Ascents."
psa133.v1.number_rsv = "(133:1)"
psa133.v1.number = "(133:1)"
psa133.v1.text = "Behold, how good and pleasant it iswhen brothers dwell in unity!"
psa133.v2.number_rsv = "(133:2)"
psa133.v2.number = "(133:2)"
psa133.v2.text = "It is like the precious oil upon the head, running down upon the beard, upon the beard of Aaron, running down on the collar of his robes!"
psa133.v3.number_rsv = "(133:3)"
psa133.v3.number = "(133:3)"
psa133.v3.text = "It is like the dew of Hermon, which falls on the mountains of Zion! For there the LORD has commanded the blessing, life for evermore."

psa134.title = "Psalm 134"
psa134.version = properties_en_US_rsv.version.designation
psa134.text = ""
psa134.v0.number_rsv = "(134:0)"
psa134.v0.number = "(134:0)"
psa134.v0.text = "A Song of Ascents."
psa134.v1.number_rsv = "(134:1)"
psa134.v1.number = "(134:1)"
psa134.v1.text = "Come, bless the LORD, all you servants of the LORD, who stand by night in the house of the LORD!"
psa134.v2.number_rsv = "(134:2)"
psa134.v2.number = "(134:2)"
psa134.v2.text = "Lift up your hands to the holy place, and bless the LORD!"
psa134.v3.number_rsv = "(134:3)"
psa134.v3.number = "(134:3)"
psa134.v3.text = "May the LORD bless you from Zion, he who made heaven and earth!"

kathisma_19.title = "KATHISMA 19."

psa135.title = "Psalm 135"
psa135.version = properties_en_US_rsv.version.designation
psa135.text = ""
psa135.v1.number_rsv = "(135:1)"
psa135.v1.number = "(135:1)"
psa135.v1.text = "Praise the LORD.Praise the name of the LORD, give praise, O servants of the LORD,"
psa135.v2.number_rsv = "(135:2)"
psa135.v2.number = "(135:2)"
psa135.v2.text = "you that stand in the house of the LORD, in the courts of the house of our God!"
psa135.v3.number_rsv = "(135:3)"
psa135.v3.number = "(135:3)"
psa135.v3.text = "Praise the LORD, for the LORD is good; sing to his name, for he is gracious!"
psa135.v4.number_rsv = "(135:4)"
psa135.v4.number = "(135:4)"
psa135.v4.text = "For the LORD has chosen Jacob for himself, Israel as his own possession."
psa135.v5.number_rsv = "(135:5)"
psa135.v5.number = "(135:5)"
psa135.v5.text = "For I know that the LORD is great, and that our Lord is above all gods."
psa135.v6.number_rsv = "(135:6)"
psa135.v6.number = "(135:6)"
psa135.v6.text = "Whatever the LORD pleases he does, in heaven and on earth, in the seas and all deeps."
psa135.v7.number_rsv = "(135:7)"
psa135.v7.number = "(135:7)"
psa135.v7.text = "He it is who makes the clouds rise at the end of the earth, who makes lightnings for the rainand brings forth the wind from his storehouses."
psa135.v8.number_rsv = "(135:8)"
psa135.v8.number = "(135:8)"
psa135.v8.text = "He it was who smote the first-born of Egypt, both of man and of beast;"
psa135.v9.number_rsv = "(135:9)"
psa135.v9.number = "(135:9)"
psa135.v9.text = "who in thy midst, O Egypt, sent signs and wondersagainst Pharaoh and all his servants;"
psa135.v10.number_rsv = "(135:10)"
psa135.v10.number = "(135:10)"
psa135.v10.text = "who smote many nationsand slew mighty kings,"
psa135.v11.number_rsv = "(135:11)"
psa135.v11.number = "(135:11)"
psa135.v11.text = "Sihon, king of the Amorites, and Og, king of Bashan, and all the kingdoms of Canaan,"
psa135.v12.number_rsv = "(135:12)"
psa135.v12.number = "(135:12)"
psa135.v12.text = "and gave their land as a heritage, a heritage to his people Israel."
psa135.v13.number_rsv = "(135:13)"
psa135.v13.number = "(135:13)"
psa135.v13.text = "Thy name, O LORD, endures for ever, thy renown, O LORD, throughout all ages."
psa135.v14.number_rsv = "(135:14)"
psa135.v14.number = "(135:14)"
psa135.v14.text = "For the LORD will vindicate his people, and have compassion on his servants."
psa135.v15.number_rsv = "(135:15)"
psa135.v15.number = "(135:15)"
psa135.v15.text = "The idols of the nations are silver and gold, the work of men\"s hands."
psa135.v16.number_rsv = "(135:16)"
psa135.v16.number = "(135:16)"
psa135.v16.text = "They have mouths, but they speak not, they have eyes, but they see not,"
psa135.v17.number_rsv = "(135:17)"
psa135.v17.number = "(135:17)"
psa135.v17.text = "they have ears, but they hear not, nor is there any breath in their mouths."
psa135.v18.number_rsv = "(135:18)"
psa135.v18.number = "(135:18)"
psa135.v18.text = "Like them be those who make them! --yea, every one who trusts in them!"
psa135.v19.number_rsv = "(135:19)"
psa135.v19.number = "(135:19)"
psa135.v19.text = "O house of Israel, bless the LORD! O house of Aaron, bless the LORD!"
psa135.v20.number_rsv = "(135:20)"
psa135.v20.number = "(135:20)"
psa135.v20.text = "O house of Levi, bless the LORD! You that fear the LORD, bless the LORD!"
psa135.v21.number_rsv = "(135:21)"
psa135.v21.number = "(135:21)"
psa135.v21.text = "Blessed be the LORD from Zion, he who dwells in Jerusalem! Praise the LORD!"

psa136.title = "Psalm 136"
psa136.version = properties_en_US_rsv.version.designation
psa136.text = ""
psa136.v1.number_rsv = "(136:1)"
psa136.v1.number = "(136:1)"
psa136.v1.text = "O give thanks to the LORD, for he is good, for his steadfast love endures for ever."
psa136.v2.number_rsv = "(136:2)"
psa136.v2.number = "(136:2)"
psa136.v2.text = "O give thanks to the God of gods, for his steadfast love endures for ever."
psa136.v3.number_rsv = "(136:3)"
psa136.v3.number = "(136:3)"
psa136.v3.text = "O give thanks to the Lord of lords, for his steadfast love endures for ever;"
psa136.v4.number_rsv = "(136:4)"
psa136.v4.number = "(136:4)"
psa136.v4.text = "to him who alone does great wonders, for his steadfast love endures for ever;"
psa136.v5.number_rsv = "(136:5)"
psa136.v5.number = "(136:5)"
psa136.v5.text = "to him who by understanding made the heavens, for his steadfast love endures for ever;"
psa136.v6.number_rsv = "(136:6)"
psa136.v6.number = "(136:6)"
psa136.v6.text = "to him who spread out the earth upon the waters, for his steadfast love endures for ever;"
psa136.v7.number_rsv = "(136:7)"
psa136.v7.number = "(136:7)"
psa136.v7.text = "to him who made the great lights, for his steadfast love endures for ever;"
psa136.v8.number_rsv = "(136:8)"
psa136.v8.number = "(136:8)"
psa136.v8.text = "the sun to rule over the day, for his steadfast love endures for ever;"
psa136.v9.number_rsv = "(136:9)"
psa136.v9.number = "(136:9)"
psa136.v9.text = "the moon and stars to rule over the night, for his steadfast love endures for ever;"
psa136.v10.number_rsv = "(136:10)"
psa136.v10.number = "(136:10)"
psa136.v10.text = "to him who smote the first-born of Egypt, for his steadfast love endures for ever;"
psa136.v11.number_rsv = "(136:11)"
psa136.v11.number = "(136:11)"
psa136.v11.text = "and brought Israel out from among them, for his steadfast love endures for ever;"
psa136.v12.number_rsv = "(136:12)"
psa136.v12.number = "(136:12)"
psa136.v12.text = "with a strong hand and an outstretched arm, for his steadfast love endures for ever;"
psa136.v13.number_rsv = "(136:13)"
psa136.v13.number = "(136:13)"
psa136.v13.text = "to him who divided the Red Sea in sunder, for his steadfast love endures for ever;"
psa136.v14.number_rsv = "(136:14)"
psa136.v14.number = "(136:14)"
psa136.v14.text = "and made Israel pass through the midst of it, for his steadfast love endures for ever;"
psa136.v15.number_rsv = "(136:15)"
psa136.v15.number = "(136:15)"
psa136.v15.text = "but overthrew Pharaoh and his host in the Red Sea, for his steadfast love endures for ever;"
psa136.v16.number_rsv = "(136:16)"
psa136.v16.number = "(136:16)"
psa136.v16.text = "to him who led his people through the wilderness, for his steadfast love endures for ever;"
psa136.v17.number_rsv = "(136:17)"
psa136.v17.number = "(136:17)"
psa136.v17.text = "to him who smote great kings, for his steadfast love endures for ever;"
psa136.v18.number_rsv = "(136:18)"
psa136.v18.number = "(136:18)"
psa136.v18.text = "and slew famous kings, for his steadfast love endures for ever;"
psa136.v19.number_rsv = "(136:19)"
psa136.v19.number = "(136:19)"
psa136.v19.text = "Sihon, king of the Amorites, for his steadfast love endures for ever;"
psa136.v20.number_rsv = "(136:20)"
psa136.v20.number = "(136:20)"
psa136.v20.text = "and Og, king of Bashan, for his steadfast love endures for ever;"
psa136.v21.number_rsv = "(136:21)"
psa136.v21.number = "(136:21)"
psa136.v21.text = "and gave their land as a heritage, for his steadfast love endures for ever;"
psa136.v22.number_rsv = "(136:22)"
psa136.v22.number = "(136:22)"
psa136.v22.text = "a heritage to Israel his servant, for his steadfast love endures for ever."
psa136.v23.number_rsv = "(136:23)"
psa136.v23.number = "(136:23)"
psa136.v23.text = "It is he who remembered us in our low estate, for his steadfast love endures for ever;"
psa136.v24.number_rsv = "(136:24)"
psa136.v24.number = "(136:24)"
psa136.v24.text = "and rescued us from our foes, for his steadfast love endures for ever;"
psa136.v25.number_rsv = "(136:25)"
psa136.v25.number = "(136:25)"
psa136.v25.text = "he who gives food to all flesh, for his steadfast love endures for ever."
psa136.v26.number_rsv = "(136:26)"
psa136.v26.number = "(136:26)"
psa136.v26.text = "O give thanks to the God of heaven, for his steadfast love endures for ever."

psa137.title = "Psalm 137"
psa137.version = properties_en_US_rsv.version.designation
psa137.text = ""
psa137.v1.number_rsv = "(137:1)"
psa137.v1.number = "(137:1)"
psa137.v1.text = "By the waters of Babylon, there we sat down and wept, when we remembered Zion."
psa137.v2.number_rsv = "(137:2)"
psa137.v2.number = "(137:2)"
psa137.v2.text = "On the willows therewe hung up our lyres."
psa137.v3.number_rsv = "(137:3)"
psa137.v3.number = "(137:3)"
psa137.v3.text = "For there our captorsrequired of us songs, and our tormentors, mirth, saying,\"Sing us one of the songs of Zion!\""
psa137.v4.number_rsv = "(137:4)"
psa137.v4.number = "(137:4)"
psa137.v4.text = "How shall we sing the LORD\"s songin a foreign land?"
psa137.v5.number_rsv = "(137:5)"
psa137.v5.number = "(137:5)"
psa137.v5.text = "If I forget you, O Jerusalem, let my right hand wither!"
psa137.v6.number_rsv = "(137:6)"
psa137.v6.number = "(137:6)"
psa137.v6.text = "Let my tongue cleave to the roof of my mouth, if I do not remember you, if I do not set Jerusalemabove my highest joy!"
psa137.v7.number_rsv = "(137:7)"
psa137.v7.number = "(137:7)"
psa137.v7.text = "Remember, O LORD, against the E\"domitesthe day of Jerusalem, how they said, \"Rase it, rase it! Down to its foundations!\""
psa137.v8.number_rsv = "(137:8)"
psa137.v8.number = "(137:8)"
psa137.v8.text = "O daughter of Babylon, you devastator! Happy shall he be who requites youwith what you have done to us!"
psa137.v9.number_rsv = "(137:9)"
psa137.v9.number = "(137:9)"
psa137.v9.text = "Happy shall he be who takes your little onesand dashes them against the rock!"

psa138.title = "Psalm 138"
psa138.version = properties_en_US_rsv.version.designation
psa138.text = ""
psa138.v0.number_rsv = "(138:0)"
psa138.v0.number = "(138:0)"
psa138.v0.text = "A Psalm of David."
psa138.v1.number_rsv = "(138:1)"
psa138.v1.number = "(138:1)"
psa138.v1.text = "I give thee thanks, O LORD, with my whole heart; before the gods I sing thy praise;"
psa138.v2.number_rsv = "(138:2)"
psa138.v2.number = "(138:2)"
psa138.v2.text = "I bow down toward thy holy templeand give thanks to thy name for thy steadfast love and thyfaithfulness; for thou hast exalted above everythingthy name and thy word."
psa138.v3.number_rsv = "(138:3)"
psa138.v3.number = "(138:3)"
psa138.v3.text = "On the day I called, thou didst answer me, my strength of soul thou didst increase."
psa138.v4.number_rsv = "(138:4)"
psa138.v4.number = "(138:4)"
psa138.v4.text = "All the kings of the earth shall praise thee, O LORD, for they have heard the words of thy mouth;"
psa138.v5.number_rsv = "(138:5)"
psa138.v5.number = "(138:5)"
psa138.v5.text = "and they shall sing of the ways of the LORD, for great is the glory of the LORD."
psa138.v6.number_rsv = "(138:6)"
psa138.v6.number = "(138:6)"
psa138.v6.text = "For though the LORD is high, he regards the lowly; but the haughty he knows from afar."
psa138.v7.number_rsv = "(138:7)"
psa138.v7.number = "(138:7)"
psa138.v7.text = "Though I walk in the midst of trouble, thou dost preserve my life; thou dost stretch out thy hand against the wrath of my enemies, and thy right hand delivers me."
psa138.v8.number_rsv = "(138:8)"
psa138.v8.number = "(138:8)"
psa138.v8.text = "The LORD will fulfil his purpose for me; thy steadfast love, O LORD, endures for ever.Do not forsake the work of thy hands."

psa139.title = "Psalm 139"
psa139.version = properties_en_US_rsv.version.designation
psa139.text = ""
psa139.v0.number_rsv = "(139:0)"
psa139.v0.number = "(139:0)"
psa139.v0.text = "To the choirmaster. A Psalm of David."
psa139.v1.number_rsv = "(139:1)"
psa139.v1.number = "(139:1)"
psa139.v1.text = "O LORD, thou hast searched me and known me!"
psa139.v2.number_rsv = "(139:2)"
psa139.v2.number = "(139:2)"
psa139.v2.text = "Thou knowest when I sit down and when I rise up; thou discernest my thoughts from afar."
psa139.v3.number_rsv = "(139:3)"
psa139.v3.number = "(139:3)"
psa139.v3.text = "Thou searchest out my path and my lying down, and art acquainted with all my ways."
psa139.v4.number_rsv = "(139:4)"
psa139.v4.number = "(139:4)"
psa139.v4.text = "Even before a word is on my tongue, lo, O LORD, thou knowest it altogether."
psa139.v5.number_rsv = "(139:5)"
psa139.v5.number = "(139:5)"
psa139.v5.text = "Thou dost beset me behind and before, and layest thy hand upon me."
psa139.v6.number_rsv = "(139:6)"
psa139.v6.number = "(139:6)"
psa139.v6.text = "Such knowledge is too wonderful for me; it is high, I cannot attain it."
psa139.v7.number_rsv = "(139:7)"
psa139.v7.number = "(139:7)"
psa139.v7.text = "Whither shall I go from thy Spirit? Or whither shall I flee from thy presence?"
psa139.v8.number_rsv = "(139:8)"
psa139.v8.number = "(139:8)"
psa139.v8.text = "If I ascend to heaven, thou art there! If I make my bed in Sheol, thou art there!"
psa139.v9.number_rsv = "(139:9)"
psa139.v9.number = "(139:9)"
psa139.v9.text = "If I take the wings of the morningand dwell in the uttermost parts of the sea,"
psa139.v10.number_rsv = "(139:10)"
psa139.v10.number = "(139:10)"
psa139.v10.text = "even there thy hand shall lead me, and thy right hand shall hold me."
psa139.v11.number_rsv = "(139:11)"
psa139.v11.number = "(139:11)"
psa139.v11.text = "If I say, \"Let only darkness cover me, and the light about me be night,\""
psa139.v12.number_rsv = "(139:12)"
psa139.v12.number = "(139:12)"
psa139.v12.text = "even the darkness is not dark to thee, the night is bright as the day; for darkness is as light with thee."
psa139.v13.number_rsv = "(139:13)"
psa139.v13.number = "(139:13)"
psa139.v13.text = "For thou didst form my inward parts, thou didst knit me together in my mother\"s womb."
psa139.v14.number_rsv = "(139:14)"
psa139.v14.number = "(139:14)"
psa139.v14.text = "I praise thee, for thou art fearful and wonderful.Wonderful are thy works! Thou knowest me right well;"
psa139.v15.number_rsv = "(139:15)"
psa139.v15.number = "(139:15)"
psa139.v15.text = "my frame was not hidden from thee, when I was being made in secret, intricately wrought in the depths of the earth."
psa139.v16.number_rsv = "(139:16)"
psa139.v16.number = "(139:16)"
psa139.v16.text = "Thy eyes beheld my unformed substance; in thy book were written, every one of them, the days that were formed for me, when as yet there was none of them."
psa139.v17.number_rsv = "(139:17)"
psa139.v17.number = "(139:17)"
psa139.v17.text = "How precious to me are thy thoughts, O God! How vast is the sum of them!"
psa139.v18.number_rsv = "(139:18)"
psa139.v18.number = "(139:18)"
psa139.v18.text = "If I would count them, they are more than the sand.When I awake, I am still with thee."
psa139.v19.number_rsv = "(139:19)"
psa139.v19.number = "(139:19)"
psa139.v19.text = "O that thou wouldst slay the wicked, O God, and that men of blood would depart from me,"
psa139.v20.number_rsv = "(139:20)"
psa139.v20.number = "(139:20)"
psa139.v20.text = "men who maliciously defy thee, who lift themselves up against thee for evil!"
psa139.v21.number_rsv = "(139:21)"
psa139.v21.number = "(139:21)"
psa139.v21.text = "Do I not hate them that hate thee, O LORD? And do I not loathe them that rise up against thee?"
psa139.v22.number_rsv = "(139:22)"
psa139.v22.number = "(139:22)"
psa139.v22.text = "I hate them with perfect hatred; I count them my enemies."
psa139.v23.number_rsv = "(139:23)"
psa139.v23.number = "(139:23)"
psa139.v23.text = "Search me, O God, and know my heart! Try me and know my thoughts!"
psa139.v24.number_rsv = "(139:24)"
psa139.v24.number = "(139:24)"
psa139.v24.text = "And see if there be any wicked way in me, and lead me in the way everlasting!"

psa140.title = "Psalm 140"
psa140.version = properties_en_US_rsv.version.designation
psa140.text = ""
psa140.v0.number_rsv = "(140:0)"
psa140.v0.number = "(140:0)"
psa140.v0.text = "To the choirmaster. A Psalm of David."
psa140.v1.number_rsv = "(140:1)"
psa140.v1.number = "(140:1)"
psa140.v1.text = "Deliver me, O LORD, from evil men; preserve me from violent men,"
psa140.v2.number_rsv = "(140:2)"
psa140.v2.number = "(140:2)"
psa140.v2.text = "who plan evil things in their heart, and stir up wars continually."
psa140.v3.number_rsv = "(140:3)"
psa140.v3.number = "(140:3)"
psa140.v3.text = "They make their tongue sharp as a serpent\"s, and under their lips is the poison of vipers."
psa140.v4.number_rsv = "(140:4)"
psa140.v4.number = "(140:4)"
psa140.v4.text = "Guard me, O LORD, from the hands of the wicked; preserve me from violent men, who have planned to trip up my feet."
psa140.v5.number_rsv = "(140:5)"
psa140.v5.number = "(140:5)"
psa140.v5.text = "Arrogant men have hidden a trap for me, and with cords they have spread a net, by the wayside they have set snares for me."
psa140.v6.number_rsv = "(140:6)"
psa140.v6.number = "(140:6)"
psa140.v6.text = "I say to the LORD, Thou art my God; give ear to the voice of my supplications, O LORD!"
psa140.v7.number_rsv = "(140:7)"
psa140.v7.number = "(140:7)"
psa140.v7.text = "O LORD, my Lord, my strong deliverer, thou hast covered my head in the day of battle."
psa140.v8.number_rsv = "(140:8)"
psa140.v8.number = "(140:8)"
psa140.v8.text = "Grant not, O LORD, the desires of the wicked; do not further his evil plot!"
psa140.v9.number_rsv = "(140:9)"
psa140.v9.number = "(140:9)"
psa140.v9.text = "Those who surround me lift up their head, let the mischief of their lips overwhelm them!"
psa140.v10.number_rsv = "(140:10)"
psa140.v10.number = "(140:10)"
psa140.v10.text = "Let burning coals fall upon them! Let them be cast into pits, no more to rise!"
psa140.v11.number_rsv = "(140:11)"
psa140.v11.number = "(140:11)"
psa140.v11.text = "Let not the slanderer be established in the land; let evil hunt down the violent man speedily!"
psa140.v12.number_rsv = "(140:12)"
psa140.v12.number = "(140:12)"
psa140.v12.text = "I know that the LORD maintains the cause of the afflicted, and executes justice for the needy."
psa140.v13.number_rsv = "(140:13)"
psa140.v13.number = "(140:13)"
psa140.v13.text = "Surely the righteous shall give thanks to thy name; the upright shall dwell in thy presence."

psa141.title = "Psalm 141"
psa141.version = properties_en_US_rsv.version.designation
psa141.text = ""
psa141.v0.number_rsv = "(141:0)"
psa141.v0.number = "(141:0)"
psa141.v0.text = "A Psalm of David."
psa141.v1.number_rsv = "(141:1)"
psa141.v1.number = "(141:1)"
psa141.v1.text = "I call upon thee, O LORD; make haste to me! Give ear to my voice, when I call to thee!"
psa141.v2.number_rsv = "(141:2)"
psa141.v2.number = "(141:2)"
psa141.v2.text = "Let my prayer be counted as incense before thee, and the lifting up of my hands as an evening sacrifice!"
psa141.v3.number_rsv = "(141:3)"
psa141.v3.number = "(141:3)"
psa141.v3.text = "Set a guard over my mouth, O LORD, keep watch over the door of my lips!"
psa141.v4.number_rsv = "(141:4)"
psa141.v4.number = "(141:4)"
psa141.v4.text = "Incline not my heart to any evil, to busy myself with wicked deedsin company with men who work iniquity; and let me not eat of their dainties!"
psa141.v5.number_rsv = "(141:5)"
psa141.v5.number = "(141:5)"
psa141.v5.text = "Let a good man strike or rebuke me in kindness, but let the oil of the wicked never anoint my head; for my prayer is continually against their wicked deeds."
psa141.v6.number_rsv = "(141:6)"
psa141.v6.number = "(141:6)"
psa141.v6.text = "When they are given over to those who shall condemn them, then they shall learn that the word of the LORD is true."
psa141.v7.number_rsv = "(141:7)"
psa141.v7.number = "(141:7)"
psa141.v7.text = "As a rock which one cleaves and shatters on the land, so shall their bones be strewn at the mouth of Sheol."
psa141.v8.number_rsv = "(141:8)"
psa141.v8.number = "(141:8)"
psa141.v8.text = "But my eyes are toward thee, O LORD God; in thee I seek refuge; leave me not defenseless!"
psa141.v9.number_rsv = "(141:9)"
psa141.v9.number = "(141:9)"
psa141.v9.text = "Keep me from the trap which they have laid for me, and from the snares of evildoers!"
psa141.v10.number_rsv = "(141:10)"
psa141.v10.number = "(141:10)"
psa141.v10.text = "Let the wicked together fall into their own nets, while I escape.R"

psa142.title = "Psalm 142"
psa142.version = properties_en_US_rsv.version.designation
psa142.text = ""
psa142.v0.number_rsv = "(142:0)"
psa142.v0.number = "(142:0)"
psa142.v0.text = "A Maskil of David, when he was in the cave. A Prayer."
psa142.v1.number_rsv = "(142:1)"
psa142.v1.number = "(142:1)"
psa142.v1.text = "I cry with my voice to the LORD, with my voice I make supplication to the LORD,"
psa142.v2.number_rsv = "(142:2)"
psa142.v2.number = "(142:2)"
psa142.v2.text = "I pour out my complaint before him, I tell my trouble before him."
psa142.v3.number_rsv = "(142:3)"
psa142.v3.number = "(142:3)"
psa142.v3.text = "When my spirit is faint, thou knowest my way! In the path where I walkthey have hidden a trap for me."
psa142.v4.number_rsv = "(142:4)"
psa142.v4.number = "(142:4)"
psa142.v4.text = "I look to the right and watch, but there is none who takes notice of me; no refuge remains to me, no man cares for me."
psa142.v5.number_rsv = "(142:5)"
psa142.v5.number = "(142:5)"
psa142.v5.text = "I cry to thee, O LORD; I say, Thou art my refuge, my portion in the land of the living."
psa142.v6.number_rsv = "(142:6)"
psa142.v6.number = "(142:6)"
psa142.v6.text = "Give heed to my cry; for I am brought very low! Deliver me from my persecutors; for they are too strong for me!"
psa142.v7.number_rsv = "(142:7)"
psa142.v7.number = "(142:7)"
psa142.v7.text = "Bring me out of prison, that I may give thanks to thy name! The righteous will surround me; for thou wilt deal bountifully with me."

psa143.title = "Psalm 143"
psa143.version = properties_en_US_rsv.version.designation
psa143.text = ""
psa143.v0.number_rsv = "(143:0)"
psa143.v0.number = "(143:0)"
psa143.v0.text = "A Psalm of David."
psa143.v1.number_rsv = "(143:1)"
psa143.v1.number = "(143:1)"
psa143.v1.text = "Hear my prayer, O LORD; give ear to my supplications! In thy faithfulness answer me, in thy righteousness!"
psa143.v2.number_rsv = "(143:2)"
psa143.v2.number = "(143:2)"
psa143.v2.text = "Enter not into judgment with thy servant; for no man living is righteous before thee."
psa143.v3.number_rsv = "(143:3)"
psa143.v3.number = "(143:3)"
psa143.v3.text = "For the enemy has pursued me; he has crushed my life to the ground; he has made me sit in darkness like those long dead."
psa143.v4.number_rsv = "(143:4)"
psa143.v4.number = "(143:4)"
psa143.v4.text = "Therefore my spirit faints within me; my heart within me is appalled."
psa143.v5.number_rsv = "(143:5)"
psa143.v5.number = "(143:5)"
psa143.v5.text = "I remember the days of old, I meditate on all that thou hast done; I muse on what thy hands have wrought."
psa143.v6.number_rsv = "(143:6)"
psa143.v6.number = "(143:6)"
psa143.v6.text = "I stretch out my hands to thee; my soul thirsts for thee like a parched land."
psa143.v7.number_rsv = "(143:7)"
psa143.v7.number = "(143:7)"
psa143.v7.text = "Make haste to answer me, O LORD! My spirit fails! Hide not thy face from me, lest I be like those who go down to the Pit."
psa143.v8.number_rsv = "(143:8)"
psa143.v8.number = "(143:8)"
psa143.v8.text = "Let me hear in the morning of thy steadfast love, for in thee I put my trust.Teach me the way I should go, for to thee I lift up my soul."
psa143.v9.number_rsv = "(143:9)"
psa143.v9.number = "(143:9)"
psa143.v9.text = "Deliver me, O LORD, from my enemies! I have fled to thee for refuge!"
psa143.v10.number_rsv = "(143:10)"
psa143.v10.number = "(143:10)"
psa143.v10.text = "Teach me to do thy will, for thou art my God! Let thy good spirit lead meon a level path!"
psa143.v11.number_rsv = "(143:11)"
psa143.v11.number = "(143:11)"
psa143.v11.text = "For thy name\"s sake, O LORD, preserve my life! In thy righteousness bring me out of trouble!"
psa143.v12.number_rsv = "(143:12)"
psa143.v12.number = "(143:12)"
psa143.v12.text = "And in thy steadfast love cut off my enemies, and destroy all my adversaries, for I am thy servant."

kathisma_20.title = "KATHISMA 20."

psa144.title = "Psalm 144"
psa144.version = properties_en_US_rsv.version.designation
psa144.text = ""
psa144.v0.number_rsv = "(144:0)"
psa144.v0.number = "(144:0)"
psa144.v0.text = "A Psalm of David."
psa144.v1.number_rsv = "(144:1)"
psa144.v1.number = "(144:1)"
psa144.v1.text = "Blessed be the LORD, my rock, who trains my hands for war, and my fingers for battle;"
psa144.v2.number_rsv = "(144:2)"
psa144.v2.number = "(144:2)"
psa144.v2.text = "my rock and my fortress, my stronghold and my deliverer, my shield and he in whom I take refuge, who subdues the peoples under him."
psa144.v3.number_rsv = "(144:3)"
psa144.v3.number = "(144:3)"
psa144.v3.text = "O LORD, what is man that thou dost regard him, or the son of man that thou dost think of him?"
psa144.v4.number_rsv = "(144:4)"
psa144.v4.number = "(144:4)"
psa144.v4.text = "Man is like a breath, his days are like a passing shadow."
psa144.v5.number_rsv = "(144:5)"
psa144.v5.number = "(144:5)"
psa144.v5.text = "Bow thy heavens, O LORD, and come down! Touch the mountains that they smoke!"
psa144.v6.number_rsv = "(144:6)"
psa144.v6.number = "(144:6)"
psa144.v6.text = "Flash forth the lightning and scatter them, send out thy arrows and rout them!"
psa144.v7.number_rsv = "(144:7)"
psa144.v7.number = "(144:7)"
psa144.v7.text = "Stretch forth thy hand from on high, rescue me and deliver me from the many waters, from the hand of aliens,"
psa144.v8.number_rsv = "(144:8)"
psa144.v8.number = "(144:8)"
psa144.v8.text = "whose mouths speak lies, and whose right hand is a right hand of falsehood."
psa144.v9.number_rsv = "(144:9)"
psa144.v9.number = "(144:9)"
psa144.v9.text = "I will sing a new song to thee, O God; upon a ten-stringed harp I will play to thee,"
psa144.v10.number_rsv = "(144:10)"
psa144.v10.number = "(144:10)"
psa144.v10.text = "who givest victory to kings, who rescuest David thy servant."
psa144.v11.number_rsv = "(144:11)"
psa144.v11.number = "(144:11)"
psa144.v11.text = "Rescue me from the cruel sword, and deliver me from the hand of aliens, whose mouths speak lies, and whose right hand is a right hand of falsehood."
psa144.v12.number_rsv = "(144:12)"
psa144.v12.number = "(144:12)"
psa144.v12.text = "May our sons in their youthbe like plants full grown, our daughters like corner pillarscut for the structure of a palace;"
psa144.v13.number_rsv = "(144:13)"
psa144.v13.number = "(144:13)"
psa144.v13.text = "may our garners be full, providing all manner of store; may our sheep bring forth thousandsand ten thousands in our fields;"
psa144.v14.number_rsv = "(144:14)"
psa144.v14.number = "(144:14)"
psa144.v14.text = "may our cattle be heavy with young, suffering no mischance or failure in bearing; may there be no cry of distress in our streets!"
psa144.v15.number_rsv = "(144:15)"
psa144.v15.number = "(144:15)"
psa144.v15.text = "Happy the people to whom such blessings fall! Happy the people whose God is the LORD!"

psa145.title = "Psalm 145"
psa145.version = properties_en_US_rsv.version.designation
psa145.text = ""
psa145.v0.number_rsv = "(145:0)"
psa145.v0.number = "(145:0)"
psa145.v0.text = "A Song of Praise. Of David."
psa145.v1.number_rsv = "(145:1)"
psa145.v1.number = "(145:1)"
psa145.v1.text = "I will extol thee, my God and King, and bless thy name for ever and ever."
psa145.v2.number_rsv = "(145:2)"
psa145.v2.number = "(145:2)"
psa145.v2.text = "Every day I will bless thee, and praise thy name for ever and ever."
psa145.v3.number_rsv = "(145:3)"
psa145.v3.number = "(145:3)"
psa145.v3.text = "Great is the LORD, and greatly to be praised, and his greatness is unsearchable."
psa145.v4.number_rsv = "(145:4)"
psa145.v4.number = "(145:4)"
psa145.v4.text = "One generation shall laud thy works to another, and shall declare thy mighty acts."
psa145.v5.number_rsv = "(145:5)"
psa145.v5.number = "(145:5)"
psa145.v5.text = "On the glorious splendor of thy majesty, and on thy wondrous works, I will meditate."
psa145.v6.number_rsv = "(145:6)"
psa145.v6.number = "(145:6)"
psa145.v6.text = "Men shall proclaim the might of thy terrible acts, and I will declare thy greatness."
psa145.v7.number_rsv = "(145:7)"
psa145.v7.number = "(145:7)"
psa145.v7.text = "They shall pour forth the fame of thy abundant goodness, and shall sing aloud of thy righteousness."
psa145.v8.number_rsv = "(145:8)"
psa145.v8.number = "(145:8)"
psa145.v8.text = "The LORD is gracious and merciful, slow to anger and abounding in steadfast love."
psa145.v9.number_rsv = "(145:9)"
psa145.v9.number = "(145:9)"
psa145.v9.text = "The LORD is good to all, and his compassion is over all that he has made."
psa145.v10.number_rsv = "(145:10)"
psa145.v10.number = "(145:10)"
psa145.v10.text = "All thy works shall give thanks to thee, O LORD, and all thy saints shall bless thee!"
psa145.v11.number_rsv = "(145:11)"
psa145.v11.number = "(145:11)"
psa145.v11.text = "They shall speak of the glory of thy kingdom, and tell of thy power,"
psa145.v12.number_rsv = "(145:12)"
psa145.v12.number = "(145:12)"
psa145.v12.text = "to make known to the sons of men thy mighty deeds, and the glorious splendor of thy kingdom."
psa145.v13.number_rsv = "(145:13)"
psa145.v13.number = "(145:13)"
psa145.v13.text = "Thy kingdom is an everlasting kingdom, and thy dominion endures throughout all generations.The LORD is faithful in all his words, and gracious in all his deeds."
psa145.v14.number_rsv = "(145:14)"
psa145.v14.number = "(145:14)"
psa145.v14.text = "The LORD upholds all who are falling, and raises up all who are bowed down."
psa145.v15.number_rsv = "(145:15)"
psa145.v15.number = "(145:15)"
psa145.v15.text = "The eyes of all look to thee, and thou givest them their food in due season."
psa145.v16.number_rsv = "(145:16)"
psa145.v16.number = "(145:16)"
psa145.v16.text = "Thou openest thy hand, thou satisfiest the desire of every living thing."
psa145.v17.number_rsv = "(145:17)"
psa145.v17.number = "(145:17)"
psa145.v17.text = "The LORD is just in all his ways, and kind in all his doings."
psa145.v18.number_rsv = "(145:18)"
psa145.v18.number = "(145:18)"
psa145.v18.text = "The LORD is near to all who call upon him, to all who call upon him in truth."
psa145.v19.number_rsv = "(145:19)"
psa145.v19.number = "(145:19)"
psa145.v19.text = "He fulfils the desire of all who fear him, he also hears their cry, and saves them."
psa145.v20.number_rsv = "(145:20)"
psa145.v20.number = "(145:20)"
psa145.v20.text = "The LORD preserves all who love him; but all the wicked he will destroy."
psa145.v21.number_rsv = "(145:21)"
psa145.v21.number = "(145:21)"
psa145.v21.text = "My mouth will speak the praise of the LORD, and let all flesh bless his holy name for ever and ever."

psa146.title = "Psalm 146"
psa146.version = properties_en_US_rsv.version.designation
psa146.text = ""
psa146.v1.number_rsv = "(146:1)"
psa146.v1.number = "(146:1)"
psa146.v1.text = "Praise the LORD! Praise the LORD, O my soul!"
psa146.v2.number_rsv = "(146:2)"
psa146.v2.number = "(146:2)"
psa146.v2.text = "I will praise the LORD as long as I live; I will sing praises to my God while I have being."
psa146.v3.number_rsv = "(146:3)"
psa146.v3.number = "(146:3)"
psa146.v3.text = "Put not your trust in princes, in a son of man, in whom there is no help."
psa146.v4.number_rsv = "(146:4)"
psa146.v4.number = "(146:4)"
psa146.v4.text = "When his breath departs he returns to his earth; on that very day his plans perish."
psa146.v5.number_rsv = "(146:5)"
psa146.v5.number = "(146:5)"
psa146.v5.text = "Happy is he whose help is the God of Jacob, whose hope is in the LORD his God,"
psa146.v6.number_rsv = "(146:6)"
psa146.v6.number = "(146:6)"
psa146.v6.text = "who made heaven and earth, the sea, and all that is in them; who keeps faith for ever;"
psa146.v7.number_rsv = "(146:7)"
psa146.v7.number = "(146:7)"
psa146.v7.text = "who executes justice for the oppressed; who gives food to the hungry.The LORD sets the prisoners free;"
psa146.v8.number_rsv = "(146:8)"
psa146.v8.number = "(146:8)"
psa146.v8.text = "the LORD opens the eyes of the blind.The LORD lifts up those who are bowed down; the LORD loves the righteous."
psa146.v9.number_rsv = "(146:9)"
psa146.v9.number = "(146:9)"
psa146.v9.text = "The LORD watches over the sojourners, he upholds the widow and the fatherless; but the way of the wicked he brings to ruin."
psa146.v10.number_rsv = "(146:10)"
psa146.v10.number = "(146:10)"
psa146.v10.text = "The LORD will reign for ever, thy God, O Zion, to all generations.Praise the LORD!"

psa147.title = "Psalm 147"
psa147.version = properties_en_US_rsv.version.designation
psa147.text = ""
psa147.v1.number_rsv = "(147:1)"
psa147.v1.number = "(147:1)"
psa147.v1.text = "Praise the LORD! For it is good to sing praises to our God; for he is gracious, and a song of praise is seemly."
psa147.v2.number_rsv = "(147:2)"
psa147.v2.number = "(147:2)"
psa147.v2.text = "The LORD builds up Jerusalem; he gathers the outcasts of Israel."
psa147.v3.number_rsv = "(147:3)"
psa147.v3.number = "(147:3)"
psa147.v3.text = "He heals the brokenhearted, and binds up their wounds."
psa147.v4.number_rsv = "(147:4)"
psa147.v4.number = "(147:4)"
psa147.v4.text = "He determines the number of the stars, he gives to all of them their names."
psa147.v5.number_rsv = "(147:5)"
psa147.v5.number = "(147:5)"
psa147.v5.text = "Great is our LORD, and abundant in power; his understanding is beyond measure."
psa147.v6.number_rsv = "(147:6)"
psa147.v6.number = "(147:6)"
psa147.v6.text = "The LORD lifts up the downtrodden, he casts the wicked to the ground."
psa147.v7.number_rsv = "(147:7)"
psa147.v7.number = "(147:7)"
psa147.v7.text = "Sing to the LORD with thanksgiving; make melody to our God upon the lyre!"
psa147.v8.number_rsv = "(147:8)"
psa147.v8.number = "(147:8)"
psa147.v8.text = "He covers the heavens with clouds, he prepares rain for the earth, he makes grass grow upon the hills."
psa147.v9.number_rsv = "(147:9)"
psa147.v9.number = "(147:9)"
psa147.v9.text = "He gives to the beasts their food, and to the young ravens which cry."
psa147.v10.number_rsv = "(147:10)"
psa147.v10.number = "(147:10)"
psa147.v10.text = "His delight is not in the strength of the horse, nor his pleasure in the legs of a man;"
psa147.v11.number_rsv = "(147:11)"
psa147.v11.number = "(147:11)"
psa147.v11.text = "but the LORD takes pleasure in those who fear him, in those who hope in his steadfast love."
psa147.v12.number_rsv = "(147:12)"
psa147.v12.number = "(147:12)"
psa147.v12.text = "Praise the LORD, O Jerusalem! Praise your God, O Zion!"
psa147.v13.number_rsv = "(147:13)"
psa147.v13.number = "(147:13)"
psa147.v13.text = "For he strengthens the bars of your gates; he blesses your sons within you."
psa147.v14.number_rsv = "(147:14)"
psa147.v14.number = "(147:14)"
psa147.v14.text = "He makes peace in your borders; he fills you with the finest of the wheat."
psa147.v15.number_rsv = "(147:15)"
psa147.v15.number = "(147:15)"
psa147.v15.text = "He sends forth his command to the earth; his word runs swiftly."
psa147.v16.number_rsv = "(147:16)"
psa147.v16.number = "(147:16)"
psa147.v16.text = "He gives snow like wool; he scatters hoarfrost like ashes."
psa147.v17.number_rsv = "(147:17)"
psa147.v17.number = "(147:17)"
psa147.v17.text = "He casts forth his ice like morsels; who can stand before his cold?"
psa147.v18.number_rsv = "(147:18)"
psa147.v18.number = "(147:18)"
psa147.v18.text = "He sends forth his word, and melts them; he makes his wind blow, and the waters flow."
psa147.v19.number_rsv = "(147:19)"
psa147.v19.number = "(147:19)"
psa147.v19.text = "He declares his word to Jacob, his statutes and ordinances to Israel."
psa147.v20.number_rsv = "(147:20)"
psa147.v20.number = "(147:20)"
psa147.v20.text = "He has not dealt thus with any other nation; they do not know his ordinances.Praise the LORD!"

psa148.title = "Psalm 148"
psa148.version = properties_en_US_rsv.version.designation
psa148.text = ""
psa148.v1.number_rsv = "(148:1)"
psa148.v1.number = "(148:1)"
psa148.v1.text = "Praise the LORD! Praise the LORD from the heavens, praise him in the heights!"
psa148.v2.number_rsv = "(148:2)"
psa148.v2.number = "(148:2)"
psa148.v2.text = "Praise him, all his angels, praise him, all his host!"
psa148.v3.number_rsv = "(148:3)"
psa148.v3.number = "(148:3)"
psa148.v3.text = "Praise him, sun and moon, praise him, all you shining stars!"
psa148.v4.number_rsv = "(148:4)"
psa148.v4.number = "(148:4)"
psa148.v4.text = "Praise him, you highest heavens, and you waters above the heavens!"
psa148.v5.number_rsv = "(148:5)"
psa148.v5.number = "(148:5)"
psa148.v5.text = "Let them praise the name of the LORD! For he commanded and they were created."
psa148.v6.number_rsv = "(148:6)"
psa148.v6.number = "(148:6)"
psa148.v6.text = "And he established them for ever and ever; he fixed their bounds which cannot be passed."
psa148.v7.number_rsv = "(148:7)"
psa148.v7.number = "(148:7)"
psa148.v7.text = "Praise the LORD from the earth, you sea monsters and all deeps,"
psa148.v8.number_rsv = "(148:8)"
psa148.v8.number = "(148:8)"
psa148.v8.text = "fire and hail, snow and frost, stormy wind fulfilling his command!"
psa148.v9.number_rsv = "(148:9)"
psa148.v9.number = "(148:9)"
psa148.v9.text = "Mountains and all hills, fruit trees and all cedars!"
psa148.v10.number_rsv = "(148:10)"
psa148.v10.number = "(148:10)"
psa148.v10.text = "Beasts and all cattle, creeping things and flying birds!"
psa148.v11.number_rsv = "(148:11)"
psa148.v11.number = "(148:11)"
psa148.v11.text = "Kings of the earth and all peoples, princes and all rulers of the earth!"
psa148.v12.number_rsv = "(148:12)"
psa148.v12.number = "(148:12)"
psa148.v12.text = "Young men and maidens together, old men and children!"
psa148.v13.number_rsv = "(148:13)"
psa148.v13.number = "(148:13)"
psa148.v13.text = "Let them praise the name of the LORD, for his name alone is exalted; his glory is above earth and heaven."
psa148.v14.number_rsv = "(148:14)"
psa148.v14.number = "(148:14)"
psa148.v14.text = "He has raised up a horn for his people, praise for all his saints, for the people of Israel who are near to him.Praise the LORD!"

psa149.title = "Psalm 149"
psa149.version = properties_en_US_rsv.version.designation
psa149.text = ""
psa149.v1.number_rsv = "(149:1)"
psa149.v1.number = "(149:1)"
psa149.v1.text = "Praise the LORD! Sing to the LORD a new song, his praise in the assembly of the faithful!"
psa149.v2.number_rsv = "(149:2)"
psa149.v2.number = "(149:2)"
psa149.v2.text = "Let Israel be glad in his Maker, let the sons of Zion rejoice in their King!"
psa149.v3.number_rsv = "(149:3)"
psa149.v3.number = "(149:3)"
psa149.v3.text = "Let them praise his name with dancing, making melody to him with timbrel and lyre!"
psa149.v4.number_rsv = "(149:4)"
psa149.v4.number = "(149:4)"
psa149.v4.text = "For the LORD takes pleasure in his people; he adorns the humble with victory."
psa149.v5.number_rsv = "(149:5)"
psa149.v5.number = "(149:5)"
psa149.v5.text = "Let the faithful exult in glory; let them sing for joy on their couches."
psa149.v6.number_rsv = "(149:6)"
psa149.v6.number = "(149:6)"
psa149.v6.text = "Let the high praises of God be in their throatsand two-edged swords in their hands,"
psa149.v7.number_rsv = "(149:7)"
psa149.v7.number = "(149:7)"
psa149.v7.text = "to wreak vengeance on the nationsand chastisement on the peoples,"
psa149.v8.number_rsv = "(149:8)"
psa149.v8.number = "(149:8)"
psa149.v8.text = "to bind their kings with chainsand their nobles with fetters of iron,"
psa149.v9.number_rsv = "(149:9)"
psa149.v9.number = "(149:9)"
psa149.v9.text = "to execute on them the judgment written! This is glory for all his faithful ones.Praise the LORD!"

psa150.title = "Psalm 150"
psa150.version = properties_en_US_rsv.version.designation
psa150.text = ""
psa150.v1.number_rsv = "(150:1)"
psa150.v1.number = "(150:1)"
psa150.v1.text = "Praise the LORD! Praise God in his sanctuary; praise him in his mighty firmament!"
psa150.v2.number_rsv = "(150:2)"
psa150.v2.number = "(150:2)"
psa150.v2.text = "Praise him for his mighty deeds; praise him according to his exceeding greatness!"
psa150.v3.number_rsv = "(150:3)"
psa150.v3.number = "(150:3)"
psa150.v3.text = "Praise him with trumpet sound; praise him with lute and harp!"
psa150.v4.number_rsv = "(150:4)"
psa150.v4.number = "(150:4)"
psa150.v4.text = "Praise him with timbrel and dance; praise him with strings and pipe!"
psa150.v5.number_rsv = "(150:5)"
psa150.v5.number = "(150:5)"
psa150.v5.text = "Praise him with sounding cymbals; praise him with loud clashing cymbals!"
psa150.v6.number_rsv = "(150:6)"
psa150.v6.number = "(150:6)"
psa150.v6.text = "Let everything that breathes praise the LORD! Praise the LORD!"

