S01.commemoration.text = "Τῇ Ϛʹ τοῦ αὐτοῦ μηνός ἀνάμνησις τῆς θείας Μεταμορφώσεως τοῦ Κυρίου καὶ Θεοῦ καὶ Σωτῆρος ἡμῶν Ἰησοῦ Χριστοῦ."
S01.couplet1.title = 
S01.couplet1.verse1 = "Θαβὼρ ὑπὲρ πᾶν γῆς ἐδοξάσθη μέρος,"
S01.couplet1.verse2 = "Ἰδὸν Θεοῦ λάμψασιν ἐν δόξῃ φύσιν."
S01.single1.title = 
S01.single1.text ="Μορφὴν ἀνδρομέην κατὰ ἕκτην Χριστὸς ἀμεῖψε."
S01.narration1.text = ""

S99.lastline.text = "Αὐτῷ ἡ δόξα καὶ τὸ κράτος εἰς τοὺς αἰῶνας. Ἀμήν."
