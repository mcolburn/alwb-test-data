S01.commemoration.text = "Τῇ Βʹ τοῦ αὐτοῦ μηνὸς μνήμη τοῦ ἁγίου Ἱερομάρτυρος Κυπριανοῦ καὶ Ἰουστίνης τῆς Παρθένου."
S01.couplet1.title = 
S01.couplet1.verse1 = "Ἀλγεῖ Σατανᾶς, τὸν πάλαι φίλον βλέπων"
S01.couplet1.verse2 = "Ξίφει φιλοῦντα συνθανεῖν Ἰουστίνῃ."
S01.single1.title = 
S01.single1.text ="Τμήθη δευτερίῃ σὺν Ἰουστίνῃ Κυπριανός."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ μνήμη τοῦ ὁσίου Πατρὸς ἡμῶν Θεοφίλου τοῦ Ὁμολογητοῦ."

S03.commemoration.text = "Ὁ ἅγιος Νεομάρτυς Γεώργιος, ὁ εἰς τὸ Καρατζασοῦ μαρτυρήσας ἐν ἔτει 1794, ξίφει τελειοῦται."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."