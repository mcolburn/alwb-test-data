S01.commemoration.text = "Τῇ ΚΓʹ τοῦ αὐτοῦ μηνὸς μνήμη τοῦ ἁγίου Ἱερομάρτυρος καὶ Ἀποστόλου Ἰακώβου τοῦ Ἀδελφοθέου."
S01.couplet1.title = 
S01.couplet1.verse1 = "Κληθεὶς ἀδελφὸς τοῦ κατακρίτου ξύλῳ,"
S01.couplet1.verse2 = "Θνῇσκεις δι̉ αὐτόν, παμμάκαρ, κρουσθεὶς ξύλῳ."
S01.single1.title = 
S01.single1.text ="Ἐσθλὸν Ἀδελφόθεον τριτάτῃ ξύλῳ εἰκάδι πλῆξαν."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ μνήμη τοῦ ὁσίου Πατρὸς ἡμῶν Ἰγνατίου, Πατριάρχου Κωνσταντινουπόλεως."

S03.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ μνήμη τοῦ ὁσίου Πατρὸς ἡμῶν Μακαρίου τοῦ Ῥωμαίου."

S04.commemoration.text = "Ὁ ὅσιος Πατὴρ ἡμῶν Νικηφόρος, ὁ ἱδρυτὴς τῆς ἐν Χαρσιανῷ Μονῆς, ἐν εἰρήνῃ τελειοῦται."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."