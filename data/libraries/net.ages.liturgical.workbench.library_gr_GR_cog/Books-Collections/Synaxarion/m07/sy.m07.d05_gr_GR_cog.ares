S01.commemoration.text = "Τῇ Εʹ τοῦ αὐτοῦ μηνός, μνήμη τοῦ Ὁσίου καὶ θεοφόρου Πατρὸς ἡμῶν Ἀθανασίου τοῦ ἐν τῷ Ἄθῳ."
S01.couplet1.title = 
S01.couplet1.verse1 = "Μέγας μὲν Ἀντώνιος, ἀρχὴ Πατέρων. "
S01.couplet1.verse2 = "Θεῖος δ' Ἀθανάσιος ἔνθεον τέλος. "
S01.couplet2.title = 
S01.couplet2.verse1 = "Κἂν Ἀθανάσιος ὕστερος χρόνοις,"
S01.couplet2.verse2 = "Ἀλλ' ὑπερέσχε καὶ παλαιοὺς τοῖς πόνοις"
S01.couplet3.title = 
S01.couplet3.verse1 = "Ἀθανασίῳ καὶ μαθητῶν ἑξάδι,"
S01.couplet3.verse2 = "Ναοὶ λύονται σωμάτων ναοῦ λύσει."
S01.single1.title = 
S01.single1.text = "Πέμπτῃ δ' Ἀθανάσιον ἆγον νόες ἄστυ θεοῖο."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Λαμπαδοῦ."
S02.couplet1.title = 
S02.couplet1.verse1 = "Ἄρδων ἐλαίῳ Λαμπαδὸς τὴν λαμπάδα "
S02.couplet1.verse2 = "Ἕτοιμος ἐγγίσαντος ἦν τοῦ Νυμφίου."
S02.narration1.text = ""

S03.commemoration.text = "Ὁ Ἅγιος Ὁσιομάρτυρος Κυπριανὸς ὁ νέος, ὁ ἐν Κωνσταντινουπόλει μαρτυρήσας ἐν ἔτει ᾳχοθʹ (1679), ξίφει τελειοῦται."
S03.couplet1.title = 
S03.couplet1.verse1 = "Ἡ Κυπριανὸς ἀγάπῃ πεφλεγμένος,"
S03.couplet1.verse2 = "Ἡγεῖτο πάντως ὡς τρυφὰς τὰς βασάνους."
S03.narration1.text = ""

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."