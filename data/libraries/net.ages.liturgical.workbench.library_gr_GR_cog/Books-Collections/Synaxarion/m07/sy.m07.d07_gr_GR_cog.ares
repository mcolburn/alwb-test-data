S01.commemoration.text = "Τῇ Ζʹ τοῦ αὐτοῦ μηνός, Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Θωμᾶ τοῦ ἐν τῷ Μαλεῷ."
S01.couplet1.title = 
S01.couplet1.verse1 = "Διεὶς πτέρυγας, εἶπεν ἂν Μωσῆς, Πάτερ, "
S01.couplet1.verse2 = "Ὡς ἀετὸς τις ἐξανέπτης πρὸς πόλον. "
S01.single1.title = 
S01.single1.text ="Ἑβδομάτῃ Θωμᾶν θάνατος μέλας ἔμφρονα εἷλεν."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τῆς Ἁγίας Μάρτυρος Κυριακῆς."
S02.couplet1.title = 
S02.couplet1.verse1 = "Κυριακὴ θανοῦσα τὴν τομὴν φθάνει "
S02.couplet1.verse2 = "Προαιρέσει, πλὴν καὶ τελειοῦται ξίφει."

S03.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τῶν Ἁγίων Μαρτύρων Περεγρίνου καὶ τῶν σὺν αὐτῷ, Λουκιανοῦ, Πομπηΐου, Ἡσυχίου, Παππίου, Σατορνίνου καὶ Γερμανοῦ."

S04.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τῶν Ἁγίων Μαρτύρων Εὐσταθίου, Πολυκάρπου καὶ Εὐαγγέλου."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."