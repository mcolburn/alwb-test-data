S01.commemoration.text = "Τῇ ΙΓʹ τοῦ αὐτοῦ μηνός, ἡ Σύναξις τελεῖται τοῦ Ἀρχαγγέλου Γαβριήλ."
S01.couplet1.title = 
S01.couplet1.verse1 = "Τῶν σῶν ἀγαθῶν ὥσπερ οὐκ ἔχω κόρον, "
S01.couplet1.verse2 = "Ὡς οὐδ' ἑορτῶν, Γαβριὴλ Ἄρχων Νόων. "
S01.single1.title = 
S01.single1.text ="Τῇ δεκάτῃ δὲ τρίτῃ συναγήοχεν ὧδε Γαβριήλ."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Στεφάνου τοῦ Σαββαΐτου."

S03.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τῆς Ἁγίας Μάρτυρος Γολινδούχ, τῆς μετονομασθείσης Μαρίας."
S03.couplet1.title = 
S03.couplet1.verse1 = "Σκηνοῖς Γολινδοὺχ εἰς Ἐδέμ, σκηναὶ δέ σοι, "
S03.couplet1.verse2 = "Τὸ τοῦ Βαλαάμ, ὡς σκιάζουσαι νάπαι."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."