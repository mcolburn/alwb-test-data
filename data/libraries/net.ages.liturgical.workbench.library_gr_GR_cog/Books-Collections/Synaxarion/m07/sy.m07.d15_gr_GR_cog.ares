S01.commemoration.text = "Τῇ ΙΕʹ τοῦ αὐτοῦ μηνός, μνήμη τῶν Ἁγίων Μαρτύρων Κηρύκου καὶ Ἰουλίττης."
S01.couplet1.title = 
S01.couplet1.verse1 = "Ἰουλίττα σύναθλος υἱῷ Κηρύκῳ, "
S01.couplet1.verse2 = "Ἡ λαιμότμητος τῷ κάραν τεθλασμένῳ, Πέμπτῃ. "
S01.single1.title = 
S01.single1.text ="Ἰουλίτταν δεκάτῃ τάμον, υἷα δ' ἔαξαν."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τοῦ Ἁγίου Μάρτυρος Λολλιανοῦ."
S02.couplet1.title = 
S02.couplet1.verse1 = "Ποσὶ προθύμοις ἔδραμε τρίβον τέλους, "
S02.couplet1.verse2 = "Ὁ Λολλιανός, οὗ ποσίκρουστον τέλους."

S03.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τοῦ Ἁγίου Μάρτυρος Ἀβουδίμου."
S03.couplet1.title = 
S03.couplet1.verse1 = "Ἀβουδίμῳ μάχαιρα πρόξενος τέλους, "
S03.couplet1.verse2 = "Αὕτη δὲ τούτῳ πρόξενος καὶ τοῦ στέφους."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."