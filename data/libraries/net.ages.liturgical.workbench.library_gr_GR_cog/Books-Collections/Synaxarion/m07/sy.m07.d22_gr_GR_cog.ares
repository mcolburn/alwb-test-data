S01.commemoration.text = "Τῇ ΚΒʹ τοῦ αὐτοῦ μηνός, μνήμη τῆς Ἁγίας Μυροφόρου καὶ Ἰσαποστόλου Μαρίας τῆς Μαγδαληνῆς."
S01.couplet1.title = 
S01.couplet1.verse1 = "Ἀφαῖς ἀΰλοις ἅπτεταί σου, Χριστέ μου, "
S01.couplet1.verse2 = "Μή μου, πρὸς ἣν ἔφησας, ἅπτου, Μαρία. "
S01.single1.title = 
S01.single1.text ="Δευτερίῃ Μαρίη μύσεν εἰκάδι Μαγδαληνή."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τῆς Ἁγίας Ὁσιομάρτυρος Μαρκέλλης."
S02.couplet1.title = 
S02.couplet1.verse1 = "Κἂν ἀγνοῶμεν πάντες ἡμεῖς, Μαρκέλλα,"
S02.couplet1.verse2 = "Ἀθλήσεως σῆς, Χριστὸς οἶδε τὸν τρόπον."
S02.couplet2.verse1 = "Πότμον ὑπὲστη ἀδικοθύτου Σῶτερ,"
S02.couplet2.verse2 = "Ἐξ ἀδίκου φεύγουσα μιγῆς πατρώας."
S02.couplet2.verse3 = "Τοῦ Δαβὶδ ἀκούσασα, Σωτήρ μου, λόγον,"
S02.couplet2.verse4 = "Ἀδικίαν φάσκοντα ἐβδελυξάμην,"
S02.couplet2.verse5 = "Ἐις νοῦν δὲ τὸν Λὼτ θυγατρογάμον φέρω."
S02.couplet2.verse6 = "Οἷα πέπονθε πράττων ἀισχύνης δρᾶμα."
S02.couplet2.verse7 = "Βέλει τέτρωμαι ἐκ πατρὸς μιαιφόνου."

S02.single1.text = ""

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."