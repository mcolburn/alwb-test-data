S01.commemoration.text = "Τῇ Κʹ τοῦ αὐτοῦ μηνός, μνήμη τῆς εἰς οὐρανοὺς πυρφόρου ἀναβάσεως τοῦ Ἁγίου ἐνδόξου Προφήτου Ἠλιοὺ τοῦ Θεσβίτου."
S01.couplet1.title = 
S01.couplet1.verse1 = "Ἐπέσχεν ὄμβρον, πῦρ τρίτον φέρων κάτω· "
S01.couplet1.verse2 = "Σχίζει δὲ ῥεῖθρον Ἠλίας τρέχων ἄνω. "
S01.single1.title = 
S01.single1.text ="Δίφρῳ ἀνηρπάγης περὶ εἰκάδα Ἠλία ἱππεῦ."
S01.narration1.text = ""

S99.lastline.text = "Ταῖς αὐτοῦ ἁγίαις πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."