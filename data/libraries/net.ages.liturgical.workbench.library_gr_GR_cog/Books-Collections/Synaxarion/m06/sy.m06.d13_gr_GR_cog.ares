S01.commemoration.text = "Τῇ ΙΓʹ τοῦ αὐτοῦ μηνός, μνήμη τῆς Ἁγίας Μάρτυρος Ἀκυλίνης."
S01.couplet1.title = 
S01.couplet1.verse1 = "Τὸν παστὸν εὐτρέπιζε λαμπρόν, Νυμφίε."
S01.couplet1.verse2 = "Ἀκυλῖνα σοι τέμνεται Νύμφη νέα."
S01.single1.title = 
S01.single1.text ="Ἀμφὶ τρίτην δεκάτην κεφαλὴν τμήθη Ἀκυλῖνα."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, μνήμη τῆς Ὁσίας Μητρὸς ἡμῶν Ἄννης, καὶ τοῦ υἱοῦ αὐτῆς Ἰωάννου."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, Χριστὲ ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."