S01.commemoration.text = "Τῇ ΙΖʹ τοῦ αὐτοῦ μηνός, μνήμη τῆς ἀθλήσεως τοῦ Ἁγίου Μάρτυρος Ἰσαύρου, καὶ τῶν σὺν αὐτῷ Βασιλείου, Ἰννοκεντίου, Φήλικος, Ἑρμείου καὶ Περεγρίνου."
S01.couplet1.title = 
S01.couplet1.verse1 = "Τμηθεὶς Ἴσαυρος σὺν συνάθλων πεντάδι, "
S01.couplet1.verse2 = "Σαύρας νοητῆς καρδίαν τέμνει μέσον."
S01.narration1.text = ""

S02.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τῶν Ἁγίων Μαρτύρων Μανουήλ, Σαβὲλ καὶ Ἰσμαήλ."
S02.couplet1.title = 
S02.couplet1.verse1 = "Σαβέλ, Μανουήλ, Ἰσμαὴλ Πέρσαι γένος. "
S02.couplet1.verse2 = "Τὸ δ' ἀξίωμα, Μάρτυρες διὰ ξίφους. "
S02.Ver3 = "Ἑβδομάτῃ δεκάτῃ κασιγνήτους τρεῖς τάμε χαλκός."

S03.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Ὑπατίου τοῦ ἐν Ῥουφιαναῖς."

S99.lastline.text = "Ταῖς αὐτῶν ἁγίαις πρεσβείαις, Χριστὲ ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."