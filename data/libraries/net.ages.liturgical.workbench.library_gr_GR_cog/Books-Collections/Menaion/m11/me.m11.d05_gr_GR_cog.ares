meVE.Stichera01.mode = misc_gr_GR_cog.Mode8
meVE.Stichera01.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meVE.Stichera01.text = "Γάλακτι τραφεὶς ἀσκήσεως, διὰ βασάνων πολλῶν, Γαλακτίων καὶ θλίψεων, εἰς Χριστοῦ κατήντησας, ἡλικίαν γενόμενος, δεκτὴ θυσία, τέλειον σφάγιον, τῇ αὐθαιρέτῳ ὁρμῇ πανόλβιε. Ὢ τῆς ἑδραίας σου, καὶ βεβαίας πίστεως! δι' ἦς Θεοῦ, ἔτυχες θεούμενος, νῦν τελεώτερον."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode8
meVE.Stichera02.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meVE.Stichera02.text = "Ἐπιστημόνως ἐζήτησας, τῶν ἀγαθῶν τὴν πηγήν, ὀρεκτῶν τὸ ἀκρότατον, ταῖς αὐτοῦ λαμπρότησι,καὶ ψυχὴν καὶ διάνοιαν, καταυγασθεῖσα Θεομακάριστε, καὶ καθελοῦσα στερραῖς ἐνστάσεσι, τὸν πολυμήχανον, καὶ ἀρχαῖον δράκοντα, μοναζουσῶν, θεῖον ἐγκαλλώπισμα, μάρτυς πολύαθλε."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode8
meVE.Stichera03.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meVE.Stichera03.text = "Δύο φωστῆρες ὑπέρλαμπροι, Ἀνατολῆς νοητῆς, ἀνατέλλουσι χάριτι, καὶ τὴν κτίσιν ἅπασαν, εὐσεβῶς καταυγάζουσιν, ὑπερβαλλούσαις ἄθλων λαμπρότησι, καὶ ἰαμάτων, θείοις πυρσεύμασιν, ὧν τὴν ὑπέρφωτον, ἑορτὴν γεραίροντες, τὸν δι' αὐτῶν, πάντας ἁγιάζοντα, Χριστὸν δοξάσωμεν."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode8
meVE.SticTheotokion.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meVE.SticTheotokion.text = me.m02.d20_gr_GR_cog.meVE.SticTheotokion.text

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode8
meVE.SticStavrotheotokion.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
meVE.SticStavrotheotokion.text = "Ὢ τοῦ παραδόξου θαύματος! ὢ μυστηρίου φρικτοῦ! ὢ φρικτῆς ἐγχειρήσεως! ἡ Παρθένος ἔλεγεν, ἐν Σταυρῷ σε ὡς ἔβλεψεν, ἐν μέσῳ δύο ληστῶν κρεμάμενον, ὃν ἀνωδίνως φρικτῶς ἐκύησεν· Ἔκλαιε λέγουσα· Οἴμοι Τέκνον φίλτατον! πῶς ὁ δεινός, δῆμος καὶ ἀχάριστος, Σταυρῷ προσὴλωσεν."

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode1
meVE.Apolytikion1.melody = he.a.m1_gr_GR_cog.TisErimouPolitis.name
meVE.Apolytikion1.text = "Τὴν λαμπρὰν ξυνωρίδα τῶν Μαρτύρων τιμήσωμεν, ὥσπερ συζυγίαν ἀρίστην καὶ κλυτὴν καὶ θεόφρονα, τὸν θεῖον Γαλακτίωνα πιστοί, ὁμοῦ σὺν Ἐπιστήμῃ τῇ σεμνῇ· δι' ἀσκήσεως γὰρ πόνων ἀθλητικήν, ἐξήνθησαν φαιδρότητα. Δόξα τῷ ἐνισχύσαντι ὑμας, δόξα τῷ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι δι' ὑμῶν, πᾶσιν ἰάματα."

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C11.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C11.text = "Τοὺς ἀνεσπέρους φωστῆρας τοὺς λογικούς, οὐρανοὺς τοὺς λάμψαντας, ὑπὲρ ἥλιον ἐν γῇ, τοῦ Χριστοῦ τοὺς θείους Ἀθλητάς, ἐν ᾠδαῖς πνευματικαῖς πίστει τιμήσωμεν."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C12.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C12.text = "Ἑλληνικῆς ἀπὸ ῥίζης στειρωτικῆς, εὐθαλὴς ἐβλάστησας θεοδώρητος βλαστός, Γαλακτίων κόσμῳ τοὺς καρπούς, προβαλλόμενος τῆς σῆς ἀτρέπτου πίστεως."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C13.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C13.text = "Ἐπιστημόνως τὴν πίστιν τὴν πρὸς Χριστόν, ἐκζητοῦσα ἔδραμες, πρὸς ἐπίγνωσιν αὐτοῦ, καὶ τυχοῦσα θείας δωρεᾶς, Ἐπιστήμη νυμφικῶς τούτῳ νενύμφευσαι."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C1T.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C1T.text = "Κατεποντίσθην ὁ τάλας ἐν τῷ βυθῷ, τῶν πολλῶν μου πράξεων, καὶ πρὸς σὲ τὴν ἀγαθήν, καταφεύγω Μῆτερ τοῦ Θεοῦ, δὸς μοι χεῖρα βοηθείας καὶ διάσωσον."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C11.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C11.text = "Ἐφέσει κρείτονος ζωῆς, καὶ τρυφῆς ἀνεκφράστου, τῶν φθαρτῶν ὑπερεῖδες, καὶ ῥευστῶν πάντα τερπνά, σὺν Ἐπιστήμῃ Χριστῷ Γαλακτίων, εὐπειθῶς ἑπόμενος."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C12.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C12.text = "Ῥυσθέντες βρόχων σαρκικῶν, καὶ παθῶν πολυπλόκων, ἐν ἀσκήσει τῷ πόθῳ, συνεδέθητε Χριστοῦ· θυσία δὲ καθαρά, ἐν ἀθλήσει, τούτῳ προσηνέχθητε."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C13.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C13.text = "Τῷ θείῳ πόθῳ τοῦ Χριστοῦ, ἀρνησάμενοι κόσμον,συνεδέθητε μᾶλλον, τῷ Πνεύματι Ἀθληταί, καὶ διὰ πόνων πολλῶν, βασιλείας, θείας ἠξιώθητε."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C1T.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C1T.text = "Οὐκ ἔστιν ὅστις πρὸς τὴν σήν, καταπέφευγε σκέπην, καὶ οὐκ ἔτυχε Κόρη, τῶν πολλῶν σου οἰκτιρμῶν· διὸ δέομαι τῆς σῆς, ὅπως τύχω, Κόρη ἀντιλήψεως."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode6
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.text = he.h.m6_gr_GR_cog.OukEstinAgios.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3Kathisma1.text = "Ἀσκήσει λαμπρύνας σου, τὸ ὀπτικὸν τῆς ψυχῆς, ἀκτῖσιν ἀθλήσεως, φωταγωγεῖς τοὺς πιστούς, Γαλακτίων μακάριε· ὅθεν σου τὴν ἁγίαν, καὶ φωσφόρον ἡμέραν, πίστει ἐπιτελοῦμεν, εὐσεβῶς σοι βοῶντες· Ὡς ἔχων παρρησίαν πρὸς Χριστόν, πρέσβευε σωθῆναι ἡμᾶς."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaT.text = "Παρθένε πανάμωμε, τὸν ὑπερούσιον, Θεὸν ἡ κυήσασα, σὺν Ἀσωμάτοις αὐτόν, ἀπαύστως ἱκέτευε, ἄφεσιν τῶν πταισμάτων, καὶ διόρθωσιν βίου, δοῦναι ἡμῖν πρὸ τέλους, τοῖς ἐν πίστει καὶ πόθῳ, ὑμνοῦσί σε κατὰ χρέος, μόνη Πανύμνητε."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaSt.text = "Σταυρῷ σε ὑψούμενον, ὡς ἐθεάσατο, ἡ ἄχραντος Μήτηρ σου, Λόγε Θεοῦ μητρικῶς, θρηνοῦσα ἐφθέγγετο. Τὶ τὸ καινὸν καὶ ξένον, τοῦτο Υἱέ μου θαῦμα; πῶς ἡ ζωὴ τῶν ὅλων, ὁμιλεῖ τῷ θανάτω; ζωῶσαι τοὺς τεθνεῶτας, θέλων ὡς εὔσπλαγχνος."

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C11.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C11.text = "Θεοῦ σε πρόνοια, βλαστὸν ἀνέδειξε, στειρευούσης νηδύος τῶν ἀρετῶν, τοὺς καρποὺς πληθύνοντα, καὶ καρποφόρον δεικνύντα, παραινέσεσι τὴν σύζυγον."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C12.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C12.text = "Χριστὸν ἐπόθησας, κόσμον ἐμίσησας, κατεφρόνησας πλούτου, δόξης, τρυφῆς, καὶ τιμῆς ἠλόγησας, φθαρτῶν τὰ ἄφθαρτα σοφῶς, ἀλλαξάμενος πανόλβιε."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C13.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C13.text = "Χριστοῦ τοῖς ἴχνεσι, κατηκολούθησας, ἀκλινῶς Γαλακτίων σὺν τῇ σεπτῇ, Ἐπιστήμῃ ἔνδοξε· διὸ καὶ δόξης παρ' αὐτοῦ, καὶ στεφάνων κατηξίωσαι."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C1T.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C1T.text = "Ναὸν ἐμόλυνα, κακῶς τοῦ σώματος, Παναγία Παρθένε τὸ καθαρόν, Θεοῦ οἰκητήριον, κάθαρον ῥύπου με παντός, τῶν παθῶν μου ταῖς πρεσβείαις σου."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C11.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C11.text = "Τῷ θείῳ φέγγει καταυγασθείς, καὶ τῆς ἀπαθείας καλλοναῖς, περιλαμφθεὶς Γαλακτίων σοφέ, ἔπεισας ἁγνεύειν σὺν σοὶ τὴν σύζυγον, ζυγὸν ἀνελομένην, τὸν ἐλαφρόν τοῦ Χριστοῦ."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C12.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C12.text = "Πυρὶ πυρούμενοι πειρασμῶν, πῦρ φιληδονίας καὶ πυράν, πολυθεΐας ἐσβέσατε, ἄμφω τὰ συμφέροντα συμφρονήσαντες, ἐν πόνοις πολυτρόποις, ψυχῆς καὶ σώματος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C13.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C13.text = "Νόμῳ πειθόμενοι τοῦ Θεοῦ, ἄραντες ἐπ' ὤμων τὸν σταυρόν, εἵλεσθε βίον τὸν ἄμεμπτον, καὶ διπλοῖς στεφάνοις κατεκοσμήθητε, ἀσκήσει καὶ νομίμοις, ἄθλοις ἐκλάμψαντες."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C1T.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C1T.text = "Ἡλίου ὄχημα φωτεινόν, κάθαρον τὸν ῥύπον τῆς ψυχῆς, κἀμοῦ τοῦ δούλου σου δέομαι. τάχυνον τὴν χάριν τῆς εὐσπλαγχνίας σου, καὶ ἴασαι νοσοῦντα, Κόρη Θεόνυμφε."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C11.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C11.text = "Τοῦ βίου τὸ βίαιον, βδελυξάμενοι σαφῶς, εἵλεσθε βίον ἄμεμπτον, ὡς βιασταὶ τῆς φύσεως καὶ διπλοῖς, ἐν ἄθλοις προλάμψαντες, διττοῖς τοῖς στεφάνοις κατεκοσμήθητε."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C12.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C12.text = "Σωμάτων διάστασιν, ὑποστάντες τῶν ψυχῶν, ἑνώσει διὰ πίστεως, ἀσάρκους πρὶν καθείλετε δυσμενεῖς, αὖθις ὀρωμένους δέ, κατεβάλετε, σθένει θείου Πνεύματος."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C13.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C13.text = "Νομίμως ἠρίστευσαν, Γαλακτίων ὁ κλεινός, καὶ ἡ θεόφρων σύζυγος, Ἐπιστήμη ἀνδρείως καὶ καρτερῶς, αὐτῶν ταῖς δεήσεσι, τὰ ἐλέη σου Λόγε πᾶσι δώρησαι."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C1T.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C1T.text = "Ἀγάθυνον Δέσποινα, τὴν ψυχήν μου ἐν πολλοῖς, κεκακωμένην πταίσμασι, καὶ τὸν δοτῆρα πάντων τῶν ἀγαθῶν, Θεὸν καθικέτευε, βασιλείας τῆς ἄνω ἀξιῶσαί με."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode6
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.text = he.h.m6_gr_GR_cog.TouViouTinThalassan.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode2
meMA.Kontakion1.melody = he.a.m2_gr_GR_cog.TaAnoZiton.name
meMA.Kontakion1.text = "Μαρτύρων Χριστοῦ, τοῖς δήμοις ἠριθμήθητε, ἀγῶσι στερροῖς, φαιδρῶς ἀγωνισάμενοι, Γαλακτίων ἔνδοξε, σὺν συζύγῳ σεπτῇ καὶ συνάθλῳ σου, Ἐπιστήμῃ, τῷ μόνῳ Θεῷ, πρεσβεύοντες ἄμφω ὑπὲρ πάντων ἡμῶν."

meMA.Oikos1.text = "Τὸν γενναῖον ἐν Μάρτυσι Γαλακτίωνα, ἐν ᾀσμάτων ᾠδαῖς εὐφημήσωμεν, σὺν τῇ πανευκλεεῖ συζύγῳ, τῇ φερωνύμῳ Ἐπιστήμῃ· οὗτοι γὰρ καθεῖλον τοῦ ἐχθροῦ τὸ φρύαγμα, καὶ εἰδώλων τὸ ἄθεον ἤλεγξαν, τὴν δὲ πίστιν Χριστοῦ ἀνεκήρυξαν. Διὸ περιφανῶς λαβόντες παρ' αὐτοῦ, τοὺς στεφάνους τῆς ἀφθαρσίας, ἀπαύστως πρεσβεύουσιν ὑπέρ πάντων ἡμῶν."

meMA.Synaxarion.text = ""

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C11.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C11.text = "Δροσοβόλος ὑμᾶς χάρις ἡ τοῦ Πνεύματος, πανσόφως προσανέψυχε, πυρπολουμένους πειρασμῶν πυρὶ καὶ ἀλγεινῶν, ἀσκήσεως καὶ τῶν αἰκισμῶν, οὓς ἐν ἀθλήσει καρτερῶς, αὖθις ὑπέστητε."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C12.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C12.text = "Τῆς ἁγνείας μὲν τῷ πόθῳ καὶ ἀσκήσεως, στέργετε τὴν διάστασιν, ἐν ὁμονοίᾳ δὲ ψυχῆς διὰ πίστεως στερρᾶς, συνδούμενοι πρὸς ἀθλητικάς, αὖθις ἡνώθητε λαβάς, Ὁσιομάρτυρες."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C13.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C13.text = "Γαλακτίων ὁ πανένδοξος θεσπίσμασι, πειθήσας τοῦ Παντάνακτος, τὰ θεσπίσματα ὡς ληρήματα τῶν δυσσεβῶν, ἡγήσατο· ὅθεν οὐρανῶν, τὴν βασιλείαν ἐν Χριστῷ κλῆρον ἀπείληφεν."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C1T.melody = he.h.m6_gr_GR_cog.DrosovolonMenTinKaminon.name
meMA.Ode7C1T.text = "Ὑπερτέρα ἐχρημάτισας Πανάμωμε, τῶν νοερῶν Δυνάμεων· ὃν γὰρ ἐκεῖνοι οὐ τολμῶσιν ὅλως κατιδεῖν, ἀγκάλαις ἐβάστασας ταῖς σαῖς, χαριστηρίοις σε φωναῖς· ὅθεν δοξάζομεν."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C11.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C11.text = "Τοῦ τῆς φύσεως πόθου φίλτρον θερμότερον, πρὸς Χριστὸν ἐσχηκότες, σαρκὶ καὶ αἵματι, ὅλω τῷ νοῒ προσανέχειν ἀπείπασθε, σχέσεως ὑλώδους, ἐμφρόνως ἀποστάντες."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C12.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C12.text = "Τῆς πρὸς σάρκα φιλίας ξένως διέστητε, τῆς ἁγνείας τῷ φίλτρῳ θείως ἡνώθητε, καὶ τῇ τοῦ Χριστοῦ ἀγάπῃ συνηρμόσθητε, κλέος συζυγίας, ἀζύγων μέγα θαῦμα."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C13.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C13.text = "Βασιλείαν τὴν ἄνω Μάρτυς θεσπέσιε, κατοικῆσαι σπουδάζων, ταύτην κατέλαβες, αἵμασι τοῖς σοῖς, Γαλακτίων ὠνούμενος, σὺν τῇ Ἐπιστήμῃ, τῇ σῇ καλῇ συνάθλῳ."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C1T.melody = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.name
meMA.Ode8C1T.text = "Ὥσπερ πύρινος θρόνος φέρεις Πανάμωμε, τὸν ἐν κόλποις πατρῴοις ἀναπαυόμενον, ὃν ὑπὲρ ἡμῶν, Θεοτόκε ἱκέτευε, ὅπως αἰωνίου, ῥυσθῶμεν καταδίκης."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode6
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.text = he.h.m6_gr_GR_cog.EkFlogosToisOsiois.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C11.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C11.text = "Χριστοῦ τῷ πόθῳ θερμῶς στομούμενοι, σχέσιν ψυχρὰν τοῦ βίου καὶ τοῦ φίλτρου τῆς φύσεως, καὶ δεσμοὺς τῆς σαρκὸς διερρήξατε, διὰ τὴν ἐν Τριάδι μίαν Θεότητα, Μάρτυρες δυάδος, ὑλικῆς ὑπεργενόμενοι."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C12.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C12.text = "Χοροῖς Μαρτύρων, Παρθένων τάγμασιν, ἐν οὐρανοῖς χορεύων, καὶ φωτὶ ἀγαλλόμενος, σὺν τῇ σῇ συζύγῳ Ἐπιστήμῃ κακῶν, λύσιν ἡμῖν βραβεύεις, νίκας τῷ Ἄνακτι, κόσμῳ Γαλακτίων, εἰρήνην καὶ μέγα ἔλεος."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C13.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C13.text = "Στερρῶς ἐν πίστει εἰς ἓν ἑνούμενος, πανευκλεῶς ὁ θεῖος Γαλακτίων ὁ πάνσοφος, καὶ τῇ σεμνῇ Ἐπιστήμῃ συνδούμενος, συνήθλησε γενναίως, τούτων δεήσεσιν, οἴκτειρον ἡμᾶς Λόγε Θεοῦ, ὡς ὑπεράγαθος."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C1T.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C1T.text = "Φωναῖς ὑμνοῦμεν χαριστηρίοις σε, οἱ διὰ σοῦ τυχόντες ἀληθῶς τῆς θεώσεως, καὶ βοῶμέν σοι· Χαῖρε Πανάμωμε, χαῖρε εὐλογημένη, χαῖρε παλάτιον, τοῦ Παμβασιλέως καὶ Θεοῦ, χαῖρε Μητρόθεε."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode6
meMA.Ode9H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9H.text = he.h.m6_gr_GR_cog.TheonAnthropois.text

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode3
meMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.OOuranonToisAstrois.name
meMA.Exaposteilarion1.text = "Τραφεὶς τῷ γάλακτι Μάρτυς, τῆς πίστεως Γαλακτίων, σὺν τῇ σεμνῇ Ἐπιστήμῃ, ἐνήθλησας μέχρι τέλους, ὑμῶν ἁγίαις πρεσβείαις, ῥυσθείημεν τῆς γεέννης."

meMA.ExapTheotokion.mode = misc_gr_GR_cog.Mode3
meMA.ExapTheotokion.melody = he.a.m3_gr_GR_cog.OOuranonToisAstrois.name
meMA.ExapTheotokion.text = "Χαριστηρίοις ὕμνοις, δοξάζοντές σε Παρθένε, σὺν τῷ Ἀγγέλῳ τὸ χαῖρε, βοῶμέν σοι Θεοτόκε· Χαῖρε ἀνύμφευτε Μήτηρ, τοῦ Βασιλέως τῆς δόξης."

meLI.Kontakion1.mode = meMA.Kontakion1.mode
