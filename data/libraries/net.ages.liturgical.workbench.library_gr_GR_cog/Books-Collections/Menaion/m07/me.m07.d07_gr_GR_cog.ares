A_Resource_Whose_Name = me.m07.d07_gr_GR_cog 

meDate.text = "Τῌ Ζʹ ΙΟΥΛΙΟΥ"
meCommemoration.text = "Μνήμη τοῦ Ἁγίου Θωμᾶ τοῦ ἐν Μαλεῷ· καὶ τῆς Ἁγίας Μάρτυρος Κυριακῆς." //full title of day
meShortCommemoration1.text = ""
meInsert1 = "τῆς Ἁγίας ἐνδόξου Μάρτυρος Κυριακῆς, " //for use in petitions and dismissal
meInsert2 = "ἧς καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ἁγίου Θωμᾶ τοῦ ἐν Μαλεῷ· καὶ τῆς Ἁγίας Μάρτυρος Κυριακῆς."
meLE.commemoration2 = ""
