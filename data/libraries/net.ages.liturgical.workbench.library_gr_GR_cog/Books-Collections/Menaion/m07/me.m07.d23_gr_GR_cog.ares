meVE.Stichera01.mode = misc_gr_GR_cog.Mode1
meVE.Stichera01.melody = he.a.m1_gr_GR_cog.TonOuranionTagmaton.name
meVE.Stichera01.text = "Ἀθλητικῶν ἐξ αἱμάτων, κρατῆρα πλήσας πιστοῖς, ἰάσεις ἐπαρδεύει, καὶ τὰς νόσους ἐλαύνει, Φωκᾶς ὁ χριστομάρτυς, ἐν ὑψηλῷ, μαρτυρίας κηρύγματι, πάντα καλῶν, οἱ διψῶντες δεῦτε πιστῶς, ποταμοὺς χαρίτων δρέψασθε."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode1
meVE.Stichera02.melody = he.a.m1_gr_GR_cog.TonOuranionTagmaton.name
meVE.Stichera02.text = "Τῆς ἀθεΐας τὴν πλάνην, θείᾳ ἐνστάσει σου, πολυθεΐας ζάλην, τῆς Τριάδος δυνάμει, Φωκᾶ μάρτυς ἐκτρέψας, ξίφους ἀκμήν, τοῦ πυρός τε τὴν ἔκκαυσιν, καὶ νιφετούς τῶν βασάνων βέλη σαφῶς, ὡς νηπίων ἐκαρτέρησας."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode1
meVE.Stichera03.melody = he.a.m1_gr_GR_cog.TonOuranionTagmaton.name
meVE.Stichera03.text = "Τοῦ ζωοοδότου σὺ κήρυξ, Θεοῦ γενόμενος, τυράννων καθαιρέτης, Ἀθλητὰ ἀνεδείχθης, καὶ θεῖος ἀθλοφόρος· ὅθεν ζωήν, τῷ ἀθανάτῳ πιστούμενος, πρὸς τὴν ἀνέσπερον ἔβης μαρμαρυγήν, τοῦ Θεοῦ Φωκᾶ μακάριε."

meVE.SticGlory.mode = misc_gr_GR_cog.Mode4
meVE.SticGlory.melody = ""
meVE.SticGlory.text = "Ἐκ βρέφους ἐγένου τοῦ Κυρίου ἐραστής, Φωκᾶ παμμακάριστε, Ἱερομάρτυς σοφέ· τὸ γὰρ ὅπλον τοῦ Σταυροῦ ἐπ' ὤμων ἀράμενος, ἀκλινῶς ἐπορεύθης τὴν ὁδὸν τῆς ἀληθείας, δι' ἧς τῶν Ἀγγέλων ὑφέστιος γέγονας, δαιμόνων ἀντίπαλος, καὶ τοῦ κόσμου πρεσβευτής, ὤφθης διαπρύσιος."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticTheotokion.melody = ""
meVE.SticTheotokion.text = oc.m4.d2_gr_GR_cog.ocMA.AposTheotokion.text

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticStavrotheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.SticStavrotheotokion.text = "Ὡς ἑώρακε Κύριε, ἡ Παρθένος καὶ Μήτηρ σου, ἐν Σταυρῷ κρεμάμενον, ἐξεπλήττετο, καὶ ἀτενίζουσα ἔλεγε· Τὶ σοι ἀνταπέδωκαν, οἱ πολλῶν σου δωρεῶν, ἀπολαύσαντες Δέσποτα; Ἀλλὰ δέομαι· Μὴ μὲ μόνην ἐάσῃς ἐν τῷ κόσμῳ, ἀλλὰ σπεῦσον ἀναστῆναι, συνανιστῶν τοὺς προπάτορας."

meVE.GeneralApolytikion1.name = "Φωκᾶ," // name of saint for General Apolytikia when not in Menaion

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion1.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meVE.Apolytikion1.text = "Φῶς ἐκλάμπουσα, τῶν ἰαμάτων, κόσμῳ δέδεικται, τῶν σῶν λειψάνων, Ἱερομάρτυς ἡ θήκη ἡ ἔνθεος· ἧς τὴν σεπτὴν κομιδὴν ἑορτάζοντες, ἁγιασμὸν ἀληθῆ κομιζόμεθα. Φωκᾶ ἔνδοξε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion2.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meVE.Apolytikion2.text = "Θείου Πνεύματος, τῇ ἐπιπνοίᾳ, προκατήγγειλας, Θεοῦ Προφῆτα, ἐσομένων μυστηρίων τὴν ἔκβασιν· τὴν τοῦ Σωτῆρος ἀπόῤῥητον κένωσιν, καὶ αἰωνίων νεκρῶν τὴν ἀνάστασιν· Ἰεζεκιὴλ ἔνδοξε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meVE.Apolytikion3.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion3.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion3.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meVE.Apolytikion3.text = "Ἀμέμπτως ἐβίωσας, ἐν ἐγκρατείᾳ πολλῇ, καὶ κόποις ἀσκήσεως, καὶ ἐν ἀγάπῃ θερμῇ, Πελαγία θεόληπτε· ὅθεν τὴν Θεοτόκον, ἐπαλλήλως κατεῖδες, μηνύουσάν σοι Εἰκόνος, τὴν ἀνεύρεσιν ταύτης· ᾗ πρέσβευε Ὁσία, ὑπὲρ τῶν τιμώντων σε."

meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς (ἄνευ τῶν Θεοτοκίων)· Φωκᾶς ὁ Φωστὴρ πᾶσιν ἐκλάμπει χάριν."

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C11.text = "Φωτὸς σε, τῆς θεϊκῆς κατηύγασαν ἀκτῖνες, Ἔνδοξε, ὑπερφυοῦς προνοίας ἐξ αὐτῶν, τῶν ὠδίνων μακάριε· διὸ φωστὴρ ὑπέρλαμπρος, Φωκᾶ ἐδείχθης ἐν τοῖς πέρασιν."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C12.text = "Ὡς φοίνιξ, ὑψιτενὴς ἀνέδραμες, ἐκ βρέφους ἔνδοξε, τῶν ἀρετῶν εἰς ὕψος νοητόν, καὶ καρποὺς τοὺς τῶν ἄθλων σου, πληθύνας κατεγλύξανας, πάντας μεθέξει τῶν θαυμάτων σου."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C13.text = "Καθεῖλες, τὰ τῶν βωμῶν ἱδρύματα, τῆς ἀθεότητος, τῇ μηχανῇ τῶν λόγων σου Σοφέ, καὶ Χριστοῦ διετήρησας, τὴν Ἐκκλησίαν ἄσειστον, κατοχυρώσας τοῖς διδάγμασιν."

meMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C14.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C14.text = "Ἀφ' ὕψους, τῆς ἀπονοίας ἔρριψας, νοῦν ὑπερήφανον, ἐν τῇ ἁπλῇ τῶν λόγων καθελών, τοῦτον χάριτι Ὅσιε, καὶ κατ' αὐτοῦ τὰ τρόπαια, ἀθλητικαῖς νίκαις κεκλήρωσαι."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.ThalassisToErythraion.name
meMA.Ode1C1T.text = "Γεννήτωρ, ἐν Παραδείσῳ γέγονε, φθόνος θανάτου βροτοῖς, ὃν γεννηθεὶς Πανάχραντε ἐκ σοῦ, ὁ Υἱὸς τοῦ Θεοῦ καὶ Πατρός, μετˈ ἤχου ἐξηφάνισεν, ἀθανασίαν δοὺς τῷ γένει ἡμῶν."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C11.text = "Συνήρμοσας τῶν πιστῶν, τὴν συμφωνίαν εἰς Χριστοῦ αἴνεσιν, καὶ ἀθεΐας φωνάς, Μάρτυς τῇ ἐνστάσει συνέχεας."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C12.text = "Ὁ πόθος σε τοῦ Θεοῦ, τῶν ἀπὸ γῆς πρὸς οὐρανὸν εἵλκυσε, καὶ νοεραῖς τάξεσιν, ἐν σαρκὶ ἀθλοῦντα ἠρίθμησεν."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C13.text = "Φωτίσας ταῖς ἀστραπαῖς, τῶν σῶν θαυμάτων τοὺς πιστούς, ἔσβεσας, τῆς ἀθεΐας πυρσούς, ἐν τοῖς ὄμβροις Μάκαρ τῶν λόγων σου."

meMA.Ode3C14.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C14.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C14.text = "Ὡς χρώμασι φαεινοῖς, ἱερωσύνης τὴν στολὴν ἔβαψας, ἀθλητικοῖς αἵμασι, καὶ Χριστῷ φωτὶ ἱερούργησας."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.name
meMA.Ode3C1T.text = "Ἐρράγη τὸ τῆς ἀρᾶς, ἡμῶν μεσότοιχον ἁγνὴ Δέσποινα, τῷ ὑπὲρ νοῦν τόκῳ σου, καὶ τῇ θείᾳ φύσει συνήφθημεν."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode4
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.text = he.h.m4_gr_GR_cog.EffrainetaiEpiSoi.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3Kathisma1.text = "Θυσίαν ἀναίμακτον, Χριστῷ προσφέρων Φωκᾶ, θυσίαν προσήνεγκας, σαυτὸν δι' αἵματος, Ἱεράρχα θεόληπτε· ὅθεν ἐν εὐφροσύνῃ, αὐλιζόμενος Πάτερ, μνήσθητι τῶν ἐν πίστει, ἐκτελούντων τὴν θείαν, καὶ εὔσημον ἡμέραν τῆς σῆς ἀθλήσεως."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaT.text = "Ἡ μόνη κυήσασα, τὸν Ποιητὴν τοῦ παντός, ἡ μόνη κοσμήσασα, τὴν ἀνθρωπότητα, τῷ τόκῳ σου Πάναγνε, ῥῦσαί με τῶν παγίδων, τοῦ δολίου Βελίαρ, στῆσόν με ἐπὶ πέτραν, τῶν Χριστοῦ θελημάτων, αὐτὸν ἐκδυσωποῦσα ἀεί, ὃν ἐσωμάτωσας."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaSt.text = "Ὑψούμενος ὕψωσας, τοὺς πεπτωκότας ποτέ, ἐμὲ δὲ κατέβαλες, τῇ ἀνυψώσει τῇ σῇ· Ὦ φῶς τῶν ὀμμάτων μου, δέχου Υἱὲ τὸ πάθος, δι' ἡμᾶς ἑκουσίως. Φέρε Σταυρόν τοὺς Ἥλους, καὶ τὸν Σπόγγον τὴν Λόγχην, δι' ὧν τῆς ἀπαθείας χάριν ἑβράβευσας."

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C11.text = "Σὺσσαλεύουσαν πελάγει τῆς ἀσεβείας, τὴν Ἐκκλησίαν εἵλκυσας, Φωκᾶ εἰς λιμένας, τοὺς τῆς εὐσεβείας Χριστοῦ, ἀπαύστως κραυγάζουσαν· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C12.text = "Τὸ γεῶδες ἐκτινάξας ζωῆς σαρκώδους, καὶ βιοτεύων ἔνδοξε, ἐν σαρκὶ πορείαν, ἄϋλον διήνυσας, Φωκᾶ· ὅθεν χάριτι, ἄγγελος ἐν σώματι πέφυκας."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C13.text = "Ἡ ἀκένωτος πηγὴ σε τοῦ Παρακλήτου, τῶν νοητῶν ἐπληρωσε, καὶ θείων θαυμάτων, Ἱεράρχα ἔνδοξε, καὶ πᾶσι προτέθεικε, ποταμὸν χαρίτων ἀείρυτον."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.EparthentaSeIdousa.name
meMA.Ode4C1T.text = "Ὡς παράδοξα καὶ ἄρρητα Θεοτόκε, τὰ τοῦ καινοῦ σου τόκου, ἀληθῶς ὑπάρχει, ξένα καὶ παράδοξα, ἐν οἷς ἡ διάνοια, καὶ ὁ νοῦς ἡττᾶται πανάχραντε."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C11.text = "Πορθήσας τὰ ἐχθροῦ, ὦ Φωκᾶ ὄχυρώματα, δυνάμει τῇ τῶν θαυμάτων, τοὺς ῥυσθέντας Κυρίῳ, ὡς σκῦλα προσενήνοχας."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C12.text = "Ἀντώθησας Σοφέ, κατὰ σοῦ τὰ πεμπόμενα, τοξεύματα εἰς καρδίας, τῶν βαλλόντων, αὐτὸς δέ, ἐσώθης ἀπαράτρωτος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C13.text = "Συνέθλασας Φωκᾶ, κεφαλὴν τὴν τοῦ δράκοντος, πατήσας τοῦτον γενναίως, τῇ ἐνθέῳ δυνάμει, τῆς στερρᾶς σου ἀθλήσεως."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.SyKyrieMouFos.name
meMA.Ode5C1T.text = "Ῥανάτω οὐρανός, εὐφροσύνην καὶ χάριν ἐν γῆ· ἀνέδωκε νῦν γὰρ αὕτη, εὐφροσύνην εἰς ὕψος, τὴν μόνην Θεομήτορα."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C11.text = "Ἰλύος, καὶ βυθοῦ ἀθεότητος Ἔνδοξε, τὴν σὴν ἀνείλκυσας ποίμνην, ἣν ἀποκαθάρας, υἱοθεσίας, τῷ ὕδατι, πρὸς τὸ φῶς τὸ τῆς χάριτος ὕψωσας."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C12.text = "Νέφεσιν, ὁ τιθεὶς ἑαυτοῦ τὴν ἐπίβασιν, ἀεροδρόμῳ πορείᾳ, σὲ Φωκᾶ ἐδόξασε τοῦ προφθάνειν, τοὺς αἰτοῦντας, ἐν κινδύνοις θαλάσσης καὶ θλίψεως."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C13.text = "Ἔλυσας, συμπλοκὰς τὰς τοῦ ψεύδους καὶ ἔδησας, ματαιορρήμονας γλῶσσας, τοῖς τῆς ἀληθείας δεσμοῖς θεόφρον, καὶ ἐν κόσμῳ, τὸν τῆς πίστεως λόγον ἐκήρυξας."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C1T.text = "Ἑνώσεως, ἀκηράτου καρπὸν ἀνεβλάστησας, Θεοκυῆτορ τῷ κόσμῳ, ἀειζώου πρόξενον ἀφθαρσίας, δεικνυμένης, τοῖς ἐν πίστει καὶ πόθῳ τιμῶσί σε."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.text = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.text

meMA.Synaxarion.text = ""

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C11.text = "Καθηλωθέντα, τῷ τοῦ Χριστοῦ σε πόθῳ Μάρτυς σοφέ, πλήθη φοβερῶν βασάνων καὶ ἀπειλῶν, οὐ κατέσεισαν οὐχ εἵλκυσαν, ἐκ γενναιότητος, τῆς πρὸς αὐτὸν συναφείας καὶ σχέσεως."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C12.text = "Λύτρον τῷ δόντι, ὑπὲρ ἡμῶν τὸ αἷμα τὸ ἑαυτοῦ, αἷμα τὸ οἰκεῖον Μάρτυς ὡς προσφοράν, ἀντιδέδωκας καὶ ἔψαλλες· Εὐλογητὸς εἶ ὁ Θεός μου κραυγάζων καὶ Κύριος."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C13.text = "Ἀποβαλόντα, δι' ἀρετῆς τὸ βάρος τὸ γεηρόν, θεῖοι τοῦ Χριστοῦ σε, Ἄγγελοι Ἀθλητά, δι' ἀέρος μετεκόμιζον, αὐτοῦ προστάγματι, ὡς τὸ πρὶν Ἀββακοὺμ συνιπτάμενον."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.EnTiKaminoAvramiaioi.name
meMA.Ode7C1T.text = "Ἄγγελοι εἶδον, Θεογεννῆτορ ξένα τῷ τόκῳ σου, φύσιν τὴν φθαρτὴν τοῦ γένους τοῦ χοϊκοῦ, πρὸς ἀκήρατον καὶ ἄφθαρτον, παλινδρομήσασαν, κατοικίαν καὶ δόξαν οὐράνιον."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C11.text = "Πῦρ ἀπαύστως ἔχων ἐν ψυχῇ, τὸ τῆς ἀγάπης Χριστοῦ, Φωκᾶ μακάριε, τὸν δρόμον ἄσχετον ἤνυσας, πρὸς αὐτὸν τὸν δι' ἀθλήσεως δι' ἧς τυχὼν τοῦ ἐφετοῦ, σὺν Ἀσωμάτοις βοᾷς· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C12.text = "Ἔρρει τῶν Ἑλλήνων τὰ σαθρά, πλάνης σεβάσματα, καὶ σεσιγήκασι, τῆς ματαιότητος ἅπασαι, αἱ διπλόαι καὶ τὸ ψεῦδος αὐτῶν, ἠλέγχθη γνώσει ἀληθεῖ, τῶν ἀθλοφόρων Χριστοῦ, κηρυττόντων, εἶναι σοφίαν Θεοῦ ἐνυπόστατον."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C13.text = "Ἰσχὺν ἀπροσμάχητον Φωκᾶ, περιζωσάμενος, Ἁγίου Πνεύματος, ἀκαταγώνιστον ἔνστασιν, καὶ ἀνδρείαν ἀπερίτρεπτον, προπολεμῶν ὑπὲρ τῆς σῆς, ποίμνης ἐνδέδειξαι, καὶ νικήσας, σὺν τοῖς Ἀγγέλοις ὑμνεῖς τὸν τῶν ὅλων Θεόν."

meMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C14.text = "Χάριν θείαν ἄφθονον Χριστός, σοὶ ἐδωρήσατο, Φωκᾶ ἀοίδιμε, τοῖς ἐξαιτοῦσί σε ἅπασι, καταλλήλως παρεχόμενον, τὰς τῶν θαυμάτων δωρεάς, ἀπαύστως κράζουσιν· Εὐλογεῖτε, πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.name
meMA.Ode8C1T.text = "Ὁ φύσει ἀπρόσιτος παντί, Υἱὸς καὶ Λόγος Θεοῦ, ἐν σοὶ σκηνώσας Ἁγνή, καὶ ἐνδυσάμενος ἄχραντε, τὸ ἡμέτερον ὡς εὔσπλαγχνος, ὤφθη εὐπρόσιτος σαρκί, καὶ ἀνεστράφη ἡμῖν, διὰ τοῦτο, πόθῳ πολλῷ σε ὑμνῶ καὶ τιμῶ καί αἰνῶ."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode4
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.text = he.h.m4_gr_GR_cog.CheirasEkpetasasDaniil.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C11.text = "Ἅρμα ὡς πυρίμορφον ἔχων, τὰς φωτοφόρους ἀρετάς σου, καὶ τὴν τῶν ἀγώνων διφρείαν, πρὸς οὐρανίους ἀνέπτης Ὅσιε, μονὰς ἐν αἷς κατέπαυσας, σὺν Ἀσωμάτοις αὐλιζόμενος."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C12.text = "Ῥύσει τῆς ἐνθέου σοφίας, καὶ πλημμυρήσει τῇ τῶν λόγων, ἔλυσας τοῦ ψεύδους τὴν ὕλην, καὶ εὐσεβούντων ψυχὰς κατήρδευσας, καὶ καρποφόρους ἔδειξας, ἐνθέων ἔργων διὰ πίστεως."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C13.text = "Ἴδες τὴν ὑπέρφωτον αἴγλην, τὴν νοουμένην θεωρίαν, ἐν τῇ τῶν ἀγώνων σου πάλῃ, τοῦ ποθουμένου ἀγωνοθέτου Χριστοῦ· καὶ φθάσαι ταύτην ἔσπευσας, Μάρτυς τῷ τέλει τῶν ἀθλήσεων."

meMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C14.text = "Νῦν ὡς τρανοτέρως τὴν δόξαν, ἐνοπτριζόμενος τοῦ Κτίστου, ὑπὲρ τῶν ἐκ πόθου Φωκᾶ σε, ἀνευφημούντων αὐτὸς δυσώπησον, τοῦ μετασχεῖν τῆς κρείττονος, τούτου καὶ δόξης καὶ λαμπρότητος."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C1T.text = "Ὑμνῶ σου τὴν χάριν Παρθένε, καὶ μεγαλύνω σου τὴν δόξαν· σὺ γὰρ φωτισμὸς τῆς ψυχῆς μου, ὑπάρχεις ὄντως καὶ ἡ παράκλησις, τῶν σὲ τιμώντων Δέσποινα, καὶ ἀνυμνούντων πάντων δούλων σου."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode4
meMA.Ode9H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9H.text = he.h.m4_gr_GR_cog.LithosAcheirotmitos.text

