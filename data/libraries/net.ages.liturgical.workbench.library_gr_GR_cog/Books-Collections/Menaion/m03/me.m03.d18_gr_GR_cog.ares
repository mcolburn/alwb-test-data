meCommemoration.text = "Μνήμη τοῦ ἐν Ἁγίοις Πατρὸς ἡμῶν Κυρίλλου Ἀρχιεπισκόπου Ἱεροσολύμων." //full title of day
meShortCommemoration1.text = ""
