meVE.Stichera01.mode = misc_gr_GR_cog.Mode4
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera01.text = "Σωφροσύνης ἐπώνυμος, ἐξ ἐνθέου προγνώσεως, προκληθεὶς Σωφρόνιος, σώφρων γέγονας, ἐπὶ τῶν ἔργων καὶ δίκαιος, ἀνδρεῖος καὶ φρόνιμος, ἀρεταῖς ταῖς γενικαῖς, γενικῶς στεφανούμενος, καὶ διένειμας, προσφυῶς ἑκατέρῳ τῇ ψυχῇ τε, καὶ τῷ σώματι, καθάπερ, διαιτητὴς ἀκριβέστατος."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera02.text = "Θεολόγου ἐκ στόματος, θεολόγους ἑβρόντησας, διδαχὰς Σωφρόνιε, παμμακάριστε, θεολογήσας τρανότατα, Πατέρα τὸν ἄναρχον, καὶ συνάναρχον Υἱόν, καὶ τὸ Πνεῦμα τὸ Ἅγιον, συναΐδιον, ἐν μονάδι Τριάδα καὶ Μονάδα, ἐν Τριάδι Θεὸν ἕνα, τῇ οὐσιώδει ταυτότητι."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.Stichera03.text = "Τῇ σαρκὶ καθ' ὑπόστασιν, ἑνωθέντα τὸν ἄσαρκον, καὶ Πατρὶ συνάναρχον, Λόγον Πάνσοφε, δίχα τροπῆς καὶ συγχύσεως, σοφῶς ἐδογμάτισας, ἐνεργοῦντα δὲ διττῶς, καταλλήλως ταῖς φύσεσιν, ἐξ ὧν σύγκειται, καὶ ἐν αἷς θεωρεῖται εἷς ὑπάρχων, ἀδιαίρετος τῷ ὄντι, τοῦτο κἀκεῖνο νοούμενος."

meVE.SticTheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticTheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.SticTheotokion.text = "Κατακρίσεως λύτρωσαι, Παναγία Θεόνυμφε, καὶ δεινῶν πταισμάτων, τὴν ταπεινήν μου ψυχήν, καὶ τοῦ θανάτου ἀπάλλαξον, εὐχαῖς σου, καὶ δώρησαι, δικαιώσεως τυχεῖν, ἐν ἡμέρᾳ ἐτάσεως, ἧς ἐπέτυχον, τῶν Ἁγίων οἱ δῆμοι, μετανοίᾳ, καθαρθέντα με πρὸ τέλους, καὶ τῶν δακρύων ταῖς χύσεσιν."

meVE.SticStavrotheotokion.mode = misc_gr_GR_cog.Mode4
meVE.SticStavrotheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
meVE.SticStavrotheotokion.text = "Ἐν Σταυρῷ ὡς κατεῖδέ σε, καθηλούμενον Κύριε, ἡ ἀμνὰς καὶ Μήτηρ σου, ἐξεπλήττετο, καὶ τὶ τὸ ὅραμα, ἔκραζεν· Υἱὲ ποθεινότατε; ταῦτά σοι ὁ ἀπειθής, δῆμος ἀνταποδίδωσιν ὁ παράνομος, ὁ πολλῶν σου, θαυμάτων ἀπολαύσας; ἀλλὰ δόξα τῇ ἀῤῥήτῳ, συγκαταβάσει σου Δέσποτα."

meVE.GeneralApolytikion1.name = "πάτερ Ἱεράρχα Σωφρόνιε," // name of saint for General Apolytikia when not in Menaion

meVE.Apolytikion1.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode5
meVE.Apolytikion1.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
meVE.Apolytikion1.text = "Σωφροσύνης τὴν αἴγλην πλουτήσας Ὅσιε, τῆς εὐσεβείας ἐκφαίνεις τὸν ὑπὲρ νοῦν φωτισμόν, ταῖς τῶν Λόγων ἀστραπαῖς Πάτερ Σωφρόνιε· σὺ γὰρ σοφίας κοινωνός, διὰ βίου γεγονώς, στηρίζεις τήν Ἐκκλησίαν, ὡς εὐκλεὴς Ἱεράρχης, καὶ πρεσβευτὴς ἡμῶν πρὸς Κύριον."

meVE.Apolytikion2.poet = poets_gr_GR_cog.MitropolitouArtisSerafim
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion2.melody = ""
meVE.Apolytikion2.text = "Χαίρει ἔχουσα ἡ πόλις Ἄρτα τὸν σὸν πάνεσπτον ναόν, Ὁσία, ἰαμάτων ἰατρεῖον ὑπάρχοντα τοῖς προσιοῦσιν αὐτῷ μετὰ πίστεως καὶ αἰτουμένοις τὴν σεπτήν σου ἀντίληψιν· Ὦ Βασίλισσα Θεοδώρα ἀξιάγαστε, Χριστὸν τὸν Θεὸν ἱκέτευε δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

meMA.C1Poet = poets_gr_GR_cog.Theofanous
meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς· Σωφροσύνης μέλπω τὸν ἐπώνυμον, αἴσιμα ῥέζων."

meMA.Ode1C1H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode1C1H.text = he.h.m4_gr_GR_cog.AnoixoToStomaMou.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C11.text = "Σωφρόνως διέπλευσας, τῆς σωφροσύνης τὸ πέλαγος, ταῖς αὔραις τοῦ Πνεύματος, οἰακιζόμενος, καὶ συνήθροισας, πολλὴν περιουσίαν, σοφίας χαρίσματι, Πάτερ Σωφρόνιε."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C12.text = "Ὡς Ἄγγελος γέγονας, τῆς τοῦ Χριστοῦ Ἀναστάσεως, καὶ πρόεδρος ἔνθεος, τοῦ θείου Μνήματος, τοῦ πλουτήσαντος, πηγὴν ἀθανασίας, Χριστὸν τὸν ἐγείραντα, πτῶσιν τοῦ γένους ἡμῶν."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C13.text = "Φαιδρότητι λόγων σου, διανοίας ὀξύτητι, τὸν κόσμον ἐφαίδρυνας, θεολογήσας τρανῶς, τρισυπόστα τον, Θεότητος οὐσίαν, Μονάδα τρισάριθμον, Πάτερ Σωφρόνιε."

meMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C14.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C14.text = "Ῥημάτων ὁ φθόγγος σου, καὶ τῶν δογμάτων ἡ εὔσημος, κιθάρα διέδραμε, τῆς γῆς τὰ πέρατα, τὸ μυστήριον, τῆς οἰκονομίας, τοῦ Λόγου διδάσκουσα, Πάτερ θεόληπτε."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C1T.text = "Ὁ ὢν φύσει ἄκτιστος, καὶ τῷ Πατρὶ συναΐδιος, καὶ χρόνων ὑπέρτερος, καὶ προαιώνιος, ἐκ σοῦ Δέσποινα, κτιστὸς καὶ ὑπὸ χρόνον, ὡς ἄνθρωπος γίνεται, σῴζων τόν ἄνθρωπον."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3C1H.text = he.h.m4_gr_GR_cog.OukEnSofia.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
meMA.Ode3C11.text = "Στηριζομένη, βακτηρίᾳ τῶν λόγων σου Ὅσιε, τὰς ὁρμὰς τῶν δυσσεβῶν, καὶ θεομάχων αἱρέσεων, ἐλαύνει διώκουσα, ἡ Ἐκκλησία Χριστοῦ."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
meMA.Ode3C12.text = "Ὑπερμαχήσας, τοῦ σεπτοῦ θεοφόρε κηρύγματος, ἀνεδείχθης νικητής, τῇ συμμαχίᾳ τοῦ Πνεύματος, σοφῶς δυναμούμενος, Πάτερ Σωφρόνιε."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
meMA.Ode3C13.text = "Νεκρώσας πᾶσαν, φθειρομένης σαρκὸς ἡδυπάθειαν, τὴν ἀκήρατον ψυχήν, δι' εὐσεβείας ἐζώωσας, καὶ ταύτην ἀνέδειξας, ἔνθεον ὄργανον."

meMA.Ode3C14.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C14.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
meMA.Ode3C14.text = "Ἡ θεολόγος, καὶ τερπνὴ καὶ μελίῤῥυτος γλῶσσά σου, τοὺς τοῦ λόγου ποταμούς, ἀναπηγάζει θεόληπτε, τὰ θεῖα διδάγματα, πᾶσι προχέουσα."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
meMA.Ode3C1T.text = "Σὲ τὴν τεκοῦσαν τὸν Θεόν, Θεοτόκον δοξάζομεν, τῷ Γεννήματι τῷ σῷ, προσηγορίαν ἁρμόζοντες, καὶ κλῆσιν κατάλληλον, πάναγνε Δέσποινα."

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma1.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3Kathisma1.text = "Τοῖς λόγοις ἐκόσμησας τὴν Ἐκκλησίαν Χριστοῦ, τοῖς ἔργοις ἐτήρησας τὸ κατ' εἰκόνα Θεοῦ, Σωφρόνιε Ὅσιε· ἔλαμψε γὰρ ἐν κόσμῳ, ἡ ἐν σοὶ σωφροσύνη, χάριτας διδαγμάτων, ἀπαστράπτουσα πᾶσι, τοῖς πίστει ἑορτάζουσι Πάτερ τὴν μνήμην σου."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaT.text = "Ἐκαίνισας Ἄχραντε τῷ θείῳ τόκῳ σου, φθαρεῖσαν τοῖς πάθεσι τῶν γηγενῶν τὴν θνητήν, οὐσίαν καὶ ἤγειρας, πάντας ἐκ τοῦ θανάτου, πρὸς ζωὴν ἀφθαρσίας· ὅθεν σε κατὰ χρέος, μακαρίζομεν πάντες, Παρθένε δεδοξασμένη, ὡς προεφήτευσας."

meMA.Ode3KathismaSt.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaSt.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaSt.text = "Σταυρῷ σε ὑψούμενον ὡς ἐθεάσατο, ἡ ἄχραντος Μήτηρ σου Λόγε Θεοῦ μητρικῶς, θρηνοῦσα ἐφθέγγετο· Τὶ τὸ καινὸν καὶ ξένον, τοῦτο θαῦμα Υἱέ μου· πῶς ἡ ζωὴ τῶν ὅλων, ὁμιλεῖς τῷ θανάτῳ, ζωῶσαι τοὺς τεθνεῶτας, θέλων ὡς εὔσπλαγχνος ;"

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode4C1H.text = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
meMA.Ode4C11.text = "Μίαν ἄναρχον οὐσίαν, ἐν τρισὶν ὑποστάσεσιν, ἰδικαῖς τελείαις, Πάτερ σὺ σοφῶς ἐδογμάτισας, διαιρουμένην ἀτμήτως, ἑνουμένην τε ἀσυγχύτως, βουλήσει μιᾷ καὶ Θεότητι."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
meMA.Ode4C12.text = "Ἔχων σώφρονας τὰς φρένας, καὶ τὸν νοῦν σωφρονέστερον, οὐ φυρμὸν ἐφρόνεις, Πάτερ οὐ τροπὴν οὐδὲ σύγχυσιν, τῶν ἡνωμένων ἀτρέπτως δύο φύσεων, ἐν ἑνὶ Χριστῷ, μονογενεῖ ἀπαστράπτουσαν."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
meMA.Ode4C13.text = "Λαμπρυνόμενος ἐνθέως, φωτισμῷ τῷ τοῦ Πνεύματος, τὴν δεινὴν μανίαν, τὴν τοῦ Νεστορίου διέλυσας, ταυτοβουλίᾳ καὶ γνώμῃ τοῦ τὴν ἕνωσιν, δογματίσαντος, ἐπὶ Χριστοῦ δύο φύσεων."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
meMA.Ode4C14.text = "Πύῤῥον ἔφλεξας ἀντάρτην,τῶ πυρὶ τῶν δογμάτων σου, ἀθετοῦντα δύο, εἶναι τοῦ Χριστοῦ τὰ θελήματα, ταῖς διαφόροις οὐσίαις τὰ κατάλληλα, ἐνεργείας τε, ἅμα διπλᾶς Πάτερ Ὅσιε."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
meMA.Ode4C1T.text = "Ὡραιώθης ὑπὲρ πᾶσαν, τῶν Ἀγγέλων εὐπρέπειαν, ὡς τεκοῦσα τούτων, Κόρη ποιητήν τε καὶ Κύριον, ἐκ σῶν ἀχράντων αἱμάτων σωματούμενον, τὸν ῥυόμενον, πάντας αὐτόν τοὺς δοξάζοντας."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode5C1H.text = he.h.m4_gr_GR_cog.AseveisOukOpsontai.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
meMA.Ode5C11.text = "Τὴν σεπτὴν ἀνάστασιν, καὶ τάφον τῆς ζωῆς, ἀκλινεῖ πόθῳ θεωρῶν, θεωρίας ἤντλησας κρυφιομύστους, καὶ πιστοῖς μετέδωκας, Ἱεράρχα τῆς ἐλλάμψεως."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
meMA.Ode5C12.text = "Ὁλικῶς ἐπόθησας, τὸν μόνον ἀγαθόν, πυρσωθεὶς φέγγει νοητῷ, καὶ πηγὴν ἠγάπησας τῆς ἀφθαρσίας, θεωρίαις Πάνσοφε, πρὸς αὐτὴν ἀνατεινόμενος."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
meMA.Ode5C13.text = "Ναὸς ζῶν καὶ ἔμψυχος, ἐγένου τοῦ Θεοῦ, νεκρωθεὶς πᾶσι τοῖς ἐν γῇ, τὴν τοῦ λόγου τράπεζαν εὐθηνουμένην, κεκτημένος Ὅσιε, καὶ λυχνίαν τὴν τῆς πίστεως."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
meMA.Ode5C1T.text = "Ἐν δυσὶ νοούμενον, οὐσίαις τὸν Χριστόν, τὸν Υἱὸν τὸν μονογενῆ, τοῦ Θεοῦ τὸν ἄναρχον, σάρκα γενόμενον, ὑπὲρ λόγον τέτοκας, Θεομῆτορ ἀπειρόγαμε."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6C1H.text = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C11.text = "Πλουσίως, ἐξεχύθη ἡ χάρις τοῦ Πνεύματος, τοῦ παναγίου θεόφρον, ἐν τοῖς χείλεσί σου· ὅθεν ὁ φθόγγος, τῶν σῶν λόγων, τοῦ ποταμοῦ μιμεῖται τὰ ῥεύματα."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C12.text = "Ὡς φοίνιξ, ἐν τῷ οἴκῳ Κυρίου ἐξήνθησας, τῇ εὐκαρπίᾳ τῶν λόγων, καθαρῷ τε βίῳ καταγλυκαίνων, τὰς καρδίας, Ἱεράρχα, τῶν πίστει τιμώντων σε."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C13.text = "Νεκροῦται, τῇ ζωτικῇ ῥομφαίᾳ τῆς γλώττης σου, τὸ τῶν αἱρέσεων στῖφος, καὶ θρασεῖα φάλαγξ τῶν θεομάχων, σφαττομένη, θανατοῦται δογμάτων σου ξίφεσιν."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.ThysoSoiMetaFonis.name
meMA.Ode6C1T.text = "Ὑμνοῦμεν, Θεοτόκε τὸν ἄχραντον τόκον σου, δι' οὗ σῳζόμεθα πάντες, καὶ θανάτου βρόχων καὶ δεινοτάτης, ἁμαρτίας, καὶ τῶν μυχῶν τοῦ ᾍδου λυτρούμεθα."

meMA.Kontakion1.poet = poets_gr_GR_cog.Gerasimou
meMA.Kontakion1.mode = misc_gr_GR_cog.Mode4
meMA.Kontakion1.melody = he.a.m4_gr_GR_cog.EpefanisSimeron.name
meMA.Kontakion1.text = "Λαμπρυνθεὶς τοῦ Πνεύματος τ«ἐπιπνοίᾳ, Ἱεράρχης ὅσιος, ὡς Ἀποστόλων μιμητής, ἐν τῇ Σιὼν ἐχρημάτισας, Πάτερ παμμάκαρ, Σωφρόνιε πάνσοφε."

meMA.Synaxarion.text = ""

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode7C1H.text = he.h.m4_gr_GR_cog.ODiasosasEnPyri.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C11.text = "Μετὰ τῆς ἄνωθεν ῥοπῆς, ταῖς αἱρετικαῖς γλωσσαλγίαις, ἀντιταττόμενος Σοφέ, νικηφόρος γενόμενος ἔψαλλες· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C12.text = "Ὁλος ἀνάθημα Θεῷ, τὰς τῶν Ἀσκητῶν φυτουργίας, περιϊπτάμενος σοφέ, τὸν λειμῶνα τὸν σὸν κατεφύτευσας, ἀρετῶν ἐμμελέτημα, τῷ Θεῷ τῷ ἐν ὑψίστοις καθιερώσας."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C13.text = "Νόμῳ πειθόμενος Χριστοῦ, τῆς σῆς ἐπιστήμης τὸν πλοῦτον, τοῖς δεομένοις εὐσεβῶς, ἀναμέλπων θεόφρον μετέδωκας· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C1T.text = "Ἁγιασθεῖσα τὴν ψυχήν, καὶ προκαθαρθεῖσα τὸ σῶμα, τῇ ἐπελεύσει ἐπὶ σέ, τοῦ Ἁγίου Πανάμωμε Πνεύματος, τοῦ Ὑψίστου τὴν δύναμιν, ἐν γαστρί σου συλλαβοῦσα τίκτεις ἀσπόρως."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8C1H.text = he.h.m4_gr_GR_cog.PaidasEvageis.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C11.text = "Ἱεραρχικῆς Ἱερουργίας, τῷ μύρῳ καθηγιάσθης τῷ τῆς χάριτος, ἔνθα πεφανέρωται, κόσμου ἡ σωτήριος Ἱεραρχία, πάνσοφε, Πάτερ Σωφρόνιε· Τὸν Κύριον ὑμνεῖτε κραυγάζων, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C12.text = "Σοφίαν τιμῶν διαφερόντως, στεφάνῳ τῷ τῶν χαρίτων ἐστεφάνωσαι, δόξαν τὴν ἀμάραντον, πλοῦτον ἀναφαίρετον, τὸ τῆς σοφίας, Ἔνδοξε δῶρον δεξάμενος, καὶ Κύριον ὑμνεῖτε, κραυγάζων, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C13.text = "Ἱεροπρεπῶς Ἱερομύστα, τὴν θείαν δικαιοσύνην ἐνδυσάμενος, ἴθυνας Πανόλβιε, Πάτερ γενναιότατα, τὸ τοῦ Δεσπότου ποίμνιον λόγοις καὶ πράξεσι· Τὸν Κύριον ὑμνεῖτε κραυγάζων, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C14.text = "Μεγαλοπρεπῶς καὶ ὑπὲρ λόγον, ὁ Λόγος ὁ τῷ Πατρὶ Πάτερ συνάναρχος, λόγον σοι δεδώρηται, λόγους διατρέφοντα, καὶ γλωσσαλγίας λύοντα τὰς τῶν αἱρέσεων, τόν Κύριον ὑμνεῖτε βοῶντα, καὶ ὑπερυψοῦτε εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C1T.text = "Ἁγιοπρεπὴς Ναὸς ἐδείχθης, τοῦ Λόγου τοῦ πᾶσαν κτίσιν ἁγιάσαντος, ὄρος πῖον ἅγιον, ὄρος ἐμφανέστατον, εὐλογημένη Δέσποινα μόνη πανύμνητε. Διὸ σε Θεομῆτορ ὑμνοῦμεν, καὶ ὑπερυψοῦμεν εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9C1H.text = he.h.m4_gr_GR_cog.LithosAcheirotmitos.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C11.text = "Ῥῆσις ἐκ χειλέων σου θείων, προερχομένη θεοῤῥῆμον, τὰς τῶν εὐσεβῶν διανοίας, καταγλυκαίνει διὰ τῆς χάριτος, ὥς περ κηρίον στάζουσα, τῶν νοημάτων τὴν εὐπρέπειαν."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C12.text = "Ἔνδον οὐρανίων ἀδύτων, περιπολεύεις θεηγόρε, περὶ τὸν τῶν ὅλων Δεσπότην καὶ Βασιλέα, ἔνθα χορεύουσιν, Ἀγγελικαὶ λαμπρότητες, καὶ τῶν Ἁγίων τὰ στρατεύματα."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C13.text = "Ζωὴν μετερχόμενος Πάτερ, ἐπὶ τῆς γῆς τὴν τῶν Ἀγγέλων, τῆς ἀγγελικῆς ἐν οὐρανοῖς, κατηξιώθης μακαριότητος· ἔνθα Χριστὸν δυσώπησον, σωθῆναι πάντας τοὺς ὑμνοῦντάς σε."

meMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C14.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C14.text = "Ὡς τῶν ἀρωμάτων φιάλαι, αἱ σιαγόνες σου θεόφρον, νάρδου πιστικῆς πολυτίμου, σοφίας γέμων ὤφθης ἀλάβαστρον, Σῶμα Χριστοῦ τὸ τίμιον, ἀρωματίζων τοῖς σοῖς δόγμασι."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.LithosAcheirotmitos.name
meMA.Ode9C1T.text = "Νεφέλην σε κούφην Παρθένε, ὁ Ἡσαΐας ἐθεώρει· ἐπὶ σοὶ γὰρ Κύριος ἐλθών, καθεῖλε πάντα τὰ χειροποίητα, καὶ τὴν αὐτοῦ ἐπίγνωσιν, τοῖς σὲ ὑμνοῦσιν ἐφανέρωσε."

meLI.Kontakion1.mode = meMA.Kontakion1.mode
meLI.Kontakion1.melody = meMA.Kontakion1.melody
meLI.Kontakion1.text = meMA.Kontakion1.text

