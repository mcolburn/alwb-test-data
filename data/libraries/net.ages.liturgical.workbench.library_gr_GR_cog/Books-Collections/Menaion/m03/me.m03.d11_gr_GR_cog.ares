A_Resource_Whose_Name = me.m03.d11_gr_GR_cog

meDate.text = "Τῌ ΙΑʹ ΜΑΡΤΙΟΥ" // date, e.g. Τῌ ΙϚʹ ΤΟΥ ΑΥΤΟΥ ΜΗΝΟΣ or July 16
meCommemoration.text = "Μνήμη τοῦ ἐν Ἁγίοις Πατρὸς ἡμῶν Σωφρονίου, Πατριάρχου Ἱεροσολύμων, καὶ τῆς Ὁσίας Μητρὸς ἡμῶν Θεοδώρας, βασιλίσσης Ἄρτης." //full title of day
meShortCommemoration1.text = ""
