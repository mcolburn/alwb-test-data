A_Resource_Whose_Name = me.m09.d27_gr_GR_cog

meDate.text = "Τῌ ΚΖʹ ΣΕΠΤΕΜΒΡΙΟΥ"
meCommemoration.text = "Μνήμη τοῦ Ἁγίου Μάρτυρος Καλλιστράτου, καὶ τῶν σὺν αὐτῷ." //full title of day
meShortCommemoration1.text = ""
meInsert1 = "καὶ τοῦ Ἁγίου Μάρτυρος Καλλιστράτου, καὶ τῶν σὺν αὐτῷ, " //for use in petitions and dismissal
meInsert2 = "ὧν καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ἁγίου Μάρτυρος Καλλιστράτου, καὶ τῶν σὺν αὐτῷ."
meLE.commemoration2 = ""
