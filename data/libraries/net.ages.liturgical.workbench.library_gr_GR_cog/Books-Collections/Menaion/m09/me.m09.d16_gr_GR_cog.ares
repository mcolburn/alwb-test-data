meDate.text = "Τῌ ΙϚʹ ΣΕΠΤΕΜΒΡΙΟΥ"
meCommemoration.text = "Μνήμη τῆς Ἁγίας Μεγαλομάρτυρος καὶ Πανευφήμου Εὐφημίας." //full title of day
meShortCommemoration1.text = ""
