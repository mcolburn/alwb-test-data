A_Resource_Whose_Name = me.m01.d13_gr_GR_cog

meDate.text = "Τῌ ΙΓʹ ΙΑΝΟΥΑΡΙΟΥ" // date, e.g. Τῌ ΙϚʹ ΤΟΥ ΑΥΤΟΥ ΜΗΝΟΣ or July 16
