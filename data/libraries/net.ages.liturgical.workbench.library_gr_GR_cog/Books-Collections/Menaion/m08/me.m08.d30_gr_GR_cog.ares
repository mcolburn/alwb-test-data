meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode2
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.text = me.m08.d29_gr_GR_cog.meVE.Apolytikion1.text 

meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion2.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
meVE.Apolytikion2.text = "Μύσται ουράνιοι ἀποδεικνύμενοι, θεῖοι ἐκφάντορες τῷ κόσμῷ ὤφθητε, τὴν Ἐκκλησίαν τοῦ Χριστοῦ, ποιμάναντες θεαρέστως, ἱερὲ Ἀλέξανδρε, τῆς Τριάδος ὁ πρόμαχος, Ἰωάννη ἔνδοξε, ὁ τῆς χάριτος τρόφιμος, καὶ Παῦλε ἱερέων ἀκρότης˙ ὅθεν ὑμᾶς ἀνευφημοῦμεν."

meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς· Τὸν θεομαρτύρητον Πρόδρομον μέλπω."

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C11.text = "Τὸν λύχνον τὸν πάμφωτον, τῆς οίκουμένης τὸν κήρυκα, τὸν θεῖον Ἀπόστολον, καὶ ὑπηρέτην Χριστοῦ, τίς ἰσχύσειεν, ἀξίως εὐφημῆσαι, τὸν θεομαρτύρητον, Κυρίου Πρόδρομον;"

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C12.text = "Ὁ θεῖος ταξίαρχος, τῶν ἀσωμάτων Δυνάμεων, πρὸς θεῖον ἀφίκετο, ναὸν Θεοῦ Ἱερόν, εὐαγγέλια, κομίζων τῷ πρεσβύτῃ, Κυρίου τὸν Πρόδρομον, τέξῃ πρεσβύτατε."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C13.text = "Νηδὺς ἔνδον ἔχουσα, στεῖρα καὶ γραῦς τὸν πανάριστον, Προφήτην καὶ Πρόδρομον, ἀπὸ κοιλίας μητρός, προσεκύνησε, Χριστόν, ὢ ξένον θαῦμα! γαστρὶ ἐμφερόμενον τῆς Θεομήτορος."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
meMA.Ode1C1T.text = "Θεοῦ Ἀρχιστράτηγος, Παρθένε, ἔφη, Θεόνυμφε, πρὸς δρόμους πορεύθητι, τῆς Ἐλισάβετ σπουδῇ· καὶ γὰρ ἔγκυον, ταύτην εὑροῦσα γνώσῃ, ὃν περ καταγγέλλω σοι τόκον τὸν ἄρρητον."

meMA.Ode1C21.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C21.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C21.text = "Τρίφωτος τῶν Πατέρων, σήμερον ἀνέτειλε διαύγεια, ἡ πανένδοξος μνήμη· ᾗ πιστῶς ἐντρυφῶντες χορεύσωμεν."

meMA.Ode1C22.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C22.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C22.text = "Ἔλυσας Ἱεράρχα, τῶν Αἱρετικῶν τὰ ὀχυρώματα, εὐσεβεῖ παρρησίᾳ, καταγγείλας Τριάδα Ἀλέξανδρε."

meMA.Ode1C23.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C23.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C23.text = "Παύλῳ τῷ θεσπεσίῳ, κοινωνήσας κλήσεως ἀοίδιμε, τῶν ἐκείνου ἐδείχθης, ἀρετῶν ἀπεικόνισμα ἔνθεον."

meMA.Ode1C24.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C24.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C24.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C24.text = "Κάλαμός σου ἡ γλῶσσα, θεογραφικῶς προαναδέδεικται, Πνεύματος Πάτερ θείου, Ἰωάννη τὴν πίστιν τρανώσασα."

meMA.Ode1C2T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode1C2T.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
meMA.Ode1C2T.text = "ᾌσωμεν τῷ Κυρίῳ, τῷ ἐκ τῆς Παρθένου εὐδοκήσαντι, σαρκωθῆναι ἀσπόρως, εἰς ἡμῶν σωτηρίαν καὶ λύτρωσιν."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
meMA.Ode3C11.text = "Ἐκ στείρας τεχθεὶς ὁ Ἰωάννης, τὴν γλῶτταν διέλυσε πατρός· εἰκότως καὶ γὰρ πέφυκε, φωνὴ τοῦ Λόγου εὔηχος, καὶ εὐλογῶν ἐφθέγγετο· Σὺ μόνος Ἅγιος Κύριος."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
meMA.Ode3C12.text = "Οὐδὲν τῶν παρόντων ἀγαπήσας, τὴν ἔρημον τρέχων παροικεῖς, ὡς πόλιν θείῳ Πνεύματι, δυναμωθεὶς πανένδοξε, ἀΰλως δὲ ἐβίωσας, ὡς Ἠλιοὺ ὁ θεσπέσιος."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
meMA.Ode3C13.text = "Μὴ φέρων τοῦ βίου τοὺς θορύβους, ὡς ὄρνις ἐμάκρυνας σοφέ, ἐρήμοις αὐλιζόμενος· εὗρές τε ἀναψύχοντα, τὸν σῴζοντά σε Κύριον, ἐκ καταιγίδος καὶ θλίψεως."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
meMA.Ode3C1T.text = "Ἀπῆλθε πρὸς οἶκον Ἐλισάβετ, Παρθένος βαστάζουσα γαστρί, Θεὸν Λόγον τὸν ἄναρχον, ἀμφοῖν ἀσπασαμένων δέ, γνοὺς παρευθὺς ὁ Πρόδρομος, ἐκ τῆς γαστρὸς προσεκύνησεν."

meMA.Ode3C21.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C21.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C21.text = "Ἄριστα ἐποίμανας, τὸ τοῦ Δεσπότου σου ποίμνιον, Ἀρχιερεῦ· ὅθεν αἰωνίως, σὲ δοξάζει Ἀλέξανδρε."

meMA.Ode3C22.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C22.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C22.text = "Ὕδατα σωτήρια, διὰ τῆς γλώττης σου ἔβλυσας, τὰ εὐσεβῆ, δόγματα σῇ ποίμνῃ, Ἰωάννη μακάριε."

meMA.Ode3C23.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C23.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C23.text = "Παῦλε παμμακάριστε, τῆς ἐκλογῆς τῆς τοῦ σκεύους σε, ὡς φανέντα, κλήσει τε καὶ βίῳ, νῦν τιμῶμεν ὁμοίωμα."

meMA.Ode3C2T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode3C2T.melody = he.h.m8_gr_GR_cog.SyEiToStereoma.name
meMA.Ode3C2T.text = "Σὲ πάντες κεκτήμεθα, καταφυγὴν καὶ σκέπην ἡμῶν, Χριστιανοί, σὲ δοξολογοῦμεν, ἀσιγήτως, Θεόνυμφε."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode8
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.text = he.h.m8_gr_GR_cog.SyEiToStereoma.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode1
meMA.Ode3Kathisma1.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meMA.Ode3Kathisma1.text = "Τὸν πρόδρομον Χριστοῦ, Βαπτιστὴν καὶ Προφήτην, τιμήσωμεν πιστοί, καθαρῷ συνειδότι, ὡς ἔνδοξον κήρυκα, μετανοίας διδάσκαλον, καὶ ὡς μάρτυρα, παναληθῆ τοῦ Σωτῆρος· τὴν γὰρ ἄνοιαν, τὴν τοῦ Ἡρῴδου ἐλέγξας, τὴν κάραν ἐκτέμνεται."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode4
meMA.Ode3Kathisma2.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3Kathisma2.text = "Πατέρων ἡ τρίφωτος, λαμπὰς ἀνέλαμψε, πιστῶν τὰ συστήματα, καταφαιδρύνουσα, πρὸς θείαν ἐπίγνωσιν, λύοντες τῶν ἀθέων, τὴν ἀσέβειαν πᾶσαν, δόγμασι καὶ σοφίᾳ, καὶ πολλῇ παρρησίᾳ, Ἀλέξανδρος καὶ Ἰωάννης ὁμοῦ, καὶ Παῦλος ὁ ἔνδοξος."

meMA.Ode3KathismaT.mode = misc_gr_GR_cog.Mode4
meMA.Ode3KathismaT.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meMA.Ode3KathismaT.text = "Ἐλπὶς ἀκαταίσχυντε τῶν πεποιθότων εἰς σέ, ἡ μόνη κυήσασα, ὑπερφυῶς ἐν σαρκί, Χριστὸν τὸν Θεὸν ἡμῶν, τοῦτον σὺν Ἰωάννῃ τῷ Προδρόμῳ δυσώπει, δοῦναι τῇ οἰκουμένῃ, ἱλασμὸν τῶν πταισμάτων, καὶ πᾶσιν ἡμῖν πρὸ τέλους βίου διόρθωσιν."

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
meMA.Ode4C11.text = "Ῥίζας καὶ μελάγριον, ὄψον τερπνόν, ἤσθιες μακάριε Πρόδρομε, καὶ τὰς ἀκρίδας· ἐκ καμήλου δὲ τριχῶν, στολὴν ἐπημφιέννυσο, ὑπὲρ ἁλουργίδα βασίλειον."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
meMA.Ode4C12.text = "Τὸν δημιουργήσαντα λόγῳ τὸ πᾶν, πᾶσι γεωργὸν μάκαρ ἤγγειλας· οὗτος γὰρ ἐστιν, ὁ τὸ πτύον ἐν χειρί, κατέχων, καὶ τὴν ἅλωνα, μάλα διακρίνων ὡς δίκαιος."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
meMA.Ode4C13.text = "Ὕπνον σοῖς οὐκ ἔδωκας μάκαρ σοφέ, ὄμμασιν ὡς ἄσαρκος ἔνδοξε, οὔτε κροτάφοις, εἰς ἀνάπαυσιν σαρκός, ἐως οὗ ᾠκοδόμησας, οἶκον σεαυτὸν θείου Πνεύματος."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
meMA.Ode4C1T.text = "Ῥῆσις πέρας ἔλαβε τοῦ Ἀββακούμ, ἐπὶ σοὶ Παρθένε πανύμνητε· καὶ γὰρ ἐξ ὄρους, κατασκίου ὁ Θεὸς ἐκ σοῦ ἦλθε σαρκούμενος, πλάνης τοὺς βροτοὺς ἐκλυτρούμενος."

meMA.Ode4C21.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C21.melody = he.h.m4_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C21.text = "Ἐν δυσώδει διέρριψας, τὸν κατὰ Ἰούδαν τόπῳ προδότην Χριστοῦ· τὴν ἐκείνου γὰρ Ἀλέξανδρε, ἀρὰν ἐπαξίως ἐκληρώσατο."

meMA.Ode4C22.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C22.melody = he.h.m4_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C22.text = "Ὡς ἐλαία κατάκαρπος, ἐν τῷ τοῦ Θεοῦ τεμένει ἱστάμενος, τοὺς πιστεύοντας προσήγαγες, αὐτῷ Ἰωάννη καρπὸν ὥριμον."

meMA.Ode4C23.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C23.melody = he.h.m4_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C23.text = "Ταπεινώσεως πτέρυξι, τὰς τοῦ δυσμενοῦς παγίδας ἐξέφυγες· ὅθεν Ὅσιε ὑψούμενος, οὐρανοπολίτης Παῦλε γέγονας."

meMA.Ode4C2T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode4C2T.melody = he.h.m4_gr_GR_cog.EisakikoaKyrie.name
meMA.Ode4C2T.text = "Τὴν Παρθένον ὑμνήσωμεν, τὴν μετὰ τὸν τόκον πάλιν Παρθένον ἁγνήν, ὡς κυήσασαν Χριστὸν τὸν Θεόν, τὸν ἐκ πλάνης κόσμον λυτρωσάμενον."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
meMA.Ode5C11.text = "Ἰσχὺν θείου Πνεύματος, ἐνδεδυμένος ἔνδοξε, πᾶσιν ἀνεβόας τοῖς παροῦσι· Μετανοεῖτε, ἰδοὺ ἀξίνη σαφῶς, τῶν δένδρων πρὸς ῥίζαν κεῖται νῦν, τέμνουσα τὰ ἄκαρπα, τὰ δ' ἐσθλὰ ἐκκαθαίρουσα."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
meMA.Ode5C12.text = "Τὴν γλῶτταν ὡς σάλπιγγα, ἔχων Κυρίου Πρόδρομε, εὐήχως ἐφώνεις τοῖς παροῦσιν· Ἐγὼ ἐν ὕδατι βαπτίζω ὑμᾶς, ὀπίσω μου δ' ἄλλος ἰσχυρός, βαπτίζων ἐλεύσεται, ἐν πυρί τε καὶ πνεύματι."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
meMA.Ode5C13.text = "Ὁ θεῖος προέφησε Προφήτης· Ἥξει Κύριος, Ἄγγελος αὐτοῦ δὲ πρὸ προσώπου, στέλλεται τρίβους προετοιμάζων αὐτοῦ, ὃς σώσει λαὸν τὸν ἀπειθῆ, καὶ κατασκευάσειεν, εἰς Θεοῦ κλῆρον ἅγιον."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
meMA.Ode5C1T.text = "Νηδὺϊ φερόμενος, τῇ μητρικῇ ὁ Πρόδρομος, τῇ παρθενικῇ ὡς ἔγνω ἔνδον, τὸν τούτου Κτίστην φρικτῶς φερόμενον, Πνεύματι Ἁγίῳ φωτισθείς, ἔνδοθεν ἐσκίρτησε, καὶ πεσὼν προσεκύνησεν."

meMA.Ode5C21.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C21.melody = he.h.m8_gr_GR_cog.OrthrizontesVoomenSoi.name
meMA.Ode5C21.text = "Τὸν Μητροφάνους θρόνον Ἀλέξανδρε, κατακοσμῶν, καὶ πρὸ θρόνου ἔλαμψας, ἐν Ἱεράρχαις τοῖς δόγμασι."

meMA.Ode5C22.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C22.melody = he.h.m8_gr_GR_cog.OrthrizontesVoomenSoi.name
meMA.Ode5C22.text = " Σὺ ἐν ποιμέσι μάκαρ διέπρεψας θαυμασίως· ὤφθης ἐν Νικαίᾳ γάρ, μέσον Πατέρων ἀγλάϊσμα."

meMA.Ode5C23.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C23.melody = he.h.m8_gr_GR_cog.OrthrizontesVoomenSoi.name
meMA.Ode5C23.text = "Τῶν λόγων σου ἡ χάρις ἐστήριξεν, Ἐκκλησίαν, Ἰωάννη· στῦλον γὰρ ὀρθοδοξίας σὲ ἔχομεν."

meMA.Ode5C2T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode5C2T.melody = he.h.m8_gr_GR_cog.OrthrizontesVoomenSoi.name
meMA.Ode5C2T.text = "Παρθένον μετὰ τόκον ὑμνοῦμέν σε, Θεοτόκε· σὺ γὰρ τὸν Θεὸν Λόγον, σαρκὶ τῷ κόσμῳ ἐκύησας."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
meMA.Ode6C11.text = "Πρὸς σὲ λαὸς πονηρότατος, ἀφίκετο πειράζων καὶ φάσκων σοι· Σὺ εἶ ὁ Κύριος, αὐτὸς δὲ ἔφης, οὐκ ἔγωγε, ἄλλος δ' ἐστὶν ὃς ἥξει, ὀπίσω πρῶτός μου."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
meMA.Ode6C12.text = "Ῥευστῆς ἐγὼ πέλω φύσεως, ὃς ἔρχεται δ' ὀπίσω μου ἄρρευστος, πέλει καὶ ἄχρονος, καὶ ποιητὴς πάσης κτίσεως, οὗ οὐκ ἰσχύω λῦσαι, οὐδὲ τὰ πέδιλα."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
meMA.Ode6C13.text = "Ὁρῶντες πάντες συνέτρεχον, πόρνοι τε καὶ τελῶναι καὶ ἄσωτοι, καὶ ἐβαπτίζοντο, ἐκ σοῦ Προφῆτα πανένδοξε, τὴν πρὸς Χριστὸν πορείαν ἐκδιδασκόμενοι."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
meMA.Ode6C1T.text = "Δεινῶν παντοίων τοὺς δούλους σου, ἐξάρπασον Παρθένε Θεόνυμφε, τοὺς προσφυγόντας σου, ὑπὸ τὴν σκέπην τὴν ἄμαχον, καὶ τῆς φρικτῆς μελλούσης ῥῦσαι κολάσεως."

meMA.Ode6C21.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C21.melody = he.h.m8_gr_GR_cog.ChitonaMoiParaschou.name
meMA.Ode6C21.text = "Ἀνδρείως τοῦ Χριστοῦ προπολεμῶν, Ἐκκλησίας ἔκτεινας, εὐχῆς Πάτερ βέλεσι, τὸν ἐπώνυμον μανίας ἱερώτατε."

meMA.Ode6C22.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C22.melody = he.h.m8_gr_GR_cog.ChitonaMoiParaschou.name
meMA.Ode6C22.text = "Ὡς κλῆμα τῆς ἀμπέλου τῆς ζωῆς, Ἰωάννη ἔντιμον, πλεῖστον ἐξήνθησας, τῷ Δεσπότῃ σου καρπὸν παμμακάριστε."

meMA.Ode6C23.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C23.melody = he.h.m8_gr_GR_cog.ChitonaMoiParaschou.name
meMA.Ode6C23.text = "Ἡγεῖσθαι παρῃτήσω εὐσεβῶς, Βασιλίδος πόλεως, Εἰκόνων προσκύνησιν, ἀθετούσης παρανόμως Παῦλε τίμιε."

meMA.Ode6C2T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode6C2T.melody = he.h.m8_gr_GR_cog.ChitonaMoiParaschou.name
meMA.Ode6C2T.text = "Ἡ μόνη διὰ λόγου ἐν σαρκί, τὸν Λόγον κυήσασα, ῥῦσαι δεόμεθα, τῶν παγίδων τοῦ ἐχθροῦ τὰς ψυχὰς ἡμῶν."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode8
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.text = he.h.m8_gr_GR_cog.ChitonaMoiParaschou.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode5
meMA.Kontakion1.melody = ""
meMA.Kontakion1.text = me.m08.d29_gr_GR_cog.meMA.Kontakion1.text

meMA.Synaxarion.text = ""

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name 
meMA.Ode7C11.text = "Ῥωμαλέως, τῷ Ἡρῴδῃ ἀνθιστάμενος, ἔνδοξε Πρόδρομε, τοῦτον ἐλέγχεις βοῶν· Οὐκ ἔξεστιν ἔχειν σε εὐνὴν ὁμαίμονος, ὡς ἀθέμιτον· ὁ δὲ μὴ ἀνασχόμενος, τὴν σὴν κάραν ἀποτέμνει."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
meMA.Ode7C12.text = "Οἷα ἔνδοξος Προφήτης πρὸ γεννήσεως, ἔγνως τὸν Κύριον, καὶ ὡς ἀμνὸν τοῦ Θεοῦ, τοῖς πᾶσιν ὑπέδειξας, πρὸς θεοσέβειαν, ἕλκων ἅπαντας, ἀναβοᾶν πρὸς Κύριον· ὁ Θεὸς εὐλογητὸς εἶ."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
meMA.Ode7C13.text = "Μέλλων τέλος, τὸ μακάριον ἐκδέχεσθαι, Πρόδρομε ἔνδοξε, σοὺς μαθητὰς πρὸς Χριστόν, τὸν Θεὸν ἀπέστειλας, ἐπερωτῶν αὐτόν· Σὺ εἶ Κύριε, τοῦ σῶσαι ὁ ἐρχόμενος, ἢ ἕτερον προσδοκῶμεν;"

meMA.Ode7C14.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C14.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
meMA.Ode7C14.text = "Ὁ Χριστὸς, τοῖς μαθηταῖς προσαπεφθέγξατο· Ἄπιτε εἴπατε, τῷ Ἰωάννῃ· ἰδού, νεκροὶ ἐξανίστανται, κωφοὶ ἀκούουσιν, ἐκκαθαίρονται λεπροί, καὶ ὥσπερ ἔλαφος οἱ χωλοὶ περιπατοῦσιν."

meMA.Ode7C21.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C21.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C21.text = "Μυστηπόλος τῆς ἄνω, ὑπερθέου Τριάδος ὤφθης πανένδοξε, ἁγνῶς ταύτην λατρεύων, καὶ μέλπων ἀνενδότως· Ἱεράρχα Ἀλέξανδρε· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητός εἶ."

meMA.Ode7C22.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C22.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C22.text = "Θησαυρον σε δογμάτων, καὶ πηγὴν τῶν θαυμάτων, Χριστὸς ἀνέδειξε, πηγάζοντα τὸν πλοῦτον, τὸν τῆς θεογνωσίας, Ἰωάννη τοῖς κράζουσιν· ὁ τῶν Πατέρων ἡμῶν, Θεός εὐλογητὸς εἶ."

meMA.Ode7C23.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C23.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C23.text = "Ὡς ἰσάγγελον βίον, κατὰ Παῦλον τὸν θεῖον, ὡς καὶ συνώνυμον, κτησαμένῳ παμμάκαρ, τὸν πλοῦτόν σοι τῶν λόγων, Χριστὸς παρέχει ἄφθονον· Εὐλογητὸς βοῶντι, Χριστός εἰς τοὺς αἰῶνας."

meMA.Ode7C2T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode7C2T.melody = he.h.m8_gr_GR_cog.OiEkTisIoudaias.name
meMA.Ode7C2T.text = "Παρθενικῆς ἐκ νηδύος, σαρκωθεὶς ἐπεφάνης, εἰς σωτηρίαν ἡμῶν· διό σου τὴν Μητέρα, εἰδότες Θεοτόκον, εὐχαρίστως κραυγάζομεν· ὁ τῶν Πατέρων ἡμῶν, Θεὸς εὐλογητός εἶ."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C11.text = "Νόμου παλαιᾶς τε καὶ τῆς νέας, Προφήτης καὶ Προφητῶν πάντων ὑπέρτερος, ὑπάρχεις πανεύφημε· οὐ γὰρ ἔφυς κάλαμος, πνεύμασι ῥιπιζόμενος, ἀλλ' ἔστης ἄσειστος. Τὸν Κύριον βοῶν εὐλογεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας. "

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C12.text = "Μαινόμενος οἴστρῳ ὁ Ἡρῴδης, μαχλώσης Ἡρῳδιάδος τὸ θυγάτριον, βλέπων κατορχούμενον, ἔφησεν ὁ ἄθλιος. Ὅ μοι αἰτήσῃς δίδωμι, ἕως ἡμίσεος, τῆς ἐμῆς βασιλείας μεθ' ὅρκου, κἀκεῖνο ἐξαιτεῖται, τὴν Κάραν τοῦ Προδρόμου."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C13.text = "Εἶχε μὲν φρουρᾷ τὸν Ἰωάννην, Ἡρῴδην δὲ πότος εὐφραινόμενον· τότε ἡ πανάσεμνος, κόρη κατωρχήσατο, ἀρέσασα δὲ εἴληφε, Κάραν τὴν πάντιμον, Προδρόμου καὶ ἁγίαν ὡς ταύτην, τῇ μητρὶ προσάξῃ, τιμαλφέστατον δῶρον."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
meMA.Ode8C1T.text = "Λύτρωσαι τὴν πόλιν σου Παρθένε, ἐκ πάσης ἀπειλῆς τε καὶ κακώσεως, σεισμοῦ καταπτώσεως, βαρβαρικῆς ἁλώσεως, καὶ φοβερᾶς κολάσεως, τοῦ αἰωνίου πυρός· σὲ γάρ μετὰ Θεὸν σωτηρίαν, ἅπαντες πλουτοῦμεν, Χριστιανοὶ Παρθένε."

meMA.Ode8C21.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C21.melody = he.h.m8_gr_GR_cog.TonEkPatrosProAionon.name
meMA.Ode8C21.text = "Τὸν δυσσεβῆ προσευχῇ τῇ παναλκεῖ σου, Ἄρειον κτείνας Ἀλέξανδρε θεόφρον, ἐν τοῖς βεβήλοις τόποις, ὑβρίσαντα ἠμύνω, βροχίσας ὡς Ἰούδαν."

meMA.Ode8C22.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C22.melody = he.h.m8_gr_GR_cog.TonEkPatrosProAionon.name
meMA.Ode8C22.text = "Ὁ θεομάχος Χριστοῦ χιτῶνα ῥήξας, ὡς ὁ προδότης πρηνὴς λακήσας μέσον, νῦν θρηνεῖ ἐν ᾍδῃ, ψυχῶν ἀδίκων μόρον, θανάτῳ τῷ δικαίῳ."

meMA.Ode8C23.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C23.melody = he.h.m8_gr_GR_cog.TonEkPatrosProAionon.name
meMA.Ode8C23.text = "Ἐν ἐγκρατείαις ὡς ἄσαρκος ἐφάνης, ἐν ἀγρυπνίαις μικροῦ δεῖν καὶ ἀναίμων, καὶ ἐν διδασκαλίαις, πηγὴ τοῦ Παραδείσου, παμμάκαρ, Ἰωάννη."

meMA.Ode8C2T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode8C2T.melody = he.h.m8_gr_GR_cog.TonEkPatrosProAionon.name
meMA.Ode8C2T.text = "Τὸν ἀνερμήνευτον τόκον σου Μαρία, τὶς μὴ ὑμνήσει βροτῶν χοροστασία; ὅτι παρθενεύεις πρὸ τόκου, καὶ ἐν τόκῳ ἀεὶ εὐλογημένη."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode8
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.text = he.h.m8_gr_GR_cog.TonEkPatrosProAionon.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
meMA.Ode9C11.text = "Πότος βδελυρός, καὶ δεῖπνος πανάσεμνος, ὅρχησις ἄτερπνος, ἔθελξαν τὸν δείλαιον, μάλα πρὸς οἶστρον ἀκολασίας δεινῆς, καὶ τῆς μαινάδος μίσθωμα, κάραν ἐκδίδωσι, τοῦ Προδρόμου, ἧς οὐδὲ ἀντάξιος, κόσμος ἅπας. Φρικτὸν τὸ μυστήριον!"

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
meMA.Ode9C12.text = "Ὢ πῶς ὁ δεινός, Ἡρῴδης οὐκ ἔφριξε, Κριτὴν ἀλάθητον! ἀλλ' ἐν τῇ ψυχῇ αὐτοῦ, ἔνδον δολίως φόνον ἐνέτρεφε, τοῦ Βαπτιστοῦ, μαινόμενος, ὡς ἐλεγχόμενος, καὶ μὴ φέρων, εὐκαιρίαν ἤθελε, καὶ εὑρών, ἀποτέμνει τὸν δίκαιον."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
meMA.Ode9C13.text = "Ὄντως ἀπὸ γῆς, εἰσέδυς εἰς Ἅγια, οἷά περ Ἄγγελος, ὡς ἀγγελικῶς ἐν γῇ, σοφὲ βιώσας Κυρίου Πρόδρομε, τῶν ὑμνητῶν σου μέμνησο, ἄνωθεν ἔνδοξε, καὶ πταισμάτων, αἴτησαι τὴν ἄφεσιν, καὶ εἰρήνην παράσχου καὶ ἔλεος."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
meMA.Ode9C1T.text = "Ὦ Θεοῦ Υἱέ, μακρόθυμε Κύριε καὶ πολυέλεε, τοῦ σοῦ Βαπτιστοῦ λιταῖς, καὶ ἱκεσίαις τῆς κυησάσης σε, καὶ πάντων τῶν Ἁγίων σου, σῶσον τοὺς δούλους σου, καὶ χορήγει, ἄνωθεν τὰ τρόπαια, βασιλεῦσιν ἡμῶν, ὡς φιλάνθρωπος."

meMA.Ode9C21.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C21.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C21.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C21.text = "Ἔννομος γενόμενος, ὁ θεῖος ὄντως φοιτητής σου, καὶ σοῖς ζωηφόροις λόγοις, πειθαρχήσας Ἀλέξανδρος, Ἱεράρχης πεφηνώς, Κύριε ὅσιος πρᾶος, σώφρων, νῦν ἐπαξίως μακαρίζεται."

meMA.Ode9C22.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C22.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C22.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C22.text = " Μύστης ἀκριβέστατος, γέγονας πόθῳ τῶν ἀρρήτων, ὦ θεομακάριστε Πατὴρ ἡμῶν, Ἰωάννη πανόλβιε, καὶ μυσταγωγὸς τῶν μυστηρίων, καὶ κήρυξ δογμάτων θείων, καὶ λατρευτῆς Τριάδος ἔνδοξε."

meMA.Ode9C23.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C23.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C23.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C23.text = "Σὲ τὸν ἀνεξίκακον, Πάτερ, καὶ ὅσιον ποιμένα, ἄμεμπτον σεπτὸν ἀμίαντον, κήρυκά τε τῆς πίστεως, τῆς ὀρθοδοξίας τὸν διδάσκαλον, ἅπαντες ἐπαξίως συμφώνως, Παῦλε μακαρίζομεν."

meMA.Ode9C2T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2T.mode = misc_gr_GR_cog.Mode8
meMA.Ode9C2T.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
meMA.Ode9C2T.text = "Ἄχραντε πανάμωμε, τὸ σκεῦος τὸ τῆς εὐωδίας, αἴτησαι Χριστὸν ὃν ἔτεκες, βασιλεῖ μὲν τὰ τρόπαια, τῷ φιλοχρίστῳ δὲ λαῷ, δοῦναι τὴν ἄφεσιν τῶν πταισμάτων· σὲ γὰρ ἀπαύστως μεγαλύνομεν."

meMA.Ode9H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9H.mode = misc_gr_GR_cog.Mode8
meMA.Ode9H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9H.text = he.h.m8_gr_GR_cog.SeTinApeirogamon.text

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.ToisMathitaisSynelthomen.name
meMA.Exaposteilarion1.text = "Ἡρῴδης ὁ παράνομος, τοῦ Προδρόμου τὴν θείαν, καὶ ἱερὰν ἀπέτεμε, κεφαλὴν τοὺς ἐλέγχους, μὴ φέρων ταύτης ὁ ἄφρων· ὤ φρικτῆς τόλμης ὄντως! Θρηνείτω οὖν ὁ ἄθλιος, ἄνομον φόνον πράξας· ἡμεῖς δὲ νῦν, τὴν τιμίαν κάραν ὕμνοις τιμῶντες, τὸν Βαπτιστὴν γεραίρομεν, τοῦ Χριστοῦ ἐπαξίως."

meMA.ExapTheotokion.mode = misc_gr_GR_cog.Mode2
meMA.ExapTheotokion.melody = he.a.m2_gr_GR_cog.ToisMathitaisSynelthomen.name
meMA.ExapTheotokion.text = "Εἰ καὶ ἡττᾶται Πάναγνε, λογικὴ πᾶσα φύσις, πρὸς ἐγκωμίων ἔπαινον, σοῦ Θεὸν τῆς τεκούσης· καὶ γὰρ σαφῶς ὑπερτέρα, πάσης κτίσεως πέλεις· ἀλλ' οὖν τὸ κατὰ δύναμιν, οὐκ ἀπόβλητον πάντως· ὅθεν κἀμοῦ, δέχου τὸν βραχύτατον ὕμνον ὥσπερ, ὁ σὸς Υἱὸς ἐδέξατο, τὰ λεπτὰ πρὶν τῆς χήρας."

meMA.AposGlory.mode = misc_gr_GR_cog.Mode8
meMA.AposGlory.melody = ""
meMA.AposGlory.text = "Πρόδρομε τοῦ Σωτῆρος, εἰ καὶ φόνῳ διέλυσεν ὁ Ἡρῴδης, σὲ τὸν τῆς ἀληθείας κήρυκα, ἀλλ' ἡ τοῦ στόματός σου φεγγοβόλος λαμπάς, καὶ τοῖς ἐν, ᾍδῃ τὸ τῆς πίστεως φέγγος καλῶς ἐδᾳδούχει· ὅθεν ἱκέτευε, ἐλεηθῆναι τὰς ψυχὰς ἡμῶν."

