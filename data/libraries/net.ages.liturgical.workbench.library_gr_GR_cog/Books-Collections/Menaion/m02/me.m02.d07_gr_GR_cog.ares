meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode1
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.text = me.m02.d02_gr_GR_cog.meVE.Apolytikion1.text 

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion2.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
meVE.Apolytikion2.text = "Τῷ μύρῳ τοῦ Πνεύματος, ποιμὴν Λαμψάκου ὀφθείς, τὴν θείαν ἐνέργειαν, παρὰ Θεοῦ δαψιλῶς, θαυμάτων ἐπλούτησας, δαίμονας ἀπελαύνειν, ἀσθενοῦντας ἰᾶσθαι, νόσους ἀποδιώκειν, καὶ πληροῦν τὰς αἰτήσεις, Παρθένε Ἱεράρχα, τῶν προσιόντων σοι."

meVE.Apolytikion3.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion3.mode = misc_gr_GR_cog.Mode1
meVE.Apolytikion3.melody = he.a.m1_gr_GR_cog.TisErimouPolitis.name
meVE.Apolytikion3.text = "Τῆς Ἑλλάδος τὸ κλέος, καὶ Ὁσίων τὸ καύχημα, καὶ τὸν τοῦ Στειρείου φωστήρα, καὶ οἰκήτορα ὅσιον, τιμήσωμεν ᾀσμάτων ἐν ᾠδαῖς, Λουκᾶν τὸν θεοφόρον εὐσεβῶς· τῷ Χριστῷ γὰρ οἰκειοῦται διαπαντός, τοὺς πίστει ἀνακράζοντας· δόξα τῷ δεδωκότι σοι ἰσχύν, δόξα τῷ σὲ στεφανώσαντι, δόξα τῷ ἐνεργοῦντι διὰ σοῦ, πᾶσιν ἰάματα."

meMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
meMA.Kathisma11.melody = he.a.m4_gr_GR_cog.KateplagiIosif.name
meMA.Kathisma11.text = me.m02.d02_gr_GR_cog.meMA.Kathisma31.text

meMA.Kathisma21.mode = misc_gr_GR_cog.Mode1
meMA.Kathisma21.melody = he.a.m1_gr_GR_cog.TonTafonSouSotir.name
meMA.Kathisma21.text = "Ὁ ὢν σὺν τῷ Πατρί, ἐπὶ θρόνου ἁγίου, ἐλθὼν ἐπὶ τῆς γῆς, ἐκ Παρθένου ἐτέχθη, καὶ βρέφος ἐγένετο, χρόνοις ὢν ἀπερίγραπτος, ὃν δεξάμενος, ὁ Συμεὼν ἐν ἀγκάλαις, χαίρων ἔλεγε· Νῦν ἀπολύεις Οἰκτίρμον, εὐφράνας τὸν δοῦλόν σου."

meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς· Τῶν θαυμάτων σου τὴν χάριν μέλπω, Μάκαρ. Ἰωσήφ."

meMA.C2Acrostic.text = "ἔχων ἀκροστιχίδα· Λουκᾶν ἐπαινῶ τὸ κλέος τῆς Ἑλλάδος. Ἀμήν."

meMA.Ode1C1H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode1C1H.text = he.h.m2_gr_GR_cog.EnVythoKatestrose.text

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C11.melody = he.h.m2_gr_GR_cog.EnVythoKatestrose.name
meMA.Ode1C11.text = "Τῷ φωτὶ τῷ θείῳ συγκραθείς, Πάτερ ἱερώτατε, δι' ἀρετῶν φῶς καθωράθης δεύτερον· διὸ τὴν φωτοφόρον σου, καὶ πανέορτον τοὺς τελοῦντας πανήγυριν, σκότους ἁμαρτίας, ταῖς σαῖς ἱκεσίαις ἐλευθέρωσον."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C12.melody = he.h.m2_gr_GR_cog.EnVythoKatestrose.name
meMA.Ode1C12.text = "Ὡς φαιδρὸς ἀνέτειλας ἀστήρ, ἐν τῷ στερεώματι, τῆς τοῦ Θεοῦ Ἐκκλησίας Παρθένιε, ἰαμάτων λάμψεσιν, ἱερώτατε τῶν παθῶν τὴν σκοτόμαιναν, χάριτι διώκων, καὶ φωταγωγῶν τοὺς προσιόντας σοι."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C13.melody = he.h.m2_gr_GR_cog.EnVythoKatestrose.name
meMA.Ode1C13.text = "Νοσημάτων σκότος πονηρόν, ἔλυσας Παρθένιε, τῷ φωτισμῷ σου τῶν σεπτῶν ἐντεύξεων· ὅθεν δυσωποῦμέν σε, τῶν ψυχῶν ἡμῶν τὰς ὀδύνας θεράπευσον, καὶ τὰς τῶν σωμάτων, πάσας μαλακίας ἀποδίωξον."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode1C1T.melody = he.h.m2_gr_GR_cog.EnVythoKatestrose.name
meMA.Ode1C1T.text = "Ἀγαθὸν κυήσασα Θεόν, ἄχραντε ἀγάθυνον, τὴν ἐν πολλοῖς κεκακωμένην πάθεσι, ταπεινὴν καρδίαν μου, καὶ τὸ θέλημα ἐκτελεῖν ἐνδυνάμωσον, τοῦ σωθῆναι πάντας, θέλοντος ἀνθρώπους Λυτρωτοῦ ἡμῶν."

meMA.Ode1C2H.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode1C2H.text = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.text

meMA.Ode1C21.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C21.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C21.text = "Λόγον μοι δίδου θεόπτα παρεστηκώς, θρόνῳ τοῦ Δεσπότου σου, ἀνυμνῆσαι τὴν φαιδράν, καὶ φωσφόρον μνήμην σου Λουκᾶ, διαλύσας τὴν ἀχλὺν τῆς διανοίας μου."

meMA.Ode1C22.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C22.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C22.text = "Ὁλοσχερῶς σε ποθήσας ὁ Ἱερός, Σῶτερ πρὸς τὴν ἄσκησιν, ἀπεδύσατο στεῤῥῶς, θλίψεις μόχθους πόνους καρτερῶν, ἀνθ' ὧν εὕρατο τρυφὴν τὴν ἀτελεύτητον."

meMA.Ode1C23.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C23.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C23.text = "Ὑπωπιάζων τὸ σῶμα Πάτερ Λουκᾶ, ἐγκρατείᾳ πάντοτε, ἀγρυπνίᾳ συνεχεῖ, κακουχίᾳ πάσῃ τε σοφῶς, τὰς ὁρμὰς τῶν ἐμπαθῶν ἡδονῶν ἔστησας."

meMA.Ode1C2T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode1C2T.melody = he.h.m6_gr_GR_cog.OsEnIpeiroPezefsas.name
meMA.Ode1C2T.text = "Κυριοτόκε Παρθένε ἡ τὸν Θεόν, ἐν σαρκὶ γεννήσασα, τὸν ἀχώρητον παντί, σαρκωθέντα οἴκτῳ τῶν βροτῶν, τυραννίδος με παθῶν δεῖξον ὑπέρτερον."

meMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3C1H.text = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.text

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C11.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
meMA.Ode3C11.text = "Ὑπῆρξας καθαιρέτης τῆς τυραννίδος, παμμάκαρ τῶν Δαιμόνων τοῦ ὀλεθρίου, εἰδώλων τε κατέστρεψας τὰ τεμένη, καὶ θείους ἤγειρας, ναοὺς εἰς αἴνεσιν, τοῦ Χριστοῦ Παρθένιε ἱερώτατε."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C12.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
meMA.Ode3C12.text = "Μεγίσταις διαπρέπων θαυματουργίαις, καρκίνου χαλεπώτατον παύεις πάθος, καὶ ὄμματα διήνοιξας πηρωθέντα, καὶ ἐφυγάδευσας Πάτερ Παρθένιε, πονηρίας πνεύματα θείᾳ χάριτι."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C13.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
meMA.Ode3C13.text = "Ἀνέστησας κακίστως θανατωθέντα, τὸν πάλαι ὑπουργοῦντα τῷ θείω ἔργῳ· Θεὸς γὰρ ὁ τὸν θάνατον καταργήσας, χάριν σοι δέδωκε μάκαρ Παρθένιε, ἀνιστᾶν τεθνήξαντας δι' ἐντεύξεως."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode3C1T.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
meMA.Ode3C1T.text = "Ὡμίλησεν ἀνθρώποις ἐκ σοῦ τὴν σάρκα, ὁ Κύριος φορέσας ἀνερμηνεύτως· αὐτὸν οὖν ἐκδυσώπησον Παναγία, τοῦ οἰκτειρῆσαί με καταπονούμενον, προσβολαῖς τοῦ ὄφεως καὶ δονούμενον."

meMA.Ode3C2H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3C2H.text = he.h.m6_gr_GR_cog.OukEstinAgios.text

meMA.Ode3C21.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C21.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C21.text = "Ἀνεπτερώθης τῷ θερμῷ, ἔρωτι τοῦ Σωτῆρος, καὶ γονέων τὸ φίλτρον, ὑπερεῖδες δι' αὐτόν, καὶ γέγονας ὦ Λουκᾶ, ξένος πάντων τῶν τερπνῶν μακάριε."

meMA.Ode3C22.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C22.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C22.text = "Ναὸν Κυρίου σεαυτόν, ἀπεργάσασθαι θέλων, ἐρημίας κατῴκεις ὡς ὁ πάλαι Ἠλιού, πρὸς δαίμονας πολεμῶν· ὅθεν εὗρες τέλος τὸ μακάριον."

meMA.Ode3C23.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C23.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C23.text = "Ἐν πάσῃ θείᾳ ἐντολῇ, εὐπειθὴς ὡς οἰκέτης, ἀνεδείχθης παμμάκαρ, καλῶς αὐτὰς ἐκτελῶν, καὶ ταπεινῶν σεαυτόν· ὅθεν ὕψος ἔλαβες οὐράνιον."

meMA.Ode3C2T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode3C2T.melody = he.h.m6_gr_GR_cog.OukEstinAgios.name
meMA.Ode3C2T.text = "Παρθένε Μήτηρ τοῦ Θεοῦ, ἡ τεκοῦσα τὸν Λόγον, ὑπὲρ λόγον ὀφθέντα, δι' ἡμᾶς ὅπερ ἡμεῖς, ἱκέτευε ἐκτενῶς, ἀλογίας πάσης με λυτρώσασθαι."

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode3
meMA.Kontakion1.melody = he.a.m3_gr_GR_cog.IParthenosSimeron.name
meMA.Kontakion1.text = "Τῶν θαυμάτων εἴληφας, τὴν θείαν χάριν θεόφρον, ἱερὲ Παρθένιε, θαυματουργὲ Θεοφόρε, ἅπαντα τὰ τῶν πιστῶν πάθη ἀποκαθαίρων, πνεύματα τῆς πονηρίας Πάτερ ἐλαύνων· διὰ τοῦτό σε ὑμνοῦμεν, ὡς μέγαν μύστην Θεοῦ τῆς χάριτος."

meMA.Oikos1.text = "Φῶς νοερὸν τὸ ἐκ φωτός, Πατρὸς τοῦ προανάρχου, Υἱὲ Θεοῦ καὶ Λόγε, ὁ καταυγάζων πᾶσαν τὴν οἰκουμένην θεϊκῶς, φώτισόν μου τὰς φρένας, τὸν νοῦν καὶ τὸν λογισμόν, τοῦ ἀνυμνῆσαι σήμερον τὴν φαιδρὰν ταύτην πανήγυριν καὶ σεμνοπρεπεστάτην τοῦ Ὁσίου Παρθενίου· αὐτὸς γὰρ ἀληθῶς ἐν γῇ θαυμασίαν διετέλεσε ζωὴν καὶ πολιτείαν· διὰ τοῦτο συνελθόντες εὐφημοῦμεν αὐτόν, ὡς μέγαν μύστην Θεοῦ τῆς χάριτος."

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma1.text = "Τῆς Λαμψάκου ὁ μέγας θαυματουργός, ἀναβὰς ἐν τῷ ὕψει τῶν ἀρετῶν, ἀστράπτεις τοῖς πέρασι, τῷ φωτὶ τῶν ἰάσεων, τοὺς ζοφώδεις Δαίμονας, σκορπίζων φαιδρότατα, καὶ ἐλαύνων νόσους, χρησταῖς ἐπικλήσεσιν· ὅθεν καὶ τῆς πλάνης, τῶν εἰδώλων καθάρας, πᾶσαν τὴν Ἑλλήσποντον, καταλάμπεις τοῖς θαύμασι, Θεοφόρε Παρθένιε, πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma2.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
meMA.Ode3Kathisma2.text = "Ἀπὸ βρέφους ποθήσας τὴν ἀρετήν, τῶν ἡδέων ἀπέσχου πάντων Σοφέ, νηστείαν καὶ ἄσκησιν, ἀκριβῆ ἐνδεικνύμενος, καὶ μητρὸς τὸν πόθον, οὐδὲν ἡγησάμενος, ἐν Σεμνείῳ φέρων, σαυτὸν λάθρα δέδωκας· πάλιν δὲ μηνύει, σὲ Θεὸς τῇ μητρί σου, θρηνούσῃ τὴν στέρησιν, σοῦ δικαίως πανάριστε, ᾗ καὶ νῦν συνηδόμενος· πρέσβευε Χριστῷ τῷ Θεῷ, τῶν πταισμάτων ἄφεσιν δωρήσασθαι, τοῖς ἑορτάζουσι πόθῳ, τὴν ἁγίαν μνήμην σου."

meMA.Ode3Kathisma3.mode = misc_gr_GR_cog.Mode8
meMA.Ode3Kathisma3.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
meMA.Ode3Kathisma3.text = "Ἐτέχθης ἐκ Μητρός, ὁ προάναρχος Λόγος, προσήχθης τῷ ναῷ, ὁ ἀκατάληπτος φύσει· χαίρων δὲ ὁ Πρεσβύτης, ἐν ταῖς ἀγκάλαις σε ὑπεδέξατο, κράζων· Νῦν ἀπολύεις, ὃν ἐπεσκέψω κατὰ τὸ ῥῆμά σου, ὁ εὐδοκήσας σῶσαι ὡς Θεός, τὸ γένος τῶν ἀνθρώπων."

meMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode4C1H.text = he.h.m2_gr_GR_cog.ElilythasEkParthenou.text

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C11.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C11.text = "Νοσήματα, προσευχῇ ἐθεράπευσας ἄμετρα, συσφίγγων Παρθένιε, τοὺς παρεθέντας ἐν χάριτι, ὥσπερ ὁ Δεσπότης σου, οὗ τὴν πολλὴν ἐμιμήσω ἀγαθότητα."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C12.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C12.text = "Σὺ δέδωκας, ἐνεργεῖν τοῖς βαφεῦσι τὰ πρόσφορα, τὸν εἴργοντα δαίμονα, ἀποσοβήσας Παρθένιε, ταῖς πρὸς τὸν φιλάνθρωπον, ἱερωτάταις σου Πάτερ παρακλήσεσι."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C13.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C13.text = "Ὁ Κύριος, τὰς ἐντεύξεις σου Πάτερ δεξάμενος, τὴν ἄγραν παρέδωκε, πλουσιωτάτως τοῖς χρῄζουσι, τῇ τοῦ ἀνανήξαντος, θαυματουργίᾳ ἰχθύος μεγαλύνων σε."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode4C1T.melody = he.h.m2_gr_GR_cog.ElilythasEkParthenou.name
meMA.Ode4C1T.text = "Τὰ τραύματα, τῆς ψυχῆς μου Παρθένε θεράπευσον, τὸν νοῦν μου εἰρήνευσον, ὑπὸ παθῶν ταραττόμενον, καὶ ταῖς παραβάσεσι, τῶν ἐντολῶν τοῦ Σωτῆρος σκοτιζόμενον."

meMA.Ode4C2H.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode4C2H.text = he.h.m6_gr_GR_cog.ChristosMouDynamis.text

meMA.Ode4C21.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C21.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C21.text = "Ἄκακος Ὅσιος, εὐθὺς καὶ ἥμερος, καὶ ἀκέραιος ὤφθης Πάτερ Λουκᾶ· ὅθεν μεμακάρισαι, παρὰ τοῦ πάντων Ποιητοῦ, καὶ Δεσπότου παμμακάριστε."

meMA.Ode4C22.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C22.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C22.text = "Ἰδὼν ὁ φθόνος σε, καλῶς διάγοντα, τὴν ζωὴν ἐξεκαύθη καὶ πειρασμῶν, πλῆθός σοι ἐξήγειρεν, ἀλλ' ἐξηλέγχετο κενὸς λύκος, μάτην τὸ τοῦ λόγου χανών."

meMA.Ode4C23.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C23.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C23.text = "Νοὸς εὐθύτητι, τὸν βίον ἴθυνας, πρὸς τὰ κρείττω παμμάκαρ τῶν ἐφετῶν, πάντων τὸ ἀκρότατον, ἐπιποθῶν Πάτερ ἰδεῖν, οὗ εὐστόχως καὶ τετύχηκας."

meMA.Ode4C2T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode4C2T.melody = he.h.m6_gr_GR_cog.ChristosMouDynamis.name
meMA.Ode4C2T.text = "Ὡραίαν κάλλει σε, πασῶν εὑράμενος, γυναικῶν ὁ ὡραῖος Κόρη σεμνή, σοῦ τὴν Μήτραν ᾤκησε, καὶ δυσμορφίας με παθῶν, καὶ τοῦ σκότους ἐλυτρώσατο."

meMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode5C1H.text = he.h.m2_gr_GR_cog.OFotismos.text

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C11.melody = he.h.m2_gr_GR_cog.OFotismos.name
meMA.Ode5C11.text = "Ἡ φωταυγής, ἐνσκηνώσασα χάρις τῆς ἀπαθείας, ἐν τῇ σῇ καρδίᾳ θεόφρον Πάτερ, φιλαργυρίας τὸ βαθύτατον σκότος, τοῦ προέδρου διώκει ἄνοσον, τοῦτον συντηροῦσα ταῖς σαῖς παραινέσεσι."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C12.melody = he.h.m2_gr_GR_cog.OFotismos.name
meMA.Ode5C12.text = "Νόσῳ πολλῇ, τὸν κρατούμενον Πάτερ καὶ γεγονότα, Ὅσιε ἡμίξηρον προσευχαῖς σου, ὅλον εἰργάσω ὑγιῆ εὐχαρίστως, τὸν Δεσπότην Θεὸν δοξάζοντα, τὸν θαυματουργίαις πολλαῖς σε δοξάσαντα."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C13.melody = he.h.m2_gr_GR_cog.OFotismos.name
meMA.Ode5C13.text = "Χρῖσμα σεπτόν, περικείμενος Πάτερ ἱερωσύνης, ταύτην κατελάμπρυνας ἐπιδείξει, πολλῶν θαυμάτων, ὧν ἐτέλεσας ζῶν τε, θεοφόρε καὶ μετὰ θάνατον, Ἄγγελος καθάπερ βιώσας Παρθένιε."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode5C1T.melody = he.h.m2_gr_GR_cog.OFotismos.name
meMA.Ode5C1T.text = "Ἀπὸ παθῶν, ἀμαυρώσεως Κόρη ἀπὸ σκανδάλων, τῶν προσγινομένων ταῖς ἐπηρείαις, τοῦ ἀλλοτρίου μελλουσῶν τε βασάνων, αἰωνίων πρὸς τὸν φιλάνθρωπον, ῥῦσαί με ταῖς σαῖς δυσωπῶ παρακλήσεσι."

meMA.Ode5C2H.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode5C2H.text = he.h.m6_gr_GR_cog.ToTheioFengeiSou.text

meMA.Ode5C21.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C21.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C21.text = "Τὴν τῶν καμάτων σου ἀμοιβήν, παρὰ τοῦ πλουσίου Δοτῆρος, ἀπολαβὼν Πάτερ Ὅσιε, πέλαγος θαυμάτων πᾶσιν ἐδείκνυσο, τὰς νόσους ἀφανίζων τῶν προσιόντων σοι."

meMA.Ode5C22.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C22.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C22.text = "Οὐ κατενάρκησας τοῦ ἐχθροῦ, Πάτερ ταῖς μυρίαις προσβολαῖς, οὐ κατεπλάγης τὴν ἔπαρσιν, ἔστης δὲ ὡς πύργος στεῤῥὸς ἀκλόνητος, ταπείνωσιν ὡς θώρακα ἐνδυσάμενος."

meMA.Ode5C23.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C23.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C23.text = "Καλῶς τελέσας τὸν ἱερόν, βίον σου Παμμάκαρ, καὶ λαβὼν ἀπὸ Θεοῦ τὴν ἀνταπόδοσιν, χάριν ἰαμάτων, τυφλοὺς ἐφώτισας, χωλοὺς καὶ παραλύτους ἀποτελῶν ὑγιεῖς."

meMA.Ode5C2T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode5C2T.melody = he.h.m6_gr_GR_cog.ToTheioFengeiSou.name
meMA.Ode5C2T.text = "Λαοὶ ὑμνεῖτε τὴν ἀληθῶς, τέξασαν τὸν Λόγον τοῦ Θεοῦ, ἀνερμηνεύτως καὶ μείνασαν, πάλιν μετὰ τόκον Παρθένον ἄφθορον, ὡς πάντων προστασίαν καὶ σωτηρίαν βροτῶν."

meMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6C1H.text = he.h.m2_gr_GR_cog.ProsKyrionEkKitous.text

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C11.melody = he.h.m2_gr_GR_cog.ProsKyrionEkKitous.name
meMA.Ode6C11.text = "Ῥημάτων σου, εὐχῶν τε δυναστείᾳ Παρθένιε, ναοὶ ἐῤῥάγησαν τῶν εἰδώλων θείᾳ χάριτι, καὶ ἄπιστος λαὸς πίστιν προσελάβετο, μεγαλύνων συμφώνως τὸν Κύριον."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C12.melody = he.h.m2_gr_GR_cog.ProsKyrionEkKitous.name
meMA.Ode6C12.text = "Ἱκέτευσας, τὸν φιλάνθρωπον Λόγον σὺν δάκρυσι, καὶ τοῖς αἰτήσασιν ὄμβρους ἄνωθεν κατήγαγες, οἰκτείρων συμπαθῶς λαὸν κινδυνεύοντα, Ἱεράρχα Κυρίου Παρθένιε."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C13.melody = he.h.m2_gr_GR_cog.ProsKyrionEkKitous.name
meMA.Ode6C13.text = "Ναμάτων σου, μυστικῶν ἑπομβρίαις κατήρδευσας, καρδίαν πᾶσαν προσφοιτῶσάν σοι Παρθένιε, καὶ ἡ γῆ ἡ τῷ αὐχμῷ ξηρανθεῖσα δέχεται, ὑετοὺς θαυμαστοὺς εὐφορήσασα."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode6C1T.melody = he.h.m2_gr_GR_cog.ProsKyrionEkKitous.name
meMA.Ode6C1T.text = "Μὴ στήσῃς με, ἐν ἡμέρᾳ τῆς δίκης κατάκριτον, μὴ στηλιτεύσῃς με ἐπὶ πάντων τὸν ἀναίσθητον, Θεέ μου Ποιητά μου, ἔχων ἱκετεύουσαν, τὴν τεκοῦσαν ἀσπόρως σε Δέσποιναν."

meMA.Ode6C2H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6C2H.text = he.h.m6_gr_GR_cog.TouViouTinThalassan.text

meMA.Ode6C21.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C21.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C21.text = "Ἐλύθη ἡ πάλαι σοι, δολιόφρον καθ' ἡμῶν, ἰσχὺς καὶ φροῦδα γέγονε, τὰ πολλά σου σοφίσματα πονηρέ· ἰδοὺ γὰρ ἁπλούστατος, καὶ ἀπόνηρος νέος σε κατέβαλε."

meMA.Ode6C22.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C22.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C22.text = "Οὐκ ἔδωκας Ὅσιε, σοῖς βλεφάροις νυσταγμόν, οὐδὲ κροτάφοις ἄνεσιν, ἕως πᾶσαν κατέβαλες μηχανήν, τοῦ πλάνου ποικίλους σοι, πειρασμοὺς καθ' ἑκάστην ἐξεγείροντος."

meMA.Ode6C23.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C23.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C23.text = "Σωφρόνως διήνυσας, σοῦ τὸν βίον καὶ καλῶς, φιλοξενῶν ἑκάστοτε, ἐλεῶν τε πλουσίως καὶ δαψιλῶς· διὸ καὶ τετύχηκας, ἀφθαρσίας καὶ δόξης ἀκηράτου Λουκᾶ."

meMA.Ode6C2T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode6C2T.melody = he.h.m6_gr_GR_cog.TouViouTinThalassan.name
meMA.Ode6C2T.text = "Τριάδος γεγέννηκας, σὺ τὸν ἕνα σαρκικῶς, Θεοκυῆτορ Πάναγνε, ὑπὲρ νοῦν τε καὶ λόγον· διὸ βοῶ, Τριάδος τῆς θείας με, ταῖς πρεσβείαις σοι δεῖξον οἰκητήριον."

meMA.Kontakion2.mode = misc_gr_GR_cog.Mode8
meMA.Kontakion2.melody = he.a.m8_gr_GR_cog.TiYpermacho.name
meMA.Kontakion2.text = "Ὁ ἐκλεξάμενος Θεὸς πρὸ τοῦ πλασθῆναί σε, εἰς εὐαρέστησιν αὐτοῦ, οἷς οἶδε κρίμασι, προσλαβόμενος ἐκ μήτρας καθαγιάζει, καὶ οἰκεῖον ἑαυτοῦ δοῦλον δεικνύει σε, κατευθύνων σου Λουκᾶ τὰ διαβήματα, ὁ φιλάνθρωπος, ᾧ νῦν χαίρων παρίστασαι."

meMA.Oikos2.text = "Ὦ Πατέρων ἀρίστη καλλονὴ καὶ σεμνότης, ὦ πάντων ἀσκητῶν κοσμιότης· Χριστοῦ τὰς ἐντολὰς γὰρ πληρώσαντος, πῶς ὑμνήσω τὴν σὴν βιοτὴν ἔνδοξε, μὴ ἔχων λόγων δύναμιν; ἀλλ' ὅμως σοι θαῤῥῶν βοήσω."
meMA.Oikos22.text = "Χαῖρε, λαμπρὸν Μοναζόντων κλέος· χαῖρε, πιστῶν ὁδηγὲ καὶ λύχνε."
meMA.Oikos23.text = "Χαῖρε, τῆς ἐρήμου τερπνότατον βλάστημα· χαῖρε, οἰκουμένης λαμπτὴρ φαεινότατε."
meMA.Oikos24.text = "Χαῖρε, ὅτι κατεφρόνησας τῶν ῥεόντων καὶ φθαρτῶν· χαῖρε, ὅτι τὰ οὐράνια σὺν Ἀγγέλοις κατοικεῖς."
meMA.Oikos25.text = "Χαῖρε, τῶν ἀθυμούντων ταχινὲ παρακλῆτορ· χαῖρε, τῶν ἐν κινδύνοις ποθεινὲ παραστάτα."
meMA.Oikos26.text = "Χαῖρε, σεπτὸν δοχεῖον τοῦ Πνεύματος· χαῖρε, κλεινὸν Χριστοῦ οἰκητήριον."
meMA.Oikos27.text = "Χαῖρε, δι' οὗ δόσις πᾶσα ηὐγάσθη· χαῖρε, δι' οὗ ὁ Θεὸς ἐδοξάσθη."
meMA.Oikos28.text = "ᾯ νῦν χαίρων παρίστασαι."

meMA.Synaxarion.text = ""

meMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode7C1H.text = he.h.m2_gr_GR_cog.AntitheonProstagma.text

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C11.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
meMA.Ode7C11.text = "Ἐν σοὶ ἐνσκηνώσασα ἡ θεία χάρις, Προφήτην σε δείκνυσι, τὰ μέλλοντα φθεγγόμενον, ἐντεῦθεν προήγγειλας τῷ διαδέχεσθαι, μέλλοντι τὸν Πρόεδρον Σοφέ, ὡραϊσμένον θείαις λαμπρότησι."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C12.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
meMA.Ode7C12.text = "Λαμψάκου τὸν Πρόεδρον τῆς Ἑλλησπόντου, λαμπτῆρα τὸν ἄδυτον, θαυματουργίαις ἅπασαν, τὴν γῆν τὸν φαιδρύναντα, καὶ ἀπελάσαντα νύκτα, πολυώδυνον παθῶν, ὑμνολογοῦμεν πάντες Παρθένιον."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C13.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
meMA.Ode7C13.text = "Πολύχουν ἀπέδωκε τὸν ταύτης σπόρον, ἡ γῆ τῇ ἐντεύξει σου, πανθαύμαστε Παρθένιε, καὶ βότρυας ἤνεγκεν ἄμπελος ἄνικμος, οὕτω σε δοξάσαντος Θεοῦ, τοῦ δοξασθέντος τῇ πολιτείᾳ σου."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode7C1T.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
meMA.Ode7C1T.text = "Ὡράθης πανάμωμε ἁγιωτέρα τῶν ἄνω Δυνάμεων· Θεὸν γὰρ ἐσωμάτωσας, ὃν Τάξεις ἀσώματοι φόβῳ δοξάζουσι· τοῦτον οὖν ἱκέτευε ἀεί, τοῦ οἰκτειρῆσαι τοὺς σὲ δοξάζοντας."

meMA.Ode7C2H.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode7C2H.text = he.h.m6_gr_GR_cog.OiPaidesEnVavyloni.text

meMA.Ode7C21.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C21.melody = he.h.m6_gr_GR_cog.OiPaidesEnVavyloni.name
meMA.Ode7C21.text = "Ἡ θήκη τῶν σῶν λειψάνων, Λουκᾶ πηγὴ ἀναδέδεικται, ἰαμάτων ἐν ᾗ πᾶς προστρέχων, ἐκλυτροῦται παντοίων παθῶν, σὲ μακαρίζων Ὅσιε, καὶ ἡμῶν τὸν Δεσπότην Χριστόν."

meMA.Ode7C22.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C22.melody = he.h.m6_gr_GR_cog.OiPaidesEnVavyloni.name
meMA.Ode7C22.text = "Σημείοις μεγίστοις Πάτερ, ἐδόξασέ σε ὁ Κύριος ὃν ἐδόξασας σύ, διὰ πόνων τῆς ἀσκήσεως πρότερον· Εὐλογητὸς γὰρ ἔκραζες, ὁ Θεὸς τῶν Πατέρων ἡμῶν."

meMA.Ode7C23.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C23.melody = he.h.m6_gr_GR_cog.OiPaidesEnVavyloni.name
meMA.Ode7C23.text = "Ἐν βίῳ περιφανής τε, καὶ ἐν τοῖς θαύμασι μέγιστος, καὶ ἐν πᾶσι καλοῖς ἀνεδείχθης, ἀπαράμιλλος Ὅσιε, διὸ τῶν πόνων εὕρηκας, ἀμοιβὴν βασιλείαν Θεοῦ."

meMA.Ode7C2T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode7C2T.melody = he.h.m6_gr_GR_cog.OiPaidesEnVavyloni.name
meMA.Ode7C2T.text = "Λαὸν καὶ πόλιν σου σῷζε, Θεοκυῆτορ πανάμωμε, προστασία ἡμῶν τῶν ἐν πίστει, ἀδιστάκτῳ βοώντων σοι· Εὐλογημενος Πάναγνε, ὁ καρπὸς τῆς κοιλίας σου."

meMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8C1H.text = he.h.m2_gr_GR_cog.KaminosPote.text

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C11.melody = he.h.m2_gr_GR_cog.KaminosPote.name
meMA.Ode8C11.text = "Μέγισται τοὺς σοὺς καμάτους ἀντιδόσεις, ἐν τοῖς ὑψίστοις διεδέξαντο, μέγιστε Παρθένιε· φῶς οἰκεῖς γὰρ τὸ ἀνέσπερον, καὶ δόξαν ἀδιάδοχον εἴληφας, ἀενάου Πάτερ χαρᾶς ἀξιούμενος."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C12.melody = he.h.m2_gr_GR_cog.KaminosPote.name
meMA.Ode8C12.text = "Ἄδυτος ἀστὴρ γενόμενος ἐν βίῳ, ἀνατολαῖς τῶν θείων ἔργων σου, ἔδυς νόμῳ φύσεως, καὶ πρὸς φέγγος μετεχώρησας ἀνέσπερον, λιπὼν ἡμῖν Ὅσιε τὰς ἀκτῖνας, τῶν ἀποῤῥήτων θαυμάτων σου."

meMA.Ode8C1Tr.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1Tr.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C1Tr.melody = he.h.m2_gr_GR_cog.KaminosPote.name
meMA.Ode8C1Tr.text = "Ἄναρχος Πατήρ, Υἱὸς καὶ θεῖον Πνεῦμα, ὁμοβασίλειος ὁμόθρονος, γνωρίζεται ἔλλαμψις, ὀρθοδόξως τοῖς λατρεύουσι, καὶ μέλπουσι σὺν τάξεσι πάντοτε τῶν Ἀγγέλων, Ἅγιος, Ἅγιος, Ἅγιος."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode8C1T.melody = he.h.m2_gr_GR_cog.KaminosPote.name
meMA.Ode8C1T.text = "Ῥύπου με παθῶν ἁγνὴ προσγινομένου, τῇ μεσιτείᾳ σου καθάρισον, καὶ λάμπρυνον δέομαι, σκοτισθεῖσαν τὴν καρδίαν μου, δαιμόνων ἀμαυρότησιν, ὅπως σε μακαρίζω, Παρθένε Θεομακάριστε."

meMA.Ode8C2H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8C2H.text = he.h.m6_gr_GR_cog.NomonPatroon.text

meMA.Ode8C21.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C21.melody = he.h.m6_gr_GR_cog.NomonPatroon.name
meMA.Ode8C21.text = "Λύμην παντοίου πάθους ἔνδοξε, ἀποδιώκεις Πάτερ Λουκᾶ μακάριε, τοὺς λεπρῶντας ἐξιώμενος, ῥωννύων ἀσθενοῦντας, καὶ παρειμένους λύων τοῦ κατέχοντος δεσμοῦ, καὶ κυλλοὺς καὶ χωλοὺς ὁμοῦ, καὶ τυφλοὺς θεραπεύων. Τὸν Κύριον ὑμνεῖτε βοῶντας, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C22.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C22.melody = he.h.m6_gr_GR_cog.NomonPatroon.name
meMA.Ode8C22.text = "Ἄπονον εὗρες δόξαν Ὅσιε, πόνους πολλοὺς ὑποίσας καὶ ἀγωνίσματα, ἀγρυπνίαν τῆξιν σώματος, νηστείαν κακουχίαν, χαμαικοιτίαν, πᾶσαν ἄλλην πίεσιν σαρκός, δι' ἀγάπην τοῦ κτίσαντος, ᾧ συνὼν συνευφραίνῃ· αὐτὸν οὖν ἐκδυσώπει σωθῆναι, πάντας τοὺς ἐν πίστει, καὶ πόθῳ σε ὑμνοῦντας."

meMA.Ode8C23.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C23.melody = he.h.m6_gr_GR_cog.NomonPatroon.name
meMA.Ode8C23.text = "Διὰ ποικίλων πόνων Ὅσιε, τῶν ἀρετῶν εἰσήχθης εἰς τὴν οὐράνιον, βασιλείαν εὐφραινόμενος, τανῦν εἰς τοὺς αἰῶνας, καὶ τοῦ Δεσπότου πάντοτε τρυφῶν ταῖς καλλοναῖς, σὺν Ἀγγέλοις μελῴδημα τὸν Τρισάγιον ᾄδεις· Τὸν Κύριον ὑμνεῖτε τὰ ἔργα, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode8C2T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode8C2T.melody = he.h.m6_gr_GR_cog.NomonPatroon.name
meMA.Ode8C2T.text = "Οὐκ ἀποστάντα τὸν ἀχώριστον, ἐκ τοῦ πατρῴου κόλπου Ἁγνὴ γεγέννηκας, ἀπειράνδρως ἀνεκφράστως τε, βροτὸν τέλειον ὄντα, ὃν ἐκδυσώπει, δοῦναί μοι ἰσχύν, τῶν πονηρῶν λογισμῶν, προσβολὴν πᾶσαν καταβαλεῖν εἰς τέλος, καὶ θέλημα τὸ θεῖον ἐκείνου, πράττειν καθ' ἑκάστην, ἵνα σε δοξάζω, εἰς πάντας τοὺς αἰῶνας."

meMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1H.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C1H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9C1H.text = he.h.m2_gr_GR_cog.AnarchouGennitoros.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C11.melody = he.h.m2_gr_GR_cog.AnarchouGennitoros.name
meMA.Ode9C11.text = "Ἰδεῖν κατηξίωσαι, ἀπολυθεὶς τοῦ σώματος, τὰ ἀθέατα κάλλη Πάτερ Παρθένιε, ὅλος ἀρετῶν εὐμορφίᾳ, ὡς ἀληθῶς καθωραϊσμένος· διὸ σε γεραίρομεν, καὶ γνησίως μακαρίζομεν."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C12.melody = he.h.m2_gr_GR_cog.AnarchouGennitoros.name
meMA.Ode9C12.text = "Ὡς κρίνον ὡς ῥόδον σε, εὐῶδες ὡς ὀσφράδιον, ἀρετῶν ποικιλίᾳ ὡραϊζόμενον, Πάτερ ὁ Χριστὸς εἰς τὰς ἄνω, περιφανῶς ἐσκήνωσε μάνδρας, ὁσίως ποιμάναντα, τόν λαὸν αὐτοῦ Παρθένιε."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C13.melody = he.h.m2_gr_GR_cog.AnarchouGennitoros.name
meMA.Ode9C13.text = "Σκιρτήσωμεν ἄνθρωποι, χορεύσωμεν γηθόμενοι, ἑορτήν τε ἁγίαν νῦν συστησώμεθα, ψάλλοντες Θεῷ ἐν αἰνέσει, τὸν θαυμαστὸν καὶ μέγαν ποιμένα, Παρθένιον σήμερον, κατὰ χρέος μακαρίζοντες."

meMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C14.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C14.melody = he.h.m2_gr_GR_cog.AnarchouGennitoros.name
meMA.Ode9C14.text = "Ἡ μνήμη σου σήμερον, ἡμῶν περιαυγάζουσα, τὰς καρδίας ὡς μέγας ἥλιος ἔλαμψεν· ἣν ἐπιτελοῦντες ἐν πίστει, ῥῦσαι παθῶν σκότους Ἱεράρχα, καὶ νόσων καὶ θλίψεων, καὶ παντοίων περιστάσεων."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode2
meMA.Ode9C1T.melody = he.h.m2_gr_GR_cog.AnarchouGennitoros.name
meMA.Ode9C1T.text = "Φωτὶ με καταύγασον, τὸν ἐν τῷ σκότει κείμενον, τῆς δεινῆς ῥαθυμίας Θεοχαρίτωτε, καὶ μὴ προαιρούμενον ὅλως, τοὺς τοῦ Θεοῦ νόμους συντηρῆσαι, ὅπως ὡς προστάτην μου, κατὰ χρέος μακαρίζω σε."

meMA.Ode9C2H.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2H.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C2H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode9C2H.text = he.h.m6_gr_GR_cog.TheonAnthropois.text

meMA.Ode9C21.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C21.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C21.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C21.text = "Σοὺς πόνους Πάτερ καὶ ἀγωνίσματα, τὶς ἐξειπεῖν δυνήσεται βροτῶν ὧν ἀντάξιον, τὸν μισθὸν ἐκομίσω παρὰ Θεοῦ; γέγονας γὰρ δοχεῖον, τοῦ θείου Πνεύματος· Ὅσιος, εὐθύς, ἀληθινός, δίκαιος, ἄμεμπτος."

meMA.Ode9C22.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C22.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C22.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C22.text = "Ἁπλοῦν τὸ ἦθος καὶ ἀπονήρευτον, καὶ ταπεινόν, ἐλέους τε μεστὸν πρὸς τοὺς πένητας, καὶ φιλόξενόν τε καὶ φιλέρημον, ἥσυχον πρᾶον ὄντως, Πάτερ ἐνέδειξας· ὅθεν συνετάγης Ἀβραὰμ καὶ Ἰακὼβ καὶ Δαυΐδ."

meMA.Ode9C23.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C23.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C23.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C23.text = "Μοναῖς ἐκείναις ὡς αὐλιζόμενος, ταῖς ὀρεκταῖς καὶ κάλλους θεϊκοῦ ἐμπιπλάμενος, Παραδείσου τε θείου τοῖς ἄνθεσι, νῦν ἐντρυφῶν εἰς κόρον, καὶ ἀγαλλόμενος, μνείαν πρὸς Θεὸν ὑπὲρ ἡμῶν ποιοῦ Πανόλβιε."

meMA.Ode9C2Tr.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2Tr.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C2Tr.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C2Tr.text = "Ἡ μία φύσις ἡ τρισυπόστατος, ἡ τριλαμπὴς οὐσία καὶ ἀχώριστος δύναμις, ὁ Πατὴρ ὁ Υἱὸς καὶ τὸ Ἅγιον Πνεῦμα, μία Θεότης, σῷζε τοὺς πίστει σε, ἕνα μεγαλύνοντας Θεόν, ἐν χαρακτῆρσι τρισί."

meMA.Ode9C2T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C2T.mode = misc_gr_GR_cog.Mode6
meMA.Ode9C2T.melody = he.h.m6_gr_GR_cog.TheonAnthropois.name
meMA.Ode9C2T.text = "Νοεῖν οὐ σθένει σου τὸ μυστήριον, βρότειος νοῦς Παρθένε Θεοτόκε πανύμνητε· πρὸ γὰρ τόκου ἀνάνδρως συνέλαβες, ἔμεινάς τε Παρθένος, καὶ μετὰ κύησιν, τέτοκας δὲ Λόγον τῷ Πατρὶ τὸν συναΐδιον."

meMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion1.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion1.text = "Λαμψάκου ὁ παμμέγιστος, Ποιμὴν καὶ περιβόητος, θαυματουργὸς Ἱεράρχης, τῆς τοῦ Χριστοῦ Ἐκκλησίας, τὸ Ἑλλησπόντου καύχημα, καὶ τῶν πιστῶν ἑδραίωμα, τιμάσθω νῦν Παρθένιος· ὑπὲρ τοῦ κόσμου γὰρ οὗτος, ἐξιλεοῦται τὸ θεῖον."

meMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion2.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion2.text = "Μονάσας ἐκ νεότητος, καὶ τὸν σταυρὸν ἀράμενος, ἐμάκρυνας ἐν ἐρήμῳ, Λουκᾶ φωστὴρ Μοναζόντων, ἐγκρατείᾳ δὲ πάσῃ, τὸ χεῖρον δουλωσάμενος, τῶ κρείττονι θεόληπτε, ὡς ἐφικτὸν συνεκράθης, τῇ ἀπροσίτῳ Τριάδι."

meMA.Exaposteilarion3.mode = misc_gr_GR_cog.Mode2
meMA.Exaposteilarion3.melody = he.a.m2_gr_GR_cog.GynaikesAkoutisthite.name
meMA.Exaposteilarion3.text = "Πληρῶν σου τὴν ἀπόῤῥητον, οἰκονομίαν Κύριε, ἐν τῷ ναῷ προσηνέχθης, ὑπὸ μητρὸς ἀπειράνδρου, ἰδὼν δὲ σε ἐκραύγαζεν, ὁ Πρέσβυς· Νῦν ἀπόλυσον, τὸν δοῦλόν σου ὦ Δέσποτα· Σωτὴρ γὰρ ἦλθες τοῦ κόσμου, τὸ Πατρικὸν φῶς Χριστέ μου."

