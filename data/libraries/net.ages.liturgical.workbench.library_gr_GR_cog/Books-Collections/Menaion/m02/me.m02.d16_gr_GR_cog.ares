meInsert2 = "οὗ καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ἁγίου Μάρτυρος Παμφίλου, καὶ τῶν σὺν αὐτῷ."
meLE.commemoration2 = ""
