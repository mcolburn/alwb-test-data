A_Resource_Whose_Name = me.m04.d27_gr_GR_cog

meDate.text = "Τῌ ΚΖʹ ΑΠΡΙΛΙΟΥ" // date, e.g. Τῌ ΙϚʹ ΤΟΥ ΑΥΤΟΥ ΜΗΝΟΣ or July 16
meCommemoration.text = "Μνήμη τοῦ Ἁγίου Ἱερομάρτυρος Συμεών, συγγενοῦς τοῦ Κυρίου" //full title of day
meShortCommemoration1.text = ""
meInsert1 = "καὶ τοῦ ἁγίου ἐνδόξου Ἱερομάρτυρος Συμεών, συγγενοῦς τοῦ Κυρίου," //for use in petitions and dismissal
meInsert2 = "οὗ καὶ τὴν μνήμην ἐπιτελοῦμεν·" //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ἁγίου Ἱερομάρτυρος Συμεών, συγγενοῦς τοῦ Κυρίου"
meLE.commemoration2 = ""
