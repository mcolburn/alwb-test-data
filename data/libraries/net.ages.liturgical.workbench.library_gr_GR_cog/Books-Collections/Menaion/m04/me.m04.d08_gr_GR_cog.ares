meInsert2 = "ὧν καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τῶν Ἁγίων Ἀποστόλων ἐκ τῶν Ἑβδομήκοντα, Ἡρῳδίωνος, Ἀγάβου, Ῥούφου, Ἀσυγκρίτου, Φλέγοντος καὶ Ἑρμοῦ."
