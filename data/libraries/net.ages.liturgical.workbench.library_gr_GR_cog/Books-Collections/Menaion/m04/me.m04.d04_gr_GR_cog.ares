meInsert2 = "οὗ καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Γεωργίου τοῦ ἐν τῷ Μαλαιῷ (ἢ Μαλεῷ)."
meLE.commemoration2 = ""
