alt1.meCommemoration.text = "Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Γεωργίου, Μητροπολίτου Μυτιλήνης." //full title of day
alt1.meShortCommemoration1.text = ""
alt1.meInsert1 = "καὶ τοῦ Ὁσίου Πατρὸς ἡμῶν Γεωργίου, Μητροπολίτου Μυτιλήνης, " //for use in petitions and dismissal
alt1.meInsert2 = "" //for use in petitions and dismissal
alt1.meNote.text = ""
alt1.meLE.commemoration1 = "Μνήμη τοῦ Ὁσίου Πατρὸς ἡμῶν Γεωργίου, Μητροπολίτου Μυτιλήνης."
alt1.meLE.commemoration2 = ""
alt1.meHO.number = ""
alt1.meHO.rank = ""
alt1.meHO.commemorations = ""
