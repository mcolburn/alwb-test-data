meVE.Stichera01.mode = misc_gr_GR_cog.Mode4
meVE.Stichera01.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
meVE.Stichera01.text = "Ἔδωκας ὑπόδειγμα, ὑπομονῆς ὑπεράγαθε, καὶ ἀνδρείας τὸν δίκαιον, Ἰὼβ τὸν πολύαθλον, ἀρεταῖς καὶ λόγοις, καὶ τοῖς θείοις ἔργοις, καὶ σωφρονίζεις ἀληθῶς, τοὺς μετεώρους τοῖς ἀτυχήμασι· διό σου τὴν ἀνείκαστον, οἰκονομίαν δοξάζομεν, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

meVE.Stichera02.mode = misc_gr_GR_cog.Mode4
meVE.Stichera02.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
meVE.Stichera02.text = "Δίκαιος καὶ ἄκακος, ἀληθινός τε καὶ ἄμεμπτος, καὶ παντὸς ἀπεχόμενος, Ἰὼβ κακοῦ πράγματος, ἐγνωρίσθης μάκαρ, κατηγλαϊσμένος, ταῖς ἀρεταῖς ὡς ἀληθῶς, καὶ εὐσεβείᾳ καταλαμπόμενος· διὸ σε μακαρίζομεν, καὶ τὴν ἁγίαν σου σήμερον, καὶ σεπτὴν ἑορτάζομεν, γεγηθότες πανήγυριν."

meVE.Stichera03.mode = misc_gr_GR_cog.Mode4
meVE.Stichera03.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
meVE.Stichera03.text = "Πλοῦτον τὴν εὐσέβειαν, καὶ ὡς πορφύραν πολύτιμον, στολιζόμενος ἄμεμπτε, Ἰὼβ τὴν συμπάθειαν, τὴν δικαιοσύνην, ὥσπερ θεῖον στέφος, περιβαλλόμενος παθῶν, τῶν τυραννούντων σὺ ἐβασίλευσας· διὸ πρὸς τὰ βασίλεια, τὰ νοητὰ ἀναβέβηκας, Βασιλεῖ τε ἀοίδιμε, αἰωνίῳ παρίστασαι."

meVE.SticGlory.mode = misc_gr_GR_cog.Mode6
meVE.SticGlory.melody = ""
meVE.SticGlory.text = "Ἐν ἀκριβείᾳ τὸν βίον τηρήσας, τῶν ἐν νόμῳ οὐ διήμαρτες· τὴν γὰρ ἐκ Θεοῦ ἐπαινετὴν εὐφημίαν σύμμαχον ἔχων, ὡς στρατιώτης καρτερώτατος, πρὸς τὸ γυμνάσιον τῆς ἀρετῆς ἐξεδύσω, καὶ ἀφορήτοις μώλωψι τῶν ἰχώρων καταπαλαίσας, τὸν ἄμετρα καυχώμενον κατέβαλες, ἐντεῦθεν τὸ νίκης βραβεῖον, παρὰ Χριστοῦ ἀναδησάμενον, αὐτὸν ἱκέτευε ὑπὲρ τῶν ψυχῶν ἡμῶν."

meVE.Apolytikion1.mode = misc_gr_GR_cog.Mode2
meVE.Apolytikion1.melody = he.ga_gr_GR_cog.TouProfitouSou.name
meVE.Apolytikion1.text = "Τοῦ δικαίου σου Ἰὼβ τὴν μνήμην, Κύριε, ἑορτάζοντες, δι' αὐτοῦ σε δυσωποῦμεν, σῶσον τὰς ψυχὰς ἡμῶν."
meVE.GeneralApolytikion1.name = "Ἰὼβ" // name of saint for General Apolytikia

meVE.Apolytikion2.poet = poets_gr_GR_cog.Gerasimou
meVE.Apolytikion2.mode = misc_gr_GR_cog.Mode3
meVE.Apolytikion2.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
meVE.Apolytikion2.text = "Θεῖον βλάστημα, τῆς Βοιωτίας, ἔμπνουν ὄργανον, τῆς ἐγκρατείας, ἀνεδείχθης Σεραφεὶμ ἀξιάγαστε· σὺ γὰρ Ὁσίων βαδίσας τοῖς ἴχνεσιν, ἀρτιφανῶς ἐν τῷ κόσμῳ ἐξέλαμψας· Πάτερ Ὅσιε, Χριστὸν τὸν Θεὸν ἱκέτευε, δωρήσασθαι ἡμῖν τὸ μέγα ἔλεος."

//not in pdf:
meVE.Apolytikion3.mode = misc_gr_GR_cog.Mode4
meVE.Apolytikion3.melody = he.a.m4_gr_GR_cog.KateplagiIosif.name
meVE.Apolytikion3.text = "Κατεπλάγη ἀληθῶς, ἅπας ἀγγέλων ὁ χορός, καὶ ἀνθρώπων ὁ ἐσμός, ἔφριξε δίκαιε ἰδών, τὴν ὑπεράνθρωπον ὄντως ὑπομονήν σου. Ὅθεν καὶ ἡμῖν τύπος δέδοσαι, τοῖς θλίψεσι τοῦ βίου ἑκάστοτε, χειμαζομένοις κρίμασι Κυρίου, καὶ πειρασμοῖς περιπίπτουσιν· Αὐτὸς γὰρ εἶπεν, ὁ ὑπομείνας, μέχρι τέλους σωθήσεται."

meMA.C1Poet = poets_gr_GR_cog.AgiouKosmaOiDeIoannouMonachou
meMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς, ἄνευ τῶν Θεοτοκίων· Τὸν μυρίαθλον ἄξιον μέλπειν Ἰώβ."

meMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C11.melody = he.h.m4_gr_GR_cog.ChoroiIsrail.name
meMA.Ode1C11.text = "Τὸν καρτερικὸν ἀδάμαντα, τῆς ὑπομονῆς, τῷ στεφάνῳ κατακοσμούμενον, ἡ Χριστοῦ Ἐκκλησία, νοεροῖς ὁρῶσα ὀφθαλμοῖς πανηγυρίζει, ἐν ἀγαλλιάσει μέλπουσα· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C12.melody = he.h.m4_gr_GR_cog.ChoroiIsrail.name
meMA.Ode1C12.text = "Οὐ γλῶσσα σοφῶν δυνήσεται, οὐδὲ νοῦς βροτῶν ἐφικέσθαι τῶν ἐγκωμίων σου· ἡ γὰρ ἄκτιστος γλῶσσα, σὲ θεοσεβῆ ἀληθινὸν ἄμεμπτον ἔφη· ὅθεν σοι συμφώνως βοῶμεν· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C13.melody = he.h.m4_gr_GR_cog.ChoroiIsrail.name
meMA.Ode1C13.text = "Νομίσας πλούτῳ φραττόμενον, ὡς οὐκ εὐχερὲς αὐτῷ ἀνατρέπειν τὸν Δίκαιον, ἐξαιτεῖ ὁ ἀλάστωρ, καὶ γυμνῷ βιαίως προσπαλαίσας κατεῤῥάγη· δι' ὑπομονῆς γὰρ ἐβόα· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
meMA.Ode1C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode1C1T.melody = he.h.m4_gr_GR_cog.ChoroiIsrail.name
meMA.Ode1C1T.text = "Χοροὶ Προφητῶν ὑμνοῦσιν Ἁγνή, καὶ τῶν Ἀσωμάτων σε τάξεις γεραίρουσι· τὸν γὰρ Κτίστην τῶν ὅλων, ἐκ νηδύος σου ἀφράστως προελθόντα καθορῶντες, τούτῳ ἀσιγήτως βοῶσιν· ᾌσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

meMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C11.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C11.text = "Μένεις ἀκλινὴς τὸ φρόνημα, τῶν φιλτάτων τέκνων, καὶ τοῦ πλούτου στερούμενος, ἀναμέλπων· Εὐλογημένον, τοῦ Κυρίου μου τὸ ὄνομα."

meMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C12.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C12.text = "Ὕλην ἀρετῆς πεποίησαι, πλοῦτον καὶ πενίαν· δι' ἀμφοῖν γὰρ πυρούμενος, ὡς χρυσίον ἑπταπλασίως, ἀνεδείχθης δοκιμώτατος."

meMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C13.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C13.text = "Ῥήσει γυναικὸς προσέβαλεν, ὡς τὸ ἀρχεγόνῳ, ὁ ἀρχέγονος ὄφις σοι· ἀλλ' ἐψεύσθη τῆς πανουργίας, ὁ εἰς ἄμετρα καυχώμενος."

meMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
meMA.Ode3C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode3C1T.melody = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.name
meMA.Ode3C1T.text = "Χαῖρε Μῆτερ ἀπειρόγαμε, ἡ τὸν Θεὸν Λόγον ἐν γαστρί σου χωρήσασα, καὶ τεκοῦσα σεσαρκωμένον, ὡς Θεὸν ὁμοῦ καὶ ἄνθρωπον."

meMA.Ode3H.ode = misc_gr_GR_cog.Ode3
meMA.Ode3H.mode = misc_gr_GR_cog.Mode4
meMA.Ode3H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode3H.text = he.h.m4_gr_GR_cog.ToxonDynatonIsthenise.text

meMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
meMA.Ode3Kathisma1.text = "Ἰὼβ τὸν ἄμεμπτον, καὶ καθαρώτατον, στῦλον τὸν ἄτρεπτον, καὶ καρτερώτατον, ταῖς μεθοδείαις τοῦ ἐχθροῦ, γενόμενον ἐν Πνεύματι, πάντες μακαρίσωμεν, καὶ πιστῶς εὐφημήσωμεν, μέγιστον θεράποντα, τοῦ Χριστοῦ χρηματίσαντα, καὶ τοῦτον ἐκτενῶς δυσωποῦντα, σῶσαι κινδύνων τὰς ψυχὰς ἡμῶν."

meMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C11.melody = he.h.m4_gr_GR_cog.EisakikoaTinEndoxon.name
meMA.Ode4C11.text = "Ἱκανώτατον ὑπόδειγμα, καὶ στήλη ἔμπνους, καὶ εἰκὼν ἀρετῆς, Ἰὼβ ὁ ἄμεμπτος ἀνεδείχθη· οὗ ζηλωτὰς ἡμᾶς βοῶντας ποίησον· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C12.melody = he.h.m4_gr_GR_cog.EisakikoaTinEndoxon.name
meMA.Ode4C12.text = "Ἀθλοθετεῖ σοι γυμνάσιον, ὁ πληρωτὴς τῶν ἀγαθῶν ἀθλητά, ἐξ ὑπογαίου ἀνακαλύπτων, τῶν ἀρετῶν σου πᾶσι τὴν λαμπρότητα, ἄμεμπτε Ἰὼβ παμμακάριστε."

meMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C13.melody = he.h.m4_gr_GR_cog.EisakikoaTinEndoxon.name
meMA.Ode4C13.text = "Θεραπεύει σοι τὸ εὔτονον, καὶ καρτερικὸν τῆς ἀκάκου ψυχῆς, ἐν τῇ σαπρίᾳ ὁ ἀθλοθέτης, τῆς ἐν Ἐδὲμ ὑπάρχον παραβάσεως, ἴαμα Ἰὼβ παμμακάριστε."

meMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C14.melody = he.h.m4_gr_GR_cog.EisakikoaTinEndoxon.name
meMA.Ode4C14.text = "Λογισμῷ κρίνων τὸ ὅσιον, ἀνεμελῴδει ὁ Ἰώβ· ἀγαθὰ οἱ δεδεγμένοι παρὰ Κυρίου, τὰ ἀλγεινὰ γενναίως οὐχ ὑποίσομεν· Δόξα τῇ δυνάμει σου Κύριε."

meMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
meMA.Ode4C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode4C1T.melody = he.h.m4_gr_GR_cog.EisakikoaTinEndoxon.name
meMA.Ode4C1T.text = "Ἀπειρόγαμε Θεόνυμφε, ἡ τὸν Σωτῆρα τεξαμένη σαρκί, σὲ ἀνυμνοῦμεν δεδοξασμένη, καὶ ἀσιγήτως ὕμνον σοι κραυγάζομεν· Χαῖρε προστασία τῶν δούλων σου."

meMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C11.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C11.text = "Οἱ παρακλήτορες πρίν, τῶν δεινῶν προστιθέντες, τοῖς τραυμασί σου ἄλγος, πλείονάς σοι ἔπλεξαν, τοὺς στεφάνους, Ἰὼβ ἔνδοξε."

meMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C12.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C12.text = "Νεανικῶς ἐν δεινοῖς, ἀκατάπτωτος ὤφθης, καὶ ἐν περιφανείαις, πρὸς τὴν ἄτακτον ὕβριν, οὐκ ἐπήρθης, Ἰὼβ ἄμεμπτε."

meMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C13.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C13.text = "Ἀκαταγώνιστον σύ, τῆς ψυχῆς τὸ φρούριον δειμάμενος, γενναίως ἔφερες Ἔνδοξε, τοῦ ἐχθροῦ τὸν δεινὸν κλύδωνα."

meMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
meMA.Ode5C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode5C1T.melody = he.h.m4_gr_GR_cog.OAnateilasToFos.name
meMA.Ode5C1T.text = "Τὴν τοῦ Ἀγγέλου φωνήν, ἀσιγήτως ἡ κτίσις, προσφέρει σοι Παρθένε· Χαῖρε Μῆτερ ἄχραντε, Ἰησοῦ τοῦ Υἱοῦ τοῦ Θεοῦ."

meMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C11.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C11.text = "Ξενίζεται, ἀκοὴ ἐντρυφῶσα τοῖς λόγοις σου, ὅτι στόμα, μυστηρίου ἐδείχθης ἀνάπλεων, τοῦ ποιήσαντός σε καὶ διδάξαντος θείῳ ἐν Πνεύματι."

meMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C12.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C12.text = "Ἰώμενος, λυπουμένης καρδίας τὸ ἄλγημα, ἐκ βλεφάρων, συμπαθείας προέχεας δάκρυα, προϊστάμενος, ὀρφανῶν καὶ χηρῶν, Ἰὼβ ἔνδοξε."

meMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C13.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C13.text = "Ὁ βίος σου, εὐσεβὴς καὶ φιλόθεος πέφηνε· διὰ τοῦτο, καὶ ὁ θάνατος ὤφθη ἀνάπαυσις, καὶ πρὸς ἄληκτον, εὐφροσύνην Ἰὼβ μετέβηκας."

meMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
meMA.Ode6C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode6C1T.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
meMA.Ode6C1T.text = "Δεόμεθα, ὑπὲρ τῶν οἰκετῶν σου ἀλόχευτε, Θεοτόκε, τὸν ἐκ σοῦ σαρκωθέντα ἱκέτευε, ὅτι σὲ καὶ μόνην, προστασίαν ἡμῶν ἐπιστάμεθα."

meMA.Ode6H.ode = misc_gr_GR_cog.Ode6
meMA.Ode6H.mode = misc_gr_GR_cog.Mode4
meMA.Ode6H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode6H.text = he.h.m4_gr_GR_cog.EvoiseProtypon.text

meMA.Kontakion1.mode = misc_gr_GR_cog.Mode8
meMA.Kontakion1.melody = he.a.m8_gr_GR_cog.OsAparchas.name
meMA.Kontakion1.text = "Ὡς ἀληθὴς καὶ δίκαιος, θεοσεβὴς καὶ ἄμεμπτος, ἡγιασμένος τε ὤφθης πανένδοξε, Θεοῦ θεράπον γνήσιε, καὶ ἐδίδαξας κόσμον, ἐν τῇ σῇ καρτερίᾳ Ἰὼβ πολύαθλε· ὅθεν πάντες τιμῶντες, ὑμνοῦμέν σου τὸ μνημόσυνον."

meMA.Oikos1.text = "Ὁ τῶν ὅλων Θεός τε καὶ Κύριος, σὺν Υἱῷ βασιλεύων καὶ Πνεύματι, ἐν μιᾷ τῇ Θεότητι ἔλαμψε, καὶ πιστοὺς εὐσεβεῖς ἀπειργάσατο, ὁ αὐτὸς καὶ τὸν Ἰὼβ ἐμαρτύρησεν, ἡ γλῶσσα ἡ ἄκακος, ἄκακον, θεοσεβῆ τε καὶ δίκαιον, πάντων τῶν κακῶν ἀπεχόμενον· Διὰ τοῦτό σε Ἰώβ, ἐν ψαλμῳδίαις τιμῶντες ἅπαντες, ὑμνοῦμέν σου τὸ μνημόσυνον."

meMA.Synaxarion.text = ""

meMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C11.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C11.text = "Νομίμων ἄγευστος ἐπῶν, ἀλλὰ προφητῶν τε καὶ νόμου, φέρων τὸ πλήρωμα Ἰώβ, δι' ἀγάπης Θεῷ εὐηρέστησας, μελῳδῶν· Ὑπερύμνητε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

meMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C12.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C12.text = "Μὴ θησαυρίσας ἐπὶ γῆς, ἀλλ' ἐν οὐρανοῖς τὴν ἐλπίδα, ὡς χοῦν λελόγισαι χρυσόν, τῷ Θεῷ ἐκβοῶν ἀγαλλόμενος· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C13.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C13.text = "Ἕλκει σε παίσας πονηρῷ. καὶ τῶν ὑπαρχόντων γυμνώσας, ὁ τῶν δικαίων δυσμενής, τῆς ψυχῆς σου τὸν τόνον οὐκ ἔκαμψε, μελῳδῶν δὲ ἐκραύγαζες· ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

meMA.Ode7C1Tr.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1Tr.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1Tr.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C1Tr.text = "Λόγον καὶ Πνεῦμα ἐκ Πατρός, μίαν τρισυπόστατον φύσιν, θεολογοῦντες εὐσεβῶς, οἱ θεόφρονες πίστει βοήσωμεν· Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητός εἶ."

meMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
meMA.Ode7C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode7C1T.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
meMA.Ode7C1T.text = "Τὴν ἀλοχεύτως ἐν γαστρί, καὶ ὑπερφυῶς δεξαμένην, τὸν ἀναλλοίωτον Θεόν, εὐσπλαγχνίᾳ βροτοῖς ὁμιλήσαντα, εὐσεβῶς ἀνυμνοῦμεν σε, ὡς μητέρα τοῦ Ὑψίστου εἰς τοὺς αἰῶνας."

meMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C11.melody = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.name
meMA.Ode8C11.text = "Πανηγυρίζει πᾶς ἑορταζόντων, ἐν ὑψίστοις ὁ χορός, τῷ θεσπεσίῳ συμμέλπων Ἰώβ· διὸ καὶ ἀνακράζων· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C12.melody = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.name
meMA.Ode8C12.text = "Ἐδιπλασίασέ σοι τοὺς στεφάνους, ἐπὶ γῆς καὶ οὐρανοῦ, ὁ ἀθλοθέτης, θεόφρον Ἰώβ· διὸ κραυγάζεις· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C13.melody = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.name
meMA.Ode8C13.text = "Ἰάματα ψυχῶν τε καὶ σωμάτων, οἱ ἰχῶρες τῆς σαρκὸς Μακάριε βρύουσι τοῖς μελῳδοῦσιν· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C14.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C14.melody = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.name
meMA.Ode8C14.text = "Νόσος ὑγεία, πλοῦτος καὶ πενία, ἐναμβρύνονται τῇ βιοτῇ σου Μακάριε· λαμπρὸς φανεὶς γάρ, διὰ πάντων κραυγάζεις· Λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

meMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
meMA.Ode8C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode8C1T.melody = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.name
meMA.Ode8C1T.text = "Τὴν ἀποῤῥήτῳ λόγῳ συλλαβοῦσαν, καὶ τεκοῦσαν τὴν χαρὰν τῇ οἰκουμένῃ, Χριστὸν τὸν Θεόν, ὑμνεῖτε παῖδες, Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, εἰς πάντας τούς αἰῶνας."

meMA.Ode8H.ode = misc_gr_GR_cog.Ode8
meMA.Ode8H.mode = misc_gr_GR_cog.Mode4
meMA.Ode8H.melody = misc_gr_GR_cog.Heirmos
meMA.Ode8H.text = he.h.m4_gr_GR_cog.TonEnMorfiAngelou.text

meMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C11.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C11.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C11.text = "Ἰὼβ μὲν τῇ τῆς ὑπομονῆς λαμπρότητι, ἐπαξίως ἐστεφάνωται, σὺ δὲ πενθεῖς καὶ σκυθρωπάζεις, οὐ φέρων τοῦ Δικαίου τὸν στέφανον· διακενῆς γὰρ ἐλπίσας ᾐσχύνθης Σατάν, μόνε Δικαίων δυσμενέστατε."

meMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C12.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C12.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C12.text = "Ὡς θέμις ἀνθρώπῳ κατιδεῖν τὴν δόξαν σου, κατοπτεύσας τὴν ἀθέατον, φόβῳ κραθεὶς καὶ εὐλαβείᾳ, ἐβόησεν Ἰὼβ λίαν ἔντρομος· Σποδὸς ὑπάρχω καὶ γῆ, σὺ δὲ Κύριος· ὅθεν σε πάντες μεγαλύνομεν."

meMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C13.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C13.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C13.text = "Βίβλος σε ἀξίως τῆς ζωῆς ἐδέξατο, καὶ ταμεῖα ἐπουράνια, σῖτον ὡς ὥριμον ἀχθέντα, καὶ πλήρη ἡμερῶν τῶν τοῦ Πνεύματος, Ἰὼβ θεράπον Κυρίου μακάριε· ὅθεν οἱ πάντες μακαρίζομεν."

meMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
meMA.Ode9C1T.mode = misc_gr_GR_cog.Mode4
meMA.Ode9C1T.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
meMA.Ode9C1T.text = "Σοφίας χωρίον ὑπὲρ νοῦν τῆς κρείττονος, ἀνεδείχθης Κόρη ἄμωμε, ἔμψυχος θρόνος τε καὶ πύλη· διὸ ὡς βασιλίδα ἠγάπησαν, παρθένοι σε Παρθένε ὀπίσω σου, ἀπενεχθεῖσαι τῆς θεόπαιδος."

