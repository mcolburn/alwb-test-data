A_Resource_Whose_Name = me.m10.d20_gr_GR_cog

meDate.text = "Τῌ Κʹ ΟΚΤΩΒΡΙΟΥ"
meCommemoration.text = "Μνήμη τοῦ Ἁγίου Μεγαλομάρτυρος Ἀρτεμίου, τῆς Ὁσίας Ματρώνης τῆς Χιοπολίτιδος καὶ τοῦ Ὁσίου καὶ θεοφόρου πατρὸς ἡμῶν Γερασίμου τοῦ ἐν Κεφαλληνίᾳ."
meShortCommemoration1.text = ""
meInsert1 = "καὶ τοῦ Ἁγίου Μεγαλομάρτυρος Ἀρτεμίου, " //for use in petitions and dismissal
meInsert2 = "οὗ καὶ τὴν μνήμην ἐπιτελοῦμεν· " //for use in petitions and dismissal
meNote.text = ""
meLE.commemoration1 = "Μνήμη τοῦ Ἁγίου Μεγαλομάρτυρος Ἀρτεμίου."
meLE.commemoration2 = "Τοῦ Ὁσίου καὶ Θεοφόρου Πατρὸς ἡμῶν Γερασίμου τοῦ ἐν Κεφαλληνίᾳ."
meHO.number = ""
meHO.rank = ""
