peMA.Kathisma12.text = oc.m5.d1_gr_GR_cog.ocMA.Kathisma12.text

peMA.Kathisma13.mode = misc_gr_GR_cog.Mode5
peMA.Kathisma13.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
peMA.Kathisma13.text = oc.m5.d1_gr_GR_cog.ocMA.Kathisma13.text

peMA.Kathisma21.mode = misc_gr_GR_cog.Mode5
peMA.Kathisma21.melody = ""
peMA.Kathisma21.text = oc.m5.d1_gr_GR_cog.ocMA.Kathisma22.text

peMA.Kathisma22.mode = misc_gr_GR_cog.Mode5
peMA.Kathisma22.melody = ""
peMA.Kathisma22.text = "Τὸ ξένον σου Σωτήρ μου μυστήριον, τῷ κόσμῳ σωτηρία γεγένηται· ἀναστὰς γὰρ ἐκ τάφου θεοπρεπῶς, τοὺς φθαρέντας συνήγειρας ὡς Θεός, ἡ πάντων ζωή, Κύριε δόξα σοι."

peMA.Kathisma23.mode = misc_gr_GR_cog.Mode5
