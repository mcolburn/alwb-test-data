peVE.Apolytikion1.mode = misc_gr_GR_cog.Mode2
peVE.Apolytikion1.melody = ""
peVE.Apolytikion1.text = oc.m2.d1_gr_GR_cog.ocVE.Apolytikion.text

peVE.Apolytikion2.mode = misc_gr_GR_cog.Mode2
peVE.Apolytikion2.melody = ""
peVE.Apolytikion2.text = oc.m2.d1_gr_GR_cog.ocMA.Kathisma11.text

peVE.Apolytikion3.mode = misc_gr_GR_cog.Mode2
peVE.Apolytikion3.melody = ""
peVE.Apolytikion3.text = oc.m2.d1_gr_GR_cog.ocMA.Kathisma12.text

peMA.Kathisma11.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma11.melody = ""
peMA.Kathisma11.text = oc.m2.d1_gr_GR_cog.ocMA.Kathisma21.text

peMA.Kathisma12.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma12.melody = ""
peMA.Kathisma12.text = "Τὸν κόλπον τὸν ἄχραντον, ἐν ὑψίστοις μὴ κενώσας, ταφὴν καὶ ἀνάστασιν, ὑπὲρ πάντων κατεδέξω. Κύριε, δόξα σοι."

peMA.Kathisma13.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma13.melody = ""
peMA.Kathisma13.text = oc.m2.d1_gr_GR_cog.ocVE.ApolTheotokionVM.text

peMA.Kathisma21.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma21.melody = ""
peMA.Kathisma21.text = "Αἱ Μυροφόροι ὄρθριαι γενόμεναι, καὶ τὸ μνῆμα κενὸν θεασσάμεναι, τοῖς Ἀποστόλοις ἔλεγον· Τὴν φθορὰν καθεῖλεν ὁ κραταιός, καὶ τοὺς ἐν Ἅδῃ ἥρπασε τῶν δεσμῶν· κηρύξατε παρρησίᾳ· ὅτι ἀνέστη Χριστὸς ὁ Θεός, δωρούμενος ἡμῖν τὸ μέγα ἔλεος."

peMA.Kathisma22.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma22.melody = ""
peMA.Kathisma22.text = "Τὰ μύρα τῆς ταφῆς σου, αἱ γυναῖκες κομίσασαι, λαθραίως πρὸς τὸ μνῆμα παρεγένοντο ὄρθριαι, τῶν Ἰουδαίων δειλῶσαι τὴν αὐθάδειαν· καὶ στρατιωτῶν προορῶσαι τὴν ἀσφάλειαν· ἀλλὰ φύσις ἀσθενὴς τὴν ἀνδρείαν ἐνίκησεν· ὅτι γνώμη συμπαθὴς τῷ Θεῷ εὐηρέστησε· προσφόρως οὖν ἐκραύγαζον· Ἀνάστα Κύριε, βοήθησον ἡμῖν, καὶ λύτρωσαι ἡμᾶς, ἕνεκεν τοῦ ὀνόματός σου."

peMA.Kathisma23.mode = misc_gr_GR_cog.Mode2
peMA.Kathisma23.melody = ""
peMA.Kathisma23.text = oc.m2.d1_gr_GR_cog.ocMA.Kathisma13.text

peMA.C1Poet = poets_gr_GR_cog.AndreouKritis
peMA.C1Acrostic.text = ""

peMA.C2Poet = ""
peMA.C2Acrostic.text = ""

//Ode 1 Canon 1

peMA.Ode1C1H.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode1C1H.text = he.h.m2_gr_GR_cog.TinMoseosOdin.text

peMA.Ode1C11.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C11.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C11.text = "Ἐσταυρώθης σαρκί, ὁ ἀπαθὴς τῇ φύσει τοῦ Πατρός, ἐκεντήθης τὴν πλευράν, πηγάσας κόσμῳ αἷμα καὶ ὕδωρ. Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε." 

peMA.Ode1C12.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C12.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C12.text = "Τὸν Σταυρόν σου τιμῶ, καὶ τὴν Ταφὴν δοξάζω ἀγαθέ, καὶ ὑμνῶ καὶ προσκυνῶ τὴν Ἔγερσίν σου, καὶ ἀνακράζω· Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε." 

peMA.Ode1C13.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C13.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C13.text = "Κἂν ἐγεύσω χολῆς, ὁ γλυκασμὸς τῆς Ἐκκλησίας, ἀλλ' ἐπήγασας ἡμῖν τὴν ἀφθαρσίαν, ἐκ τῆς πλευρᾶς σου. Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε." 

peMA.Ode1C14.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C14.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C14.text = "Ἐλογίσθης Σωτήρ, μετὰ νεκρῶν νεκροὺς ἐγείρας· ἀπεγεύσω τῆς φθορᾶς· διαφθορὰν γὰρ ὅλως οὐκ ἔγνως. Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε." 

peMA.Ode1C15.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C15.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C15.text = "Εὐφραινέσθω Σιών, ἀγαλλιάσθω καὶ ὁ οὐρανός, ἐξανέστη Χριστός, νεκροὺς ἐγείρας ὑμνολογοῦντας· Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε." 

peMA.Ode1C1F1.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C1F1.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C1F1.text = "Ὀθονίοις εἱλήσας, Ἰωσὴφ τὸ σῶμά σου Χριστέ, ἐν μνημείῳ τῷ καινῷ, κατέθετό σε τὴν σωτηρίαν, νεκροὺς δὲ ὡς Θεὸς ἐξανέστησας."

peMA.Ode1C1F2.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C1F2.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C1F2.text = "Προλαβοῦσαι τὸν ὄρθρον, αἱ Γυναῖκες εἶδον τὸν Χριστόν, καὶ τοῖς θείοις Μαθηταῖς, ἐβόων· Ὄντως Χριστὸς ἀνέστη, δεῦτε σὺν ἡμῖν ἀνυμνεῖτε αὐτόν."

peMA.Ode1C1Tr.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C1Tr.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C1Tr.text = "Παναγία Τριάς, μία Θεότης, ἄναρχε μονάς, ὁ Πατὴρ καὶ ὁ Υἱός, καὶ θεῖον Πνεῦμα, σῷζε τὸν κόσμον. Σὺ ἡμῶν Θεός, καὶ δοξάζομέν σε."

peMA.Ode1C1T.ode = misc_gr_GR_cog.Ode1
peMA.Ode1C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode1C1T.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
peMA.Ode1C1T.text = "Τὴν ἀρχαίαν ἀράν, ἐν γαστρί σου λύσασα Σεμνή, ἀνεβλάστησας ἡμῖν, τὴν εὐλογίαν, βρέφος τεκοῦσα· οὗτος γὰρ Θεός, εἰ καὶ σάρκα φορεῖ."

peMA.Ode3C1H.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode3C1H.text = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.text

peMA.Ode3C11.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C11.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C11.text = "Τὰς παλάμας Ἰησοῦ, προσηλώσας ἐν Σταυρῷ τὰ ἔθνη πάντα, ἐκ τῆς πλάνης συλλαβών, πρὸς ἐπίγνωσιν τὴν σήν, συνεκαλέσω Σωτήρ." 

peMA.Ode3C12.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C12.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C12.text = "Ἰουδαίων ὁ λαός, τῷ Πιλάτῳ ἐκβοᾷ· Ἀπόλυσόν μοι τὸν κακοῦργον λῃστήν, ἆρον, ἆρον, σταύρωσον τὸν ἀναμάρτητον." 

peMA.Ode3C13.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C13.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C13.text = "Σταυρωθέντος σου Χριστέ, συνεσκότασε τὸ φῶς, ἡ γῆ ἐσείσθη, καὶ ἐκ τάφου νεκροί, ἐξανέστησαν πολλοί, φόβῳ τοῦ κράτους σου." 

peMA.Ode3C14.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C14.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C14.text = "Παρεστῶσα τῷ Σταυρῶ, ἡ ἀμνάς σου Ἰησοῦ, κλαυθμῷ ἐβόα· Ποῦ πορεύῃ Υἱὲ; ποῦ ἀπέρχῃ ὁ ἀμνός, ὁ ὑπὲρ πάντων σφαγείς;" 

peMA.Ode3C15.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C15.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C15.text = "Προσκυνῶ σου τὸν Σταυρόν, ἀνυμνῶ καὶ τὴν ταφήν, τιμῶ τὰ Πάθη, καὶ τοὺς ἥλους τῶν χειρῶν, καὶ τὴν λόγχην Ἰησοῦ, καὶ τὴν Ἀνάστασιν." 

peMA.Ode3C16.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C16.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C16.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C16.text = "Ἐξανέστης Ἰησοῦ, ἐσκυλεύθη ὁ ἐχθρός, Ἀδὰμ ἐρρύσθη, καὶ ἡ Εὔα σὺν αὐτῷ, τῶν δεσμῶν καὶ τῆς φθορᾶς, τῇ Ἀναστάσει σου." 

peMA.Ode3C17.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C17.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C17.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C17.text = "Ἀναστάντος σου Χριστέ, συνετρίβησαν μοχλοί, καὶ πύλαι ᾍδου, καὶ θανάτου τὰ δεσμά, διελύθησαν εὐθύς, φόβῳ τοῦ κράτους σου." 

peMA.Ode3C1F1.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C1F1.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C1F1.text = "Θεοδόχε Ἰωσήφ, δεῦρο στήθι μεθ' ἡμῶν. Ἀνέστη κράζων, Ἰησοῦς ὁ λυτρωτής, ὁ ἐγείρας τὸν Ἀδάμ, τῇ εὐσπλαγχνίᾳ αὐτοῦ."

peMA.Ode3C1F2.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C1F2.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C1F2.text = "Εὐφραινέσθω σὺν ἡμῖν, Μαθητῶν ἡ δωδεκάς, σὺν Μυροφόροις γυναιξί, καὶ Ἰωσήφ, καὶ τοῖς ἄλλοις Μαθηταῖς, καὶ Μαθητρίαις Χριστοῦ."

peMA.Ode3C1Tr.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C1Tr.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C1Tr.text = "Σὺν Πατρὶ καὶ τὸν Υἱόν, καὶ τὸ Πνεῦμα τὸ εὐθές, τὴν μίαν φύσιν, προσκυνῶ καὶ ἀνυμνῶ, τοῖς προσώποις διαιρῶν, καὶ τῇ οὐσίᾳ ἑνῶν."

peMA.Ode3C1T.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode3C1T.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
peMA.Ode3C1T.text = "Μακαρία εἶ Σεμνή, ἡ ἐκ ῥίζης Ἱεσσαί, ἐξ ἧς ἡ ῥάβδος, καὶ τὸ ἄνθος ὁ Χριστός, κατὰ σάρκα δι' ἡμᾶς, ἀναβλαστήσασα."

peMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode2
peMA.Ode3Kathisma1.melody = he.a.m2_gr_GR_cog.TaAnoZiton.name
peMA.Ode3Kathisma1.text = "Τὰ μύρα θερμῶς, τῷ τάφῳ προσκομίζουσαι, Γυναῖκες Σωτήρ, Ἀγγέλου τῇ λαμπρότητι, τὰς ψυχὰς ἠγάλλοντο, καὶ Θεόν σε τῶν ὅλων ἐκήρυττον, καὶ Μαθηταῖς ἐβόων· Ὄντως ἀνέστη ἐκ τάφου ἡ πάντων ζωή."

peMA.Ode3Kathisma2.mode = misc_gr_GR_cog.Mode2
peMA.Ode3Kathisma2.melody = ""
peMA.Ode3Kathisma2.text = oc.m2.d1_gr_GR_cog.ocMA.Kathisma22.text

peMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode4C1H.text = he.h.m2_gr_GR_cog.TinEkParthenou.text

peMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C11.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C11.text = "Διὰ Σταυροῦ ᾐχμαλώτευσας, τὴν τοῦ ᾍδου γαστέρα, καὶ συνήγειρας νεκρούς, καὶ τοῦ θανάτου κατέλυσας τὴν τυραννίδα· ὅθεν οἱ ἐξ, Ἀδὰμ προσκυνοῦντες, ὑμνοῦμεν τὴν ταφήν σου καὶ τὴν Ἔγερσιν Χριστέ." 

peMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C12.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C12.text = "Ὁ εὐδοκήσας Σωτὴρ ἡμῶν, διὰ σπλάγχνα ἐλέους, προσπαγῆναι τῷ Σταυρῷ, καὶ τῆς πατρῴας λυτρώσασθαι ἡμᾶς κατάρας, λῦσόν μου τὰ δεσμὰ τῶν πολλῶν ἐγκλημάτων· δύνασαι γὰρ δσα θέλεις ἐκπληροῦν." 

peMA.Ode4C13.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C13.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C13.text = "Ὁ ἐν Σταυρῷ προσηλώσας μου, τὴν ἀρχαίαν κατάραν, καὶ πηγάσας μοι Σωτήρ, τὴν εὐλογίαν δι' αἵματος, ἐκ τῆς πλευρᾶς σου, λῦσόν μου τὰ δεσμὰ τῶν πολλῶν ἐγκλημάτων· δύνασαι γὰρ ὅσα θέλεις ἐκπληροῦν." 

peMA.Ode4C14.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C14.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C14.text = "ᾍδης Σωτὴρ συναντήσας σοι, ἐν τοῖς καταχθονίοις ἐπικράνθη, ἐνορῶν, ὅτι οὓς πάλαι κατέπιεν ἰσχύσας, ἄρτι ἄκων ἀποδιδοὺς ἐρευνᾶται τὰ κάτω, καὶ γυμνοῦται καὶ σκυλεύεται νεκρούς." 

peMA.Ode4C15.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C15.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C15.text = "Εἰ καὶ ὁ λίθος ἐσφράγισται, μετὰ τῆς κουστωδίας, ὦ παράνομοι ἐχθροί· ἀλλ' ἐξανέστη ὁ Κύριος καθὼς προεῖπε, λύων μου τὰ δεσμὰ τῶν πολλῶν ἐγκλημάτων· δύναται γὰρ ὅσα θέλει ἐκπληροῦν." 

peMA.Ode4C16.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C16.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C16.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C16.text = "Ὁ ἀναστὰς ἐκ τοῦ μνήματος, καὶ σκυλεύσας τὸν ᾍδην, καὶ ζωώσας τοὺς νεκρούς, καὶ ἀφθαρσίαν πηγάσας μοι, τῇ σῇ Ἐγέρσει, λῦσόν μου τὰ δεσμὰ τῶν πολλῶν ἐγκλημάτων· δύνασαι γὰρ ὅσα θέλεις ἐκπληροῦν." 

peMA.Ode4C17.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C17.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C17.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C17.text = "Ὄντως αἰσχύνθητε ἄνομοι· ὁ Χριστὸς γὰρ ἀνέστη, καὶ συνήγειρε νεκρούς, κράζων, θαρσεῖτε, νενίκηκα ἐγὼ τὸν κόσμον, πείσθητε οὖν αὐτῷ, ἢ σιγήσατε πλάνοι, ἀθετοῦντες τὴν Ἀνάστασιν αὐτοῦ." 

peMA.Ode4C1F1.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C1F1.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C1F1.text = "Ὁ Μυροφόροις τό, Χαίρετε, ἀναστὰς ἐκ τοῦ τάφου, προσφωνήσας ἀγαθέ, καὶ Ἀποστόλοις, κηρύξατε τὴν Ἔγερσίν μου, λῦσόν μου τὰ δεσμά, τῶν πολλῶν ἐγκλημάτων· δύνασαι γὰρ ὅσα θέλεις ἐκπληροῦν."

peMA.Ode4C1F2.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C1F2.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C1F2.text = "Ἰωσὴφ τὸν εὐσχήμονα, ζηλωτὴν εὐσεβείας, βουλευτὴν καὶ μαθητήν, σὺν Μυροφόροις τιμήσωμεν καὶ Ἀποστόλοις, κράζοντες σὺν αὐτοῖς, καὶ πιστῶς ἀνυμνοῦντες τοῦ Σωτῆρος τὴν Ἀνάστασιν φαιδρῶς."

peMA.Ode4C1Tr.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C1Tr.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C1Tr.text = "Ὑπερουσίου Θεότητος, τὴν ἀμέριστον δόξαν, τίς ἰσχύσει ἐξειπεῖν· ἓν γὰρ τῇ φύσει ὑπάρχουσα, Τριὰς ὑμνεῖται, ἄναρχος, συμφυής, ὡς μονὰς ἐν Τριάδι ὑμνουμένη, ὑποστάσεσιν ἁπλαῖς."

peMA.Ode4C1T.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode4C1T.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
peMA.Ode4C1T.text = "Τὸν ἐν γαστρί σου σκηνώσαντα, ἀλοχεύτως τεκοῦσα, Μητροπάρθενε ἁγνή, ἀκαταπαύστως ἱκέτευε, ὡς Θεοτόκος, λῦσαί μου τὰ δεσμὰ τῶν πολλῶν ἐγκλημάτων· δύνασαι γὰρ ὅσα θέλεις βοηθεῖν."

peMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode5C1H.text = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.text

peMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C11.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C11.text = "Σὺ τὴν ἀρχαίαν στολήν μου, ἣν μοι ἐξύφανεν, οἴμοι! ὁ σπορεὺς τῆς ἁμαρτίας ἐξέδυσάς με Σωτήρ μου, ἐνδυσάμενος ἐμέ." 

peMA.Ode5C12.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C12.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C12.text = "Φύλλα συκῆς ἔρραψέ μοι, ἡ ἁμαρτία μου οἴμοι! μὴ φυλάξαντι Σωτήρ μου τὴν ἄχραντον ἐντολήν σου, τῇ τοῦ ὄφεως βουλῇ." 

peMA.Ode5C13.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C13.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C13.text = "Τὴν πληγωθεῖσαν ψυχήν μου, τοῖς λῃστρικοῖς λογισμοῖς μου, ἐπιστὰς ὁ ἐκ Μαρίας, καὶ ἔλαιον ἐπιχύσας, ἐθεράπευσε Χριστός." 

peMA.Ode5C14.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C14.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C14.text = "Ἐν τῷ Σταυρῷ παρεστῶσα, ἡ ἄμεμπτος Θεοτόκος, μητρικῶς ἀνεκαλεῖτο. Κατέλιπές με τὴν μόνην, Υἱέ μου καὶ Θεέ." 

peMA.Ode5C15.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C15.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C15.text = "Σὺ τὸν ἀρχέκακον ὄφιν, τῷ ὅπλῳ τῷ τοῦ Σταυροῦ σου, καθελὼν τῇ σῇ Ἐγέρσει, συνέτριψας τὸ κέντρον, τοῦ θανάτου Ἰησοῦ." 

peMA.Ode5C16.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C16.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C16.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C16.text = "Ποῦ σου θάνατε τὸ κέντρον, ποῦ σου, ᾍδη τὸ νῖκος, ὁ Ἀδὰμ συνεκβοάτω· συντέτριψαι ζωώσει, τοῦ ἐγείραντος νεκρούς." 

peMA.Ode5C17.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C17.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C17.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C17.text = "Αἱ Μυροφόροι γυναῖκες, τὸ μνῆμα καταλαβοῦσαι, τοῦ ζωώσαντος τοὺς κάτω, φωνῆς ἤκουον λεγούσης· Ἐξανέστη ὁ Χριστός." 

peMA.Ode5C1F1.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C1F1.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C1F1.text = "Τῶν εὐσεβῶν Μυροφόρων, τὴν μνήμην ἐπιτελοῦντες, καὶ πάντων τῶν Μαθητῶν σου, ἐν τῇ φαιδρᾷ σου Ἐγέρσει, ἀνυμνοῦμέν σε Χριστέ."

peMA.Ode5C1F2.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C1F2.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C1F2.text = "Τὸν εὐσχήμονα πάντες, τιμήσωμεν ἐπαξίως, ὃς τὸ σῶμα τοῦ Κυρίου, ἀπὸ τοῦ ξύλου καθεῖλε, καὶ ἐκήδευσε πιστῶς."

peMA.Ode5C1Tr.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C1Tr.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C1Tr.text = "Σὲ τὸν Θεὸν καὶ Πατέρα, καὶ τὸν Υἱὸν καὶ τὸ Πνεῦμα, προσκυνῶ τρεῖς ὑποστάσεις καὶ ἓν τὰ τρία πιστεύω, οὐσιότητι μιᾷ."

peMA.Ode5C1T.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode5C1T.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
peMA.Ode5C1T.text = "Σὲ τὴν ἀσπόρῳ κυήσει, γεννήσασαν ὑπὲρ φύσιν, τὸν καινίσαντα τὴν φύσιν, Χριστὸν τὸν μόνον Δεσπότην, ἀνυμνοῦμεν οἱ πιστοί."

peMA.Ode6C1H.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C1H.melody = misc_gr_GR_cog.Heirmos 
peMA.Ode6C1H.text = he.h.m2_gr_GR_cog.VythoAmartimaton.text

peMA.Ode6C11.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C11.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C11.text = "Νεκρός ἐστιν ὁ ᾍδης, θαρσεῖτε γηγενεῖς· ὁ γὰρ Χριστὸς ἐπὶ ξύλου κρεμάμενος, ἀπέρριψε τὴν ῥομφαίαν κατʹ αὐτοῦ, καὶ κεῖται νεκρός· οὓς εἶχε γὰρ ἐσκυλεύθη γυμνωθείς." 

peMA.Ode6C12.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C12.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C12.text = "Ἐσκύλευται ὁ ᾍδης, θαρσεῖτε οἱ νεκροί, καὶ τὰ μνημεῖα ἠνοίχθη, ἐγείρεσθε, βοᾷ ὑμῖν ἐκ τοῦ ᾍδου ὁ Χριστός, ὁ πάντας ἐλθὼν λυτρώσασθαι, ἐκ θανάτου καὶ φθορᾶς." 

peMA.Ode6C13.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C13.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C13.text = "Νεκροὺς οὕς περ ἰσχύσας, κατέπιες ποτέ, νῦν ἀπαιτούμενος ᾍδης, ἀπόδος μοι· βοᾷ σοι ὁ ζωοδότης καὶ Θεός, ὁ πάντας ἐλθὼν λυτρώσασθαι, τῆς ἀπλήστου σου γαστρός." 

peMA.Ode6C14.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C14.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C14.text = "Ὁ Κύριος ἀνέστη σκυλεύσας τὸν ἐχθρὸν καὶ τοὺς δεσμίους ἐκσπάσας, ἀνήγαγε πάντας, καὶ τὸν πρωτόπλαστον Ἀδάμ, αὐτὸν ἀνιστῶν ὡς εὔσπλαγχνος, καὶ φιλάνθρωπος Θεός." 

peMA.Ode6C1F1.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C1F1.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C1F1.text = "Σινδόνι σε εἱλήσας, κατέθετο Χριστέ, Ἰωσὴφ ὁ εὐσχήμων ἐν μνήματι, μυρίσας δὲ τὸν λυθέντα σου ναόν, τοῦ σώματος, προσεκύλισε, λίθον μέγαν τῇ σορῷ."

peMA.Ode6C1F2.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C1F2.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C1F2.text = "Γυναῖκες Μυροφόροι, τί σπεύδετε λοιπόν, τί δὲ τὰ μύρα τῷ ζῶντι κομίζετε; ἀνέστη καθὼς προεῖπεν ὁ Χριστός, παυσάσθω ὑμῶν τὰ δάκρυα, μετελθόντα εἰς χαράν."

peMA.Ode6C1Tr.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C1Tr.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C1Tr.text = "Τριάδα ἐν μονάδι ὑμνήσωμεν πιστοί, σὺν τῷ Υἱῷ τὸν Πατέρα δοξάζοντες, καὶ Πνεῦμα τὸ ὁμοούσιον Υἱῷ, καὶ ὃν ἐν Πατρὶ συνάναρχον, καὶ ἀΐδιον Θεόν."

peMA.Ode6C1T.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode6C1T.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
peMA.Ode6C1T.text = "Γαστρὶ ἀγεωργήτως συνέλαβες Ἁγνή, τῆς ἀφθαρσίας τὸν βότρυν ὡς ἄμπελος, ἐξ οὗ τῆς ἀθανασίας οἱ κρουνοί, ὡς οἶνον ἡμῖν πηγάζουσι, τὴν αἰώνιον ζωήν."

peMA.Kontakion1.mode = misc_gr_GR_cog.Mode2
peMA.Kontakion1.melody = ""
peMA.Kontakion1.text = "Τὸ Χαῖρε ταῖς Μυροφόροις φθεγξάμενος, τὸν θρῆνον τῆς προμήτορος Εὔας κατέπαυσας, τῇ Ἀναστάσει σου, Χριστὲ ὁ Θεός, τοῖς Ἀποστόλοις δὲ τοῖς σοῖς κηρύττειν ἐπέταξας, ὁ Σωτὴρ ἐξανέστη τοῦ μνήματος."

peMA.Oikos1.text = "Ἐπὶ τὸν τάφον σου Σωτήρ, αἱ Μυροφόροι πορευόμεναι, πρὸς ἑαυτὰς διηπόρουν τῷ νοΐ, καὶ ἐφθέγγοντο. Τίς ἡμῖν τὸν λίθον ἀποκυλίσει τοῦ μνήματος; Καὶ ἀναβλέψασαι, ὁρῶσιν ὅτι ὁ λίθος ἀποκεκύλισται, τῇ μορφῇ δὲ τοῦ Ἀγγέλου σὺν τῇ στολῇ ἐθαμβοῦντο, συνείχοντο δὲ τρόμῳ, καὶ φεύγειν ἐνόμιζον, καὶ πρὸς αὐτὰς ὁ νεανίσκος ἐβόησε. Μὴ φοβεῖσθε ὑμεῖς, ὃν ζητεῖτε, ἀνέστη, δεῦτε, ἴδετε τὸν τόπον, ὅπου ἔκειτο τὸ σῶμα τοῦ Ἰησοῦ, καὶ ταχὺ πορευθεῖσαι, ἀπαγγείλατε τοῖς Μαθηταῖς· ὁ Σωτὴρ ἐξανέστη τοῦ μνήματος."

peMA.Synaxarion.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Κυριακῇ τρίτῃ ἀπὸ τοῦ Πάσχα, τὴν τῶν ἁγίων γυναικῶν Μυροφόρων ἑορτὴν ἑορτάζομεν· ἔτι δὲ μνείαν ποιούμεθα καὶ τοῦ ἐξ Ἀριμαθαίας Ἰωσήφ, ὅς ἦν μαθητὴς κεκρυμμένος· πρὸς δὲ καὶ τοῦ νυκτερινοῦ μαθητοῦ Νικοδήμου."
peMA.Synaxarion.couplet1.title = titles_gr_GR_cog.Stichoi
peMA.Synaxarion.couplet1.verse1 = "Χριστῷ φέρουσιν αἱ Μαθήτριαι μύρα."
peMA.Synaxarion.couplet1.verse2 = "Ἐγὼ δὲ ταύταις ὕμνον, ὡς μύρον φέρω."
peMA.Synaxarion.lastline.text = "Ταῖς τῶν ἁγίων Μυροφόρων πρεσβείαις, ὁ Θεός, ἐλέησον ἡμᾶς. Ἀμήν."

peMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode7C1H.text = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.text

peMA.Ode7C11.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C11.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C11.text = "Σὺ ὡς φιλάνθρωπος θέλων, πάντας σῶσαι ἐκ πλάνης, οὓς ἔπλασας, ἠνέσχου, προσηλωθῆναι τῷ σταυρῷ, ἵνα τὴν συγχωσθεῖσαν εἰκόνα τοῖς πάθεσι, ταύτην τῇ σαρκὶ σου ἀνανεώσῃς Σωτήρ, καὶ ᾍδην καθελών, συνεξανέστησας, καὶ τοὺς θανόντας σεαυτῷ." 

peMA.Ode7C12.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C12.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C12.text = "Ἐν τῷ σταυρῷ ὑψούμενος πάντας προσεκαλέσω, πρὸς σεαυτὸν οἰκτίρμον ὡς ἐπηγγείλω ἀγαθέ, ὅτι ἐν ἀληθείᾳ ταῦτα πάντα, διὰ τὰς ἁμαρτίας ἡμῶν παθεῖν ηὐδόκησας· ὅθεν καὶ τῷ Λῃστῇ, τὰς τοῦ Παραδείσου πύλας ἠνέῳξας Σωτήρ." 

peMA.Ode7C13.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C13.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C13.text = "Σὺ τὸν ναὸν τοῦ σώματος ἤγειρας τὸν λυθέντα, τριήμερον ἐκ τάφου, ὡς ἐπηγγείλω ἀγαθέ, ἵνα ἐν ἀληθείᾳ γνωρίσῃς τὴν δόξαν σου, ἣν ἡμῖν πηγάζεις διὰ τῆς πίστεως, δεσμίους ἀφελῶν οὓς κατεῖχε πάλαι ᾍδης πεπεδημένους." 

peMA.Ode7C14.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C14.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C14.text = "Ὢ Ἰουδαίων ἄνοια! ὢ μανία ἀνόμων! Τί ἄπιστον ἰδόντες, οὐκ ἐπιστεύσατε Χριστῷ; ὅτι τοὺς ἀσθενοῦντας ἐν λόγῳ οὐκ ἤγειρεν, ἢ ὅτι πάντα κόσμον αὐτὸς οὐκ ἔσωσε; Πεισάτωσαν ὑμᾶς κἂν οἱ στρατιῶται, οἱ ἀναστάντες ἐκ νεκρῶν." 

peMA.Ode7C1F1.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C1F1.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C1F1.text = "Οἱ νεκρωθέντες φύλακες, νῦν λεγέτωσαν, ὅπως ἐκλάπη ὃν οὐκ εἶδον, οἱ ἀγνώμονες παντί. Εἰ γὰρ ἐξαναστάντα οὐκ εἶδον, οὐκ ᾔσθοντο, πῶς κλαπέντα οὕτω νοῆσαι εἶχόν ποτε: Πεισάτωσαν ὑμᾶς, κἂν ὁ λίθος οὗτος, καὶ τὰ ἐντάφια Χριστοῦ."

peMA.Ode7C1F2.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C1F2.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C1F2.text = "Τί ὡς νεκρὸν φυλάττετε; τί σφραγῖδας τῷ λίθῳ τεθήκατε; Ἑβραῖοι, φόβον φοβούμενοι κλοπῆς, ἰδοὺ ἐν ἀληθείᾳ ὁ τάφος ἐσφράγισται, πῶς οὖν ἐξανέστη, εἰ μὴ Θεός ἦν ὁ Χριστός; Πεισάτωσαν ὑμᾶς οἱ ἐξαναστάντες, καὶ ὁραθέντες τοῖς πολλοῖς."

peMA.Ode7C1Tr.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C1Tr.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C1Tr.text = "Σὺν τῷ Πατρὶ δοξάζομεν, τὸν Υἱὸν καὶ τὸ Πνεῦμα τὸ ἅγιον, βοῶντες, ἀκαταπαύστῳ τῇ φωνῇ· Τριὰς μοναδικὴ οὐσία, ἐλέησον, σῶσον πάντας, ἡ ἐν τρισὶ προσώποις μονάς, οἰκτίρησον ἡμᾶς, ὁ δεδοξασμένος Θεὸς εἰς τοὺς αἰῶνας."

peMA.Ode7C1T.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode7C1T.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
peMA.Ode7C1T.text = "Ἐν τῇ γαστρί σου Ἄχραντε, πῶς ἐχώρησας βρέφος, ὃν τρέμουσι δυνάμεις, αἱ τῶν Ἀγγέλων ὡς Θεόν; εἰ μὴ ὡς ἠβουλήθη, ὡς οἶδεν, ἐσκήνωσε, πάντας θέλων, σῶσαι τοὺς ἐξ Ἀδὰμ γηγενεῖς, λύσας τὸν Ἀδὰμ τῆς ἀρᾶς ἐκείνης, τῆς διὰ βρώσεως πικρᾶς."

peMA.Ode8C1H.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode8C1H.text = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.text

peMA.Ode8C11.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C11.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C11.text = "Αἱ τοῦ ἡλίου αὐγαί, συνεστέλλοντο φόβῳ, τῶν Χριστοῦ παθημάτων, καὶ ἀνίσταντο νεκροί, καὶ ὄρη ἐκλονεῖτο, καὶ ἐδονεῖτο ἡ γῆ, καὶ ᾍδης ἐγυμνοῦτο." 

peMA.Ode8C12.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C12.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C12.text = "Οἱ ἐν καμίνῳ ποτέ, τρισμακάριοι Παῖδες, ἀνυψοῦντες τὰς χεῖρας, προετύπουν ἀγαθέ, τὸν ἄχραντον Σταυρόν σου, δι' οὗ καθεῖλες Χριστέ, ἐχθροῦ τὴν δυναστείαν." 

peMA.Ode8C13.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C13.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C13.text = "Ὦ Ἰουδαῖοι τυφλοί, πλάνοι καὶ παραβάται, οἱ Χριστοῦ τῇ Ἐγέρσει, ἀπειθοῦντες ὡς ψευδεῖ; τί ἄπιστον ὁρᾶτε; ὅτι ἀνέστη Χριστός, ὁ τοὺς νεκροὺς ἐγείρας;" 

peMA.Ode8C14.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C14.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C14.text = "Ὦ Ἰουδαῖοι ἐχθροί, κἂν ἡμῖν ἀπιστεῖτε, τοὺς ὑμῶν στρατιώτας, ἐρωτήσατε ὑμεῖς, τί ἔπαθον ἐκεῖνοι; τίς ὁ κυλίσας χερσί, τὸν λίθον τοῦ μνημείου;" 

peMA.Ode8C15.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C15.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C15.text = "Τίς ὁ ξηράνας συκῆν; τίς δὲ τὴν ξηρανθεῖσαν, ἰασάμενος χεῖρα; τίς ὁ χορτάσας ποτέ, τὰ πλήθη ἐν ἐρήμῳ; εἰμὴ Χριστὸς ὁ Θεός, ὁ τοὺς νεκροὺς ἐγείρας." 

peMA.Ode8C16.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C16.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C16.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C16.text = "Τίς ὁ φωτίσας τυφλούς, καὶ λεπροὺς ἐκκαθάρας, καὶ χωλοὺς ἀνορθώσας, καὶ πεζεύσας ὡς ξηράν, τὴν θάλασσαν ἀβρόχως; εἰ μὴ Χριστὸς ὁ Θεός, ὁ τοὺς νεκροὺς ἐγείρας." 

peMA.Ode8C17.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C17.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C17.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C17.text = "Τίς ὁ ἐγείρας νεκρόν, τεταρταῖον ἐκ τάφου, καὶ τῆς χήρας τὸν υἱόν; τίς ὁ σφίγξας ὡς Θεός, παράλυτον ἐν κλίνῃ; εἰ μὴ Χριστὸς ὁ Θεός, ὁ τοὺς νεκροὺς ἐγείρας." 

peMA.Ode8C1F1.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C1F1.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C1F1.text = "Κράζει ὁ λίθος αὐτός, αἱ σφραγῖδες βοῶσιν, ἃς ὑμεῖς ἐπιθέντες, κατεστήσατε φρουροὺς φυλάσσοντας τὸ μνῆμα. Ὄντως ἀνέστη Χριστός, καὶ ζῇ εἰς τοὺς αἰῶνας."

peMA.Ode8C1F2.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C1F2.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C1F2.text = "Ὄντως ἀνέστη Χριστός, ἐσκυλεύθη ὁ ᾍδης, ἐνεκρώθη ὁ ὄφις, ἐλυτρώθη ὁ Ἀδάμ, ἐσώθησαν οἱ κάτω. Τί ἀπιστεῖτε λοιπόν, ἐχθροὶ καὶ παραβάται;"

peMA.Ode8C1Tr.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C1Tr.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C1Tr.text = "Σὺν τῷ Πατρὶ τὸν Υἱόν, καὶ τὸ ἅγιον Πνεῦμα, τὴν ἁγίαν Τριάδα, ἐν Θεότητι μιᾷ, δοξάζομεν βοῶντες· Ἅγιος, Ἅγιος, Ἅγιος εἶ εἰς τοὺς αἰῶνας."

peMA.Ode8C1T.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C1T.mode = misc_gr_GR_cog.Mode2
peMA.Ode8C1T.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
peMA.Ode8C1T.text = "Ἐν τῇ γαστρί σου Ἁγνή, τὸν ἀείζωον ἄρτον, φυραθέντα ἀφύρτως, ἐν φυράματι ἡμῶν, ἐγέννησας ἀτρέπτως, ἕνα Χριστὸν τὸν Θεόν, ἐν δυσὶ ταῖς οὐσίαις."

peMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C1H.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode9C1H.text = he.h.m2_gr_GR_cog.TinYperfyosSarki.text

peMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C11.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C11.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C11.text = "Τὸν ἐν τῷ Σταυρῷ Λῃστήν, ἐπιγνόντα σε Θεόν, κληρονόμον εἰργάσω, νοητοῦ Παραδείσου, τὸ Μνήσθητι βοῶντα, παντοδύναμε Σωτήρ." 

peMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C12.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C12.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C12.text = "Ἐρραπίσθης δι' ἡμᾶς, ἐνεπτύσθης Ἰησοῦ, ὑπὸ τῶν παρανόμων, ὁ τὰς πλάκας τοῦ νόμου, χαράξας ἐν Σιναίῳ, τῷ θεράποντι Μωσεῖ." 

peMA.Ode9C13.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C13.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C13.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C13.text = "Ὄξος καὶ χολὴν Σωτήρ, ἐποτίσθης δι' ἡμᾶς, ὁ δοὺς ἡμῖν τὸ Σῶμα, καὶ τὸ τίμιον Αἷμα, εἰς βρῶσίν τε καὶ πόσιν, αἰωνίου σου ζωῆς." 

peMA.Ode9C14.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C14.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C14.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C14.text = "Τὴν ζωοποιὸν πλευράν, λόγχῃ κεντηθεὶς Χριστέ, τὸ ἄχραντόν σου Αἷμα, καὶ τὸ τίμιον Ὕδωρ, ἐπήγασας τῷ κόσμῳ, ὡς ἀείζωον πηγήν." 

peMA.Ode9C15.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C15.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C15.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C15.text = "Ἑλογίσθης ἐν νεκροῖς, ὁ ζωώσας τοὺς νεκρούς, ἐν τάφῳ κατετέθης, ὁ κενώσας τοὺς τάφους, ἐσκύλευσας τὸν ᾍδην, ἀναστήσας τὸν Ἀδάμ." 

peMA.Ode9C16.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C16.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C16.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C16.text = "Ἐξανέστης Ἰησοῦ, ἐδεσμεύθη ὁ ἐχθρός, ἐσκυλεύθη ὁ ᾍδης, ἐγυμνώθησαν τάφοι, ἠγέρθησαν οἱ κάτω, προσκυνοῦντές σε Χριστέ, ὁ κλέψας τὸν νεκρόν." 

peMA.Ode9C17.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C17.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C17.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C17.text = "Τίς, μάλιστα δὲ καὶ γυμνόν; τί πλανᾶσθε Ἑβραῖοι; ὁ Χριστὸς ἐξανέστη, καὶ λέλυνται τοῦ ᾍδου, τὰ δεσμὰ καὶ οἱ μοχλοί." 

peMA.Ode9C18.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C18.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C18.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C18.text = "Δόξα σοι Χριστὲ Σωτήρ, ὁ πηγάσας τὴν ζωήν, καὶ τὸ φῶς ἀνατείλας, τοῖς ἐν σκότει ἀγνοίας, καὶ πᾶσαν καταλάμψας, τῇ Ἐγέρσει σου τὴν γῆν." 

peMA.Ode9C1F1.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C1F1.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C1F1.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C1F1.text = "Ὁ εὐσχήμων βουλευτής, ἀνυμνείσθω Ἰωσήφ, μετὰ τῶν Μυροφόρων, καὶ τῶν θείων Μαθητῶν, ὡς κήρυξ ὢν καὶ οὗτος, τῆς Ἐγέρσεως Χριστοῦ."

peMA.Ode9C1F2.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C1F2.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C1F2.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C1F2.text = "Ἰωσὴφ τὸν θαυμαστόν, εὐφημήσωμεν πιστοί, συνάμα Νικοδήμῳ, καὶ πισταῖς Μυροφόροις, ὁ Κύριος ἀνέστη, ἐκβοῶντες ἀληθῶς."

peMA.Ode9C1Tr.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C1Tr.mode = misc_gr_GR_cog.Mode2
peMA.Ode9C1Tr.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
peMA.Ode9C1Tr.text = "Ἄναρχος εἶ ὁ Πατήρ, ἄκτιστος εἶ ὁ Υἱός, σύνθρονον καὶ τὸ Πνεῦμα, ἓν τὰ τρία τῇ φύσει, καὶ τρία τοῖς προσώποις, εἷς Θεὸς ἀληθινός."

peMA.Ode9C1T.ode = misc_gr_GR_cog.Ode9
