peMA.Ode8C21.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C21.mode = misc_gr_GR_cog.Mode4
peMA.Ode8C21.melody = he.h.m4_gr_GR_cog.LyeiTaDesma.name
peMA.Ode8C21.text = "Μνήμην ὁ Χριστὸς τῶν βροτοσσόων ἐπῶν, Ἅ πατρακουσθεὶς τοῖς Ἀποστόλοις ἔφη, Τὸ Πνεῦμα τεύχει γλωσσοπυρσεύτῳ θέᾳ, Ἐφίζον· εὐλογητὸν οἰκειουμένη, Ἠλλοτριωμένη δὲ μέλπει σε κτίσις." 
peMA.Ode8C211.text = "Μνήμην ὁ Χριστὸς τῶν βροτοσσόων ἐπῶν,"
peMA.Ode8C212.text = "Ἅ πατρακουσθεὶς τοῖς Ἀποστόλοις ἔφη,"
peMA.Ode8C213.text = "Τὸ Πνεῦμα τεύχει γλωσσοπυρσεύτῳ θέᾳ,"
peMA.Ode8C214.text = "Ἐφίζον· εὐλογητὸν οἰκειουμένη,"
peMA.Ode8C215.text = "Ἠλλοτριωμένη δὲ μέλπει σε κτίσις."

peMA.Ode8C22.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C22.mode = misc_gr_GR_cog.Mode4
peMA.Ode8C22.melody = he.h.m4_gr_GR_cog.LyeiTaDesma.name
peMA.Ode8C22.text = "Σωτηριωδῶς, αὐτοδεσπότως ἰόν, Φῶς αὐτολαμπές, καὶ παρεκτικὸν φάους, Ὑπάρχον, ἦκες ἐμφοροῦν Ἀποστόλοις, Τιμῆεν ὡς ἄημα· τοῖς σοῖς οἰκέταις, Λελιπαρημένον δὲ Πνεῦμα προσνέμοις." 
peMA.Ode8C221.text = "Σωτηριωδῶς, αὐτοδεσπότως ἰόν,"
peMA.Ode8C222.text = "Φῶς αὐτολαμπές, καὶ παρεκτικὸν φάους,"
peMA.Ode8C223.text = "Ὑπάρχον, ἦκες ἐμφοροῦν Ἀποστόλοις,"
peMA.Ode8C224.text = "Τιμῆεν ὡς ἄημα· τοῖς σοῖς οἰκέταις,"
peMA.Ode8C225.text = "Λελιπαρημένον δὲ Πνεῦμα προσνέμοις."

peMA.Ode8C23.ode = misc_gr_GR_cog.Ode8
peMA.Ode8C23.mode = misc_gr_GR_cog.Mode4
peMA.Ode8C23.melody = he.h.m4_gr_GR_cog.LyeiTaDesma.name
peMA.Ode8C23.text = "ᾞσε Προφητῶν πνευματέμφορον στόμα, Σὴν σωματωδῶς, ὦ μέδων, ἐνδημίαν, Καὶ Πνεῦμα κόλπων πατρικῶν προηγμένον, Ἀκτιστοσυμπλαστουργοσύνθρονον σέθεν, Ἱεὶς ἐνανθρωπήσεως πιστοῖς σέβας." 
peMA.Ode8C231.text = "ᾞσε Προφητῶν πνευματέμφορον στόμα,"
peMA.Ode8C232.text = "Σὴν σωματωδῶς, ὦ μέδων, ἐνδημίαν,"
peMA.Ode8C233.text = "Καὶ Πνεῦμα κόλπων πατρικῶν προηγμένον,"
peMA.Ode8C234.text = "Ἀκτιστοσυμπλαστουργοσύνθρονον σέθεν,"
peMA.Ode8C235.text = "Ἱεὶς ἐνανθρωπήσεως πιστοῖς σέβας."

peMA.Ode9C1H.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C1H.mode = misc_gr_GR_cog.Mode7
peMA.Ode9C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode9C1H.text = he.h.m7_gr_GR_cog.MiTisFthoras.text

peMA.Ode9C11.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C11.mode = misc_gr_GR_cog.Mode7
peMA.Ode9C11.melody = he.h.m7_gr_GR_cog.MiTisFthoras.name
peMA.Ode9C11.text = "Ἐπιπαφλάζοντος πάλαι πυρίνου ἅρματος, ὁ ζηλωτὴς καὶ πυρίπνους χαίρων ὀχούμενος, τὴν νῦν ἐκλάμψασαν ἐπίπνοιαν ἐδήλου, ἐξ ὕψους Ἀποστόλοις, ὑφ' ἧς καταλαμφθέντες, τὴν Τριάδα πᾶσιν ἐγνώρισαν." 

peMA.Ode9C12.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C12.mode = misc_gr_GR_cog.Mode7
peMA.Ode9C12.melody = he.h.m7_gr_GR_cog.MiTisFthoras.name
peMA.Ode9C12.text = "Νόμου τῶν φύσεων δίχα ξένον ἠκούετο· τῶν Μαθητῶν τῆς μιᾶς γὰρ φωνῆς ἀπηχουμένης, Πνεύματος χάριτι, ποικίλως, ἐνηχοῦντο, λαοί, φυλαὶ καὶ γλῶσσαι, τὰ θεῖα μεγαλεῖα, τῆς Τριάδος γνῶσιν μυούμενοι." 

peMA.Ode9C2H.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C2H.mode = misc_gr_GR_cog.Mode4
peMA.Ode9C2H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode9C2H.text = he.h.m4_gr_GR_cog.ChairoisAnassa.text
peMA.Ode9C2H1.text = he.h.m4_gr_GR_cog.ChairoisAnassa1.text
peMA.Ode9C2H2.text = he.h.m4_gr_GR_cog.ChairoisAnassa2.text
peMA.Ode9C2H3.text = he.h.m4_gr_GR_cog.ChairoisAnassa3.text
peMA.Ode9C2H4.text = he.h.m4_gr_GR_cog.ChairoisAnassa4.text
peMA.Ode9C2H5.text = he.h.m4_gr_GR_cog.ChairoisAnassa5.text

peMA.Ode9C21.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C21.mode = misc_gr_GR_cog.Mode4
peMA.Ode9C21.melody = he.h.m4_gr_GR_cog.ChairoisAnassa.name
peMA.Ode9C21.text = "Ὕδειν ἔοικε τὴν φυσίζωον Κόρην. Μόνη γὰρ ἐν δίνησι κεκρύφει Λόγον Νοσοῦσαν ἀλθαίνοντα τὴν βροτῶν φύσιν. Ὃς δεξιοῖς κλισμοῖσι νῦν ἱδρυμένος Πατρός, πέπομφε τὴν χάριν τοῦ Πνεύματος." 
peMA.Ode9C211.text = "Ὕδειν ἔοικε τὴν φυσίζωον Κόρην."
peMA.Ode9C212.text = "Μόνη γὰρ ἐν δίνησι κεκρύφει Λόγον"
peMA.Ode9C213.text = "Νοσοῦσαν ἀλθαίνοντα τὴν βροτῶν φύσιν."
peMA.Ode9C214.text = "Ὃς δεξιοῖς κλισμοῖσι νῦν ἱδρυμένος"
peMA.Ode9C215.text = "Πατρός, πέπομφε τὴν χάριν τοῦ Πνεύματος."

peMA.Ode9C22.ode = misc_gr_GR_cog.Ode9
peMA.Ode9C22.mode = misc_gr_GR_cog.Mode4
peMA.Ode9C22.melody = he.h.m4_gr_GR_cog.ChairoisAnassa.name
peMA.Ode9C22.text = "Ὅσοις ἔπνευσεν ἡ θεόρρυτος χάρις, Λάμποντες, ἀστράπτοντες, ἠλλοιωμένοι, Ὀθνείαν ἀλλοίωσιν εὐπρεπεστάτην Ἰσοσθενοῦσαν τὴν ἄτμητον εἰδότες, Σοφὴν τρίφεγγον οὐσίαν δοξάζομεν." 
peMA.Ode9C221.text = "Ὅσοις ἔπνευσεν ἡ θεόρρυτος χάρις,"
peMA.Ode9C222.text = "Λάμποντες, ἀστράπτοντες, ἠλλοιωμένοι,"
peMA.Ode9C223.text = "Ὀθνείαν ἀλλοίωσιν εὐπρεπεστάτην"
peMA.Ode9C224.text = "Ἰσοσθενοῦσαν τὴν ἄτμητον εἰδότες,"
peMA.Ode9C225.text = "Σοφὴν τρίφεγγον οὐσίαν δοξάζομεν."

peMA.Exaposteilarion1.mode = misc_gr_GR_cog.Mode3
peMA.Exaposteilarion1.melody = he.a.m3_gr_GR_cog.OOuranonToisAstrois.name
peMA.Exaposteilarion1.text = "Τὸ πανάγιον Πνεῦμα, τὸ προϊὸν ἐκ τοῦ Πατρός, καὶ δι' Υἱοῦ ἐνδημῆσαν, τοῖς ἀγραμμάτοις Μαθηταῖς, τοὺς σὲ Θεὸν ἐπιγνόντας, σῶσον, ἁγίασον πάντας."

peMA.Exaposteilarion2.mode = misc_gr_GR_cog.Mode3
peMA.Exaposteilarion2.melody = he.a.m3_gr_GR_cog.OOuranonToisAstrois.name
peMA.Exaposteilarion2.text = "Φῶς ὁ Πατήρ, φῶς ὁ Λόγος, φῶς καὶ τὸ ἅγιον Πνεῦμα, ὅπερ ἐν γλώσσαις πυρίναις, τοῖς Ἀποστόλοις ἐπέμφθη, καὶ δι' αὐτοῦ πᾶς ὁ κόσμος φωταγωγεῖται, Τριάδα σέβειν ἁγίαν."

peMA.Lauds1.mode = misc_gr_GR_cog.Mode4
peMA.Lauds1.melody = ""
peMA.Lauds1.text = "Παράδοξα σήμερον εἶδον τὰ ἔθνη πάντα ἐν πόλει Δαυΐδ, ὅτε τὸ Πνεῦμα κατῆλθε τὸ ἅγιον ἐν πυρίναις γλώσσαις, καθὼς ὁ θεηγόρος Λουκᾶς ἀπεφθέγξατο. Φησὶ γάρ· Συνηγμένων τῶν Μαθητῶν τοῦ Χριστοῦ, ἐγένετο ἦχος καθάπερ φερομένης βιαίας πνοῆς, καὶ ἐπλήρωσε τὸν οἶκον, οὗ ἦσαν καθήμενοι, καὶ πάντες ἤρξαντο φθέγγεσθαι, ξένοις ῥήμασι, ξένοις δόγμασι, ξένοις διδάγμασι, τῆς ἁγίας Τριάδος."

peMA.Lauds2.mode = misc_gr_GR_cog.Mode4
peMA.Lauds2.melody = ""
peMA.Lauds2.text = "Τὸ Πνεῦμα τὸ ἅγιον, ἦν μὲν ἀεί, καὶ ἔστι καὶ ἔσται, οὕτε ἀρξάμενον, οὔτε παυσόμενον, ἀλλ' ἀεὶ Πατρὶ καὶ Υἱῷ συντεταγμένον, καὶ συναριθμούμενον; ζωή, καὶ ζωοποιοῦν, φῶς, καὶ φωτὸς χορηγόν; αὐτάγαθον, καὶ πηγὴ ἀγαθότητος· δι' οὗ Πατὴρ γνωρίζεται, καὶ Υἱὸς δοξάζεται, καὶ παρὰ πάντων γινώσκεται, μία δύναμις, μία σύνταξις, μία προσκύνησις, τῆς ἁγίας Τριάδος."

peMA.Lauds3.mode = misc_gr_GR_cog.Mode4
peMA.Lauds3.melody = ""
peMA.Lauds3.text = "Τὸ Πνεῦμα τὸ ἅγιον, φῶς, καὶ ζωή, καὶ ζῶσα πηγὴ νοερά, Πνεῦμα σοφίας, Πνεῦμα συνέσεως, ἀγαθόν, εὐθές, νοερόν, ἡγεμονεῦον καθαῖρον τὰ πταίσματα, Θεὸς καὶ θεοποιοῦν, πῦρ, ἐκ πυρὸς προϊόν, λαλοῦν, ἐνεργοῦν, διαιροῦν τὰ χαρίσματα· δι' οὗ Προφῆται ἅπαντες, καὶ Θεοῦ Ἀπόστολοι, μετὰ Μαρτύρων ἐστέφθησαν. Ξένον ἄκουσμα, ξένον θέαμα, πῦρ διαιρούμενον εἰς νομὰς χαρισμάτων."

