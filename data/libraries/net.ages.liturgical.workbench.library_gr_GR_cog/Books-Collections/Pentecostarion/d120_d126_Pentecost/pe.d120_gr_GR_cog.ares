peMA.Ode8C2H.text = he.h.m4_gr_GR_cog.LyeiTaDesma.text
peMA.Ode8C2H1.text = he.h.m4_gr_GR_cog.LyeiTaDesma1.text
peMA.Ode8C2H2.text = he.h.m4_gr_GR_cog.LyeiTaDesma2.text
peMA.Ode8C2H3.text = he.h.m4_gr_GR_cog.LyeiTaDesma3.text
