peMA.Ode6C2H1.text = he.h.m4_gr_GR_cog.IlasmosIminChriste1.text
peMA.Ode6C2H2.text = he.h.m4_gr_GR_cog.IlasmosIminChriste2.text
peMA.Ode6C2H3.text = he.h.m4_gr_GR_cog.IlasmosIminChriste3.text
peMA.Ode6C2H4.text = he.h.m4_gr_GR_cog.IlasmosIminChriste4.text
peMA.Ode6C2H5.text = he.h.m4_gr_GR_cog.IlasmosIminChriste5.text

peMA.Ode6C21.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C21.mode = misc_gr_GR_cog.Mode4
peMA.Ode6C21.melody = he.h.m4_gr_GR_cog.IlasmosIminChriste.name
peMA.Ode6C21.text = "Ἱμερτὸν ἡμῖν εὐθὲς ἐν τοῖς ἐγκάτοις, Αἰωνίως ἕξουσι Πνεῦμα καινίσαις, Πατροπροβλήτως πάντοτε ξυνημμένον, Ὕλης ἀπεχθοῦς καυστικῶν μολυσμάτων, Ῥύπου τε φρενῶν ῥυπτικὸν Παντοκράτορ." 
peMA.Ode6C211.text = "Ἱμερτὸν ἡμῖν εὐθὲς ἐν τοῖς ἐγκάτοις,"
peMA.Ode6C212.text = "Αἰωνίως ἕξουσι Πνεῦμα καινίσαις,"
peMA.Ode6C213.text = "Πατροπροβλήτως πάντοτε ξυνημμένον,"
peMA.Ode6C214.text = "Ὕλης ἀπεχθοῦς καυστικῶν μολυσμάτων,"
peMA.Ode6C215.text = "Ῥύπου τε φρενῶν ῥυπτικὸν Παντοκράτορ."

peMA.Ode6C22.ode = misc_gr_GR_cog.Ode6
peMA.Ode6C22.mode = misc_gr_GR_cog.Mode4
peMA.Ode6C22.melody = he.h.m4_gr_GR_cog.IlasmosIminChriste.name
peMA.Ode6C22.text = "Ὀρεκτὸν ἀξίωμα τοῖς Ἀποστόλοις, Σιωνίταις μίμνουσι σὴν παρουσίαν, Γνώρισμα Πνεῦμα πατρογεννήτου Λόγου, Λέσχην ἀπηνῆ τῶν ἐθνῶν ποππυσμάτων, Ὤκιστα δεικνύς, πυρπνόως καθιδρύεις." 
peMA.Ode6C221.text = "Ὀρεκτὸν ἀξίωμα τοῖς Ἀποστόλοις,"
peMA.Ode6C222.text = "Σιωνίταις μίμνουσι σὴν παρουσίαν,"
peMA.Ode6C223.text = "Γνώρισμα Πνεῦμα πατρογεννήτου Λόγου,"
peMA.Ode6C224.text = "Λέσχην ἀπηνῆ τῶν ἐθνῶν ποππυσμάτων,"
peMA.Ode6C225.text = "Ὤκιστα δεικνύς, πυρπνόως καθιδρύεις."

peMA.Kontakion1.mode = misc_gr_GR_cog.Mode8
peMA.Kontakion1.melody = ""
peMA.Kontakion1.text = "Ὅτε καταβὰς τὰς γλώσσας συνέχεε, διεμέριζεν ἔθνη ὁ Ὕψιστος· ὅτε τοῦ πυρὸς τὰς γλώσσας διένειμεν, εἰς ἑνότητα πάντας ἐκάλεσε, καὶ συμφώνως δοξάζομεν τὸ πανάγιον Πνεῦμα."

peMA.Oikos1.text = "Ταχεῖαν καὶ σταθηρὰν δίδου παραμυθίαν τοῖς δούλοις σου, Ἰησοῦ, ἐν τῷ ἀκηδιάσαι τὰ πνεύματα ἡμῶν, μὴ χωρίζου τῶν ψυχῶν ἡμῶν ἐν θλίψεσι, μὴ μακρύνου τῶν φρενῶν ἡμῶν ἐν περιστάσεσιν, ἀλλὰ ἀεὶ ἡμᾶς πρόφθασον. Ἔγγισον ἡμῖν, ἔγγισον ὁ πανταχοῦ, ὥς περ καὶ τοῖς Ἀποστόλοις σου πάντοτε συνῇς, οὕτω καὶ τοῖς σὲ ποθοῦσιν ἕνωσον σαυτὸν οἰκτίρμον, ἵνα συνημμένοι σοι ὑμνῶμεν, καὶ δοξολογῶμεν τὸ πανάγιόν σου Πνεῦμα."

peMA.Synaxarion.commemoration.text = "Τῇ αὐτῇ ἡμέρᾳ, Κυριακῇ ογδόῃ ἀπὸ τοῦ Πάσχα, τὴν ἁγίαν Πεντηκοστὴν ἑορτάζομεν."
peMA.Synaxarion.couplet1.title = titles_gr_GR_cog.Stichoi
peMA.Synaxarion.couplet1.verse1 = "Πνοῇ βιαίᾳ γλωσσοπυρσεύτως νέμει,"
peMA.Synaxarion.couplet1.verse2 = "Χριστὸς τὸ θεῖον Πνεῦμα τοῖς Ἀποστόλοις."
peMA.Synaxarion.single1.text = "Ἐκκέχυται μεγάλῳ ἑνὶ ἤματι Πνεῦμ' ἁλιεῦσι."
peMA.Synaxarion.lastline.text = "Ταῖς τῶν ἁγίων Ἀποστόλων πρεσβείαις, Χριστὲ ὁ Θεὸς ἡμῶν, ἐλέησον ἡμᾶς. Ἀμήν."

peMA.Ode7C1H.ode = misc_gr_GR_cog.Ode7
peMA.Ode7C1H.mode = misc_gr_GR_cog.Mode7
