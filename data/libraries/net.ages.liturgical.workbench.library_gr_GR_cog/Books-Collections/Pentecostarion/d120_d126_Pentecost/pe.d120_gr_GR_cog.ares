peMA.Ode4C2H5.text = he.h.m4_gr_GR_cog.AnaxAnakton5.text

peMA.Ode4C21.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C21.mode = misc_gr_GR_cog.Mode4
peMA.Ode4C21.melody = he.h.m4_gr_GR_cog.AnaxAnakton.name
peMA.Ode4C21.text = "Λουτρὸν τὸ θεῖον τῆς παλιγγενεσίας, Λόγῳ κεραννὺς συντεθειμένῃ φύσει, Ὀμβροβλυτεῖς μοι ῥεῖθρον ἐξ ἀκηράτου Νενυγμένης σου πλευρᾶς, ὦ Θεοῦ Λόγε, Ἐπισφραγίζων τῇ ζέσει τοῦ Πνεύματος." 
peMA.Ode4C211.text = "Λουτρὸν τὸ θεῖον τῆς παλιγγενεσίας,"
peMA.Ode4C212.text = "Λόγῳ κεραννὺς συντεθειμένῃ φύσει,"
peMA.Ode4C213.text = "Ὀμβροβλυτεῖς μοι ῥεῖθρον ἐξ ἀκηράτου"
peMA.Ode4C214.text = "Νενυγμένης σου πλευρᾶς, ὦ Θεοῦ Λόγε,"
peMA.Ode4C215.text = "Ἐπισφραγίζων τῇ ζέσει τοῦ Πνεύματος."

peMA.Ode4C22.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C22.mode = misc_gr_GR_cog.Mode4
peMA.Ode4C22.melody = he.h.m4_gr_GR_cog.AnaxAnakton.name
peMA.Ode4C22.text = "Κάμπτει τὰ πάντα τῷ Παρακλήτῳ γόνυ, Γόνῳ τε Πατρός, Πατρὶ συμφυεστάτῳ. Ἐν γὰρ προσώποις οἶδε τριττοῖς οὐσίαν, Νημερτές, ἀπρόσιτον, ἄχρονον, μίαν. Ἔλαμψε φῶς γὰρ ἡ χάρις τοῦ Πνεύματος." 
peMA.Ode4C221.text = "Κάμπτει τὰ πάντα τῷ Παρακλήτῳ γόνυ,"
peMA.Ode4C222.text = "Γόνῳ τε Πατρός, Πατρὶ συμφυεστάτῳ."
peMA.Ode4C223.text = "Ἐν γὰρ προσώποις οἶδε τριττοῖς οὐσίαν,"
peMA.Ode4C224.text = "Νημερτές, ἀπρόσιτον, ἄχρονον, μίαν."
peMA.Ode4C225.text = "Ἔλαμψε φῶς γὰρ ἡ χάρις τοῦ Πνεύματος."

peMA.Ode4C23.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C23.mode = misc_gr_GR_cog.Mode4
peMA.Ode4C23.melody = he.h.m4_gr_GR_cog.AnaxAnakton.name
peMA.Ode4C23.text = "Τελεῖσθε πάντες τῇ θεαρχικωτάτῃ, Ὅσοι λατρευταὶ τῆς τριφεγγοῦς οὐσίας. Ὑπερφυῶς τελεῖ γὰρ ὡς εὐεργέτης, Καὶ πυρσολαμπεῖ Χριστὸς εἰς σωτηρίαν, Ὅλην πορίζων τὴν χάριν τοῦ Πνεύματος." 
peMA.Ode4C231.text = "Τελεῖσθε πάντες τῇ θεαρχικωτάτῃ,"
peMA.Ode4C232.text = "Ὅσοι λατρευταὶ τῆς τριφεγγοῦς οὐσίας."
peMA.Ode4C233.text = "Ὑπερφυῶς τελεῖ γὰρ ὡς εὐεργέτης,"
peMA.Ode4C234.text = "Καὶ πυρσολαμπεῖ Χριστὸς εἰς σωτηρίαν,"
peMA.Ode4C235.text = "Ὅλην πορίζων τὴν χάριν τοῦ Πνεύματος."

peMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C1H.mode = misc_gr_GR_cog.Mode7
peMA.Ode5C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode5C1H.text = he.h.m7_gr_GR_cog.ToDiaTonFovonSou.text

peMA.Ode5C11.ode = misc_gr_GR_cog.Ode5
peMA.Ode5C11.mode = misc_gr_GR_cog.Mode7
peMA.Ode5C11.melody = he.h.m7_gr_GR_cog.ToDiaTonFovonSou.name
peMA.Ode5C11.text = "Ἡ ἐπιφοιτήσασα ἰσχὺς σήμερον, αὕτη Πνεῦμα ἀγαθόν, Πνεῦμα σοφίας Θεοῦ, Πνεῦμα ἐκ Πατρὸς ἐκπορευτόν, καὶ δι' Υἱοῦ πιστοῖς ἡμῖν πεφηνός, μεταδοτικόν, ἐν οἷς κατοικίζεται φύσει, τῆς ἐν ᾗ κατοπτεύεται ἁγιότητος." 

