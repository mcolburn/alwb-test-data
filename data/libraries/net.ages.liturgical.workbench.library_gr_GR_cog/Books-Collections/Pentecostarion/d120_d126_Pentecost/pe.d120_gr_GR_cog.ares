peMA.Ode3C21.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C21.mode = misc_gr_GR_cog.Mode4
peMA.Ode3C21.melody = he.h.m4_gr_GR_cog.ErrixeGastros.name
peMA.Ode3C21.text = "Ἄληπτός ἐστιν ἡ θεαρχικωτάτη· Ῥήτρας γὰρ ἐξέφηνε τοὺς ἀγραμμάτους, Ἅλις σοφιστὰς συστομίζοντας λόγῳ, Καὶ τῆς βαθείας νυκτὸς ἐξαιρουμένους, Λαοὺς ἀπείρους, ἀστραπῇ τοῦ Πνεύματος." 
peMA.Ode3C211.text = "Ἄληπτός ἐστιν ἡ θεαρχικωτάτη·"
peMA.Ode3C212.text = "Ῥήτρας γὰρ ἐξέφηνε τοὺς ἀγραμμάτους,"
peMA.Ode3C213.text = "Ἅλις σοφιστὰς συστομίζοντας λόγῳ,"
peMA.Ode3C214.text = "Καὶ τῆς βαθείας νυκτὸς ἐξαιρουμένους,"
peMA.Ode3C215.text = "Λαοὺς ἀπείρους, ἀστραπῇ τοῦ Πνεύματος."

peMA.Ode3C22.ode = misc_gr_GR_cog.Ode3
peMA.Ode3C22.mode = misc_gr_GR_cog.Mode4
peMA.Ode3C22.melody = he.h.m4_gr_GR_cog.ErrixeGastros.name
peMA.Ode3C22.text = "Ἦν ἐκπορευτὸν ἐξ ἀγεννήτου φάους, Τὸ πανσθενουργόφωτον ἄφθιτον σέλας, Οὗ τὴν δι' Υἱοῦ πατρικῆς ἐξουσίας, Νῦν ἐμφανίζει συμφυῆ φρυκτωρίαν, Πυρῶδες ἦχος ἐν Σιὼν τοῖς ἔθνεσιν." 
peMA.Ode3C221.text = "Ἦν ἐκπορευτὸν ἐξ ἀγεννήτου φάους,"
peMA.Ode3C222.text = "Τὸ πανσθενουργόφωτον ἄφθιτον σέλας,"
peMA.Ode3C223.text = "Οὗ τὴν δι' Υἱοῦ πατρικῆς ἐξουσίας,"
peMA.Ode3C224.text = "Νῦν ἐμφανίζει συμφυῆ φρυκτωρίαν,"
peMA.Ode3C225.text = "Πυρῶδες ἦχος ἐν Σιὼν τοῖς ἔθνεσιν."

peMA.Ode3Kathisma1.mode = misc_gr_GR_cog.Mode8
peMA.Ode3Kathisma1.melody = he.a.m8_gr_GR_cog.ToProstachthen.name
peMA.Ode3Kathisma1.text = "Οἱ τοῦ Σωτῆρος ἐρασταὶ χαρᾶς ἐπλήσθησαν, καὶ θάρσος ἔλαβον, οἱ πρῴην δειλιῶντες, ὡς τὸ Πνεῦμα τὸ ἅγιον σήμερον ἐξ ὕψους, κατῆλθεν ἐπὶ τὸν οἶκον τῶν Μαθητῶν, καὶ ἄλλος ἄλλα ἐλάλει πρὸς τοὺς λαούς· γλῶσσαι γὰρ διεσπάρησαν, ὁρώμεναι ὡσεὶ πυρός, καὶ τούτους οὐ κατέφλεξαν, ἀλλὰ μᾶλλον ἐδρόσισαν."

peMA.Ode4C1H.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C1H.mode = misc_gr_GR_cog.Mode7
peMA.Ode4C1H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode4C1H.text = he.h.m7_gr_GR_cog.KatanoonOProfitis.text

peMA.Ode4C11.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C11.mode = misc_gr_GR_cog.Mode7
peMA.Ode4C11.melody = he.h.m7_gr_GR_cog.KatanoonOProfitis.name
peMA.Ode4C11.text = "Ὁ ἐν Προφήταις λαλήσας, καὶ διὰ νόμου κηρυχθείς, πρῴην τοῖς ἀτελέσι, Θεὸς ἀληθὴς ὁ Παράκλητος, τοῖς τοῦ Λόγου ὑπηρέταις καὶ μάρτυσι, γνωρίζεται σήμερον." 

peMA.Ode4C12.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C12.mode = misc_gr_GR_cog.Mode7
peMA.Ode4C12.melody = he.h.m7_gr_GR_cog.KatanoonOProfitis.name
peMA.Ode4C12.text = "Σῆμα Θεότητος φέρον, τοῖς Ἀποστόλοις ἐν πυρί, Πνεῦμα κατεμερίσθη, καὶ ξέναις ἐν γλώσσαις ἐνέφηνεν, ὡς πατρόθεν θεῖον σθένος, ἐρχόμενον ἐστὶν αὐτοκέλευστον." 

peMA.Ode4C2H.ode = misc_gr_GR_cog.Ode4
peMA.Ode4C2H.mode = misc_gr_GR_cog.Mode4
peMA.Ode4C2H.melody = misc_gr_GR_cog.Heirmos
peMA.Ode4C2H.text = he.h.m4_gr_GR_cog.AnaxAnakton.text
