A_Resource_Whose_Name = oc.m3.d3_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera1.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera1.text = "Τὰ πάθη τῆς ψυχῆς Σωτήρ μου καὶ τοῦ σώματος, δεινῶς ἐπαναστάντα, βυθίζουσί με ἄμφω, καὶ πρὸς βάθος ἀπογνώσεώς με ἕλκουσιν, ἀλλ' ὡς ποτὲ θάλασσαν, ταῦτα κατεύνασον, καὶ γαλήνην μοι δίδου."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera2.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera2.text = "Οὐκ ἔχω παρρησίαν ὅλως ὁ κατάκριτος, εἰς ὕψος ἀτενίσαι, τοῦ οὐρανοῦ καὶ βλέψαι, ἀπὸ πλήθους καθελκόμενος πταισμάτων μου, λοιπὸν ὡς ὁ Τελώνης, βοῶ· Ἱλάσθητι, τῷ ἀθλίῳ μοι Σῶτερ."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera3.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera3.text = "Μεγάλη καὶ φρικτή σου Δέσποτα ἡ ἔλευσις, ἐν ᾗ καθίσῃς κρίσιν, δικαίαν ἐκτελέσαι· μὴ οὖν κρίνῃς με τὸν κατακεκριμένον, ἀλλ' ὡς Θεὸς φεῖσαί μου, καὶ ἐλευθέρωσον, τῆς μελλούσης ἀνάγκης."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera4.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera4.text = "Ὡς ὄρθρος ἐξανέτειλας πᾶσι τοῖς πέρασι, φωτίζων τὰς καρδίας, τῶν σὲ ὑμνούντων ἔνδοξε˙ διὸ φώτισον ἡμῶν τὰς διανοίας, καὶ τὰς ψυχὰς Προφῆτα, τοῦ γεραίρειν σε, Βαπτιστὰ τοῦ Σωτῆρος."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera5.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera5.text = "Μὴ παύσῃ ἱκετεύειν Πρόδρομε μακάριε, ὑπὲρ παντὸς τοῦ κόσμου, δεόμεθά σου πάντες, ὅπως λάβωμεν ἐξ ὕψους θείαν χάριν, καταπατεῖν τὴν κάραν, τοῦ κακογνώμονος, οἱ πλουτοῦντές σε πρέσβυν."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode3
ocVE.Stichera6.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.Stichera6.text = "Ἡνίκα τοῦ φθαρτοῦ με μέλλει Κύριος, χωρίζειν τότε σκήνους, προστάτην εὕροιμί σε, καὶ ἀντίληψιν καὶ σκέπην, Ἰωάννη Πρόδρομε καὶ Προφῆτα, καθοδηγοῦντά με, πρὸς ἀνέσπερον φέγγος."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode3
ocVE.SticTheotokion.melody = he.a.m3_gr_GR_cog.MegaliTouStavrouSou.name
ocVE.SticTheotokion.text = "Μὴ παύσῃ ἱκετεύουσα ὑπὲρ τῆς ποίμνης σου, ἀλλὰ ταῖς σαῖς πρεσβείαις, μελλούσης ἡμᾶς λύτρωσαι, ἀπειλῆς καὶ ἑνεστώσης, πάσης βλάβης, καὶ τῆς ὀργῆς Κυρίου, πάντας ἐξάρπασον, Θεοτόκε σοὺς δούλους."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m3.d2_gr_GR_cog.ocVE.Aposticha1.text

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha2.melody = misc_gr_GR_cog.Automelon
ocVE.Aposticha2.text = he.a.m3_gr_GR_cog.SosonMeKyrie.text

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocVE.Aposticha3.melody = misc_gr_GR_cog.Automelon
ocVE.Aposticha3.text = he.a.m3_gr_GR_cog.MegaliTonMartyronSou.text

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode3
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.text = "Θεοτόκε ἡ προστασία πάντων τῶν δεομένων, εἰς σὲ θαρροῦμεν, εἰς σὲ καυχώμεθα, ἐν σοὶ πᾶσα ἡ ἐλπὶς ἡμῶν ἐστι, πρέσβευε τῷ ἐκ σοῦ τεχθέντι, ὑπὲρ ἀχρείων δούλων σου."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode3
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.text = oc.m3.d3_gr_GR_cog.ocMA.Kathisma13.text

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.text = oc.m3.d2_gr_GR_cog.ocMA.Kathisma11.text

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma12.melody = ""
ocMA.Kathisma12.text = oc.m3.d2_gr_GR_cog.ocMA.Kathisma12.text

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.text = "Καταφυγὴ καὶ δύναμις ἡμῶν Θεοτόκε, ἡ κραταιὰ βοήθεια τοῦ κόσμου, ταῖς πρεσβείαις σου σκέπε τοὺς δούλους σου, ἀπὸ πάσης ἀνάγκης, μόνη εὐλογημένη."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.text = "Τὸν βυθὸν τῶν πταισμάτων μου, σὺ γινώσκεις Κύριε, δός μοι χεῖρα ὡς τῷ Πέτρῳ, καὶ σῶσόν με."

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma22.melody = ""
ocMA.Kathisma22.text = "Δικαίας κρίσεως ἐπερχομένης, καὶ τοῦ Κριτοῦ τῶν ὅλων δικάζοντος, ποίαν ἀπολογίαν δώσεις ψυχή μου; εἰμὴ πρὸ τοῦ τέλους βοήσῃς· Ἥμαρτον ὁ Θεός, καὶ ἐλέησόν με."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma23.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
ocMA.Kathisma23.text = "Θεομακάριστε, Μήτηρ ἀνύμφευτε, τὴν ἀσθενοῦσαν μου, ψυχὴν θεράπευσον, ὅτι συνέχομαι δεινῶς, τοῖς πταίσμασιν ὁ ἄθλιος· ὅθεν καὶ κραυγάζω σοι, στεναγμῷ τῆς καρδίας μου· Δέξαι με Πανάμωμε, τὸν πολλὰ πλημμελήσαντα, ἵνα ἐν παρρησίᾳ βοῶ σοι· Χαῖρε ἡ Κεχαριτωμένη."

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma31.melody = he.a.m3_gr_GR_cog.TheiasPisteos.name
ocMA.Kathisma31.text = "Ἐν τῇ σκέπῃ σου, προσπεφευγότες, πίστει κράζομεν, ψυχῆς ἐκ βάθους, θεομακάριστε Προφῆτα καὶ Πρόδρομε, τῶν πειρασμῶν καὶ κινδύνων τὸν τάραχον, καὶ νοσημάτων τὸν σάλον κατεύνασον, καὶ ματαίωσον ἐχθρῶν δυσμενῶν βουλεύματα, αἰτούμενος ἡμῖν τὸ μέγα ἔλεος."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma32.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
ocMA.Kathisma32.text = "Τῶν ἀθλοφόρων σου, τὴν μνήμην Κύριε, ὑπερεφαίδρυνας, ὡς παντοδύναμος, ὅτι ἐνίσχυσας αὐτούς, τὰ πάθη σου μιμήσασθαι, ἐνίκησαν ἀνδρείως δέ, τοῦ Βελίαρ τήν δύναμιν· ὅθεν καὶ ἀπέλαβον, ἰαμάτων χαρίσματα. Αὐτῶν ταῖς ἱκεσίαις οἰκτίρμον, εἰρήνην παράσχου τῷ λαῷ σου."

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma33.melody = ""
ocMA.Kathisma33.text = "Ἕκαστος ὅπου σῴζεται, ἐκεῖ δικαίως καὶ προστρέχει, καὶ ποία ἄλλη τοιαύτη καταφυγή, ὡς σὺ Θεοτόκε, σκέπουσα τὰς ψυχὰς ἡμῶν;"

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχίς, ἄνευ τῶν Θεοτοκίων, ἐκτὸς τῆς θ' ᾠδῆς· Ἐμοῖ(ς στεν)αγμοῖς προσχήσω, ὦ Θεοῦ Λόγε. Ἰωσήφ."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m3_gr_GR_cog.ThavmastosEndoxos.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C11.text = "Ἑαυτοὺς θερμῶς ἀδελφοὶ κλαύσωμεν, ὅλῃ τῇ ψυχῇ, ἐγγίζει ὁ Νυμφίος, ἀνάψωμεν τὰς πράξεις, ὥσπερ φαιδρὰς λαμπάδας, ὅπως θείου νυμφῶνος τύχωμεν ἐκεῖ."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C12.text = "Μεταγνοὺς ἐξ ὅλης ψυχῆς, σέσωσται, πάλαι Μανασσῆς· ἐβόησε γὰρ μέσον, δεσμῶν κατειλημμένος, πρὸς μόνον τὸν Δεσπότην, τοῦτον ζήλου ψυχή, καὶ σώζῃ εὐχερῶς."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C13.text = "Ὀφθαλμῶν πηρώσεις, χειρῶν στέρησιν, γλώσσης ἐκκοπήν, ποδῶν τε ἀφαιρέσεις, μηρῶν καὶ βραχιόνων, ὑπήνεγκαν οἱ θεῖοι Ἀθλοφόροι, εὐχαριστοῦντες Ἰησοῦ Χριστῷ."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C14.text = "Ἰατρεῖον ὤφθη ἡμῖν Μάρτυρες, πᾶσι τοῖς πιστοῖς, ἡ θήκη τῶν λειψάνων, ἐξ ἧς ἀντλοῦμεν ῥῶσιν, ψυχῶν τε καὶ σωμάτων, κατὰ χρέος ὑμᾶς γεραίροντες ἀεί."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C1T.text = "Ἀθανασίας μάννα Χριστὸν φέρουσα, στάμνε λογική, πικρίας ψυχοφθόρων, παθῶν ἐκλύτρωσαί με, Παρθένε Παναγία, ὅπως πίστει δοξολογῶ σε εὐσεβῶς."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m3_gr_GR_cog.OTaYdata.name
ocMA.OdeW1C21.text = "Στειρευούσης νηδύος βλάστημα θεῖον, καρπούς με τῶν ἀρετῶν ἐκβλαστάνειν, τῷ Θεῷ πρέσβευε, τῆς ἀκαρπίας μου, λύων τὴν κατήφειαν, καὶ ἀπελαύνων τὸν ζόφον, ἐκ τῆς διανοίας μου, Κυρίου Πρόδρομε."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m3_gr_GR_cog.OTaYdata.name
ocMA.OdeW1C22.text = "Τοῦ Ἡλίου τῆς δόξης μέγιστος, ὥσπερ προτρέχων ἀστὴρ ἐν γῇ σὺ ἐφάνης, τοῦ τὴν γῆν ἅπασαν, καταφαιδρύνοντος· τοῦτον οὖν ἱκέτευε, τὴν σκοτισθεῖσαν ψυχήν μου, φαύλαις ἐνθυμήσεσι, φωτίσαι Πρόδρομε."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m3_gr_GR_cog.OTaYdata.name
ocMA.OdeW1C23.text = "Τὴν ζωὴν τοῖς ἐν ᾅδῃ, Πνεύματι θείῳ ἐγγίζουσαν, προμηνύσας Προφῆτα, τὴν ἐμὴν ζώωσον νενεκρωμένην ψυχήν, σοῦ ταῖς παρακλήσεσι, καὶ ὡς ἐκ τάφου πταισμάτων, ἀνάστησον δέομαι, ἔνδοξε Πρόδρομε."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m3_gr_GR_cog.OTaYdata.name
ocMA.OdeW1C2T.text = "Μετὰ τῶν Ἀρχαγγέλων, μετὰ τῶν Ἀγγέλων Παρθένε, μετὰ Ἁγίων ἁπάντων, τὸν ἐκ σοῦ Κύριον ἐπιφανέντα ἡμῖν, αἴτησαι δεόμεθα, τοὺς ἀληθῆ Θεοτόκον, καθομολογοῦντάς σε, κινδύνων ῥύσασθαι."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m3_gr_GR_cog.SteiraPsychi.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C11.text = "Νόμον Θεοῦ ἠθέτησα, ὁ ἀσυνείδητος, καὶ μέλλω κατακρίνεσθαι, καὶ τί ποιήσω οὐκ ἐπίσταμαι· Κριτὰ δικαιότατε, οἴκτειρον σῶσόν με, τῇ εὐσπλαγχνίᾳ σου."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C12.text = "Ἀνατολὴ φιλάνθρωπε, ὑπάρχων ἀνατολῶν, ἀνάτειλόν μοι δέομαι, δικαιοσύνης φῶς, ἐξαίρων με παθῶν ἀμαυρώσεως, καὶ σκότους κολάσεως, Πολυέλεε."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C13.text = "Τῆς νοητῆς οἰκήτορες, ξύλα κατάκαρπα, τοῦ Παραδείσου Μάρτυρες, πηγαὶ τὸ θεῖον ὕδωρ ἔχουσαι, κρατῆρες προχέοντες, τὸ πόμα τὸ ἅγιον, ἀνεδείχθητε."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C14.text = "Μίαν ἐν πλείστοις σώμασι, γνώμην περιφέροντες, οἱ ἀθλοφόροι Μάρτυρες, τὰς μυριάδας ἐτροπώσαντο, ἐχθροῦ κοσμοκράτορος, Τριάδα ἀμέριστον, καταγγέλλοντες."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C1T.text = "Παρθενομῆτορ ἄχραντε, μετανοοῦσιν ἡμῖν, καὶ τῷ Χριστῷ προστρέχουσι, καὶ τῶν πταισμάτων λύσιν θέλουσι λαβεῖν, τοῦτον ἵλεων, τοῖς πᾶσιν ἀπέργασαι, πάντων Δέσποινα."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C21.text = "Φωνὴ τοῦ Λόγου γενόμενος, Βαπτιστά, φωνὰς τῶν τιμώντων σε, πρὸς αὐτὸν νῦν κατεύθυνον, καὶ πταισμάτων ἄφεσιν, τῇ μεσιτείᾳ ἡμῖν τῇ σῇ παράσχου."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C22.text = "Ἡμάρτηκά σοι, ἠνόμησα, καὶ δεινῶς Σωτὴρ ἐπλημμέλησα, καὶ ψυχὴν κατερρύπωσα· διὰ τοῦτο δέομαι, διὰ τοῦ σοῦ Βαπτιστοῦ οἰκτείρησόν με."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C23.text = "Ἐν ἐρημίᾳ πλανώμενον ἡδονῶν, ἐρήμου τὸ θρέμμα σε, ὁδηγόν τε τοῦ νέου λαοῦ, ἱκετεύω Πρόδρομε, πρὸς μετανοίας ὁδὸν ὁδήγησόν με."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C2T.text = "Σὺν Ἀποστόλοις, Προφήταις, Ἱερουργοῖς, σὺν Μάρτυσιν Ἄχραντε, σὺν ταῖς ἄνω Δυνάμεσι, τῷ Υἱῷ σου πρέσβευε, τοῦ οἰκτειρῆσαι ἡμᾶς τοὺς σὲ ὑμνοῦντας."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m3_gr_GR_cog.ToKataskionOros.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C11.text = "Ὁ πηγάσας ἐκ πέτρας ὕδωρ, πάλαι λαῷ ἀπειθοῦντι Χριστέ, καὶ ἀντιλέγοντι, ὕδωρ τῆς δίψης ἴαμα, ἐκ τῆς ἀνίκμου ψυχῆς μου ὁ Θεός, κατανύξεως σταγόνας, ἐκπλυνούσας με βλῦσον."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C12.text = "Ἰατρὲ τῶν νοσούντων, τὰ τῆς καρδίας μου πάθη, ἰάτρευσον εὐσπλάγχνῳ ῥοπῇ, τῆς μετανοίας φάρμακα, προσεπιπλάττων ὡς ἀγαθός, δωρούμενός μοι ῥῶσιν, τὴν ψυχικήν ὡς οἰκτίρμων."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C13.text = "Συμπαθοῦς ἀθλοφόροι, τῆς πρὸς τὸ σῶμα φιλίας, ἀποζευγνύντες τὸν λογισμόν, τοῖς τιμωρεῖν ἐθέλουσι, παραδεδώκατε ἑαυτούς, ὅπως γνήσιοι τοῦ Κτίστου, χρηματίζητε φίλοι."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C14.text = "Πολυώδυνα ἄλγη, πολυειδῶν ἐκ βασάνων ὑπέστητε, ὁπλῖται τοῦ Χριστοῦ, καὶ χάριν ἐκομίσασθε, ἐκ τῶν τοῦ Πνεύματος δωρεῶν, τῶν παθῶν ἡμῶν διώκειν, τὰς χρονίους ὀδύνας."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C1T.text = "Πολυώνυμε Κόρη, χαῖρε ἐξ ἧς ὁ Λόγος ἐτέχθη, ἀλογίας ἡμᾶς, λύων ἀτόπων πράξεων. Χαῖρε νεφέλη φωτεινή, ἡ σκεδάζουσα τὰ νέφη, τῆς ἡμῶν ἀθυμίας."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C21.text = "Λόγου σε φωνήν, τοῦ σαρκὶ φανέντος ὑπάρχουσαν, Πρόδρομε Κυρίου δυσωπῶ, ἀλογωτάτων ῥῦσαί με πράξεων, τῶν λόγοις τιμῶντά σε, καὶ κατὰ χρέος πίστει μακαρίζοντα."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C22.text = "Στέναξον ψυχή, καὶ Θεῷ τῷ κτίστῃ σου βόησον· Ἥμαρτον, ἱλάσθητι Χριστέ, καὶ φοβερᾶς με ῥῦσαι κολάσεως, κινδύνων καὶ θλίψεων, ταῖς τοῦ Προδρόμου θείαις παρακλήσεσιν."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C23.text = "Κύμασι πολλοῖς, χαλεπῶν παθῶν βυθιζόμενον, καὶ κλυδωνιζόμενον δεινῶς, καὶ συνεχῶς ἀεὶ βυθιζόμενον, Βαπτιστὰ ἐξάρπασον, καὶ σωτηρίας ὅρμῳ καθοδήγησον."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C2T.text = "Μήτηρ τοῦ Θεοῦ, Σεραφεὶμ ὑπέρτερον ὄχημα, μετὰ τῶν ἀΰλων Λειτουργῶν, μετὰ ἁγίων ἁπάντων, ἱκέτευε Χριστόν, ὃν ἐκύησας, ἀπεγνωσμένον σῶσαί με Πανάμωμε."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.name
ocMA.OdeW5C11.text = "Ῥαντίσας με μετανοίας ὑσσώπῳ, κάθαρον ῥύπου τῶν παθῶν, ἵνα ὀφθῶ σοι καθαρός, ὅταν μέλλῃς Ἰησοῦ κρῖναι τὰ σύμπαντα, τῇ δικαίᾳ σου κρίσει."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.name
ocMA.OdeW5C12.text = "Οἱ μώλωπες συνεσάπησαν Λόγε, τῆς παναθλίας μου ψυχῆς, ἰατρὲ τῶν ἀσθενῶν, καὶ δοτὴρ τῶν ἀγαθῶν, ἴασαι σῶσόν με, διὰ πλῆθος ἐλέους."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.name
ocMA.OdeW5C13.text = "Συντέτριπται τὰ ὀστράκινα σκεύη, τῶν Ἀθλοφόρων ἐπὶ γῆς, ἀλλ' ὁ τόνος τῆς ψυχῆς ἐκρατύνετο πλέον, καὶ ἐλαμπρύνετο, τοῦ Χριστοῦ τῇ δυνάμει."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.name
ocMA.OdeW5C14.text = "Χεόμενον τῶν Ἁγίων τὸ αἷμα, πᾶσαν ἡγίασε τὴν γῆν, καὶ κατήρδευσε ψυχάς, καὶ ἐξήρανε σαφῶς τῆς ματαιότητος, θολεροὺς τοὺς χειμάρρους."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m3_gr_GR_cog.OrthrizontesAnymnoumen.name
ocMA.OdeW5C1T.text = "Τῷ τόκῳ σου ἀπεστείρωσας Κόρη, τὴν τοῦ προπάτορος ἀράν, καὶ ἐπήγασας ἡμῖν, εὐλογίας ποταμούς, τοῖς εὐλογοῦσί σε, καὶ δοξάζουσι πίστει."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m3_gr_GR_cog.EpiTisGis.name
ocMA.OdeW5C21.text = "Ἐπὶ τῆς γῆς, ὥσπερ Ἄγγελος μάκαρ, ἐπολιτεύσω σαφῶς μετὰ σαρκός· διό σου δέομαι, σαρκικοῦ φρονήματος, τὴν ψυχήν μου ἐλευθέρωσον."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m3_gr_GR_cog.EpiTisGis.name
ocMA.OdeW5C22.text = "Τὸν εἰς βυθὸν ἁμαρτίας πεσόντα, καὶ ἡδοναῖς τὴν ψυχὴν μολυνθέντα, καὶ κινδυνεύοντα, τοῦ Κυρίου Πρόδρομε, προσφυγόντα σοι διάσωσον."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m3_gr_GR_cog.EpiTisGis.name
ocMA.OdeW5C23.text = "Σὺν Προφητῶν περισσότερος ὤφθης· καὶ γὰρ αὐτὸν ἐθεάσω, Προφῆτα τὸν κηρυττόμενον, ὃν ἀπαύστως αἴτησαι, τοῦ φωτίσαι τὰς ψυχὰς ἡμῶν."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m3_gr_GR_cog.EpiTisGis.name
ocMA.OdeW5C2T.text = "Τῶν οὐρανῶν πλατυτέρα, σὺ ὤφθης ὡς ἀληθῶς, Θεοτόκε Παρθένε θεοχαρίτωτε, τῆς στενοχωρούσης με, ἁμαρτίας ἐλευθέρωσον."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C11.text = "Ἐπλήγην ἁμαρτιῶν τῇ ῥομφαίᾳ, καὶ κεῖμαι ἀνενέργητος ὅλος, ὁ πληγωθεὶς δι' ἐμὲ λόγχῃ ποτέ, Χριστὲ ὁ Θεός, οἰκτείρησόν με τῇ σῇ εὐσπλαγχνίᾳ."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C12.text = "Στενάζω καὶ κακοῖς ἐπιμένω, δακρύω, καὶ τὴν κρίσιν οὐ φρίττω, ἀναισθησίαν νοσῶν. Λόγε Θεοῦ, οἰκτείρησόν με, καὶ σῶσον τοῖς ἀγαθοῖς κρίμασί σου."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C13.text = "Ὡς ἄρνες οὐκ ἐρίζοντες ὅλως, οὐ λόγον, οὐ φωνὴν ἀφιέντες, πανευκλεεῖς ἀθληταί, πρὸς τὴν σφαγήν, καὶ τοὺς αἰκισμούς, προσήγεσθε, τὸν Χριστὸν ἀνυμνοῦντες."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C14.text = "Θηρίοις παρεδίδοσθε βρῶμα, θαλάσσης τοῖς βυθοῖς ὡμιλεῖτε, ἀγαλλομένῃ ψυχῇ· ὅθεν ὑμᾶς Χριστός, Ἀθληταί, νίκης στεφάνοις, τοῖς ἀφθάρτοις ἐκόσμει."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C1T.text = "Ἡ θύρα τῶν πιστῶς σῳζομένων, ἡ πύλη ἣν διώδευσε μόνος, ὁ σαρκωθεὶς δι' ἡμᾶς, πύλας ἡμῖν δικαιοσύνης, ὑπάνοιξον, τοῖς πιστῶς σε ὑμνοῦσι."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m3_gr_GR_cog.AvyssosEschati.name
ocMA.OdeW6C21.text = "Ὕδατι χειμάρρουν τὸν τῆς τρυφῆς, ὑποκλιθέντα, τῇ χειρί σου ἐβάπτισας, ἀλλ' αὐτὸν ἱκέτευε, καταπέμψαι μοι ὕδωρ, σοφὲ κατανύξεως, τῷ πολλὰ πλημμελήσαντι."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m3_gr_GR_cog.AvyssosEschati.name
ocMA.OdeW6C22.text = "Ἄβυσσον ἐλέους ἐν ποταμῷ, Προφῆτα λούεις, ὑπερῷα τοῖς ὕδασιν, οὐρανοὺς στεγάζοντα, Ἰησοῦν τὸν φιλάνθρωπον, ὃν δυσώπησον, ἑπομβρῆσαί μοι ἄφεσιν."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m3_gr_GR_cog.AvyssosEschati.name
ocMA.OdeW6C23.text = "Ἤγγικεν ἡ βασιλεία τῶν οὐρανῶν, μετανοεῖτε, ἀνεκραύγαζες Πρόδρομε, ἧς τυχεῖν ἀξίωσον, τοὺς πόθῳ σε γεραίροντας, καὶ τῇ σκέπῃ σου, τῇ σεπτῇ καταφεύγοντας."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m3_gr_GR_cog.AvyssosEschati.name
ocMA.OdeW6C2T.text = "Ἄχραντε ἡ σάρκα τῷ πλαστουργῷ, δανεισαμένη, σὺν ταῖς ἄνω Δυνάμεσι, σὺν Προφήταις ἅπασιν, Ἀποστόλοις καὶ Μάρτυσι, τοῦτον πρέσβευε, οἰκτειρῆσαι καὶ σῶσαί με."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m3_gr_GR_cog.OTinFloga.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m3_gr_GR_cog.OTinFloga.name
ocMA.OdeW7C11.text = "Ἐγυμνώθην χιτῶνα ἀφθαρσίας, ἐνεδύθην δὲ ἔργα ἀτιμίας· διὸ βοῶ σοι οἰκτίρμον Θεέ· Ἀρετῶν στολαῖς με καταφαίδρυνον."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m3_gr_GR_cog.OTinFloga.name
ocMA.OdeW7C12.text = "Ὀφθαλμοῖς ἀκολάστοις ἐσπιλώθην, ἐπαφῇ ἀκρασίας ἐρρυπώθην, καὶ ἐβδελύχθην ἐνώπιόν σου Ἰησοῦ, ὡς τὸν Ἄσωτον δέξαι με."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m3_gr_GR_cog.OTinFloga.name
ocMA.OdeW7C13.text = "Ὑπερτέραν ζωὴν ἠγαπηκότες, ὑπερτέρους ἠνέγκατε τοὺς πόνους, Χριστοῦ ὁπλῖται, φωστῆρες θεῖοι· διὰ τοῦτο πιστῶς μακαρίζεσθε."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m3_gr_GR_cog.OTinFloga.name
ocMA.OdeW7C14.text = "Λαμπρυνθέντες τῇ αἴγλῃ τῶν βασάνων, ἀπηστράψατε πλέον τοῦ ἡλίου, καὶ πάντα ζόφον τῆς ἀθεΐας ἐδιώξατε, ἅγιοι Μάρτυρες."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m3_gr_GR_cog.OTinFloga.name
ocMA.OdeW7C1T.text = "Ὡς τὸν πάντων τεκοῦσα συνοχέα, τοῦ συνέχοντος ζόφου τῆς ἀγνοίας, καὶ ἁμαρτίας ἀπάλλαξόν με, Δέσποινα, Θεοτόκε Ἀειπάρθενε."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C21.text = "Ὁ λύχνος τοῦ Ἡλίου, τὴν ψυχήν μου καταύγασον, ἐσβεσμένην ῥαθυμίᾳ καὶ τυφλώττουσαν, μετανοίας πρὸς τρίβους καθοδηγῶν με, Βαπτιστὰ Κυρίου Πρόδρομε."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C22.text = "Τῆς κρίσεως τὴν ὥραν ἐννοῶν, ὅλος δέδοικα, περικείμενος πληθὺν ἀτόπων πράξεων. Βαπτιστὰ τοῦ Σωτῆρος, πρόστηθι ῥῦσαί με, πυρὸς ἀποκειμένου μοι."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C23.text = "Προστάτα τῆς ζωῆς μου, ἀντιλῆπτόρ μου Πρόδρομε, ἐξ ἐχθρῶν μου ἀοράτων ὀρωμένων τε, περιφύλαττε σκέπε, καὶ Βασιλείας οὐρανῶν, μέτοχον ποίησον."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C2T.text = "Παρθένε Θεοτόκε, τὸν Υἱόν σου ἱκέτευε, σὺν Προφήταις, Ἀποστόλοις τε καὶ Μάρτυσι, τῆς μελλούσης ἀνάγκης, ἀπολυτρώσασθαι ἡμᾶς, τοὺς σὲ τιμῶντας ἀεί."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m3_gr_GR_cog.TonYpAngelon.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C11.text = "Ὅτι τὸν φόβον σου οὐκ ἔχω, ἐνοικοῦντα τῇ καρδίᾳ τῇ ἐμῇ, πᾶσαν ἐτέλεσα σαρκός, ἁμαρτίαν ὁ ἀσυνείδητος, καὶ τρέμω σου τὴν δίκην, παμβασιλεῦ, μή με βδελύξῃ, νυνὶ μεταγνόντα."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C12.text = "Γῆς τῆς ἁγίας ἐπιβῆναι, Πολυέλεε ἀξίωσον κἀμέ, ἣν κατοικοῦσιν οἱ πραεῖς, μετανοίᾳ ἀποπλύνας με, γηΐνης ἁμαρτίας, ὁ ἐπὶ γῆς ἀναμαρτήτως, γεννηθεὶς ἐκ Παρθένου."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C13.text = "Ἐν τῷ οἰκείῳ ὑμῶν αἵματι, κατεβάφησαν οἱ πόδες ὑμῶν, ὑποσκελίζοντες ἐχθρούς, καὶ πορείαν τὴν οὐράνιον, ποιούμενοι ὁσίως, πανευκλεεῖς Χριστοῦ, τοῦ πάντων Θεοῦ Ἀθλοφόροι."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C14.text = "Ἐναπεδύσασθε προθύμως, πρὸς τὰ μέγιστα παλαίσματα ὑμεῖς, ἀπογυμνώσαντες ἐχθρόν, καὶ αἰσχύνην ἐπενδύσαντες· διὸ στεφανηφόροι ἐν οὐρανοῖς χορεύετε, εὐκλεεῖς Χριστοῦ Ἀθλοφόροι."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C1T.text = "Τοῦ Ἰακὼβ τὴν καλλονήν σε, ὁ ὡραῖος κάλλει Κύριος ἁγνή, ἠγαπηκὼς τὴν σὴν νηδύν, Παναμώμητε κατῴκησεν, ἀνθρώπων τὴν οὐσίαν ταῖς καλλοναῖς, φαιδρύνων τῶν ὑπὲρ νοῦν δωρημάτων."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m3_gr_GR_cog.AstektoPyri.name
ocMA.OdeW8C21.text = "Ἀμνὸν τοῦ Θεοῦ ὁ κηρύξας, τὸν τὰς ἁμαρτίας τῶν ἀνθρώπων ἀφαιροῦντα, Ἰωάννη θεῖε Προφῆτα, τοῦτον ἐκδυσώπησον, τὸ φορτίον λῦσαι τῶν ἁμαρτημάτων μου, καὶ τῶν σῳζομένων μερίδος ἀξιῶσαι."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m3_gr_GR_cog.AstektoPyri.name
ocMA.OdeW8C22.text = "Καμίνου φλογὸς καιομένης, σκότους ἀφεγγοῦς τε ἐξωτέρου λύτρωσαί με, τὸν ἐν σκότει πονηρῶν ἔργων, ὅλον συνεχόμενον, δυσωπῶ σε Λόγε τοῦ Θεοῦ προάναρχε, διὰ τοῦ ἐνδόξου καὶ θείου Βαπτιστοῦ σου."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m3_gr_GR_cog.AstektoPyri.name
ocMA.OdeW8C23.text = "Ἐρήμοις ψυχαῖς χερσωθείσαις, διὰ μετανοίας εὐκαρπίαν ὁ κηρύξας, τοῦ Κυρίου θεῖε Προφῆτα, ἀκανθηφορήσασαν τὴν ψυχήν μου, πάσαις ἡδοναῖς καθάρισον, ὅπως ἐκβλαστήσω ἀγαθῶν ἔργων στάχυν."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m3_gr_GR_cog.AstektoPyri.name
ocMA.OdeW8C2T.text = "Ὡς Μήτηρ Θεοῦ ἐκδυσώπει, μετὰ τῶν ἁγίων Ἀγγέλων, Προφητῶν τε, Ἀποστόλων καὶ τῶν Μαρτύρων, κινδύνων καὶ θλίψεων, καὶ μελλούσης πάντας κολάσεως ῥύσασθαι, τοὺς σὲ Θεοτόκον ἀεὶ ὁμολογοῦντας."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m3_gr_GR_cog.EvlogitosKyrios.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m3_gr_GR_cog.EvlogitosKyrios.name
ocMA.OdeW9C11.text = "Ἰδοὺ καιρὸς εὐπρόσδεκτος, καὶ ἡμέρα ἱλασμοῦ, ἐπιστρέψαι θέλησον λοιπόν, ὦ ψυχή, ποιοῦσα καρπὸν μετανοίας, μὴ ἄκαρπόν σε εὕρῃ, θανάτου ἀξίνη φοβερά, καὶ ὡς συκῆν τὴν πάλαι ἐκτεμοῦσα, τῷ πυρὶ τῷ ἐκεῖθεν πέμψῃ."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m3_gr_GR_cog.EvlogitosKyrios.name
ocMA.OdeW9C12.text = "Ὡς ὁ ποτὲ πλούσιος, ἐπαγάλλομαι τρυφαῖς, ἀσπλαγχνίαν ἔχων πρὸς τὸν πέλας πολλήν, καὶ πῦρ δειλιῶ τὸ ἀκοίμητον· διό μου τὴν λιθώδη, ἀπόπλυνον Δέσποτα ψυχήν, ὅπως κἂν ἐπὶ τέλους, εὐσπλαγχνίᾳ λαμπρυνθῶ ὁ ἐσκοτισμένος."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m3_gr_GR_cog.EvlogitosKyrios.name
ocMA.OdeW9C13.text = "Σημειωθέντες αἵματι, θεῖοι Μάρτυρες Χριστοῦ, ἐν σημείοις πίστει ἐναθλοῦντες στερρῶς, ἀπίστους ἐχθροὺς ἐτροπώσασθε, καὶ πλάνης ἀνιέρου πολλούς, ἀφηρπάσατε λαούς, μεταβολαῖς ἐνθέοις, λαμπρυθέντας τῷ φωτὶ τῆς θεογνωσίας."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m3_gr_GR_cog.EvlogitosKyrios.name
ocMA.OdeW9C14.text = "Ἠκονημένα ὤφθητε, ξίφη τέμνοντα ἐχθροῦ, παρατάξεις θεῖοι Ἀθλοφόροι Χριστοῦ, καὶ σκεύη τὴν αἴγλην χωρήσαντα, τῆς ἁγίας Τριάδος, καὶ λύχνοι πυρσεύοντες τὸ φῶς, τῆς εὐσεβείας πίστει, καὶ δόξης τῆς νοητῆς, ἀληθεῖς ὁπλῖται."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m3_gr_GR_cog.EvlogitosKyrios.name
ocMA.OdeW9C1T.text = "Φωτοειδῆ νεφέλην σε, ὁ Προφήτης προορᾷ, ἐξ ἧς μέγας Ἥλιος ἐπέφανεν ἡμῖν, Χριστὸς ὁ Θεός, καὶ ἐφώτισε τὰ πρὶν ἐσκοτισμένα· αὐτὸν οὖν δυσώπει ἀγαθή, τὰ τῶν παθῶν μου νέφη ἐκδιῶξαι, καὶ φωτὶ θείῳ φαιδρυνθῆναι."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m3_gr_GR_cog.EnNomoSkia.name
ocMA.OdeW9C21.text = "Ἐκ νόμου σκιᾶς προκύψας, τὴν αἴγλην τῆς θείας χάριτος, πλουσίως ἐθεάσω, τὰ τῆς γῆς πληρώματα σοφὲ φωταγωγοῦσαν, καὶ σκότος ἀποδιώκουσαν, τῆς ἀγνωσίας Προφῆτα· διὰ τοῦτό σε γεραίρομεν."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m3_gr_GR_cog.EnNomoSkia.name
ocMA.OdeW9C22.text = "Ὡς μάρτυς Χριστοῦ, ὡς θεῖος Βαπτιστής, ὡς μετανοίας λαμπτήρ, ὡς ὄρθρος εὐσεβείας, παλαιὰς μεσίτης καὶ καινῆς, παλαιωθεῖσαν κακίᾳ, τὴν ταπεινήν μου ψυχήν, δι' ἐπιγνώσεως θείας, καινουργήσας φωταγώγησον."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m3_gr_GR_cog.EnNomoSkia.name
ocMA.OdeW9C23.text = "Ἐν ὥρᾳ φρικτῇ, ἐν ὥρᾳ φοβερᾷ, ἐν ὥρᾳ δίκης σοφέ, κρινόμενόν με ῥῦσαι, ἀπειλῆς μενούσης με ἐκεῖ, καθυπακούοντα ἔχων, τῶν σῶν δεήσεων, οἷά περ φίλος νυμφίον, τὸν Σωτῆρα τῶν ψυχῶν ἡμῶν."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m3_gr_GR_cog.EnNomoSkia.name
ocMA.OdeW9C2T.text = "Ὡς Μήτηρ Θεοῦ, ὡς Μήτηρ τοῦ ἐκ σοῦ τεχθέντος Λόγου Θεοῦ, μετὰ τῶν Ἀσωμάτων Ἀποστόλων τε καὶ Προφητῶν, Ἱεραρχῶν καὶ Μαρτύρων, τοῦτον δυσώπει ἀεί, τοῦ εἰρηνεῦσαι τὸν κόσμον, Παναγία Μητροπάρθενε."

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha1.melody = ""
ocMA.Aposticha1.text = oc.m3.d2_gr_GR_cog.ocMA.Aposticha1.text

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha2.melody = ""
ocMA.Aposticha2.text = oc.m3.d2_gr_GR_cog.ocMA.Aposticha2.text

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha3.melody = ""
ocMA.Aposticha3.text = "Βασιλέων καὶ τυράννων τὸν φόβον, ἀπώσαντο οἱ Χριστοῦ Στρατιῶται, καὶ εὐθαρσῶς καὶ ἀνδρείως αὐτὸν ὡμολόγησαν, τῶν ἁπάντων Κύριον, Θεὸν καὶ Βασιλέα, καὶ πρεσβεύουσιν ὑπὲρ τῶν ψυχῶν ἡμῶν."

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode3
ocMA.AposTheotokion.melody = ""
ocMA.AposTheotokion.text = "Ἀσπόρως συνέλαβες ἐκ Πνεύματος Ἁγίου, καὶ δοξολογοῦντες ἐκβοῶμεν σοι· Χαῖρε Παναγία Παρθένε."

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode3
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.text = oc.m3.d3_gr_GR_cog.ocMA.Kathisma33.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.text = oc.m3.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.text = "Ταῖς τοῦ βίου ἡδοναῖς, ἐσπιλωμένην, κεκτημένος τὴν ψυχήν, τοῖς οἰκτιρμοῖς σου προσέρχομαι, κατεγνωσμένος ὅλος, καὶ ἡττημένος κράζω σοι Σωτήρ, τῷ εἰδότι μόνῳ τὰ κρύφια· Κάθαρόν με τῇ σῇ, εὐσπλαγχνίᾳ Χριστέ."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.text = "Ὡς μεσίτης παλαιᾶς, καὶ τῆς καινῆς γινόμενος, θεῖε Βαπτιστά, παλαιωθέντα με πταίσμασιν, ἀνακαινίσας δίδου, σαῖς ἱκεσίαις βαίνειν ἀσφαλῶς, πρός, μετανοίας τρίβους Πανεύφημε, εἰσαγούσας, καλῶς, πρὸς Βασιλείαν Χριστοῦ."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.text = "Τὸν ἀγῶνα τὸν καλόν, ἀγωνισάμενοι, γενναῖοι Ἀθληταί, μυρίους πόνους ἠνέγκατε, καὶ διὰ τοῦτο πάντα πόνον κουφίζετε διηνεκῶς, καὶ πνευμάτων λώβην διώκετε· ὅθεν πίστει ὑμᾶς Ἅγιοι δοξάζομεν."

ocLI.BeatitudesGlory.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesGlory.melody = ""
ocLI.BeatitudesGlory.text = "Φῶς καὶ ζωὴ καὶ παντουργός, ἡ τρισυπόστατος, Μονὰς ὡς ἀληθῶς ὑπάρχει, ἥν περ δοξάζομεν. Πατὴρ γάρ, Υἱὸς καὶ Πνεῦμα, ὁ ἑνιαῖος καὶ συνεκτικός, τῶν ἁπάντων Δεσπότης καὶ Κύριος, εἷς Θεὸς ἐν τρισί, Προσώποις γνωρίζεται."

ocLI.BeatitudesBoth.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesBoth.melody = ""
ocLI.BeatitudesBoth.text = "Ἁμαρτάνοντα ἀεί, καὶ παροργίζοντα Θεὸν τὸν ἀγαθόν, Παρθενομῆτορ οἰκτείρησον, καὶ μετανοίας τρόποις, ὡς ἀγαθὴ βελτίωσον Ἁγνή, ὅπως τὰς μελλούσας κολάσεις φύγω, ὑμνῶν ἐκτενῶς, Κόρη τὴν πρεσβείαν σου."

//Octoechos in Triodion

