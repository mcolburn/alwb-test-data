ocMA.Kathisma14.text = oc.m3.d1_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.text = oc.m3.d6_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma22.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
ocMA.Kathisma22.text = "Ὅτε τῷ λόγῳ σου, παραστησόμεθα, τῷ ἀδεκάστῳ σου, βήματι Κύριε, μὴ καταισχύνῃς τοὺς εἰς σέ, Σωτὴρ ἡμῶν πεπιστευκότας· πάντες γὰρ ἡμάρτομεν, ἀλλὰ σοῦ οὐκ ἀπέστημεν· ὅθεν δυσωποῦμέν σε, ἐν σκηναῖς τῶν δικαίων σου, Χριστέ, οὓς προσελάβου ἀνάπαυσον, ὡς μόνος ὑπάρχων πολυέλεος."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma23.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
ocMA.Kathisma23.text = "Ξύλῳ πεπτώκαμεν, πικρῶς εἰς θάνατον, ξύλῳ τιμίῳ δέ, πάλιν ἀνέστημεν· διὰ Σταυροῦ σου γάρ Σωτήρ, τὸν θάνατον ἐνέκρωσας, καὶ τὴν ἀτελεύτητον, ζωὴν πᾶσιν ἐπήγασας, ἧς περ καταξίωσον, τοὺς πρὸς σὲ ἐκδημήσαντας, οἰκέτας σου πιστοὺς Ἐλεῆμον, δωρούμενος αὐτοῖς τὴν Βασιλείαν σου."

ocMA.Kathisma24.mode = misc_gr_GR_cog.Mode3
ocMA.Kathisma24.melody = he.a.m3_gr_GR_cog.TinOraiotita.name
ocMA.Kathisma24.text = "Τὸν πολυτάραχον, τοῦ βίου κλύδωνα, τοὺς ἐκπεράσαντας, τῷ θείῳ νεύματι, καὶ μεταστάντας εὐσεβῶς, οἰκέτας σου Πανάμωμε, δέξαι καὶ προσάγαγε, τῷ Κριτῇ καὶ Θεῷ ἡμῶν, τούτοις ἀπολύτρωσιν, παρασχεῖν ἱκετεύουσα· καὶ γὰρ ὡς τοῦ ποιήσαντος Μήτηρ, ἔχεις τὸ θέλειν καὶ τὸ δύνασθαι."

ocMA.C1Acrostic.text = " φέρων ἀκροστιχίδα, ἄνευ τῶν Θεοτοκίων· Μύστ(αις π)ροσοίσω τοῦ Θεοῦ θεῖον μέλος. Ἰωσήφ."

ocMA.C2Poet = poets_gr_GR_cog.Theofanous
ocMA.C2Acrostic.text = " οὗ ἡ ἀκροστιχίς· Τοῦτο τρίτον μέλισμα θνήσκουσιν νέμω."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m3_gr_GR_cog.ThavmastosEndoxos.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C11.text = "Μαρτυρίου κλέος σοφοὶ εὕρατε, πάντες Ἀθληταί, βασάνους πολυτρόπους, ἐνεγκόντες γενναίως ὑπὲρ τῆς ἀληθείας· ὅθεν πίστει ὑμᾶς δοξάζομεν ἀεί."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C12.text = "Ὑπερτέραν σχόντες ζωήν, ὤφθητε προφῆται καὶ σοφοί, Κυρίου ἱεράρχαι, τοῦ κενωθέντος Λόγου, ποιμάναντες ὁσίως, τοῦ Σωτῆρος τὴν ποίμνην σθένει θεϊκῷ."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C13.text = "Σταυρωθέντες κόσμῳ σαρκός, ὅσιοι πάσας ἡδονὰς ἠρνήσασθε, καὶ σκεύη, τοῦ Πνεύματος δειχθέντες, τὰ πνεύματα πλάνης, θείῳ σθένει ἐξηφανίσατε σαφῶς."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C14.text = "Τοὺς ἐκ γῆς προστάξει τῇ σῇ Κύριε, πίστει εὐσεβεῖ, οἰκτίρμον μεταστάντας, ζωῆς ἀτελευτήτου, καὶ φωτὸς ἀνεσπέρου, ἀξιώσας ἀνάπαυσον ὡς ἀγαθός."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m3_gr_GR_cog.ThavmastosEndoxos.name
ocMA.OdeW1C1T.text = "Ὡς Θεὸν τεκοῦσαν σαρκὶ Πάναγνε, γνώμῃ εὐσεβεῖ, τιμῶμέν σε ἀπαύστως, μετὰ σεμνῶν Γυναίων, ἀσκήσει λαμπρυνθέντων, καὶ ἀθλήσει καταβαλόντων τὸν ἐχθρόν."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m3_gr_GR_cog.TemnomeninThalassan.name
ocMA.OdeW1C21.text = "Τῇ ταφῇ σου θάνατον θανατώσας, καὶ καθελὼν τοῦ ᾅδου τὴν τυραννίδα, εἰς οὐρανοὺς πρόδρομος ἡμῶν, ἀναπεφοίτηκας, Ἀθλοφόρων δῆμον συνανυψώσας, νῦν τὰς ψυχάς ἀνάπαυσον, τῶν μεταστάντων Χριστέ."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m3_gr_GR_cog.TemnomeninThalassan.name
ocMA.OdeW1C22.text = "Ὁ τοὺς θείους Μάρτυρας ἐνισχύσας, καὶ δι' αὐτῶν τὴν πλάνην ἐξαφανίσας, ταῖς προσευχαῖς τούτων, ὡς Θεός, τοὺς τελευτήσαντας, τῆς παρὰ σοῦ Σῶτερ ἀθανασίας, καὶ μακαρίας λήξεως, τυχεῖν ἀξίωσον."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m3_gr_GR_cog.TemnomeninThalassan.name
ocMA.OdeW1C23.text = "Υπὲρ δούλων Δέσποτα, τὸ σὸν αἷμα, σφαγιασθείς, ἐξέχεας ὡς οἰκτίρμον, τὴν ὀφειλὴν τὴν ὑπὲρ αὐτῶν, καταβαλόμενος· διὰ τοῦτο εὔσπλαγχνε, σὲ αἰτοῦμεν, νῦν τάς ψυχὰς ἀνάπαυσον, τῶν κοιμηθέντων Χριστέ."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m3_gr_GR_cog.TemnomeninThalassan.name
ocMA.OdeW1C2T.text = "Τὴν ἁγνὴν ὑμνήσωμεν Θεοτόκον, τὴν τὸν Θεὸν τεκοῦσαν, τὸν τῷ θανάτῳ τῷ ἑαυτοῦ, θάνατον ἡμῖν ἀναμοχλεύσαντα, καὶ ζωὴν πηγάσαντα τὴν ἀγήρω, καὶ δι' αἰῶνος μένουσαν μακαριότητα."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m3_gr_GR_cog.SteiraPsychi.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C11.text = "Ῥώμῃ Θεοῦ σθενούμενοι, Μάρτυρες ἔνδοξοι, ὀλεθροτόκον δύναμιν, τοῦ ἐν κακίᾳ δυνατοῦ δυσμενοῦς, τελείως ὠλέσατε, καὶ νίκης εἰλήφατε θεῖον στέφανον."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C12.text = "Ὁ τοὺς ὁσίους Κύριε, δείξας ἐχθροῦ νικητάς, ταῖς αὐτῶν Ἰησοῦ παρακλήσεσιν, ἁγίασον, φώτισον, καὶ σῶσον, τοὺς ἐν πίστει καὶ πόθῳ, ἀπαύστως ἀνυμνοῦντάς σε."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C13.text = "Σὺν τοῖς Προφήταις ἅπασιν, ἀνευφημήσωμεν, τοὺς τῶν Γυναίων θείους χορούς, τοὺς ἐν ἀσκήσει, καὶ ἐν ἀθλήσει διαλάμψαντας, καὶ τὸν πολυμήχανον ὄφιν πατήσαντας."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C14.text = "Οὓς ἐξ ἡμῶν μετέστησας, θείᾳ βουλήσει Χριστέ, μετὰ Ἁγίων σύνταξον, τὰ ἐν τῷ βίῳ παραβλέψας αὐτῶν, οἰκτίρμον ἐγκλήματα, πρεσβείαις δεόμεθα τῶν Ἁγίων σου."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m3_gr_GR_cog.SteiraPsychi.name
ocMA.OdeW3C1T.text = "Οἱ σεσωσμένοι Πάναγνε, σοῦ τῷ σεπτῷ τοκετῷ, τοῦ Γαβριὴλ βοῶμέν σοι πιστῶς, τὸ Χαῖρε καὶ δεόμεθα, τῇ παρακλήσει σου, πταισμάτων ἄφεσιν πᾶσιν αἴτησαι."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C21.text = "Ὁ διαπλάσας ἐκ γῆς με, καὶ πρὸς τὴν γῆν, στραφέντα τὸν δείλαιον, ἀναπλάσας λαμπρότερον, τὰς ψυχὰς ἀνάπαυσον, τῶν κοιμηθέντων, πρεσβείαις τῶν Μαρτύρων."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C22.text = "Τῶν Ἀθλοφόρων Μαρτύρων τοὺς αἰκισμούς, στρεβλώσεις καὶ μώλωπας, καὶ πληγὰς δυσωπούμενος, τὰς ψυχὰς ἀνάπαυσον τῶν μεταστάντων, πρὸς σὲ τὸν πάντων κτίστην."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C23.text = "Ῥῦσαι γεέννης πυρός τε, καὶ ζοφερῶν, τοῦ ᾅδου φιλάνθρωπε, σκηνωμάτων τοὺς δούλους σου, τοὺς ἐν πίστει Δέσποτα, κεκοιμημένους, τῇ σῇ ἐλπίδι πάντας."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m3_gr_GR_cog.OEkMiOnton.name
ocMA.OdeW3C2T.text = "Ἱερωτέρα ὡς μήτηρ τοῦ πλαστουργοῦ, τῆς κτίσεως πέφηνας, τοῦ τὸν θάνατον λύσαντος, Θεοτόκε Πάνσεμνε, καὶ ἀφθαρσίαν ἡμῖν δεδωρημένου."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m3_gr_GR_cog.ToKataskionOros.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C11.text = "Ἱερεῖα καθάπερ, προσανηνέχθητε πίστει, σφαγέντες, ὥσπερ ἄρνες σοφοί, ἀμνῷ τῷ ἐπὶ ξύλου Σταυροῦ, ἐπευδοκήσαντι ἀθληταί, ἐθελουσίως ταθῆναι, ὑπὲρ γένους ἀνθρώπων."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C12.text = "Σὺν ὁσίοις τιμῶμεν, τοὺς εὐκλεεῖς Ἱεράρχας, φωστῆρας γεγονότας πιστῶν, παθῶν μὲν σκότος λύοντας, τῆς δὲ ἀχλύος ἐκκαθάραντας, καὶ πρὸς ἄδυτον ἐν πίστει, προσχωρήσαντας φέγγος."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C13.text = "Ὡραιότητι λόγων, οἱ θεηγόροι Προφῆται, φαιδρύνουσι πιστῶν τὰς ψυχάς, ἀγώνων ταῖς λαμπρότησι, καὶ τῶν θαυμάτων ἀνατολαῖς, καταυγάζουσι καρδίας, θεοφόροι Γυναῖκες."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C14.text = "Τοὺς τὸν βίον λιπόντας, ὑπερκοσμίου δόξης, μετόχους δεῖξον Λόγε Θεοῦ, πταισμάτων ἀπολύτρωσιν, τούτοις δωρούμενος ἀγαθέ, ὧν περ ἔπραξαν ἐν γνώσει, ἐπὶ γῆς καὶ ἀγνοίᾳ."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m3_gr_GR_cog.ToKataskionOros.name
ocMA.OdeW4C1T.text = "Τῶν Μαρτύρων τὴν δόξαν, Ἱεραρχῶν καὶ Ὁσίων, τὸ θεῖον ἐγκαλλώπισμα, τῶν Προφητῶν τὸ κήρυγμα, καὶ τῶν Γυναίων σεμνολόγημα, ἀνυμνήσωμεν ἀξίως, τοῦ Θεοῦ τήν Μητέρα."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C21.text = "Τάφον δι' ἡμᾶς, συμπαθῶς κατῴκησας Δέσποτα, τάφους ὁ κενώσας ὡς Θεός, καὶ νικηφόρους δείξας τοὺς Μάρτυρας, ἐν τόποις ἀνέσεως, τοὺς κοιμηθέντας δούλους σου κατάταξον."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C22.text = "Ὁ τῶν Ἀθλητῶν, τοὺς στερροὺς ἀγῶνας καὶ μάστιγας, καὶ τὰς τῶν μελῶν ἀποκοπάς, ἃς ὑπὲρ σοῦ ὑπέμειναν Δέσποτα, ἀσμένως δεξάμενος, οὓς προσελάβου ῥῦσαι κολάσεως."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C23.text = "Νεύματι τῷ σῷ, τῆς ζωῆς ὁ ἔχων τὰ τάλαντα, τῆς ἀτελευτήτου σου ζωῆς, καὶ ἀκηράτου δόξης ἀξίωσον, οὓς πρὸς σὲ μετέστησας, τῆς ἁμαρτίας λύσας τὸ μεσότοιχον."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m3_gr_GR_cog.EthouProsImas.name
ocMA.OdeW4C2T.text = "Μήτηρ ἀληθῶς, καὶ παρθένος ὤφθης Πανάμωμε, γέννησιν καὶ τόκον ἀληθῆ, τῇ παρθενίᾳ μόνῃ συνάψασα· Θεὸν γὰρ ἐγέννησας, τὸν τοῦ θανάτου λύσαντα τὴν δύναμιν."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.name
ocMA.OdeW5C11.text = "Οἱ μώλωπες ὑμῶν καὶ αἱ πληγαί, ἐχθροῦ μὲν ὤφθησαν πληγαὶ ἀνίατοι, τὰς ἡμῶν πληγὰς δὲ νῦν θεραπεύουσι, πάντων πιστῶν, Ἀθληταὶ Κυρίου."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.name
ocMA.OdeW5C12.text = "Ὑμνήσωμεν Ὁσίων τὴν πληθύν, καὶ μακαρίσωμεν τοὺς Ἱεράρχας Χριστοῦ, καὶ τιμήσωμεν τοὺς Προφήτας νῦν, ὑπὲρ ἡμῶν αὐτὸν δυσωποῦντας."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.name
ocMA.OdeW5C13.text = "Θεὸν τὸν σαρκωθέντα δι' ἡμᾶς, ποθοῦσαι πανευκλεεῖς Γυναῖκες, γνώμῃ εὐθείᾳ ἐναθλήσασαι, καὶ ἀσκήσασαι, τῆς Βασιλείας καταξιοῦνται."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.name
ocMA.OdeW5C14.text = "Ἐν Παραδείσου Κύριε σκηναῖς, ἐν χώρᾳ ζώντων Χριστέ, ἔνθα σου λάμπει τὸ φῶς, κατασκήνωσον, οὓς μετέστησας, ἀπὸ τῆς γῆς, πιστούς σου οἰκέτας."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m3_gr_GR_cog.ToFosSouToAnesperon.name
ocMA.OdeW5C1T.text = "Ἐκ σοῦ Θεὸς σεσάρκωται Ἁγνή, καὶ ἀνωτέραν σε νῦν Ἀγγέλων ἀνέδειξε, ποιημάτων τε ὑπερέχουσαν, πάντων· διό σε ὑμνολογοῦμεν."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m3_gr_GR_cog.ProsSeOrthrizo.name
ocMA.OdeW5C21.text = "Ἐν Ἐκκλησίᾳ, τῶν πρωτοτόκων ἀγαθέ, τοὺς ἐκδημήσαντας ἐξ ἡμῶν, πρεσβείαις τῶν Ἀθλητῶν, καὶ σεπτῶν Μαρτύρων, δικαίοις συναρίθμησον."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m3_gr_GR_cog.ProsSeOrthrizo.name
ocMA.OdeW5C22.text = "Λύτρον πταισμάτων, τὸ τῶν Μαρτύρων σου Χριστέ, παναγιώτατον αἷμα, νῦν προσδέχου, τῶν εὐσεβῶς κοιμηθέντων Σῶτερ, καὶ τούτους διανάπαυσον."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m3_gr_GR_cog.ProsSeOrthrizo.name
ocMA.OdeW5C23.text = "Ἴθυνον Λόγε, τῶν ἀπελθόντων τὰς ψυχάς, εἰς τὸν αἰώνιον τόπον τῆς τρυφῆς, καὶ τῆς ὑπὲρ νοῦν τῶν ἁγίων θείας λαμπρότητος ἀξίωσον."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m3_gr_GR_cog.ProsSeOrthrizo.name
ocMA.OdeW5C2T.text = "Σὲ τὴν Παρθένον, τὴν ὑπὲρ λόγον ἐν γαστρί, τὸν ἀπερίληπτον Λόγον συλλαβοῦσαν, τὸν τοῖς νεκροῖς τὴν ζωὴν διδόντα, ἀξίως μακαρίζομεν."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m3_gr_GR_cog.VythosMoiTonPathon.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C11.text = "Οἱ λύχνοι τῆς φρικτῆς φρυκτωρίας, οἱ ἄρνες τοῦ Ἀμνοῦ καὶ Ποιμένος, οἱ στρατιῶται Χριστοῦ, οἱ πανευκλεεῖς αὐτοῦ Ἀθληταί, τιμάσθωσαν, ἱεραῖς μελῳδίαις."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C12.text = "Ὑψώθη τῶν Ὁσίων ὁ δῆμος, ὑψώσας Θεὸν ἐν ταπεινώσει, καὶ ἐδοξάσθη πληθὺς Ἱεραρχῶν, ἐν ἔργοις ἀεί, δοξάσαντες, τὴν ἁγίαν Τριάδα."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C13.text = "Θαυμάτων ἐπιδείξεσι θείαις, καὶ πόνων καρτερίᾳ τελείᾳ, τοὺς ἀνομοῦντας ἐχθρούς, ὁ ἱερὸς Γυναίων χορός, κατέπληξεν, ἱερῶς ἀριστεύσας."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C14.text = "Ἐν πίστει οὓς ἐκ γῆς προσελάβου, ἁγίων συναρίθμησον δήμοις, καὶ τοῦ πιστοῦ Ἀβραάμ, τάξον Χριστέ, ἐν κόλποις Σωτήρ, δοξάζειν σου, τὴν πολλὴν εὐσπλαγχνίαν."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m3_gr_GR_cog.VythosMoiTonPathon.name
ocMA.OdeW6C1T.text = "Μαρτύρων Προφητῶν τε ἁγίων, τῶν πάντων ἀπ' αἰῶνος Δικαίων, καύχημα σὺ εἶ Ἁγνή· ὅθεν φωναῖς, σαφῶς ἱεραῖς, τιμῶμέν σε, σὺν αὐτοῖς εἰς αἰῶνας."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m3_gr_GR_cog.AnagageEkFthoras.name
ocMA.OdeW6C21.text = "Μετάβαλε εἰς χαρὰν τῶν πίστει κεκοιμημένων, τὸν κοπετόν, πρεσβείαις τῶν Ἀθλοφόρων, περιζωννὺς τούτους εὐσεβῶς, τὴν εὐφροσύνην Χριστέ, καὶ πρὸς φῶς τὸ σόν, εὔθυνον καὶ ἐφοδήγησον."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m3_gr_GR_cog.AnagageEkFthoras.name
ocMA.OdeW6C22.text = "Ἀνάπαυσον ὁ Θεὸς τῷ πλήθει τῶν ἐν οἰκτιρμῶν σου, Πατριαρχῶν, κόλποις τοὺς μεταστάντας, ἔνθα τὸ φῶς λάμπει τὸ φαιδρόν, τοῦ σοῦ προσώπου Χριστέ, παρορῶν αὐτῶν, ἅπαντα τὰ παραπτώματα."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m3_gr_GR_cog.AnagageEkFthoras.name
ocMA.OdeW6C23.text = "Θαυμάστωσον ἐπὶ τοὺς ἐντεῦθεν ἀπηλλαγμένους, καὶ τῶν δεινῶν, τοῦ βίου λελυτρωμένους, τὸ θαυμαστὸν ἔλεος Χριστέ, τῆς φιλανθρωπίας σου, καὶ χαρᾶς τῆς σῆς, ἔμπλησον καὶ ἱλαρότητος."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m3_gr_GR_cog.AnagageEkFthoras.name
ocMA.OdeW6C2T.text = "Νεκρώσεως καὶ φθορᾶς ἐρρύσω με, καὶ θανάτου, τῷ παραδόξῳ τόκῳ σου Θεομῆτορ· σὺ γὰρ Ἁγνὴ τέτοκας ἡμῖν, ἀφθαρσίας πηγήν, καὶ τῷ σῷ φωτί, κόσμον ὅλον κατελάμπρυνας."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C11.text = "Ἱστάμενοι ἐν μέσῳ, τοῦ πυρὸς ἐδέξασθε, θείαν δρόσον οὐρανόθεν, Ἀθλοφόροι Χριστοῦ, καὶ ὁμιλοῦντες βασάνοις, τὸν πολυμήχανον ἐχθρόν, νεκρὸν εἰργάσασθε."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C12.text = "Οἱ θεῖοι Ἱεράρχαι, ὡς ὁλκάδα ἰθύναντες, τὴν Ἐκκλησίαν τοῦ Θεοῦ, ἀκαταπόντιστον, ταύτην ἐτήρησαν πλάνης, διεκφυγοῦσαν ἀβλαβῶς, τὰ κύματα."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C13.text = "Νεκρώσαντες τὴν σάρκα, ἱεροῖς ἀγωνίσμασιν, ἀσκηταὶ τῆς ἀληθείας ἐκληρώσασθε, τὴν ζωὴν τὴν ἀγήρω, μετὰ ἁγίων Προφητῶν, καὶ ἱερῶν Γυναικῶν."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C14.text = "Μὴ δείξῃς ὑπευθύνους, ἐν τῇ κρίσει τοὺς δούλους σου, οὓς μετέστησας πιστούς, ἀλλὰ τάξον αὐτούς, ἐν ταῖς λαμπρότησι Λόγε, ταῖς τῶν Ἁγίων, καὶ ἡμᾶς, τῇ εὐσπλαγχνίᾳ σου."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m3_gr_GR_cog.TreisPaidesEnKamino.name
ocMA.OdeW7C1T.text = "Μαρτύρων καὶ Ὁσίων, Προφητῶν τε Πανάμωμε, Γυναικῶν καὶ πάντων τῶν Ἱεραρχῶν, τῶν ἱερῶς βιωσάντων, σὺ εἶ τὸ κλέος ἀληθῶς, μεθ' ὧν τιμῶμέν σε."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m3_gr_GR_cog.OsPalaiTousEfseveis.name
ocMA.OdeW7C21.text = "Ἡ πάντων ζωαρχική, αἰτία καὶ δύναμις, ποιητικὴ Λόγε τοῦ Θεοῦ, τῆς ἐπηγγελμένης τοῖς Μάρτυσι τρυφῆς, τὰς ψυχὰς ἀξίωσον, τῶν μετασταντων δούλων σου, ὁ Θεός τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m3_gr_GR_cog.OsPalaiTousEfseveis.name
ocMA.OdeW7C22.text = "Σωμάτων μὲν ὡς φθαρτῶν, εὐτόνως ηὐλόγησαν, οἱ εὐκλεεῖς Μάρτυρες Χριστοῦ, νῦν δὲ παρρησίᾳ πρεσβεύουσι πρὸς σέ. Τὰς ψυχὰς ἀνάπαυσον, τῶν κοιμηθέντων δούλων σου, ὁ Θεὸς τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m3_gr_GR_cog.OsPalaiTousEfseveis.name
ocMA.OdeW7C23.text = "Καλάμῳ τῷ τοῦ Σταυροῦ, ὑπέγραψας ἄφεσιν, ἁμαρτιῶν πᾶσι τοῖς πιστοῖς, ἧς νῦν τὰς ψυχάς, ὧν μετέστησας πρὸς σέ, μετασχεῖν ἀξίωσον, ἐν εὐφροσύνῃ ψάλλειν σοι· Ὁ Θεὸς τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m3_gr_GR_cog.OsPalaiTousEfseveis.name
ocMA.OdeW7C2T.text = "Ὁ Λόγος ὁ τοῦ Πατρός, ὁ πάντα βουλήματι, δημιουργῶν, φύσιν τῶν βροτῶν, τὴν συγκεχωσμένην τοῖς πάθεσιν εὑρών, ὡς Θεὸς ἀνέπλασεν. Εὐλογημένος Πάναγνε, ὁ καρπός τῆς κοιλίας σου."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m3_gr_GR_cog.TonYpAngelon.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C11.text = "Ἐπιφοραῖς τῶν ἀλγηδόνων, καὶ τῶν πόνων ἐπιδόσεσι, περιστατούμενοι σφοδρῶς, οὐκ ἠρνήσασθε τὴν ὄντως ζωήν, Κυρίου Ἀθλοφόροι, οὐδὲ γλυπτοῖς ἐνείματε, πονηρότατον σέβας."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C12.text = "Λύχνοι καθάπερ ἐν λυχνίᾳ, ἐπικείμενοι σεπτῶν ἀρετῶν, πᾶσαν αὐγάζετε τὴν γῆν, ἄπαν σκότος ἐκδιώκοντες τῆς πλάνης, Ἀθλοφόροι ἱερουργοί, τῶν οὐρανίων Νόων συμπολῖται."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C13.text = "Οἱ ἀξιέπαινοι Προφῆται, τῶν Ὁσίων ὁ κατάλογος, τῶν Γυναικῶν ὁ ἱερός, Ἀθλησάντων θεῖος ὅμιλος, ὑμνείσθωσαν ἀξίως, ὑπὲρ ἡμῶν πρεσβεύοντες, τῷ Χριστῷ καὶ Σωτῆρι."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C14.text = "Σὺ ἡ ζωὴ τῶν ζώντων Κύριε, οὓς ἐκ γῆς πιστῶς μετέστησας, μετὰ ἁγίων ἐν φωτί, τοῦ προσώπου σου ἀνάπαυσον, συγχώρησιν πταισμάτων, διὰ πολλὴν παρέχων τούτοις, Κύριε εὐσπλαγχνίαν."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m3_gr_GR_cog.TonYpAngelon.name
ocMA.OdeW8C1T.text = "Ἡ πανύμνητος Παρθένος, Προφητῶν τὸ περιήχημα, Ἱεραρχῶν καὶ Ἀθλητῶν, καὶ Ὁσίων ἐγκαλλώπισμα, Γυναίων τε ἁγίων ἡ χαρμονή, ὑμνείσθω χρεωστικῶς εἰς αἰῶνας."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m3_gr_GR_cog.LatreveinZontiTheo.name
ocMA.OdeW8C21.text = "Ὑπάρχων ὁ θησαυρός, τῆς ἀθανασίας σὺ ζωαρχικώτατε, τοῖς θνητοῖς ἀφθαρσίαν κατήγγειλας, ἣν τοῖς σοῖς Μάρτυσι Χριστέ, τοῖς εὐσεβῶς πίστει σοι βοῶσι δεδώρησαι· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m3_gr_GR_cog.LatreveinZontiTheo.name
ocMA.OdeW8C22.text = "Σοφίᾳ πνευματικῇ, καὶ ὑπομονῇ τῶν ἔργων, οἱ στερρότατοι Ἀθληταί, τῶν βασάνων καταπτύσαντες, ταῖς ψυχαῖς τῶν μεταστάντων παρὰ Χριστοῦ, ἄνεσιν αἰτοῦνται, κραυγάζοντες· Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m3_gr_GR_cog.LatreveinZontiTheo.name
ocMA.OdeW8C23.text = "Ἱλάσθητι Ἀγαθέ, λόγχῃ τῆς πλευρᾶς σου, ῥήξας τὸ χειρόγραφον, τῶν πρὸς σὲ μεταστάντων, καὶ μεσότοιχον καθελὼν τὸ τῶν πταισμάτων, ὑμνολογεῖν σε οὓς προσελάβου εὐδόκησον. Εὐλογεῖτε πάντα τὰ ἔργα Κυρίου τὸν Κύριον."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m3_gr_GR_cog.LatreveinZontiTheo.name
ocMA.OdeW8C2T.text = "Νεφέλην σε νοητήν, τοῖς προκεκοιμημένοις δίψει τῆς νεκρώσεως, ὕδωρ ζῶν τῆς ἀφέσεως πηγάζουσαν, καὶ θνητοῖς ἀθανασίαν διηνεκῆ, πᾶσι τοῖς βοῶσι παρέχουσαν· Εὐλογοῦμεν εὐλογημένη Παρθένε τὸν Τόκον σου."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m3_gr_GR_cog.EnSinaioToOrei.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.name
ocMA.OdeW9C11.text = "Ἱερεῖα καθάπερ προσήχθητε, τῷ τυθέντι δι' ἡμᾶς, καὶ τοὺς θείους ναοὺς ἐπληρώσατε, τῶν Ἀγγέλων χαρμονῆς, Ἀθλοφόροι Κυρίου· διὸ ταῖς εὐχαῖς ὑμῶν πάντες στηρίξατε, καὶ τῆς τοῦ ἐχθροῦ βλαβερᾶς, πλάνης ῥύσασθε."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.name
ocMA.OdeW9C12.text = "Ὡς φωστῆρες ζωῆς λόγον ἔχοντες, ἐφωτίσατε ψυχάς, Ἱεράρχαι Χριστοῦ ἱερώτατοι, θείας δόξης κοινωνοί, ὡς τὸ πῦρ δεδεγμένοι, τοῦ Πνεύματος Ὅσιοι, πάθη ἐφλέξατε, καὶ τὴν τῶν δαιμόνων, πυρὰν ἠφανίσατε."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.name
ocMA.OdeW9C13.text = "Σὺν Προφήταις ἁγίοις τιμήσωμεν, τῶν Ὁσίων τὴν πληθύν, τοὺς πρὸ νόμου ἐκλάμψαντας νόμῳ τε, πολιτείᾳ καθαρᾷ. Γυναικῶν τε ἁγίων, τοὺς χοροὺς αἰνέσωμεν καὶ ἐκβοήσωμεν, Κύριε· πρεσβείαις αὐτῶν, σῶσον πάντας ἡμᾶς."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.name
ocMA.OdeW9C14.text = "Ἡ ταφή σου Χριστὲ καὶ Ἀνάστασις, πάντων γέγονε ζωή· διὰ τοῦτο τολμῶντες βοῶμέν σοι· Μετὰ πάντων ἐκλεκτῶν, οὓς πιστῶς προσελάβου, δούλους σου ἀνάπαυσον, τούτων τὰ πταίσματα, πάντα συγχωρῶν, ὡς Θεὸς καὶ φιλάνθρωπος."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m3_gr_GR_cog.EnSinaioToOrei.name
ocMA.OdeW9C1T.text = "Φοβερὸς οὗτος τόπος ἐβόησεν, ὁπηνίκα Ἰακώβ, σοῦ τὸν τύπον ἐν κλίμακι ἔβλεψε, Θεοτόκε ἀψευδῶς, τῶν Μαρτύρων ἡ δόξα, Ὁσίων τὸ καύχημα, Ἀγγέλων ἀγλάϊσμα, πάντων Προφητῶν, καὶ πιστῶν τὸ διάσωσμα."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m3_gr_GR_cog.TinEnVatoKaiPyri.name
ocMA.OdeW9C21.text = "Νῦν ὡς μόνος ἀγαθός, καὶ φιλάνθρωπος Θεός, τοὺς μεταστάντας πρὸς σέ, τῶν Μαρτύρων ταῖς λιταῖς, ἐν χώρᾳ τῶν πρᾳέων γῆς κατασκήνωσον, τὴν λύσιν τῶν πταισμάτων παρέχων, Ἐλεῆμον αὐτοῖς, διὰ μέγα καὶ πλούσιον ἔλεος."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m3_gr_GR_cog.TinEnVatoKaiPyri.name
ocMA.OdeW9C22.text = "Ἐν Ἁγίων σου σκηναῖς, ἐν τοῖς κόλποις Ἀβραάμ, οὓς προσελάβου Σωτήρ, συναρίθμησον τοῖς σοῖς δικαίοις, ἔνθα λάμπει τὸ φῶς, τοῦ προσώπου σου, τὸ ἄχραντον καὶ θεῖον, καὶ μένει δι' αἰῶνος, ἡ ὄντως ἀΐδιος ἀγαλλίασις."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m3_gr_GR_cog.TinEnVatoKaiPyri.name
ocMA.OdeW9C23.text = "Μακαρίας σου ζωῆς, αἰωνίων ἀγαθῶν, ἀκαταπαύστου τρυφῆς, εὐφροσύνης ἀληθοῦς, ἀξίωσον τοὺς δούλους σου, οὓς μετέστησας βουλήσει ζωοδότα, ἐν τόπῳ τῷ τῆς χλόης, ἐπὶ τῶν ὑδάτων τῆς ἀναπαύσεως."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode3
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m3_gr_GR_cog.TinEnVatoKaiPyri.name
ocMA.OdeW9C2T.text = "Ὡς ἁγία Κιβωτός, μαρτυρίου τε Σκηνή, τὸν ζωοδότην Θεόν, ὑπεδέξω ἐν γαστρί, Παναμώμητε τὸν Κτίστην σου, τὴν ἀπόφασιν κατάρας τῆς ἀρχαίας, καὶ νόμον τοῦ θανάτου, ἰδίῳ θανάτῳ ἀναμοχλεύσαντα"

ocMA.LaudsRubric1.text = oc.m1.d7_gr_GR_cog.ocMA.LaudsRubric1.text

ocMA.Lauds1.mode = misc_gr_GR_cog.Mode3
ocMA.Lauds1.melody = ""
ocMA.Lauds1.text = oc.m3.d3_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds2.mode = misc_gr_GR_cog.Mode3
ocMA.Lauds2.melody = ""
ocMA.Lauds2.text = oc.m3.d4_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds3.mode = misc_gr_GR_cog.Mode3
ocMA.Lauds3.melody = ""
ocMA.Lauds3.text = oc.m3.d5_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds4.mode = misc_gr_GR_cog.Mode3
ocMA.Lauds4.melody = ""
ocMA.Lauds4.text = "Ἄνθρωποι, τί μάτην ταραττόμεθα; ὁ δρόμος βραχύς ἐστιν, ὃν τρέχομεν, καπνὸς ὑπάρχει ὁ βίος, ἀτμὶς καὶ τέφρα καὶ κόνις, πρὸς ὀλίγον φαινόμενος, καὶ ταχέως ἀχρειούμενος· διὸ Χριστῷ τῷ ἀθανάτῳ Βασιλεῖ βοήσωμεν· Τοὺς μεταστάντας ἐξ ἡμῶν ἀνάπαυσον, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία ἐν σοί."

ocMA.LaudsTheotokion.mode = misc_gr_GR_cog.Mode3
ocMA.LaudsTheotokion.melody = ""
ocMA.LaudsTheotokion.text = oc.m3.d2_gr_GR_cog.ocVE.AposTheotokion.text

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha1.melody = he.a.m3_gr_GR_cog.ToTypoTouStavrou.name
ocMA.Aposticha1.text = "Δοξάζω τὸν σταυρόν σου τὸν τίμιον, δι' οὗ ζωὴ δεδώρηται, καὶ τρυφῆς ἡ ἀπόλαυσις, τοῖς ἐν πίστει καὶ πόθῳ, σὲ ὑμνοῦσι μόνε πολυέλεε· διὸ βοῶμέν σοι· Χριστὲ ὁ Θεός, τοὺς μεταστάντας ἐξ ἡμῶν ἀνάπαυσον, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία ἐν σοί, δοξάζειν σου τὴν Θεότητα."

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha2.melody = he.a.m3_gr_GR_cog.ToTypoTouStavrou.name
ocMA.Aposticha2.text = "Ὁ μόνος ἐλεήμων καὶ εὔσπλαγχνος, ὁ ἔχων ἀκατάληπτον ἀγαθότητος πέλαγος, ὁ γινώσκων τὴν φύσιν τῶν ἀνθρώπων, ἣν ἐδημιούργησας, σὲ ἱκετεύομεν, Χριστὲ ὁ Θεός, τοὺς μεταστάντας ἐξ ἡμῶν ἀνάπαυσον, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία ἐν σοί, δοξάζειν σου τὴν Θεότητα."

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode3
ocMA.Aposticha3.melody = he.a.m3_gr_GR_cog.ToTypoTouStavrou.name
ocMA.Aposticha3.text = "Ὑπνώσας ἐν τῷ τάφῳ ὡς ἄνθρωπος, δυνάμει ἀηττήτῳ σου, ὡς Θεὸς ἐξανέστησας, τοὺς ἐν τάφοις ὑπνοῦντας, ἀσιγήτως ὕμνον σοι προσφέροντας· διὸ βοῶμέν σοι Χριστὲ ὁ Θεός, τοὺς μεταστάντας ἐξ ἡμῶν ἀνάπαυσον, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία ἐν σοί, δοξάζειν σου τὴν Θεότητα."

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode3
ocMA.AposTheotokion.melody = he.a.m3_gr_GR_cog.ToTypoTouStavrou.name
ocMA.AposTheotokion.text = "Λαμπάδα νοητήν σε βαστάζουσαν, τὸ φέγγος τῆς Θεότητος, ὁμιλῆσαν παχύτητι, τῆς ἀνθρώπων οὐσίας Θεοτόκε, πάντες ἐπιστάμεθα· διὸ τὸν σὸν δυσώπησον Υἱὸν καὶ Θεόν, τοὺς μεταστάντας ἐξ ἡμῶν αὐλίζεσθαι, ἔνθα πάντων ἐστίν, εὐφραινομένων ἡ κατοικία Ἁγνή, δοξάζειν σε τὴν Πανάμωμον."

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode3
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.text = oc.m3.d5_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.text = oc.m3.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.text = "Τῷ πυρὶ τῶν αἰκισμῶν, προσομιλοῦντες, Ἀθλοφόροι τοῦ Χριστοῦ, δρόσον οὐράνιον εὕρατε, ἐπαναψυχοῦσάν τε, καὶ δυναμοῦσαν φέρειν τῆς σαρκός, τὰς ὀδύνας· ὅθεν κουφίζετε, πάντα πόνον ἀεί, ἀπὸ τῶν ψυχῶν ἡμῶν."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.text = "Ἱεράρχαι ἱεροί, Προφῆται ἔνδοξοι, Ὁσίων ἡ πληθύς, Γυναίων θείων ὁμήγυρις, ἐναθλησάντων πίστει, καὶ καθελόντων πλάνην τοῦ ἐχθροῦ, οὐρανίου δόξης ἐπέτυχον, ὧν πρεσβείαις Σωτήρ, οἴκτειρον τοὺς δούλους σου."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode3
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.text = "Ἔνθα λάμπει σου τὸ φῶς, ἐν ταῖς σκηναῖς, τῶν ἐκλεκτῶν σου ὁ Θεός, ὅπου τρυφὴ ἡ ἀέναος, τοὺς ἐξ ἡμῶν ἐν πίστει, μετατεθέντας τάξον Ἰησοῦ, παρορῶν τὰ τούτων ἐγκλήματα, ἵνα σε ἐκτενῶς, Δέσποτα δοξάζωμεν."

ocLI.BeatitudesGlory.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesGlory.melody = ""
ocLI.BeatitudesGlory.text = "Τὴν προσώποις ἐν τρισί, προσκυνουμένην, μίαν ἄναρχον πιστοί, θείαν Μονάδα δοξάσωμεν, ὅπως ἀξιωθῶμεν, ἁμαρτιῶν ἀφέσεως τυχεῖν, καὶ τῆς Βασιλείας τευξώμεθα, ἀνυμνοῦντες αὐτῆς, τὸ ἄπειρον ἔλεος."

ocLI.BeatitudesBoth.mode = misc_gr_GR_cog.Mode3
ocLI.BeatitudesBoth.melody = ""
ocLI.BeatitudesBoth.text = "Φωτισμὸς καὶ ἱλασμός, ἐκ σοῦ ἐπέφανεν ὁ πάντων Λυτρωτής, θεοχαρίτωτε Δέσποινα, ὃν ἐκτενῶς δυσώπει, ἐν τῇ μελλούσῃ δίκῃ φοβερᾷ, καταδίκης πάσης λυτρώσασθαι, Ἄχραντε τοὺς πιστῶς, πάντοτε ὑμνοῦντάς σε."

