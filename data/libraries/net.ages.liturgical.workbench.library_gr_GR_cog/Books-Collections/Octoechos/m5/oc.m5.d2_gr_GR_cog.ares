octrMA.Photagogic.mode = misc_gr_GR_cog.Mode5
octrMA.Photagogic.text = "Φωτοδότα Κύριε, ἐξαπόστειλον τὸ φῶς σου, καὶ φώτισον τὴν καρδίαν μου,"
