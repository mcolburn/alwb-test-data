A_Resource_Whose_Name = oc.m5.d6_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera1.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera1.text = "Παῦσαι τοὺς πόνους θέλων βροτῶν, καὶ τὰ ὀνείδη, ὑπεράγαθε Κύριε, Σωτήρ μου σταύρωσιν φέρεις, τὴν ἐπονείδιστον, χολῆς δὲ ἐγεύσω ἀνεξίκακε, ἡμῶν τὴν πικρίαν, πᾶσαν ἐξαίρων τὴν κάκιστον, ἐπλήγης λόγχῃ, τὴν πλευρὰν τὴν ἀκήρατον, τὰς πληγὰς ἡμῶν, ὡς Δεσπότης ἰώμενος· ὅθεν νῦν ἀνυμνοῦμέν σου, τὴν ἔνδοξον σταύρωσιν, καὶ προσκυνοῦντες τιμῶμεν, λόγχην, τὸν σπόγγον, τὸν κάλαμον, δι' ὧν ἐδωρήσω, σοῦ τῷ κόσμῳ τὴν εἰρήνην, καὶ μέγα ἔλεος."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera2.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera2.text = "Πάθος τὸ σὸν Χριστὲ προτυπῶν, Μωσῆς ὁ μέγας σου θεράπων ἀνύψωσεν, οἰκτίρμον χαλκοῦν τὸν ὄφιν, ὄφεων βλάβης βροτούς, ἰοβόλου δήγματος ἐξαιρούμενος, καὶ νῦν σταυρωθέντος σου, ἐπὶ ξύλου μακρόθυμε, κόσμος ἐκ βλάβης, ἐλυτρώθη τοῦ ὄφεως, καὶ ἀνυψώθη, ἀπὸ γῆς πρὸς οὐράνια· ὅθεν καὶ ἀγαλλόμενοι, ὑμνοῦμεν τὸ κράτος σου, καὶ τὸν Σταυρόν σου τιμῶμεν, καὶ προσκυνοῦμεν φιλάνθρωπε, δι' οὗ πᾶσα κτίσις, εὗρεν ἄπονον ἀξίαν, καὶ μέγα ἔλεος."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera3.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera3.text = "Θέλων τὰς διαθέσεις Ἀδάμ, τοῦ πρωτοπλάστου ἐξηλῶσαι φιλάνθρωπε, εἰς τέλος πᾶσαν ὀδύνην, ὁ πλαστουργὸς τοῦ Ἀδάμ, ἥλοις προσηλώθης ἀναμάρτητε, Χριστὲ καὶ τῇ λόγχῃ, θείαν πλευρὰν τρωθεὶς δι' ἡμᾶς, κωλύεις Λόγε, τὴν ῥομφαίαν τὴν φλέγουσαν, μὴ κωλύειν ἡμῖν, τοὶς σοῖς δούλοις τὴν εἴσοδον· ὅθεν δοξολογοῦντές σε, ὑμνοῦμεν τὸ κράτος σου, καὶ τὸν Σταυρόν σου τιμῶμεν, καὶ προσκυνοῦμεν φιλάνθρωπε, δι' οὗ πᾶσα κτίσις, εὗρεν ἄπονον ἀξίαν, καὶ μέγα ἔλεος."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera4.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera4.text = "Ἔχων πεφορτισμένον ἀεί, τὸ τῆς ψυχῆς μου ἐπαυχένιον Δέσποινα, ἐκ πλήθους ἁμαρτημάτων, καὶ συμφορῶν χαλεπῶν, οὐ τολμῶ πρὸς ὕψος ἆραι ὄμματα· διὸ νενευκὼς εἰς γῆν, ἐκ καρδίας κραυγάζω σοι· Ἐλέησόν με, ἡ Θεὸν πολυεύσπλαγχνον, ἀνατείλασα, ἐξ ἀχράντου νηδύος σου, δεῖξον τῶν θαυμασίων σου, τὸ πλῆθος τὸ ἄπειρον, καὶ τὰς ἀχράντους καὶ θείας, πρὸς τὸν Υἱόν σου ἐκτείνασα, Παρθένε παλάμας, σωτηρίαν μοι παράσχου, ταῖς ἱκεσίαις σου."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera5.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera5.text = "Λαῖλαψ ἁμαρτιῶν χαλεπή, ἐν τῷ πελάγει με τοῦ βίου κατέλαβε, καὶ ζάλη καὶ τρικυμία, καὶ πρὸς βυθὸν συνωθεῖ, τὴν ἐμὴν καρδίαν ἀπογνώσεως. Ἁγνὴ ταῖς πρεσβείαις σου, τὴν ζωήν μου κυβέρνησον, πρὸς τὸν λιμένα, τῆς ζωῆς τὸν ἀκύμαντον, πρὸς μετάνοιαν, καὶ τελείαν διόρθωσιν, ἴδε μου τὴν ἀσθένειαν, καὶ πρόσχες τῇ κρίσει μου, δίδου μοι χεῖρα κειμένῳ, σῆς βοηθείας Πανύμνητε, Χριστὸν ἡ τεκοῦσα, τὸν παρέχοντα τῷ κόσμῳ, τὸ μέγα ἔλεος."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode5
ocVE.Stichera6.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.Stichera6.text = "Ὕλην ἁμαρτιῶν χαλεπῶν, ἐπισυνάξας ἐμαυτῷ ἐθησαύρισα, καὶ τρέμω ἀπὸ προσώπου, τοῦ σοῦ Υἱοῦ Ἀγαθή, πῶς ὑποίσω τότε τὸ κριτήριον· καὶ γὰρ ποταμὸς πυρός, ἕλκει βήματος ἔμπροσθεν, καὶ μυριάδες, τῶν Ἀγγέλων παρίστανται, τοὺς ἀνομοῦντας, ἐν αὐτῷ ἀπορρίπτοντες· ὅθεν πρὸ τῆς ἐξόδου μου, τοῦ βίου Πανάχραντε, καθικετεύω πρεσβεῦσαι, ὑπὲρ ἐμοῦ τὸν φιλάνθρωπον, κριτὴν ἐλεῆσαι, καὶ συγχώρησίν μοι δοῦναι, καὶ μέγα ἔλεος."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode5
ocVE.SticTheotokion.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.SticTheotokion.text = "Ὅλην ἀναλαβὼν τὴν ἐμήν, μορφὴν ὁ πλάστης καὶ Θεὸς ἐνεδύσατο, μορφῶσαι τὴν πρὶν πεσοῦσαν μορφὴν Ἀδὰμ βουληθείς, ἐν Σταυρῷ ὑψώθη, ὡς ὑπεύθυνος, ἑκὼν καὶ τὰς χεῖρας, προσηλωθεὶς νῦν ἰάσατο, τὰς πρὶν τῷ ξύλῳ, παρειμένας τῆς βρώσεως, ὃν ἡ Πάναγνος, θεωροῦσα ἠλάλαζε. Τίς ἐστιν ἡ τοσαύτη σου, Υἱέ μου ἀνείκαστος, μακροθυμία; οὐ φέρω, ἐπὶ Σταυροῦ ἀναρτώμενον, ὁρᾷν σε τὸν πάντα, τῇ δρακὶ διακρατοῦντα, Χριστὲ τὰ σύμπαντα."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode5
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m5.d4_gr_GR_cog.ocVE.Aposticha1.text

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode5
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.text = oc.m5.d4_gr_GR_cog.ocVE.Aposticha2.text

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode5
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.text = oc.m5.d2_gr_GR_cog.ocVE.Aposticha3.text

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode5
ocVE.AposTheotokion.melody = he.a.m5_gr_GR_cog.ChairoisAskitikon.name
ocVE.AposTheotokion.text = "Λόγχῃ σου τὴν πλευρὰν ὦ Υἱέ, ὀρυττομένην καθορῶσα τιτρώσκομαι, τῆς λύπης δεινὴ ῥομφαία, καὶ στεναγμὸν ἐκ ψυχῆς, προϊέναι ὅλως οὐ δεδύνημαι, ἡ Πάναγνος ἔλεγε, παρεστῶσα τῷ πάθει σου, καὶ τὴν σφαγήν σου, καθορῶσα τὴν ἄδικον, ἀνεξίκακε, ὑπεράγαθε Κύριε, ποῦ μοι τὰ εὐαγγέλια, βοῶσα σὺν δάκρυσι, ποῦ ὁ τὸ Χαῖρέ μοι λέξας; ποῦ ἡ λοχεία ἡ ἄφραστος; ποῦ Πέτρος ὁ φίλος; ἀλλὰ δόξα τῇ ἀφάτῳ μακροθυμίᾳ σου."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode5
ocVE.ApolTheotokionVM.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocVE.ApolTheotokionVM.text = oc.m5.d4_gr_GR_cog.ocMA.Kathisma23.text

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.text = oc.m5.d4_gr_GR_cog.ocMA.Kathisma11.text

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma12.melody = ""
ocMA.Kathisma12.text = oc.m5.d4_gr_GR_cog.ocMA.Kathisma12.text

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma13.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma13.text = "Τῷ Σταυρῷ τοῦ Κυρίου παρισταμένη, θρηνῳδοῦσα ἐβόα ἡ Θεοτόκος· Οἴμοι Τέκνον θεῖον, φῶς μου γλυκύτατον! πῶς ἐτανύθης ἐν Σταυρῷ, ὁ τείνας θεϊκῶς τὸν οὐρανόν ὡσεὶ δέρριν, καὶ ἐκ θαλάσσης ἀνάγων, πηγὰς ὑδάτων τῷ σῷ προστάγματι."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma21.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma21.text = oc.m5.d4_gr_GR_cog.ocMA.Kathisma21.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma22.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma22.text = oc.m5.d4_gr_GR_cog.ocMA.Kathisma22.text

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma23.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma23.text = "Ἐν τῷ Σταυρῷ σε ὁρῶσα Χριστὲ ἡ Μήτηρ σου, ἑκουσίως παγέντα ἀμνὸν ὡς ἄκακον, θρῆνον ἐκίνει γοερόν, καὶ κατετρύχετο. Οἴμοι! βοῶσα τὸ ἐμόν, πῶς ἀπεσβέσθης φῶς πικρῶς; καὶ πῶς ὡς ἄδικος θνῄσκεις, μέσον κακούργων Υἱέ μου, ὁ Βασιλεὺς πάσης κτίσεως;"

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma31.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma31.text = "Ἐκ τοῦ ξύλου πικρίαν Ἀδὰμ ἐτρύγησε, πρὸς φθορὰν ὀλισθήσας φθόνῳ τοῦ ὄφεως, σοῦ δὲ παγέντος Ἰησοῦ, ζωὴν ἐτρύγησε, καὶ διὰ ξύλου τοῦ Σταυροῦ, πάλιν οἰκεῖ τοὺς οὐρανούς, καὶ κατηργήθη ὁ ὄφις, καὶ ἡ φθορὰ κατεπόθη, καὶ πάντες δόξαν σοι προσάγομεν."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.text = oc.m5.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode5
ocMA.Kathisma33.melody = he.a.m5_gr_GR_cog.TonSynanarchonLogon.name
ocMA.Kathisma33.text = "Γαβριήλ μοι τὸ Χαῖρε πρὶν προσεκόμισε, μετ' ἐμοῦ σε φωνήσας ἔσεσθαι Κύριε, ἡ Θεοτόκος ἐν κλαυθμῷ πικρῶς ἐφθέγγετο· πῶς οὖν εἰς λύπην ἡ χαρά, νῦν μοι ἐγένετο Υἱὲ; καὶ πῶς ὡς ἄπαις φανοῦμαι, ἡ ἀπειράνδρως τεκοῦσα, σὲ τὸν Σωτῆρα τῶν ψυχῶν ἡμῶν;"

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχίς μετὰ τῶν Σταυροθεοτοκίων ἢ Θεοτοκίων· Παθῶν με σώζοις τῷ πάθει σου, Χριστέ μου, τὸν Ἰωσήφ."

ocMA.C2Acrostic.text = " οὗ ἡ ἀκροστιχίς μέχρι τοῦ μέσου τῆς η' ᾠδῆς· Πέμπτη δέησις τῇ Θεοτόκῳ πέλει."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m5_gr_GR_cog.ToSotiriTheo.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m5_gr_GR_cog.ToSotiriTheo.name
ocMA.OdeW1C11.text = "Παθοκτόνον Χριστέ, ἐθελουσίως αὐτός, ὑπέστης πάθος, καὶ ἀπέκτεινας τὸν ἀποκτείναντα, πάλαι ἐν Παραδείσῳ ἡμᾶς· διό σου δοξάζομεν, τὴν ἀγαθότητα."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m5_gr_GR_cog.ToSotiriTheo.name
ocMA.OdeW1C12.text = "Ἀνυψώθης Σταυρῷ, καὶ κατεπόθη ἐχθρός, καὶ οἱ πεσόντες ἀνυψώθημεν, καὶ Παραδείσου πολῖται, Χριστὲ γεγόναμεν, τὸ κράτος δοξάζοντες, τῆς Βασιλείας σου."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m5_gr_GR_cog.ToSotiriTheo.name
ocMA.OdeW1C13.text = "Θυρεῷ τοῦ Σταυροῦ, καθοπλισθέντες καλῶς, πρὸς πάλην πᾶσαν τοῦ ἀλάστορος, προσεχωρήσατε σοφοὶ Μεγαλομάρτυρες, καὶ τοῦτον νικήσαντες, δόξης ἐτύχετε."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m5_gr_GR_cog.ToSotiriTheo.name
ocMA.OdeW1C14.text = "Ὡς ἀρνία σεμνά, τῷ δι' ἡμᾶς Ἀθληταί, ἀμνῷ τυθέντι προσηνέχθητε, τὰς ἐναγεῖς περιφανῶς θυσίας παύοντες· διὸ μακαρίζομεν, ὑμᾶς πανεύφημοι."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m5_gr_GR_cog.ToSotiriTheo.name
ocMA.OdeW1C1T.text = "Νέον βρέφος ἡμῖν, τὸν παλαιὸν ἡμερῶν, ἁγνὴ Παρθένε ἀπεκύησας, παλαιωθεῖσαν τὴν φύσιν ἀνακαινίσαντα, ἀνθρώπων Πανάμωμε, τῷ θείῳ πάθει αὐτοῦ."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m5_gr_GR_cog.IpponKaiAnavatin.name
ocMA.OdeW1C21.text = "Πύλη τοῦ ἀπροσίτου, φωτὸς Πανάμωμε, μετανοίας τὰς πύλας τῇ ψυχῇ μου διάνοιξον, καὶ δίδου τὴν εἴσοδον, τῆς ἐκεῖσε Κόρη, ἀγαλλιάσεως καὶ τερπνότητος."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m5_gr_GR_cog.IpponKaiAnavatin.name
ocMA.OdeW1C22.text = "Ἔχων σε Θεοτόκε, προστάτιν ἄμαχον, καὶ ἀπόρθητον τεῖχος, καὶ σκέπην ἀνυπέρβλητον, ῥυσθείην τοῦ δράκοντος, τοῦ δολίου Πάναγνε, τοῦ πάλαι ἐκζητοῦντος, καταπιεῖν με σφοδρῶς."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m5_gr_GR_cog.IpponKaiAnavatin.name
ocMA.OdeW1C23.text = "Μή με ἀπογυμνώσῃς, τῆς θείας σκέπης σου, μὴ κενὸν ἀποδείξῃς, τῆς χάριτός σου Ἄχραντε, μηδὲ ἀποπέμψῃς, κατῃσχυμένον Δέσποινα, ἀλλὰ παράσχου μοι τὸ σὸν ἔλεος."

ocMA.OdeW1C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW1C24.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C24.melody = he.h.m5_gr_GR_cog.IpponKaiAnavatin.name
ocMA.OdeW1C24.text = "Παῦσον τῆς ταλαιπώρου, καὶ παναθλίας μου, Θεοτόκε καρδίας, πόνον τὸν ἀφόρητον, καὶ τοῦ σωτηρίου σου παράσχου μοι, Θεομῆτορ, θείαν αἴγλην καὶ σωτήριον."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.name
ocMA.OdeW3C11.text = "Μαραίνεις τὸν καρπὸν τῆς φθορᾶς, ἐν ξύλῳ Σωτὴρ ἀνυψούμενος, καὶ ἀφθαρσίας πηγὰς ἡμῖν, ἐκ πλευρᾶς ἐκβλύζεις Δέσποτα."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.name
ocMA.OdeW3C12.text = "Ἐτύθης ἐν Σταυρῷ ὡς ἀμνός, φλιὰς σφραγίζων τῶν ψυχῶν ἡμῶν, θείῳ σου αἵματι Δέσποτα· ὅθεν φόβῳ σε δοξάζομεν."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.name
ocMA.OdeW3C13.text = "Στρεβλούμενοι Χριστοῦ Ἀθληταί, καὶ πολυτρόπως αἰκιζόμενοι, καὶ τοῖς θηρίοις ῥιπτόμενοι, ἀπερίτρεπτοι ἐμείνατε."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.name
ocMA.OdeW3C14.text = "Ὡς βότρυες ἀμπέλου ζωῆς, τοῦ μαρτυρίου οἶνον ἔβλυσαν, πιστῶν καρδίας εὐφραίνοντες, τοῦ Θεοῦ ἡμῶν οἱ Μάρτυρες."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m5_gr_GR_cog.DynameiTouStavrouSou.name
ocMA.OdeW3C1T.text = "Ζωῆς ἡμῖν πρόξενος Ἁγνή, θανὼν ἐν ξύλῳ ἀναδέδεικται, ὁ σὸς Υἱός τε καὶ Κύριος, δοξάζων τοὺς ὑμνοῦντάς σε."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m5_gr_GR_cog.OPixasEpOudenos.name
ocMA.OdeW3C21.text = "Τοῦ κόσμου τὸ προσφύγιον σὺ εἶ Πανάχραντε, καὶ ἐν σοὶ προστρέχων, ἐκ διανοίας θερμῆς, πᾶς τις ἐκλυτροῦται τῶν δεινῶν· διό με προσδραμόντα, ἐν τῇ σῇ σκέπῃ ἀπολύτρωσαι, πάσης πονηρᾶς ἐνοχλήσεως."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m5_gr_GR_cog.OPixasEpOudenos.name
ocMA.OdeW3C22.text = "Ἡ μόνη τῶν γηγενῶν ἁπάντων ἀντίληψις, σὺ ἀντιλαβοῦ μου ἱλέῳ ὄμματι, Δέσποινα τοῦ κόσμου ἀγαθή, καὶ φρούρησον τῇ θείᾳ, ἐπισκοπῇ τῆς δυναστείας σου· ἔχεις γὰρ ἰσχὺν ἀνυπέρβλητον."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m5_gr_GR_cog.OPixasEpOudenos.name
ocMA.OdeW3C23.text = "Δυνάμει τῇ σῇ χειρός με τοῦ πολεμήτορος, καὶ τῆς τυραννίδος τούτου ἀφάρπασον, Δέσποινα τοῦ κόσμου ἀγαθή· μή με ἰσχύσας λάβῃ, καὶ καταπίῃ, καὶ πρὸς ὄλεθρον, ἄξῃ αἰωνίου κολάσεως."

ocMA.OdeW3C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW3C24.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C24.melody = he.h.m5_gr_GR_cog.OPixasEpOudenos.name
ocMA.OdeW3C24.text = "Ἐν φόβῳ καὶ ἐν τρόμῳ προσπίπτων κραυγάζω σοι, Θεοτόκε. Σύ μοι γενοῦ βοήθεια, Δέσποινα τοῦ κόσμου ἀγαθή, ἐν ὥρᾳ τοῦ θανάτου, ἡνίκα τῶν βεβιωμένων μοι, μέλλω τὰς εὐθύνας εἰσπράττεσθαι."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.name
ocMA.OdeW4C11.text = "Ὅτε ἔδυς ἐν Σταυρῷ, δικαιοσύνης Ἥλιε Χριστέ, φῶς ἀνέσπερον ἀνέτειλας ἡμῖν, τοῖς ὑμνοῦσί σου Λόγε, τὴν φρικτὴν συγκατάβασιν."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.name
ocMA.OdeW4C12.text = "Ἱστάμενος πρὸ δικαστικοῦ, Χριστέ μου βήματος ποτέ, κατεδίκασας τὸν ἄδικον ἐχθρόν, καὶ ἐσταυρώθης μέσον καταδίκων, ἡμᾶς δικαιῶν."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.name
ocMA.OdeW4C13.text = "Στεφανούμενοι νικητικῶς, οἱ τοῦ Χριστοῦ Ἀθληταί, τὸν ἀόρατον κατῄσχυναν ἐχθρόν, καὶ ἐκραύγαζον· Δόξα τῇ δυνάμει σου Κύριε."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.name
ocMA.OdeW4C14.text = "Τὰ ἀμάραντα ἄνθη πιστοί, τοῦ νοητοῦ Παραδείσου, τὰ πολύτιμα σκεύη τοῦ Χριστοῦ, δεῦτε τοὺς Ἀθλοφόρους, ἐν ὕμνοις εὐφημήσωμεν."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m5_gr_GR_cog.EisakikoaTinAkoin.name
ocMA.OdeW4C1T.text = "Ὡς ἑώρακας ἐν τῷ Σταυρῷ, Χριστὸν ὃν ἔτεκες Ἁγνή, ἀπεθαύμαζες τὴν ἄφατον αὐτοῦ, μακροθυμίαν· ὅθεν σὺν αὐτῷ σε δοξάζομεν."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m5_gr_GR_cog.TinTheianEnnoisas.name
ocMA.OdeW4C21.text = "Ἡ πάντων γεννητῶν ὑπερέχουσα, ὡς τὸν Δεσπότην καὶ Θεόν, ἐν τῇ γαστρί σου βαστάσασα, τὸν ὑπὲρ πάντας ἀνθρώπους, ἐξαμαρτάνοντά με κατοικτείρησον."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m5_gr_GR_cog.TinTheianEnnoisas.name
ocMA.OdeW4C22.text = "Σαρκός μου τὰς ἀτάκτους κατάσβεσον, ὀρέξεις ὄμβροις σῶν εὐχῶν, καὶ τῆς ψυχῆς μου διάναψον, τὸν λύχνον τὸν ἐσβεσμένον, πυρὶ τοῦ θείου ἔρωτος Πάναγνε."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m5_gr_GR_cog.TinTheianEnnoisas.name
ocMA.OdeW4C23.text = "Ἰσχὺς τῶν ἀσθενούντων Πανάμωμε, ἀπεγνωσμένων ἡ ἐλπίς, καὶ τῶν πενθούντων παράκλησις, χαροποιόν μοι παράσχου πένθος, δι' οὗ εὑρήσω τὴν ἄφεσιν."

ocMA.OdeW4C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW4C24.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C24.melody = he.h.m5_gr_GR_cog.TinTheianEnnoisas.name
ocMA.OdeW4C24.text = "Σὲ τεῖχος καὶ ἀσφάλειαν Δέσποινα, σὲ προστασίαν ἀψευδῆ, σὲ πύργον ἀκαταμάχητον, πιστῶς ἀεὶ κεκτημένος, ἐλπίζω διὰ σοῦ σωτηρίας τυχεῖν."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.name
ocMA.OdeW5C11.text = "Πέτραι σε ἐπὶ ξύλου αἰσθόμεναι, ὑψωθέντα Χριστέ, διεσχίσθησαν, καὶ γῆς ἐσείσθη θεμέλια."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.name
ocMA.OdeW5C12.text = "Ἀπέθετο λαμπάδα ὁ ἥλιος, ὑψωθέντος σου, ἐν ξύλῳ Ἥλιε, δικαιοσύνης μακρόθυμε."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.name
ocMA.OdeW5C13.text = "Θαυμάτων ἀπαστράπτεσθε Ἅγιοι, λαμπηδόνας, τὸ σκότος διώκοντες, τῶν νοσημάτων ἐν χάριτι."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.name
ocMA.OdeW5C14.text = "Ἐτέμνεσθε τὸ σῶμα, ὦ Μάρτυρες, τὸ δὲ πνεῦμα θείας ἀγαπήσεως, ἄτμητον εἴχετε ἅγιοι."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m5_gr_GR_cog.OrthrizontesVoomenSoi.name
ocMA.OdeW5C1T.text = "Ἰδοῦσα ἐπὶ ξύλου ὑπνώσαντα, τὸν Δεσπότην, θρηνοῦσα ἠλάλαζες, Παρθενομῆτορ πανάμωμε."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m5_gr_GR_cog.OAnavallomenos.name
ocMA.OdeW5C21.text = "Τίς μου τὸ χειρόγραφον, τῶν παραπτώσεων, καὶ τῶν ἀμέτρων πλημμελημάτων, Ἁγνὴ διαρρήξειεν, εἰμὴ σὺ προφθάσῃς, καὶ δῴης μοι τὴν λύτρωσιν."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m5_gr_GR_cog.OAnavallomenos.name
ocMA.OdeW5C22.text = "Ἦλθον εἰς ἀπόγνωσιν, ἐξ ἀπορίας πολλῆς, τὴν τῶν κακῶν μου, πληθὺν Παρθένε ἀναλογιζόμενος· διό σοι κραυγάζω· Ἐλέησον καὶ σῶσόν με."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m5_gr_GR_cog.OAnavallomenos.name
ocMA.OdeW5C23.text = "Θεοῦ Μῆτερ ἄφθορε, Θεοῦ λοχεύτρια, Θεοῦ τοῦ πάντα νεύματι μόνῳ, ἐκτελοῦντος ῥῦσαί με, τῆς αἰωνιζούσης, καταδίκης τὸν δοῦλόν σου."

ocMA.OdeW5C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW5C24.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C24.melody = he.h.m5_gr_GR_cog.OAnavallomenos.name
ocMA.OdeW5C24.text = "Ἔχουσα τὸ καύχημα, παρθενίας φαιδρόν, ὡς Μήτηρ αὖθις Θεοῦ γεραίρῃ, ὑπερτέρως Ἄχραντε· διό σοι τὸ Χαῖρε, τοῦ Γαβριὴλ προσφέρομεν."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.name
ocMA.OdeW6C11.text = "Σταυρὸς ἐν γῇ ἐπήγνυτο, καὶ πτῶσις δαιμόνων ἐγίνετο, καὶ πίστεως ἕδρασμα ἀρχὴν ὑπεδέχετο, καὶ ἡ κακία ἐκ μέσου ἀπελήλατο."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.name
ocMA.OdeW6C12.text = "Ὁ ἥλιος ἐσβέννυτο, τὴν σάρκα ὡς λύχνον ἀνάψαντος, ἐν ξύλῳ σου Κύριε, δραχμὴ δὲ εὑρίσκετο, ἡ κεχωσμένη, ἐσκοτισμένοις πάθεσι."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.name
ocMA.OdeW6C13.text = "Ὑψούμενος φιλάνθρωπε, ἐν ξύλῳ χοροὺς ἑπομένους σου, τοῖς ἴχνεσιν ἔσχηκας, Μαρτύρων τὸ πάθος σου, ἐκμιμουμένους, τὸ ἀπαθείας πρόξενον."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.name
ocMA.OdeW6C14.text = "Χειμάρρους ἐξηράνατε, τῆς πλάνης, αἱμάτων τοῖς ῥεύμασι, καὶ πῦρ ἀπεσβέσατε, δαιμόνων ἀλλότριον, τῇ θείᾳ δρόσῳ, στεφανηφόροι Μάρτυρες."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m5_gr_GR_cog.EkykloseMeAvyssos.name
ocMA.OdeW6C1T.text = "Ῥομφαία τὴν καρδίαν σου, διῆλθεν ἡνίκα σταυρούμενον τὸν κτίστην τεθέασαι, Παρθένε πανάμωμε, καὶ λόγχῃ θείαν, πλευρὰν ἐξορυττόμενον."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m5_gr_GR_cog.MainomeninKlydoni.name
ocMA.OdeW6C21.text = "Ἡ ζωὴν κυήσασα, οὐσιώδη, Δέσποινα ἁγνή, νοητοῦ θανάτου με ἐξάρπασον, καὶ τῆς ζωῆς, τῆς αἰωνίου ἀξίωσον."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m5_gr_GR_cog.MainomeninKlydoni.name
ocMA.OdeW6C22.text = "Τὴν ὄντως ἀσύγκριτον, ἐν τῷ κάλλει, πάντων γεννητῶν, σὲ αἰτοῦμαι Δέσποινα Θεόνυμφε, τῆς εἰδεχθοῦς κακίας με ἐλευθέρωσον."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m5_gr_GR_cog.MainomeninKlydoni.name
ocMA.OdeW6C23.text = "Οὐκ ἔχω ἀντίληψιν, ἐν τῷ κόσμῳ, πλήν σου ἀγαθή· διό σοι προσπίπτω καὶ κραυγάζω σοι· Μὴ ἀποστῇς, ἀπὸ τοῦ δούλου σου Πάναγνε."

ocMA.OdeW6C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW6C24.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C24.melody = he.h.m5_gr_GR_cog.MainomeninKlydoni.name
ocMA.OdeW6C24.text = "Κακουργῶν ὁ δόλιος, ταῖς ἡδοναῖς, τὸν νοῦν μου ἀεί, ἐκταράσσει. Δέσποινα βοήθει μοι, καὶ τῆς τούτου, πονηρίας με ἐξάρπασον."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m5_gr_GR_cog.OEnKaminoPyros.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m5_gr_GR_cog.OEnKaminoPyros.name
ocMA.OdeW7C11.text = "Ἵνα ῥυσθῶμεν ἡμεῖς, τῆς ἐνηδόνου ἁμαρτίας, χολῆς ἐγεύσω Χριστέ, ὁ γλυκασμὸς τῆς ζωῆς."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m5_gr_GR_cog.OEnKaminoPyros.name
ocMA.OdeW7C12.text = "Τοῦ ἐν Σταυρῷ Ἰησοῦ, τραυματισθέντος ἰατρεύθη, τὰ τραύματα τοῦ Ἀδάμ, τὰ πολυχρόνια."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m5_gr_GR_cog.OEnKaminoPyros.name
ocMA.OdeW7C13.text = "Τετιμηκότες Χριστόν, δι' ἐπιπόνου ἀτιμίας, τῆς ἀνωτάτω τιμῆς ἐτύχετε Ἀθληταί."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m5_gr_GR_cog.OEnKaminoPyros.name
ocMA.OdeW7C14.text = "Ἐθελουσίοις ὁρμαῖς, κεχωρηκότες πρὸς τὸ πάθος, οἱ Ἀθληταὶ νικηταί, ἐναπεφάνθησαν."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m5_gr_GR_cog.OEnKaminoPyros.name
ocMA.OdeW7C1T.text = "Μετὰ τὸν τόκον ἡ Ἁγνή, ὡς πρὸ τοῦ τόκου διαμένεις· Θεὸς γὰρ ἦν ὁ τεχθείς, ἵνα θεώσῃ βροτούς."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m5_gr_GR_cog.OYperypsoumenos.name
ocMA.OdeW7C21.text = "Ὦ Θεομακάριστε, Θεοτόκε πάναγνε, ψυχῆς μου τὴν κάκωσιν, ἴασαι καὶ δώρησαι, γαλήνην σωτηρίας, καὶ ζωῆς τὴν εὐφροσύνην."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m5_gr_GR_cog.OYperypsoumenos.name
ocMA.OdeW7C22.text = "Πύλη ἀδιόδευτε, λόγοις ματαιότητος, τὰς πύλας ἀπόκλεισον, δι' ὧν μοι ὁ θάνατος, καὶ ὁ τῆς ἁμαρτίας, ὄλεθρος ὑπεισέδυ."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m5_gr_GR_cog.OYperypsoumenos.name
ocMA.OdeW7C23.text = "Ἔβλυσεν ὁ τόκος σου, ἀφθαρσίας νάματα· ἀλλὰ ταῖς πρεσβείαις σου, βλῦσαι παρασκεύασον, κᾀμοὶ τῆς συμπαθείας, τὸν αὐτοῦ ἄμετρον πλοῦτον."

ocMA.OdeW7C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW7C24.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C24.melody = he.h.m5_gr_GR_cog.OYperypsoumenos.name
ocMA.OdeW7C24.text = "Λύτρωσαί με Πάναγνε, ἐκ πάσης κακώσεως, βίου περιστάσεων, συμφορῶν καὶ θλίψεων, καὶ νόσων καὶ κινδύνων, καὶ δεινῆς συκοφαντίας."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.name
ocMA.OdeW8C11.text = "Ὁ τὴν κατάραν του ξύλου, διὰ ξύλου ἰασάμενος, καὶ εὐλογίαν πηγάσας, τοῖς ἀνθρώποις ὑπεράγαθε, σὲ Σωτὴρ ὑμνοῦμεν, καὶ σὲ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.name
ocMA.OdeW8C12.text = "Ὑπεραιρόμενον ὄφιν, τῷ Σταυρῷ σου ἐταπείνωσας, καὶ τὸν δεινῶς συμπτωθέντα, ταπεινούμενον ἀνύψωσας, σὲ Σωτὴρ ὑμνοῦμεν, καὶ σὲ ὑπερυψοῦμεν, εἰς πάντας τούς αἰῶνας."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.name
ocMA.OdeW8C13.text = "Τοὺς καθαιρέτας τῆς πλάνης, καὶ προμάχους θείας πίστεως, τῆς Ἐκκλησίας τοὺς στύλους, καὶ στερροὺς ὄντως ἀδάμαντας, τοὺς Χριστοῦ ὁπλίτας, πιστοὶ τοὺς Ἀθλοφόρους, τιμήσωμεν ἐν ὕμνοις."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.name
ocMA.OdeW8C14.text = "Οἱ εὐκλεεῖς Ἀθλοφόροι, ὥσπερ ἥλιος ἐκλάμψαντες, τῶν ἀλγηδόνων τὰ νέφη, διεσκέδασαν ἐν χάριτι, καὶ τῆς ἀσεβείας, διέλυσαν τὸν ζόφον, τῇ πίστει τῆς Τριάδος."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m5_gr_GR_cog.TonEkPatrosProAionon.name
ocMA.OdeW8C1T.text = "Νυμφαγωγός σοι Παρθένε, Γαβριὴλ ἀποστελλόμενος· Χαῖρε ἐβόησε λέγων, φωτεινότατον παλάτιον, τοῦ παμβασιλέως Χριστοῦ, ἐν ᾧ οἰκήσας, βροτοὺς θεώσει πάντας."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m5_gr_GR_cog.SoiToPantourgo.name
ocMA.OdeW8C21.text = "Ἐν τοῖς πειρασμοῖς, εὕρημί σε προστάτιν, ἐν θλίψεσι πρόμαχον, ἐν συμφοραῖς βοηθόν, ἐν τοῖς κινδύνοις σωτήριον λιμένα, καὶ ἐν πάσῃ λύπῃ, Σεμνὴ παραμυθίαν."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m5_gr_GR_cog.SoiToPantourgo.name
ocMA.OdeW8C22.text = "Ἴδε τὴν ἐμήν, σεμνὴ Παρθένε πίστιν, ἴδε τὸν πόθον μου, ὃν εἰς σὲ κέκτημαι, ἴδε ψυχῆς μου τὸν ἔρωτα τὸν θεῖον, καὶ τὴν σήν μοι δίδου, πολυπλασίως χάριν."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m5_gr_GR_cog.SoiToPantourgo.name
ocMA.OdeW8C23.text = "Φῶς τὸ νοητόν, σὺ ἐν γαστρὶ λαβοῦσα, φώτισον τὰ ὄμματα, τὰ τῆς καρδίας μου, λῦσον τὸν ζόφον τῶν ἐμῶν ὀφλημάτων, καὶ τῶν παθημάτων, σκέδασον τὴν ὁμίχλην."

ocMA.OdeW8C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW8C24.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C24.melody = he.h.m5_gr_GR_cog.SoiToPantourgo.name
ocMA.OdeW8C24.text = "Ἡ τὴν ὑπὲρ νοῦν, τεκοῦσα σωτηρίαν, τὸν πᾶσι βραβεύοντα, τὴν σωτηρίαν βροτοῖς, Κόρη τὴν θείαν δίδου μοι σωτηρίαν, τῶν ἁμαρτημάτων, ἀναστέλλουσα λύμην."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m5_gr_GR_cog.SeTinYperNoun.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m5_gr_GR_cog.SeTinYperNoun.name
ocMA.OdeW9C11.text = "Ἰσχὺς τοῦ ἐχθροῦ, καὶ κράτος ἀφῄρηται, κραταιὲ μόνε Κύριε, Σταυρῷ ὑψωθέντος σου, καὶ δακτύλους, ἐν τούτῳ αἱματώσαντος."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m5_gr_GR_cog.SeTinYperNoun.name
ocMA.OdeW9C12.text = "Ὤρυξαν Χριστέ, χεῖρας καὶ πόδας σου, καὶ ὀστᾶ σου ἠρίθμησαν, σταυροῦντες οἱ ἄνομοι, καὶ χολὴν μετὰ ὄξους σε ἐπότισαν."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m5_gr_GR_cog.SeTinYperNoun.name
ocMA.OdeW9C13.text = "Στόματι λαμπρῷ, Θεὸν ἐκηρύξατε, χρηματίσαντα ἄνθρωπον, τυράννων ἐνώπιον, Ἀθλοφόροι καὶ δόξαν ἐκληρώσασθε."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m5_gr_GR_cog.SeTinYperNoun.name
ocMA.OdeW9C14.text = "Ἤλγησαν ἐχθροί, ἀλγείνοντες μάστιξι, καὶ ποικίλαις κολάσεσιν, ὑμᾶς πανσεβάσμιοι, ἰατροὶ ἀλγηδόνων θεῖοι Μάρτυρες."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m5_gr_GR_cog.SeTinYperNoun.name
ocMA.OdeW9C1T.text = "Φῶς ἡμῖν ἐκ σοῦ, Ἁγνὴ ἀνατέταλκεν, Ἰησοῦς καὶ ἐφαίδρυνε, τὴν κτίσιν σταυρούμενος, καὶ δαιμόνων τὸ σκότος ἀπεδίωξε."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m5_gr_GR_cog.IsaiaChoreve.name
ocMA.OdeW9C21.text = "Ἀλγεινοῖς κυκλούμενος, πολλοῖς γόνυ, κλίνω σοι Σεμνή, τὸ πρόσωπον εἰς γῆν ῥίπτω δυστυχῶς, δακρύων κραυγάζω σοι· Τῶν ζητούντων ῥῦσαι Ἀγαθή, καὶ θυμηδίας μοι, ἀνατολὴ ἐπιφάνηθι."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m5_gr_GR_cog.IsaiaChoreve.name
ocMA.OdeW9C22.text = "Τὸ τῆς ἀπογνώσεως, ἐνισχύει, κράτος ἐν ἐμοί, χειλέων ἐκ ῥυπαρῶν, ποία σοι ᾠδὴ; καρδίας αἰτήματος κεχραμένης, τίς ἀποδοχή, ἀλλὰ θαυμάστωσον, τοῖς ἀθλίοις τὰ ἐλέη σου."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m5_gr_GR_cog.IsaiaChoreve.name
ocMA.OdeW9C23.text = "Ἠλλοιώθην θλίψεσιν, ἠμαυρώθη νοῦς καὶ ὀφθαλμός, κυκλοῦσί μου τὴν ζωήν, ἄλγη χαλεπά· πρὸς τούτοις ἐκτήκει με, τὰ ἐκεῖθεν δείματα Σεμνή· ὧν με διάσωσον, τάς ὀδύνας μεταβάλλουσα."

ocMA.OdeW9C24.mode = misc_gr_GR_cog.Mode5
ocMA.OdeW9C24.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C24.melody = he.h.m5_gr_GR_cog.IsaiaChoreve.name
ocMA.OdeW9C24.text = "Ὁ βραχὺς ἀνήλωται, τῆς ζωῆς μου, βίος ἐν κακοῖς, καὶ πλήθει τῶν λυπηρῶν, καὶ τῶν ἀλγεινῶν· διὸ ἡ κυήσασα, Θεοτόκε πάντων τὴν χαράν, ταῦτα πρεσβείαις σου, εὐπροσδέκτοις ἐπικούφισον."

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode5
ocMA.Aposticha1.melody = ""
ocMA.Aposticha1.text = oc.m5.d4_gr_GR_cog.ocMA.Aposticha1.text

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode5
ocMA.Aposticha2.melody = ""
ocMA.Aposticha2.text = oc.m5.d4_gr_GR_cog.ocMA.Aposticha2.text

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode5
ocMA.Aposticha3.melody = ""
ocMA.Aposticha3.text = "Εὐλογημένος ὁ στρατὸς τοῦ Βασιλέως τῶν οὐρανῶν· εἰ γὰρ καὶ γηγενεῖς ὑπῆρχον οἱ Ἀθλοφόροι, ἀλλὰ ἀγγελικὴν ἀξίαν ἔσπευδον φθάσαι, τῶν σωμάτων καταφρονήσαντες, καὶ διὰ τῶν παθημάτων, τῆς τῶν Ἀσωμάτων ἀξιωθέντες τιμῆς. Εὐχαῖς αὐτῶν, Κύριε, κατάπεμψον ἡμῖν τὸ μέγα ἔλεος."

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode5
ocMA.AposTheotokion.melody = ""
ocMA.AposTheotokion.text = "Παῦσον τὸ ἄλγος τῆς πολυωδύνου ψυχῆς μου, ἐβόα ἡ Ἄμεμπτος ὀδυρομένη Χριστῷ· εἰ γὰρ βροτοὺς ἐν τῷ πάθει σῴζεις, ἀλλὰ τιτρώσκεις ψυχὴν ἐμὴν Λόγε. Σὺ φῶς μου γλυκύτατον, σὺ τέκνον καὶ πλάστης μου, ἀνυμνῶ σου τὸ μακρόθυμον."

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode5
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.text = oc.m5.d2_gr_GR_cog.ocMA.ApolTheotokion.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode5
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.text = oc.m5.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode5
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.text = "Θανατωθεὶς ἐπὶ Σταυροῦ, τοὺς διὰ βρώσεως πεσόντας ἡμᾶς, ἀνεζώωσας, Ζωοδότα οἰκτίρμον, καὶ πολίτας πάλιν Ἀγαθέ, παραδείσου ἀνέδειξας."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode5
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.text = "Προσηλώθης τῷ Σταυρῷ, Χριστὲ ἡ ἄμπελος ἡ ἀληθινή, καὶ ἀπέσταξας πόμα σωτηριῶδες, ἐπευφραῖνον πάντων τῶν πιστῶν, καρδίας ὑμνούντων σε."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode5
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.text = "Τυμπανιζόμενοι σοφοί, καὶ μεληδὸν κατατεμνόμενοι, εἰκονίσατε τὴν σφαγὴν τοῦ Δεσπότου· διὰ τοῦτο Μάρτυρες Χριστοῦ, ἀεὶ μακαρίζεσθε."

ocLI.BeatitudesGlory.mode = misc_gr_GR_cog.Mode5
ocLI.BeatitudesGlory.melody = ""
ocLI.BeatitudesGlory.text = "Ἐπὶ Σταυροῦ ὁ τοῦ Πατρός, καὶ θείου Πνεύματος ἀχώριστος, ἐθελούσιον καθυπέμεινε πάθος, πάθη πάντα τὰ φθοροποιά, ἡμῶν ἐξαιρούμενος."

ocLI.BeatitudesBoth.mode = misc_gr_GR_cog.Mode5
ocLI.BeatitudesBoth.melody = ""
ocLI.BeatitudesBoth.text = "Θρήνοις συνέκοπτες σαυτήν, ὁρῶσα Δέσποινα ἐν ξύλῳ Σταυροῦ, τὴν ζωὴν ἡμῶν ἑκουσίως θανοῦσαν· ὅθεν πάντες θείαις σε φωναῖς, ἀεὶ μακαρίζομεν."

//Octoechos in Triodion

