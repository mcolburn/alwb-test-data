octrMA.Kathisma11.mode = misc_gr_GR_cog.Mode5
octrMA.Kathisma11.melody = ocMA.Kathisma21.melody
octrMA.Kathisma11.text = ocMA.Kathisma21.text

octrMA.Kathisma12.mode = misc_gr_GR_cog.Mode5
octrMA.Kathisma12.melody = ocMA.Kathisma21.melody
octrMA.Kathisma12.text = ocMA.Kathisma22.text

octrMA.Kathisma1T.mode = misc_gr_GR_cog.Mode5
octrMA.Kathisma1T.melody = oc.m5.d6_gr_GR_cog.ocMA.Kathisma13.melody
octrMA.Kathisma1T.text = oc.m5.d6_gr_GR_cog.ocMA.Kathisma13.text

octrMA.Ode7Martyricon.mode = misc_gr_GR_cog.Mode5
octrMA.Ode7Martyricon.melody = ""
octrMA.Ode7Martyricon.text = "Τοὺς Ἀθλοφόρους σου Κύριε, μεγάλους ἀντιλήπτορας ἀνέδειξας, Σωτὴρ τῇ Οἰκουμένῃ, δι' ὧν τὰ πάθη φυγαδεύονται· ταῖς αὐτῶν πρεσβείαις σῶσον τὰς ψυχὰς ἡμῶν."

octrMA.Photogogic.mode = misc_gr_GR_cog.Mode5
octrMA.Photogogic.text = oc.m5.d2_gr_GR_cog.octrMA.Photagogic.text
