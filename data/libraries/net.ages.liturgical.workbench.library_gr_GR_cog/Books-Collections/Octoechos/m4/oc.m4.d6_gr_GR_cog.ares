A_Resource_Whose_Name = oc.m4.d6_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera1.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera1.text = "Ὅτε σε σταυρούμενον, ἡ κτίσις πᾶσα ἑώρακεν, ἠλλοιοῦτο καὶ ἔτρεμεν, ἡ γῆ δὲ ἐσείετο, ὅλη κλονουμένη, μακρόθυμε Λόγε, τὸ καταπέτασμα ναοῦ, φόβῳ ἐσχίσθη ὑβριζομένου σου, καὶ πέτραι διερράγησαν, ἀπὸ τοῦ φόβου καὶ ἥλιος, τὰς ἀκτῖνας συνέστειλε, ποιητήν σε γινώσκοντα."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera2.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera2.text = "Ῥήγνυται χειρόγραφον, τὸ ἀπ' αἰῶνος νυγείσης σου, τῆς πλευρᾶς πολυέλεε, Ἀδὰμ τοῦ προπάτορος, καὶ ἡ ἀπωσμένη, φύσις τῶν ἀνθρώπων, ῥανίσιν αἵματος τοῦ σοῦ, καθαγιάζεται ἀνακράζουσα· Δόξα τῇ δυναστείᾳ σου, δόξα τῇ θείᾳ σταυρώσει σου, Ἰησοῦ παντοδύναμε, ὁ Σωτὴρ τῶν ψυχῶν ἡμῶν."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera3.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera3.text = "Πῶς ὁ ἀνομώτατος, δῆμος τολμᾷ κατακρῖναί σε, τὸν Κριτὴν τὸν ἀθάνατον, τὸν νόμον χαράξαντα, ἐν ἐρήμῳ πάλαι, Μωσῇ τῷ θεόπτῃ; πῶς ἐπὶ ξύλου τὴν ζωήν, τὴν τῶν ἁπάντων ὁρῶντες θνήξασαν, οὐδόλως ἐπτοήθησαν, ουδ' ἐπὶ νοῦν ἐλογίσαντο, ὅτι σὺ μόνος Κύριος, καὶ Δεσπότης τῆς κτίσεως;"

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera4.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera4.text = "Δαιμόνων κλονούμενον, ταῖς προσβολαῖς καὶ εἰς βάραθρον, ἀπωλείας ὠθούμενον, οἰκτείρησον Δέσποινα, καὶ στερέωσόν με, ἀρετῶν ἐν πέτρᾳ καὶ τὰς βουλὰς τῶν δυσμενῶν, διασκεδάσασα καταξίωσον, ποιεῖν με τὰ προστάγματα, τοῦ σοῦ Υἱοῦ καὶ Θεοῦ ἡμῶν, ὅπως τύχω ἀφέσεως, ἐν ἡμέρᾳ τῆς κρίσεως."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera5.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera5.text = "Λῃσταῖς περιέπεσα, τοῖς μιαιφόνοις Πανάμωμε, καὶ τὸ ἄφθαρτον ἔνδυμα, τῆς ἄνω λαμπρότητος, ταῖς τούτων ἐφόδοις, ἐξεδύθην Κόρη, καὶ ὑπ' αὐτῶν ἀνηλεῶς, ἐτραυματίσθην καὶ καταβέβλημαι, ἐν τόπῳ τῆς κακώσεως, ἡμιθανὴς ἀλλὰ πρόφθασον. Καὶ τὴν χεῖρά σου ἔκτεινον, καὶ ἀνάστησον δέομαι."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode4
ocVE.Stichera6.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.Stichera6.text = "Ἐν κλίνῃ κατάκειμαι, τῆς ἀμελείας Πανάμωμε, καὶ ῥαθύμως διέρχομαι, τὸν βίον καὶ δέδοικα, τὸν τῆς τελευτῆς μου, καιρὸν Θεοτόκε, μὴ ὥσπερ λέων ἀφειδῶς, διασπαράξῃ ὁ ὄφις ὁ τὴν ταπεινήν μου ψυχήν, παμπόνηρος· διὸ τῇ σῇ ἀγαθότητι, πρὸ τοῦ τέλους προφθάσασα, πρὸς μετάνοιαν, ἔγειρον."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode4
ocVE.SticTheotokion.melody = he.a.m4_gr_GR_cog.EdokasSimeiosin.name
ocVE.SticTheotokion.text = "Ἰδών σε ὁ ἥλιος, καὶ ἡ σελήνη φιλάνθρωπε, ἐπὶ ξύλου κρεμάμενον, ἀκτῖνας ἀπέκρυψαν, τῆς δικαιοσύνης, Ἤλιε Χριστέ μου, καὶ τὰ θεμέλια τῆς γῆς, διεδονήθη φόβῳ τοῦ κράτους σου, ἡ Μήτηρ σου τὰ σπλάγχνα δέ, τιτρωσκομένη ἐβόα σοι· Ἰησοῦ ὑπεράγαθε, δόξα τῇ εὐσπλαγχνίᾳ σου."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode4
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m4.d4_gr_GR_cog.ocVE.Aposticha1.text

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode4
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.text = oc.m4.d4_gr_GR_cog.ocVE.Aposticha2.text

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode4
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.text = oc.m4.d2_gr_GR_cog.ocVE.Aposticha3.text

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode4
ocVE.AposTheotokion.melody = he.a.m4_gr_GR_cog.OExYpsistouKlitheis.name
ocVE.AposTheotokion.text = "Μὴ ἐποδύρου μου Μῆτερ καθορῶσα, ἐν ξύλῳ κρεμάμενον, τὸν σὸν Υἱὸν καὶ Θεόν, τὸν ἐφ' ὑδάτων κρεμάσαντα, τὴν γῆν ἀσχέτως, καὶ πᾶσαν κτίσιν δημιουργήσαντα· καὶ γὰρ ἀναστήσομαι, καὶ δοξασθήσομαι, καὶ τὰ τοῦ ᾍδου βασίλεια, συντρίψω σθένει, καὶ ἀφανίσω τούτου τὴν δύναμιν, καὶ τοὺς δεσμίους ἐκλυτρώσομαι, τῆς αὐτοῦ κακουργίας ὡς εὔσπλαγχνος, καὶ Πατρὶ τῷ ἰδίῳ, προσαγάγω ὡς φιλάνθρωπος."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode4
ocVE.ApolTheotokionVM.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
ocVE.ApolTheotokionVM.text = oc.m4.d6_gr_GR_cog.ocMA.Kathisma23.text

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.text = oc.m4.d4_gr_GR_cog.ocMA.Kathisma11.text

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma12.melody = misc_gr_GR_cog.Automelon
ocMA.Kathisma12.text = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.text

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma13.melody = he.a.m4_gr_GR_cog.OYpsotheisEnToStavro.name
ocMA.Kathisma13.text = "Τὴν ἐθελούσιον σφαγην τοῦ Δεσπότου, κατανοοῦσα ἡ Ἄγνη καὶ Παρθένος, ὀδυρομένη ἔλεγε δακρύουσα· Τέκνον ποθεινότατον, πῶς θανεῖν ᾑρετίσω; πῶς δὲ τεθανάτωσαι, τῶν κακούργων ἐν μέσῳ, ὁ κατὰ φύσιν δίκαιος Θεός; Δοξολογῶ σου, Υἱὲ τὸ μακρόθυμον."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma21.melody = misc_gr_GR_cog.Automelon
ocMA.Kathisma21.text = he.a.m4_gr_GR_cog.TachyProkatalave.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma22.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
ocMA.Kathisma22.text = oc.m4.d4_gr_GR_cog.ocMA.Kathisma22.text

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma23.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
ocMA.Kathisma23.text = "Παρθένε πανάμωμε, Μήτηρ Χριστοῦ τοῦ Θεοῦ, ῥομφαία διῆλθέ σου, τὴν παναγίαν ψυχήν, ἡνίκα σταυρούμενον, ἔβλεψας ἑκουσίως, τὸν Υἱὸν καὶ Θεόν σου, ὅν περ εὐλογημένη, δυσωποῦσα μὴ παύσῃ, συγχώρησιν πταισμάτων, ἡμῖν δωρήσασθαι."

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma31.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
ocMA.Kathisma31.text = "Σταυρὸν καὶ τὸν θάνατον, ὑπὲρ ἡμῶν ἀγαθέ, δι' ἄμετρον ἔλεος, ἑκὼν ὑπέμεινας, καὶ κρίσιν τὴν ἄδικον, ὅπως τῆς καταδίκης, καὶ ἀρχαίας κατάρας, πάντας ἐλευθερώσῃς, τοὺς ἀπάτῃ φθαρέντας· διό σου προσκυνοῦμεν, Λόγε τὴν Σταύρωσιν."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.text = oc.m4.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode4
ocMA.Kathisma33.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
ocMA.Kathisma33.text = "Σταυρῷ σε ὑψούμενον, ὡς ἐθεάσατο ἡ Ἄχραντος Μήτηρ σου, Λόγε Θεοῦ μητρικῶς, θρηνοῦσα ἐφθέγγετο· Τί τὸ καινὸν καὶ ξένον, τοῦτο θαῦμα Υἱέ μου; πῶς ἡ ζωὴ τῶν ὅλων, ὁμιλεῖς τῷ θανάτῳ; ζωῶσαι τοὺς θανέντας, θέλων ὡς εὔσπλαγχνος;"

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν Σταυροθεοτοκίων, Σταυρῷ παγείς, ὁ Χριστός, τὴν πλάνην λύει. Ἰωσήφ."

ocMA.C2Poet = poets_gr_GR_cog.Theofanous

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m4_gr_GR_cog.AnoixoToStomaMou.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C11.text = "Σταυρῷ διεπέτασας, θείας παλάμας μακρόθυμε, καὶ τὸν ἀπολλύμενον, κόσμον ἐκάλεσας, πρὸς ἐπίγνωσιν, τοῦ κράτους σου οἰκτίρμον· ὅθεν μεγαλύνομεν, τὴν εὐσπλαγχνίαν σου."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C12.text = "Τὸν ὄφιν ἀνύψωσεν, ὁ Μωϋσῆς εἰκονίζοντα, τὴν θείαν σου σταύρωσιν, Λόγε προάναρχε, δι' ἧς πέπτωκεν, ὁ ἰοβόλος ὄφις, τοῦ πτώματος πρόξενος, Ἀδὰμ γενόμενος."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C13.text = "Ἁγίων λαμπρότητας, νῦν κατοικεῖν ἠξιώθητε, ἀσάλευτον Μάρτυρες, παραλαβόντες σαφῶς, ὡς ἐδήλωσεν, ὁ Παῦλος Βασιλείαν, καὶ δόξης συμμέτοχοι, Χριστοῦ γεγόνατε."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C14.text = "Ὑψούμενα κύματα, τῶν ἀνυποίστων κολάσεων, ὑμῶν οὐκ ἐβύθισε, τὸ σκάφος Μάρτυρες· κυβερνήσει γάρ, τοῦ πάντων Βασιλέως, λιμένας ἐφθάσατε, τῆς ἀναπαύσεως."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C1T.text = "Ῥομφαία ὡς ἔφησεν, ὁ Συμεὼν τὴν καρδίαν σου, διῆλθεν ὡς ἔβλεψας, Χριστὸν σταυρούμενον, μόνον Δέσποινα, νυττόμενον τῇ λόγχῃ· ὅθεν ἀλαλάζουσα, πόνους ὑπέφερες."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C21.text = "Ὁ ὢν φύσει ἄκτιστος, καὶ τῷ Πατρὶ συναΐδιος, καὶ χρόνων ὑπέρτερος, καὶ προαιώνιος, ἐκ σοῦ Δέσποινα, κτιστὸς καὶ ὑπὸ χρόνον, ὡς ἄνθρωπος γίνεται, σῴζων τόν ἄνθρωπον."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C22.text = "Σὺ μόνη Θεόνυμφε, τὸν ἐν ὑψίστοις καθήμενον, ἀγκάλαις ἐβάστασας, σάρκα γενόμενον· σὺ γὰρ πέφηνας, ἐκ πάντων τῶν αἰώνων, δοχεῖον ἐπάξιον, τοῦ Παντοκράτορος."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m4_gr_GR_cog.AnoixoToStomaMou.name
ocMA.OdeW1C23.text = "Μαρίαν τὴν Ἄχραντον, δεῦτε ὑμνήσωμεν ἅπαντες, ὡς μόνην κυήσασαν, Χριστὸν τὸν Κύριον, τὸν καινίσαντα, τὴν ἀνθρωπίνην φύσιν, Παρθένον τε μείνασαν ἀδιαλώβητον."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m4_gr_GR_cog.TousSousYmnologous.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
ocMA.OdeW3C11.text = "Ὡς πρόβατον εἷλκε πρὸς σφαγήν σε, λαὸς ὁ παράνομος Χριστέ, Ἀμνὸν Θεοῦ ὑπάρχοντα, καὶ θέλοντα λυτρώσασθαι, λύκου πικροῦ τὰ πρόβατα, ἃ φιλανθρώπως ἠγάπησας."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
ocMA.OdeW3C12.text = "Παρέστης κριτὸς κριτῇ ἀδίκῳ, ὁ κρίνων δικαίως πᾶσαν γῆν , καὶ ἤνεγκας ῥαπίσματα, ἐλευθερῶσαι θέλων με, δεδουλωμένον Κύριε, τῷ πονηρῷ κοσμοκράτορι."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
ocMA.OdeW3C13.text = "Ἀθλήσαντες Ἅγιοι νομίμως, ἀνόμους ᾐσχύνατε ἐχθρούς, καὶ γνώμῃ θανατούμενοι, διὰ τὴν πάντων ἔγερσιν, τὸν τοῦ θανάτου πρόξενον, κατεπαλαίσατε δράκοντα."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
ocMA.OdeW3C14.text = "Γῆθεν ἀνυψώθησαν εὐκλείας, ἤδη ἐπιβάντες ἀληθοῦς, μαρτυρικῶς οἱ Μάρτυρες, καὶ ταῖς ἀΰλοις Τάξεσιν, οἱ ὑλικοὶ ἡνώθησαν, χαρᾶς ἀρρήτου πληρούμενοι."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m4_gr_GR_cog.TousSousYmnologous.name
ocMA.OdeW3C1T.text = "Ἐκ σοῦ ἡ ἀνάπλασις τῆς Εὔας, ὡράθη Παρθένε ἀληθῶς, Θεὸς σαρκὶ τικτόμενος, καὶ ἐν Σταυρῷ ὑψούμενος, καὶ καταράσσων δαίμονας, θεοχαρίτωτε Δέσποινα."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
ocMA.OdeW3C21.text = "Ὁ διαπλάσας, κατ' ἀρχὰς ἐκ χοός με τὸν ἄνθρωπον, πλαστουργεῖται δι' ἐμέ, ἐν τῇ γαστρί σου Πανάμωμε, τὴν πάλαι κατάπτωσιν, ἐπανορθούμενος."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
ocMA.OdeW3C22.text = "Κράτος θανάτου, καὶ κατάρας ἀρχαίας ἀπόφασιν, συλλαβοῦσα τὴν ζωήν, Θεογεννῆτορ κατήργησας· διό σου τὴν ἄχραντον, σέβομεν γέννησιν."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
ocMA.OdeW3C23.text = "Σὲ προστασίαν, ἀσφαλῆ Θεομῆτορ κεκτήμεθα, τὰς ἐλπίδας ἐπὶ σοί, ἀνατιθέντες σῳζόμεθα, πρὸς σὲ καταφεύγοντες, περιφρουρούμεθα."

ocMA.OdeW3C24.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW3C24.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C24.melody = he.h.m4_gr_GR_cog.OukEnSofia.name
ocMA.OdeW3C24.text = "Σὲ τὴν τεκοῦσαν, τὸν Θεὸν Θεοτόκον δοξάζομεν, τῷ γεννήματι τῷ σῷ, προσηγορίαν ἁρμόζοντες, καὶ κλῆσιν κατάλληλον, Πάναγνε Δέσποινα."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m4_gr_GR_cog.TinAnexichniaston.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
ocMA.OdeW4C11.text = "῞Ινα ἀπολύσῃς με ἐκ τῶν δεσμῶν, τῶν τῆς ἁμαρτίας φιλάνθρωπε, ἐθελουσίως ἐδεσμεύθης καὶ Σταυρῷ, ὥσπερ κακοῦργος τέθνηκας· Δόξα τῇ πολλῇ εὐσπλαγχνίᾳ σου."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
ocMA.OdeW4C12.text = "Στίγματα ὑπήνεγκας Λόγε Θεοῦ, καὶ τὸν ἐπονείδιστον θάνατον, ἀθανατίζων, τὴν οὐσίαν τῶν βροτῶν, θανατωθεῖσαν πάθεσι. Δόξα τῇ πολλῇ εὐσπλαγχνίᾳ σου."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
ocMA.OdeW4C13.text = "Οἱ τὴν ἀγαλλίασιν τὴν τοῦ Θεοῦ, μέλλοντες κληροῦσθαι ἐν Πνεύματι, τῷ Παναγίῳ, ἀγαλλόμενοι ψυχῇ, τοὺς αἰκισμοὺς ὑπήνεγκαν, καὶ τὸν βιαιότατον θάνατον."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
ocMA.OdeW4C14.text = "Χεῖρας ἀφαιρούμενοι καὶ κεφαλάς, γλῶσσάν τε κοπτόμενοι Μάρτυρες, τὴν θεολόγον, καὶ στερούμενοι φωτός, καὶ μεληδὸν τεμνόμενοι, ἄτμητοι Χριστῷ διεμείνατε."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m4_gr_GR_cog.TinAnexichniaston.name
ocMA.OdeW4C1T.text = "Ῥήγνυται χειρόγραφον τὸ τοῦ Ἀδάμ, λόγχῃ νυττομένου σου Δέσποτα, ἡ Θεοτόκος, ἀνεβόα τῷ Σταυρῷ , παρισταμένη Κύριε, καὶ ὀδυνηρῶς ἀλαλάζουσα."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
ocMA.OdeW4C21.text = "Ὡραιώθης ὑπὲρ πᾶσαν, των Ἀγγέλων εὐπρέπειαν, ὡς τεκοῦσα τούτων, Κόρη ποιητήν τε καὶ Κύριον, ἐκ σῶν ἀχράντων αἱμάτων σωματούμενον, τὸν ῥυόμενον, πάντας αὐτόν τοὺς δοξάζοντας."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
ocMA.OdeW4C22.text = "Ῥύπον πάντα τῆς προτέρας, ἀποθέμενοι βρώσεως, τῆς ζωῆς τὸν ἄρτον, τὸν ἐξ οὐρανοῦ σιτιζόμεθα, τὸν ἐκ τῆς γῆς τῆς Παρθένου ἀνατείλαντα, ἣν ὡς πρόξενον, τῶν ἀγαθῶν ἀνυμνήσωμεν."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
ocMA.OdeW4C23.text = "Ἡ ἁγία Θεοτόκος, ἡ σκηνὴ ἡ ἀμόλυντος, τοῦ φωτὸς ἡ πύλη, τράπεζα καὶ στάμνος ἡ πάγχρυσος, τὸ ἀλατόμητον ὄρος καὶ κατάσκιον, ἡ χωρήσασα, τὸν πλαστουργὸν μακαρίζεται."

ocMA.OdeW4C24.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW4C24.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C24.melody = he.h.m4_gr_GR_cog.OKathimenosEnDoxi.name
ocMA.OdeW4C24.text = "Χαρισμάτων σου τὰ ῥεῖθρα, καὶ πηγαὶ τῶν ἰάσεων, καὶ τοῦ θείου πλούτου, αἱ διανομαὶ διαδίδονται, ζωαρχικῆς ἐκ παλάμης Θεονύμφευτε, τοῦ τεχθέντος, ἐκ τῆς σῆς γαστρὸς Παντοκράτορος."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m4_gr_GR_cog.ExestiTaSympanta.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
ocMA.OdeW5C11.text = "Ἰδών σε ὁ ἥλιος, ἐπὶ Σταυροῦ ὑψούμενον, Δέσποτα συνέστειλεν ἀκτῖνας, φαίνειν μὴ σθένων, σοῦ Σῶτερ δύναντος, καὶ φωταγωγοῦντος τοὺς νυκτί, τῆς πλάνης καθεύδοντας, προσκυνοῦντας τὸ κράτος σου."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
ocMA.OdeW5C12.text = "Σταυροῦσαι καὶ σῴζεις με, δι' εὐσπλαγχνίαν Κύριε, ὄξους καὶ χολῆς δέχῃ τὴν γεῦσιν, τῆς ἐνηδόνου, ἀπολυτρούμενος, γεύσεως ἡμᾶς ὡς ἀγαθός, δι' ἧς ἡπατήθημεν, καὶ φθορᾷ ὑπεπέσαμεν."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
ocMA.OdeW5C13.text = "Τῆς πλάνης ἐλύσατε, χειμῶνα θεῖοι Μάρτυρες, Πνεύματος ἁγίου ἐπιθάλψει, καὶ πρὸς τὸ ἔαρ, τῆς ἀναπαύσεως, χαίροντες ἐφθάσατε ὁμοῦ, πάντων προϊστάμενοι, ὑπαρχόντων ἐν θλίψεσιν."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
ocMA.OdeW5C14.text = "Ὄμβροις θείου αἵματος, τὴν γῆν πᾶσαν ἠρδεύσατε, ἐναποξηράναντες τοὺς ὄμβρους, τῆς ἀθεΐας, ἅγιοι Μάρτυρες· ὅθεν πρὸς τὸ ὕδωρ τῆς ζωῆς, νῦν κατεσκηνώσατε, ὑπὲρ πάντων πρεσβεύοντες."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m4_gr_GR_cog.ExestiTaSympanta.name
ocMA.OdeW5C1T.text = "Σαρκὶ ὃν ἐκύησας, Χριστὸν θεοχαρίτωτε, ξύλῳ ἀναρτώμενον ὡς εἶδες, θρήνων ἐπλήσθης, καὶ τὸ μακρόθυμον, τούτου κατεπλήττου ἀληθῶς· ὅθεν ἐμεγάλυνες, τὴν αὐτοῦ συγκατάβασιν."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
ocMA.OdeW5C21.text = "Ἀνατείλας Ἥλιος ἐκ σοῦ ὁ νοητός, τηλαυγεῖς τὰς μαρμαρυγάς, τῆς αὐτοῦ Θεότητος, πᾶσιν ἐφήπλωσε, Θεοτόκε Δέσποινα· διὸ πάντες σε δοξάζομεν."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
ocMA.OdeW5C22.text = "Ἀληθῆ Θεὸν ἡμῖν τεκοῦσαν σαρκικῶς, Πατρὸς τὸν μονογενῆ, Θεοτόκον πάναγνον, σὲ ὀνομάζομεν, τὴν φωνὴν κατάλληλον, προσαρμόζοντες τῷ τόκῳ σου."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m4_gr_GR_cog.AseveisOukOpsontai.name
ocMA.OdeW5C23.text = "Συμφυὴς καὶ σύμμορφος ὑπάρχων τῷ Πατρί, ὁ Υἱὸς ὁ μονογενής, τοῖς ἀνθρώποις γέγονεν, θέλων ὁμόφυλος, σαρκωθεὶς ὁ Ὕψιστος, ἐκ γαστρός σου Μητροπάρθενε."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m4_gr_GR_cog.TinTheianTaftin.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
ocMA.OdeW6C11.text = "Τοῖς πόνοις οὕς περ ὑπέμεινας, σταυρούμενος τοὺς πόνους κατέπαυσας, τῆς ἀνθρωπότητος, καὶ πρὸς τὴν ἄπονον ἅπαντας, διαγωγὴν μετάγεις, εὔσπλαγχνε Κύριε."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
ocMA.OdeW6C12.text = "Ἡλίου σέλας ἐκρύπτετο, ναοῦ δὲ ἡ φαιδρότης ἐρρήγνυτο, γῆ κατεσείετο, πέτραι σὺν φόβῳ ἐσχίζοντο, ἐπὶ Σταυροῦ τὸν Κτίστην, ὁρᾷν μὴ σθένουσαι."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
ocMA.OdeW6C13.text = "Νεκρὸς ὁ ὄφις ἐγένετο, βασάνοις νεκρουμένους θεώμενος, τοὺς θείους Μάρτυρας, καὶ τὴν ζωὴν τὴν αἰώνιον, κληρονομοῦντας ὄντως, τῇ θείᾳ χάριτι."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
ocMA.OdeW6C14.text = "Πολλὰς βασάνους ὑπέστητε, πολλῶν καὶ τῶν ἐπάθλων ἐτύχετε, πολυαρίθμητα, πλήθη Μαρτύρων ἀείζῳα· διὸ τὰ τῶν κακῶν μου, πλήθη μειώσατε."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m4_gr_GR_cog.TinTheianTaftin.name
ocMA.OdeW6C1T.text = "Λιμὴν γενοῦ μοι Πανάμωμε, πελάγει τῶν δεινῶν θαλαττεύοντι, ἡ ναυαγήσασαν, πᾶσαν τὴν φύσιν τῷ τόκῳ σου, Θεογεννῆτορ Κόρη, νῦν διασώσασα."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
ocMA.OdeW6C21.text = "Ἁγνείας σε, καθαρὸν εὑρηκὼς ἐνδιαίτημα, ἐν γαστρί σου, καθαρῶς ὁ Δεσπότης ἐσκήνωσεν, ἐκκαθάραι θέλων, τῶν ἀνθρώπων Παρθένε τὸ φύραμα."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
ocMA.OdeW6C22.text = "Οὐράνωσας, γεωθεῖσαν τὴν φύσιν Πανάμωμε, τῶν ἀνθρώπων, καὶ φθαρεῖσαν αὐτὴν ἐκαινούργησας· διὰ τοῦτο μόνην, ἀσιγήτοις φωναῖς σε γεραίρομεν."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m4_gr_GR_cog.EvoiseProtypon.name
ocMA.OdeW6C23.text = "Συνέλαβες, ἐν γαστρὶ Θεομῆτορ τὸν ἄναρχον, Θεὸν Λόγον, δι' ἡμᾶς τοὺς ἀνθρώπους γενόμενον, καθ' ἡμᾶς ἀτρέπτως, ἐν δυσὶ γνωριζόμενον φύσεσιν."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m4_gr_GR_cog.OukElatrefsan.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
ocMA.OdeW7C11.text = "Ἀνετράπη, ὁ πολέμιος καὶ βέβληται, πτῶμα ἐξαίσιον, ἀνυψωθέντος Χριστοῦ, ἐν ξύλῳ, καὶ σέσωσται, ὁ πρὶν κατάκριτος, ἐκβοῶν αὐτῷ· ὁ τῶν Πατέρων Κύριος, καὶ Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
ocMA.OdeW7C12.text = "Νεκρωθέντα, διὰ ξύλου με ἐθέωσας, θανὼν ἐν ξύλῳ Χριστέ, καὶ θείοις τραύμασι σοῖς, ἰάσω τὰ τραύματα, τὰ τῆς καρδίας μου. Ὑπερύμνητε, ὁ τῶν Πατέρων Κύριος, καὶ Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
ocMA.OdeW7C13.text = "Ἠφανίζετο, τὰ σώματα πληττόμενα, ἀπείροις μάστιξιν, ἀλλ' ὄντως τὸ εὐπρεπές, ψυχῆς ὡραιότερον, ὑμῶν ἐδείκνυτο, Χριστομάρτυρες, ἀεὶ βοώντων· Κύριε, ὁ Θεός εὐλογητὸς εἶ."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
ocMA.OdeW7C14.text = "Ναυαγήσας, ὁ πολέμιος τοῖς αἵμασιν, ὑμῶν ἀπώλετο, σὺν μυριάσιν αὐτοῦ, ὑμεῖς δὲ πανεύφημοι, Μάρτυρες χαίροντες, ἀνεμέλπετε, ὁ τῶν Πατέρων Κύριος, καὶ Θεός εὐλογητὸς εἶ."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m4_gr_GR_cog.OukElatrefsan.name
ocMA.OdeW7C1T.text = "Νύμφη ἄμωμος, παλάτιον τοῦ κτίσαντος, θρόνος πυρίμορφος, καὶ ἀγεώργητος γῆ, ἐδείχθης Πανάμωμε· ὅθεν βοῶμέν σοι· Χαῖρε Δέσποινα, ἡ τοὺς βροτοὺς θεώσασα, θεϊκῇ κυοφορίᾳ."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
ocMA.OdeW7C21.text = "Ἀνακαλούμενος ἡμᾶς, αἰχμαλωτισθέντας Παρθένε, ὁ Λόγος ὁ ὢν ἐν ἀρχῇ, ἐν γαστρί σου σκηνώσας σεσάρκωται, ὁ φιλάνθρωπος Κύριος, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογημένος."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
ocMA.OdeW7C22.text = "Πύλη ἐφάνης νοητή, τῆς Ἀνατολῆς τῆς ἐξ ὕψους, ἐπιφανείσης ἐπὶ γῆς, τοῖς ἀνθρώποις ἐκ σοῦ Θεονύμφευτε, ὑπὲρ λόγον καὶ ἔννοιαν, τοῦ Θεοῦ τοῦ τῶν Πατέρων εὐλογημένου."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
ocMA.OdeW7C23.text = "Πεποικιλμένη ἀρεταῖς, τοῦ παμβασιλέως παρέστης, ἐκ δεξιῶν τοῦ ἐκ τῶν σῶν, σαρκωθέντος αἱμάτων Ἀνύμφευτε, ἐκτενῶς ἱκετεύουσα, λυτρωθῆναι ἀπὸ πάσης ἡμᾶς ἀνάγκης."

ocMA.OdeW7C24.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW7C24.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C24.melody = he.h.m4_gr_GR_cog.ODiasosasEnPyri.name
ocMA.OdeW7C24.text = "Ῥῆξον πταισμάτων τὰς σειράς, τῇ σῇ μητρικῇ παρρησίᾳ, τῶν εὐσεβῶς τε καὶ πιστῶς, μελῳδούντων Παρθένε τῷ Τόκῳ σου. Ὑπερύμνητε Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων εὐλογητὸς εἶ."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m4_gr_GR_cog.PaidasEvageis.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C11.text = "Λύεις τῶν δεσμῶν με τῶν χρονίων, ἐν χρόνῳ ὁ ἄχρονος γενόμενος, δεθεὶς δὲ θελήματι, καὶ τὸν ὑπερήφανον, δεσμοῖς ἀλύτοις Δέσποτα, παραπεμψάμενος, σῴζεις με Σταυρῷ καὶ τῷ πάθει· ὅθεν εὐλογῶ σε, Χριστὲ εἰς τοὺς αἰῶνας."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C12.text = "Ὑψώθης ἐν ξύλῳ ἑκουσίως, καὶ πᾶσαν τὴν κτίσιν συνανύψωσας, Λόγε ὑπερύμνητε, ἄναρχε ἀόρατε, ὁ τὰς ἀρχάς του σκότους τε, καὶ ἐξουσίας Χριστέ, τῷ πάθει σου παραδειγματίσας· ὅθεν σε ὑμνοῦμεν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C13.text = "Ἐπέβητε αἵμασιν οἰκείοις, ὡς ἅρματι Μάρτυρες παγκόσμιοι, καὶ πρὸς ὑπερκόσμια, ἤρθητε σκηνώματα, παρὰ Χριστοῦ ἐπάξια, γέρα λαμβάνοντες, καὶ Κύριον ὑμνεῖτε βοῶντες, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C14.text = "Ἰκρίῳ ὑψούμενοι καὶ βόθρῳ, ῥιπτόμενοι θηρσί τε διδόμενοι, πυρί τε καὶ ὕδατι, διαμεριζόμενοι, οἱ ἀθλοφοροι Μάρτυρες, χαίροντες ἔψαλλον· Τὸν Κύριον ὑμνεῖτε τὰ ἔργα, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C1T.text = "Ἰδοῦσα ὑπνώσαντα ἐν ξύλῳ, τὸν πᾶσιν ἐγρήγορσιν παρέχοντα, θείαν καὶ σωτήριον, Μήτηρ ἡ Πανάμωμος, ἀλαλαγμῷ ὠδύρετο καὶ ἀνεκραύγαζε· Τί τοῦτο τὸ καινότατον θαῦμα; ὁ ζωοποιήσας, τὰ πάντα θνῄσκει θέλων."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C21.text = "Ἀπάσαις ἀπρόσιτος ὑπάρχων, ὁ Κτίστης ταῖς ἀνωτέραις Κόρη τάξεσιν, ᾤκησε τὴν μήτραν σου, ἄφλεκτον τηρήσας σε, καὶ μετὰ τόκον, ᾧ νῦν κραυγάζομεν· Τὸν ἄφθορον, Κύριον ὑμνεῖτε τὰ ἔργα, καὶ ὑπερυψοῦτε, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C22.text = "Μώμων καθαρὰ καὶ μολυσμάτων, καὶ πάσης ἁγιωσύνης οὖσα τέμενος, Λόγον τὸν πανάγιον, πάντας ἁγιάζοντα, καὶ τῷ Πατρὶ συνάναρχον, ἐκυοφόρησας· διό σε τὴν Παρθένον ὑμνοῦμεν, καὶ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C23.text = "Στόματι καὶ γνώμῃ Θεοτόκον, φρονοῦντες ὁμολογοῦμέν σε Πανάμωμε· τέτοκας γὰρ Πάναγνε, σάρκα περιθέμενον, τὸν Ποιητὴν καὶ Κύριον, καὶ Βασιλέα Χριστόν· διό σε τὴν Παρθένον ὑμνοῦμεν, καὶ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C24.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW8C24.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C24.melody = he.h.m4_gr_GR_cog.PaidasEvageis.name
ocMA.OdeW8C24.text = "Σὲ τὴν κιβωτὸν τὴν παναγίαν, τὴν ἐπισκιαζομένην θείῳ Πνεύματι, τὴν τὸν προαιώνιον, Λόγον καὶ ἀΐδιον, δι' εὐσπλαγχνίαν ἄφατον, ἐνανθρωπήσαντα, τεκοῦσαν Θεοτόκον ὑμνοῦμεν, καὶ ὑπερυψοῦμεν, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m4_gr_GR_cog.ApasGigenis.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
ocMA.OdeW9C11.text = "Ἵστασο κριτός, ὁ κρῖναι τὰ σύμπαντα, μέλλων φιλάνθρωπε, στέφος ἀνεδήσω δέ, ἐθελουσίως, Σῶτερ ἀκάνθινον, παρακοῆς τὴν ἄκανθαν, Χριστὲ προθέλυμνον, ὑπεκτίλλων, πᾶσιν ἐμφυτεύων τε, τὴν τῆς σῆς εὐσπλαγχνίας ἐπίγνωσιν."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
ocMA.OdeW9C12.text = "Ὢ πῶς ὁ λαός, ὁ ἄδικος δίκαιον, ὄντα καὶ ἄμεμπτον, φθόνῳ σκοτιζόμενος, σὲ φωτοδότα, Σταυρῷ προδίδωσιν! οὗ περ τὸ πάθος ἥλιος, ὁρῶν σκοτίζεται, καὶ φαιδρότης, Ναοῦ διαρρήγνυται, καὶ δονεῖται τῆς γῆς τὰ θεμέλια."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
ocMA.OdeW9C13.text = "Σύμμορφοι παθῶν, Χριστοῦ χρηματίζετε, ἅγιοι Μάρτυρες, καὶ συγκληρονόμοι δέ, τῆς βασιλείας, καὶ τῆς λαμπρότητος· διὸ φωταγωγήσατε, τοὺς ὑμνῳδοὺς ὑμῶν, ἁμαρτίας, ζόφου ἀπαλλάττοντες, καὶ ποικίλων σοφοὶ περιστάσεων."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
ocMA.OdeW9C14.text = "Ἤδη πρὸς αὐτά, σοφοὶ τὰ οὐράνια, κατεσκηνώσατε, δόξης τε λαβόμενοι, τῆς ἀϊδίου, καὶ ταῖς μεθέξεσι, ταῖς ἱεραῖς θεούμενοι, μέμνησθε πᾶν πίστει τήν τῶν ἡμῶν, τῶν τιμώντων, πανίερον, καὶ σεπτὴν ὑμῶν μνήμην πανεύφημοι."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m4_gr_GR_cog.ApasGigenis.name
ocMA.OdeW9C1T.text = "Φώτισον Ἁγνή, τοὺς πόθῳ ὑμνοῦντάς σε, καὶ μεγαλύνοντας, λῦσον τῶν παθῶν ἡμῶν, τὸ σκότος Κόρη, παῦσον τὸν κλύδωνα, τοῦ πονηροῦ τὰ σκάνδαλα, ἐκ μέσου ποίησον, τοὺς τῆς Ἄγαρ, γόνους καθυπόταξον, Βασιλεῖ εὐσεβεῖ ταῖς πρεσβείαις σου."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
ocMA.OdeW9C21.text = "Ἄνθρακα τὸν νοητὸν Ἁγνὴ θεόνυμφε, Ἡσαΐας ὃν τεθέαται, φέρεις ἀγκάλαις Θεοτόκε, μορφῇ τῇ ἡμετέρᾳ ἑνούμενον, καὶ κόσμῳ σωτηρίαν πυρσεύοντα· ὅθεν σε πάντες μεγαλύνομεν."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
ocMA.OdeW9C22.text = "Σύμβολα Πανάμωμε τῆς σῆς γεννήσεως, οἱ Προφῆται προεκήρυξαν, πόρρωθεν ταῦτα μυηθέντες καὶ θείᾳ ἐπιπνοίᾳ τοῦ Πνεύματος, τῷ κόσμῳ διαπρυσίως κηρύξαντες, ὧν τὰς ἐκβάσεις νῦν θαυμάζομεν."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode4
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m4_gr_GR_cog.EvaMenToTisParakois.name
ocMA.OdeW9C23.text = "Ὡραῖος τῷ κάλλει ὁ καρπὸς καὶ ἔκλαμπρος, παρὰ πάντας τῆς κοιλίας σου· σὺ γὰρ Παρθένε Θεοτόκε, Θεὸν σεσαρκωμένον ἐγέννησας φανέντα, διὰ τὸ σῶσαι τὸν ἄνθρωπον· ὅθεν σε πάντες μεγαλύνομεν."

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode4
ocMA.Aposticha1.melody = ""
ocMA.Aposticha1.text = oc.m4.d4_gr_GR_cog.ocMA.Aposticha1.text

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode4
ocMA.Aposticha2.melody = misc_gr_GR_cog.Automelon
ocMA.Aposticha2.text = he.a.m4_gr_GR_cog.EdokasSimeiosin.text

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode4
ocMA.Aposticha3.melody = ""
ocMA.Aposticha3.text = oc.m4.d2_gr_GR_cog.ocMA.Aposticha3.text

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode4
ocMA.AposTheotokion.melody = he.a.m4_gr_GR_cog.OsGennaionEnMartysi.name
ocMA.AposTheotokion.text = "Ὡς ἑώρακε Κύριε, ἡ Παρθένος καὶ Μήτηρ σου, ἐν Σταυρῷ κρεμάμενον, ἐξεπλήττετο, καὶ ἀτενίζουσα ἔλεγεν· Υἱὲ ποθεινότατε, πῶς ἐν ξύλῳ τοῦ Σταυροῦ, ἀνηρτήθης μακρόθυμε; πῶς τὰς χεῖράς σου, καὶ τοὺς πόδας σου Λόγε προσηλώθης, ὑπ' ἀνόμων καὶ τὸ αἷμα, τὸ σὸν ἐξέχεας Δέσποτα;"

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode4
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.text = oc.m4.d3_gr_GR_cog.ocMA.Kathisma23.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode4
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.text = oc.m4.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode4
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.text = "Καθορῶσαί σε Σταυρῷ, τεταμένον μόνε Μακρόθυμε, αἱ Δυνάμεις οὐρανῶν, ἀποροῦσαι τρόμῳ ἐξίσταντο, ἡ γῆ ἐκυμαίνετο, καὶ φωστήρων κάλλος ἐναπεσβέννυτο. Ἀδάμ ὁ κατάκριτος, ἀδίκως φιλάνθρωπε κρινομένου σου, ἐδικαιοῦτο ὑμνῶν σου τὴν ἀγαθότητα."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode4
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.text = "Ἐν τῷ Κρανίῳ ὑψωθείς, τοῦ ἐχθροῦ τὴν κάραν συνέτριψας, ἐπὶ ξύλου δὲ θανών, τοὺς καρπῷ τοῦ ξύλου τεθνήξαντας, ἐζώωσας Δέσποτα, καὶ Παραδείσου πολίτας ἔδειξας, ἀπαύστως ὑμνοῦντάς σε, Χριστὲ ὁ Θεὸς ἡμῶν καὶ βοῶντάς σοι· Μνήσθητι καὶ ἡμῶν ἐν τῇ Βασιλείᾳ σου."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode4
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.text = "Ὥσπερ ὅπλον τὸν Σταυρόν, ἀναλαβόντες ἅγιοι Μάρτυρες, πρὸς παράταξιν ἐχθρῶν, λογισμῷ γενναίῳ ἐξήλθετε, καὶ τούτους ὀλέσαντες, ἀφθαρσίας στέφανον ἀνεδήσασθε, καὶ δόξης ἐτύχετε, καὶ θείας λαμπρότητος ἀξιάγαστοι· ὅθεν πίστει ὑμᾶς πάντες μακαρίζομεν."

ocLI.BeatitudesGlory.mode = misc_gr_GR_cog.Mode4
ocLI.BeatitudesGlory.melody = ""
ocLI.BeatitudesGlory.text = "Ἵνα δείξῃς ἐμφανῶς, τὴν πρὸς ἡμᾶς Σωτὴρ εὐσπλαγχνίαν σου, προσηλώθης τῷ Σταυρῷ , ὁ Πατρὶ συνὼν καὶ τῷ Πνεύματι, καὶ σπόγγον καὶ κάλαμον, ἐμπαιγμοὺς καὶ μάστιγας καθυπέμεινας, βουλόμενος ῥύσασθαι, πυρὸς αἰωνίζοντος τοὺς κραυγάζοντας· Μνήσθητι καὶ ἡμῶν ἐν τῇ Βασιλείᾳ σου."

ocLI.BeatitudesBoth.mode = misc_gr_GR_cog.Mode4
ocLI.BeatitudesBoth.melody = ""
ocLI.BeatitudesBoth.text = "Ὁ ἀχώρητος παντί, ἀστενοχωρήτως κατῴκησε, τὴν ἁγίαν σου νηδύν, Θεογεννῆτορ Ἄχραντε Δέσποινα, καὶ ξύλῳ νεκρούμενος, τὴν ζωὴν τῷ κόσμῳ ἐπήγασεν. Αὐτὸν οὖν ἱκέτευε, νεκρῶσαι τὸ φρόνημα τῆς σαρκὸς ἡμῶν, καὶ σῶσαι ἡμᾶς πάντας ὡς φιλάνθρωπος."

//Octoechos in Triodion

octrMA.Triadic1.mode = misc_gr_GR_cog.Mode4
octrMA.Triadic1.text = oc.m4.d2_gr_GR_cog.octrMA.Triadic1.text

octrMA.Triadic2.mode = misc_gr_GR_cog.Mode4
octrMA.Triadic2.text = oc.m4.d2_gr_GR_cog.octrMA.Triadic2.text

octrMA.Triadic3.mode = misc_gr_GR_cog.Mode4
octrMA.Triadic3.text = oc.m4.d2_gr_GR_cog.octrMA.Triadic3.text

octrMA.Kathisma11.mode = misc_gr_GR_cog.Mode4
octrMA.Kathisma11.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
octrMA.Kathisma11.text = ocMA.Kathisma22.text

octrMA.Kathisma12.mode = misc_gr_GR_cog.Mode4
octrMA.Kathisma12.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
octrMA.Kathisma12.text = "Τὸ φῶς τῶν ψυχῶν ἡμῶν, νῦν καθορῶντες Χριστέ, Σταυρόν σου τὸν ἄχραντον, καὶ προσκυνοῦντες αὐτόν, φαιδρῶς ἐκβοῶμέν σοι· Δόξα σοι ὁ ἐν τούτῳ, ὑψωθῆναι θελήσας, δόξα σοι ὁ φωτίσας, δι' αὐτοῦ πᾶσαν κτίσιν, ἐν ᾧ σε ἀκαταπαύστως, ὕμνοις δοξάζομεν."

octrMA.Kathisma1T.mode = misc_gr_GR_cog.Mode4
octrMA.Kathisma1T.melody = he.a.m4_gr_GR_cog.TachyProkatalave.name
octrMA.Kathisma1T.text = ocMA.Kathisma33.text

octrMA.Ode7Martyricon.mode = misc_gr_GR_cog.Mode4
octrMA.Ode7Martyricon.melody = ""
octrMA.Ode7Martyricon.text = "Ἡ σεπτὴ τῶν Ἀθλοφόρων πανήγυρις, Οὐρανὸν τὴν Ἐκκλησίαν ἔδειξε, καὶ τοῖς ἀνθρώποις συγχορεύουσιν Ἄγγελοι, ταῖς αὐτῶν ἱκεσίαις, Χριστὲ ὁ Θεός, σῶσον τάς ψυχὰς ἡμῶν."

octrMA.Photogogic.mode = misc_gr_GR_cog.Mode4
octrMA.Photogogic.text = oc.m4.d2_gr_GR_cog.octrMA.Photagogic.text
