octrMA.Ode7Martyricon.melody = ""
octrMA.Ode7Martyricon.text = ocMA.Kathisma32.text

octrMA.Photagogic.mode = misc_gr_GR_cog.Mode2
octrMA.Photagogic.text = "Τὸ φῶς σου τὸ ἀΐδιον, ἐξαπόστειλον Κύριε, καὶ φώτισον τὰ ὄμματα τὰ κρυπτὰ τῆς καρδίας μου,"
