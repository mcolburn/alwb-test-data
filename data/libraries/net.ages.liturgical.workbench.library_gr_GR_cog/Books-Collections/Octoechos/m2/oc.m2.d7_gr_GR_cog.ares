ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode2
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.text = oc.m2.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode2
ocMA.Kathisma22.melody = he.a.m2_gr_GR_cog.EfsplagchniasYparchousa.name
ocMA.Kathisma22.text = "Ὁ νεκρῶν καὶ τῶν ζώντων ὡς Θεός, ἐξουσίαν ἁπάντων ἐσχηκὼς Ζωοδότα, πρόσχες ταῖς δεήσεσι τῶν δούλων σου, δεῖξόν σου τὰ σπλάγχνα τὰ φιλάνθρωπα, καὶ δίδου τὴν ἄφεσιν ψυχαῖς, ἃς μετέστησας, ἐπ' ἐλπίδι τῇ εἰς σέ, ὁ δι' εὐσπλαγχνίαν ὑπεράγαθος."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode2
ocMA.Kathisma23.melody = ""
ocMA.Kathisma23.text = "Μνήσθητι Κύριε, ὡς ἀγαθὸς τῶν δούλων σου, καὶ εἴ τι ἐν βίῳ, ἥμαρτον συγχώρησον· οὐδεὶς γὰρ ἀναμάρτητος, εἰμὴ σὺ ὁ δυνάμενος, καὶ τοῖς μεταστᾶσι, δοῦναι τήν ἀνάπαυσιν."

ocMA.Kathisma24.mode = misc_gr_GR_cog.Mode2
ocMA.Kathisma24.melody = ""
ocMA.Kathisma24.text = oc.m2.d2_gr_GR_cog.ocMA.ApolTheotokion.text

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχίς ἄνευ τῶν Θεοτοκίων, ἐκτὸς τῆς θ' ᾠδῆς· Δούλ(οις ἔ)παινον γνησίοις Θεοῦ φέρω, ὁ Ἰωσήφ."

ocMA.C2Poet = poets_gr_GR_cog.Theofanous
ocMA.C2Acrostic.text = " οὗ ἡ ἀκροστιχίς· Τοῖς τεθνεῶσι δεύτερον πλέκω μέλος."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m2_gr_GR_cog.TinMoseosOdin.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
ocMA.OdeW1C11.text = "Διωγμοὺς χαλεπούς, καὶ αἰκισμοὺς σφοδροὺς καρτερικῶς, ἐνεγκόντες Ἀθληταί, πᾶσαν τὴν πλάνην ἐκ τῶν περάτων, σθένει θεϊκῷ ἐδιώξατε."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
ocMA.OdeW1C12.text = "Οἱ Θεοῦ λειτουργοί, καὶ Ἱεράρχαι φέγγει νοητῷ, ἀπαστράπτοντες σαφῶς, πρὸς φῶς ὡδήγησαν εὐσεβείας, πάντων εὐσεβῶν τὰ πληρώματα."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
ocMA.OdeW1C13.text = "Ὑπερήφανον νοῦν, ταπεινωθέντες Ὅσιοι καλῶς, κατεκλίνατε εἰς γῆν, καὶ ὑψωθέντες ἐνθέοις τρόποις, πᾶσι ταπεινοῖς βοηθεῖτε ἀεί."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
ocMA.OdeW1C14.text = "Λαμπροτάτου φωτός, καὶ αἰωνίου δεῖξον κοινωνούς, εὐφροσύνης ὡς Θεός, οὕς περ μετέστησας τῶν προσκαίρων, δούλους σου πιστούς, ὁ Θεὸς ἡμῶν."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m2_gr_GR_cog.TinMoseosOdin.name
ocMA.OdeW1C1T.text = "Αἱ ἀσκήσει στερρῶς, ἠγωνισμέναι καὶ ἀθλητικῶς, καθελοῦσαι τὸν ἐχθρόν, σεμναὶ Γυναῖκες Θεοκυῆτορ, σὲ περιεστῶσαι, εὐφραίνονται."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
ocMA.OdeW1C21.text = "Τῷ θανάτῳ σου, καταπατήσας θάνατον, θείας ζωῆς ἀνέβλυσας ἀϊδιότητα, ἣν παράσχου ἀγαθέ, ταῖς τῶν κοιμηθέντων ψυχαῖς, ταῖς τῶν Μαρτύρων σου, πρεσβείαις ἄφεσιν, πταισμάτων χαριζόμενος."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
ocMA.OdeW1C22.text = "Ὁ τὸ πλούσιον, ἀεὶ πηγάζων ἔλεος, τοῖς εὐσεβῶς φιλάνθρωπε προσδεχομένοις σε, ἐν τῷ οἴκῳ σου Χριστέ, ἐν τῇ θαυμαστῇ σου σκηνῇ, τόπον ἀνέσεως παράσχου Δέσποτα, τοῖς δούλοις σου ὡς εὔσπλαγχνος."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
ocMA.OdeW1C23.text = "Ἰσχυρότατος, κατὰ θανάτου γέγονας· ὅθεν αὐτὸν ἐδέσμευσας, ἡμᾶς ῥυσάμενος, καὶ νῦν ῥῦσαι τῆς αὐτοῦ, τοὺς κεκοιμημένους φθορᾶς ὡς ὑπεράγαθος, τῆς σῆς ἐλλάμψεως, τὴν μέθεξιν δωρούμενος."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m2_gr_GR_cog.ToTinAvaton.name
ocMA.OdeW1C2T.text = "Στερεώσασα, τὴν κλονουμένην φρένα μου, Μήτηρ Θεοῦ βεβαίωσον, θείοις προστάγμασι, τοῦ τεχθέντος ἐκ τῆς σῆς, καθηγιασμένης γαστρός, καὶ καταργήσαντος, τοῦ ᾅδου Δέσποινα, τὸ ζοφερὸν βασίλειον. "

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
ocMA.OdeW3C11.text = "Πυρωθέντες τῷ πυρί, τῆς ἀγάπης τοῦ Χριστοῦ οἱ Ἀθλοφόροι, τῶν βασανων τὴν πυράν, δροσισμῷ τοῦ παντουργοῦ, ἔσβεσαν Πνεύματος."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
ocMA.OdeW3C12.text = "Ἁγιώτατοι Χριστοῦ, Ἱεράρχαι καὶ σεπτοί, Ὁσίων δῆμοι, ὑπὲρ πάντων ἡμῶν, τὸν φιλάνθρωπον Θεόν, ἐκδυσωπήσατε."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
ocMA.OdeW3C13.text = "Ἱερώτατος χορός, τῶν ἐνθέων Προφητῶν ἐμεγαλύνθη, καὶ Γυναίων πληθύς, ἐναθλήσασα στερρῶς, δόξης ἐπέτυχον."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
ocMA.OdeW3C14.text = "Νεκρωθεὶς ἐπὶ Σταυροῦ, ἐδωρήσω τοῖς θνητοῖς ἀθανασίαν, ἧς τυχεῖν τοὺς πρὸς σέ, καταξίωσον Χριστέ, μεταχωρήσαντας."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m2_gr_GR_cog.SteirothentaMouTonNoun.name
ocMA.OdeW3C1T.text = "Μετὰ πάντων Προφητῶν, καὶ Γυναίων ἱερῶν, Θεὸν Παρθένε, τὸν τεχθέντα ἐκ σοῦ, δυσώπει ἐκτενῶς, τοῦ οἰκτειρῆσαι ἡμᾶς."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
ocMA.OdeW3C21.text = "Ταῖς τῆς σῆς ὡραιότητος λαμπηδόσιν, αὐγάζεσθαι τοὺς πίστει κεκοιμημένους, εὐδόκησον ὁ πλούσιος ἐν ἐλέει, μετὰ Μαρτύρων σου· σὺ γὰρ ὁ Θεὸς ἡμῶν, καὶ οὐκ ἔστι δίκαιος, πλήν σου Κύριε."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
ocMA.OdeW3C22.text = "Ἐν τόποις ἀναψύξεως, ὡς οἰκτίρμων, ἐν χώρᾳ Ἀβραάμ τε τοῦ ἐκλεκτοῦ σου, ἀξίωσον αὐλίζεσθαι τοὺς σοὺς δούλους, τοὺς σοὶ κραυγάζοντας· Σὺ εἶ ὁ Θεὸς ἡμῶν, καὶ οὐκ ἔστι δίκαιος, πλήν σου Κύριε."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
ocMA.OdeW3C23.text = "Θαλάμοις οὐρανίοις περιπολεύειν, φρονίμοις ταῖς παρθένοις συνεισελθόντας, ὑπόδεξαι τοὺς δούλους σου λαμπαδούχους, οὕς περ μετέστησας, τῷ σῷ βουλήματι, ἐκ τῶν προσκαίρων Δέσποτα, μόνε φιλάνθρωπε."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m2_gr_GR_cog.EnPetraMeTisPisteos.name
ocMA.OdeW3C2T.text = "Νεκρωθέντα, καὶ πρὸς γῆν με ἀποστραφέντα, ἀνέστησας τεκοῦσα τὸν ζωοδότην, ἀνήγαγες ἐξ ᾅδου με κατωτάτου, τὸν Θεοτόκον σε πίστει δοξάζοντα, καὶ τιμῶντα Πάναγνε, σὲ τὴν Πανύμνητον."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m2_gr_GR_cog.TinEkParthenou.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C11.text = "Οἱ τοῦ Χριστοῦ τὰ παθήματα, εὐκλεεῖς Ἀθλοφόροι, μιμησάμενοι καλῶς, κατατεινόμενοι ἔχαιρον, πολλαῖς βασάνοις, τὰς αἰωνιζούσας ἀντιδόσεις σκοποῦντες, ὧν τυχόντες, μακαρίζονται ἀεί."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C12.text = "Νόμους τηροῦντες τοῦ Πνεύματος, Ἱεράρχαι προφρόνως, ἐπωκείλατε λαούς, ὡς κυβερνῆται πανάριστοι, πρὸς θεῖον ὅρμον, καὶ μετατεθέντες ἐκ τῶν κάτω θορύβων, πρὸς γαλήνην μετηνέχθητε ζωῆς."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C13.text = "Γῆς παρεπίδημοι ὤφθητε, μεταθέντες Πατέρες, τὸ πολίτευμα ὑμῶν ἐν οὐρανοῖς, καθὼς γέγραπται, εὐσεβοφρόνως, καὶ σαρκὸς τὰ πάθη, χαλινώσαντες πόνοις ἐγκρατείας, τῇ δυνάμει τοῦ Χριστοῦ."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C14.text = "Νέκρωσιν θείαν ποθήσασαι, τὴν ζωὴν τὴν ἀγήρω, προξενοῦσαν ἀληθῶς, διὰ βασάνων καὶ θλίψεων, καὶ ἐγκρατείας, τίμιαι Γυναῖκες, ἠξιώθητε ταύτης, δυσωποῦσαι τὸν Χριστὸν ὑπὲρ ἡμῶν."

ocMA.OdeW4C15.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C15.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C15.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C15.text = "Ἣν μετεβίβασας Κύριε, τῶν προσκαίρων τοῦ βίου, πολυάριθμον πληθύν, ἐν ὀρθοδόξῳ λατρεύσασαν, Λόγε τῇ πίστει, πάσῃ σῳζομένων, τῇ πληθύϊ συνάψας, αἰωνίου καταξίωσον ζωῆς."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m2_gr_GR_cog.TinEkParthenou.name
ocMA.OdeW4C1T.text = "Ὄρος κατάσκιον βλέπει σε, ἀρεταῖς ὁ Προφήτης, Θεοτόκε Ἀββακούμ, ἐξ οὗ ἐπέφανεν ἀνθρώποις ὁ Θεός, ἀφράστως ἀρετῇ, καλύψας οὐρανούς, καὶ τὸ γένος ἀνθρώπων, διασώσας ἐκ φθορᾶς."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m2_gr_GR_cog.YmnoSeAkoi.name
ocMA.OdeW4C21.text = "Ἐλπίδι, καὶ ἀγάπῃ ζήσαντας, καὶ φρονήματι ὀρθοδόξῳ, τῆς ὑπὲρ νοῦν δόξης σου ἀξίωσον τοὺς σοὺς οἰκέτας Χριστέ, τῇ πολλῇ φιλανθρωπίᾳ σου, τῶν εὐκλεῶν Μαρτύρων ταῖς δεήσεσιν."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m2_gr_GR_cog.YmnoSeAkoi.name
ocMA.OdeW4C22.text = "Ὡς ἔχων, τῆς τρυφῆς ἀέναον, τὸν χειμάρρουν ἀεὶ ποτίζεις, τοὺς ἐκλεκτοὺς Κύριε, μεθ' ὧν καὶ νῦν τοὺς μεταστάντας Χριστέ, τῇ ἀφάτῳ εὐσπλαγχνίᾳ σου, παρὰ τὸ ὕδωρ θρέψον τῆς ἀφέσεως."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m2_gr_GR_cog.YmnoSeAkoi.name
ocMA.OdeW4C23.text = "Σὺ ζώντων, κυριεύεις Δέσποτα, καὶ θανάτου ἐξουσιάζεις. Σὺ τοὺς ἐκ γῆς χώματι, δυνάμει σου ἐξανέστησας· διὸ τοὺς πρὸς σὲ μεταφοιτήσαντας, ἐν ταῖς αὐλαῖς σου Σῶτερ κατασκήνωσον."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m2_gr_GR_cog.YmnoSeAkoi.name
ocMA.OdeW4C2T.text = "Ἰάσω, τὸ τῆς Εὔας σύντριμμα, καὶ τὴν πάλαι ταλαιπωρίαν· τὸν κτίστην γὰρ τέτοκας, τοὺς πτώματι καταρραγέντας ἡμᾶς, ἀνορθῶσαι τὸν δυνάμενον, Θεογεννῆτορ μόνη Μητροπάρθενε."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
ocMA.OdeW5C11.text = "Σὲ ποθοῦντες οἰκτίρμον, ἐμίσησαν πάντα κόσμον, οἱ γενναῖοι Ἀθλοφόροι, προσομιλοῦντες βασάνοις, ἀπαρνήσει τῆς σαρκός."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
ocMA.OdeW5C12.text = "Ἱεράρχαι Προφῆται, καὶ Ὅσιοι θεοφόροι, ταῖς τοῦ Πνεύματος ἀκτῖσι, φωταγωγοῦντες τὴν κτίσιν, σκότος λύετε παθῶν."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
ocMA.OdeW5C13.text = "Οἱ ὅσιοι Πατέρες, Προφῆται καὶ Ἱεράρχαι, καὶ ἀοίδιμοι Γυναῖκες, σοὶ τῷ Δεσπότῃ τῶν ὅλων, εὐηρέστησαν Χριστῷ."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
ocMA.OdeW5C14.text = "Ἱκετεύομεν Σῶτερ, οὓς ἐξ ἡμῶν προσελάβου, ἐν τῷ χορῷ τῶν ἐκλεκτῶν σου κατασκηνώσας, μετόχους δεῖξον κρείττονος ζωῆς."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m2_gr_GR_cog.TinAchlynTisPsychisMou.name
ocMA.OdeW5C1T.text = "Παναγία Παρθένε, τὸ καύχημα τῶν Μαρτύρων, καὶ Ὁσίων καὶ Δικαίων, πάσης ἡμᾶς τυραννίδος, ἐξελοῦ τῶν πειρασμῶν."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
ocMA.OdeW5C21.text = "Νενεκρωμένους ἡμᾶς, καὶ τεθειμένους ἐν φρουρᾷ, ἤγειρας ἐκ ζοφερῶν τοῦ ᾅδου κευθμώνων, ἁρπάσας ἀγαθέ, καὶ δέίξας ἁγίων Μαρτύρων στρατεύματα."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
ocMA.OdeW5C22.text = "Διὰ τὸ σῶσαι ἡμᾶς, ὡς παραγέγονας Χριστέ, δέξαι νῦν, τοὺς εὐσεβῶς πρὸς σὲ μεταστάντας, καὶ κόλποις Ἀβραάμ, καὶ μετὰ Λαζάρου αὐτοὺς κατασκήνωσον."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
ocMA.OdeW5C23.text = "Ἔλυσας Δέσποτα, τὸν χρόνιόν μου καὶ μακρὸν πόλεμον, τὸν πρὶν πρὸς σέ, μεσίτης καὶ πρέσβυς εἰρήνης γεγονώς, καὶ νῦν τοὺς σοὺς δούλους, οἰκτείρας ἀνάπαυσον."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m2_gr_GR_cog.OTouFotosChorigos.name
ocMA.OdeW5C2T.text = "Ὑπὸ τὴν σκέπην σου, οἱ πεποιθότες ἐπὶ σοί, ζήσονται. Μήτηρ Θεοῦ, τὸν γὰρ ζωοδότην ἐγέννησας ἡμῖν, τὸν ζωοποιοῦντα, τὰ πάντα βουλήματι."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m2_gr_GR_cog.VythoAmartimaton.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
ocMA.OdeW6C11.text = "Στερρότητι καρδίας, ἐχθρὸν οἱ ἀθληταί, καταπαλαίσαντες, τοῦτον κατέβαλον, καὶ ἔλαβον νίκης στέφος ἐκ Θεοῦ· διὸ ἐκτενῶς πρεσβεύουσι, τοῦ σωθῆναι νῦν ἡμᾶς."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
ocMA.OdeW6C12.text = "Χριστοῦ τοὺς Ἱεράρχας, τιμήσωμεν πιστῶς, καὶ τοὺς Ὁσίους αὐτοῦ μακαρίσωμεν, σῳζόμενοι ταῖς δεήσεσιν αὐτῶν, ἐκ πάσης ὀργῆς καὶ θλίψεως, καὶ κακίας τοῦ ἐχθροῦ."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
ocMA.OdeW6C13.text = "Ἐνήθλησε Γυναίων, ὁ ἔνθεος χορός, καὶ ἐν ἀσκήσει Θεὸν ἐθεράπευσε, καὶ ἔτυχε Βασιλείας οὐρανῶν. Αὐτῶν προσευχαῖς τὸν κόσμον σου, σῶσον μόνε Ἰησοῦ."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
ocMA.OdeW6C14.text = "Ὁ πλάσας ζωοδότα, τὸν ἄνθρωπον ἐκ γῆς, τοὺς ἐξ ἡμῶν μεταστάντας ἀνάπαυσον, τὴν ἄφεσιν παρεχόμενος αὐτοῖς Χριστέ, τῶν κακῶν ὡς εὔσπλαγχνος, καὶ οἰκτίρμων Ἰησοῦ."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m2_gr_GR_cog.VythoAmartimaton.name
ocMA.OdeW6C1T.text = "Ἁγία Θεοτόκε, ἁγίασον ἡμῶν, τοὺς λογισμούς, καὶ τὸ φρόνημα στήριξον, καὶ σῶσον ἐκ τῶν βελῶν τοῦ πονηροῦ, ἀτρώτους ἡμᾶς δοξάζοντας, σοῦ τὸ ἔλεος ἁγνή."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m2_gr_GR_cog.EnAvyssoPtaismaton.name
ocMA.OdeW6C21.text = "Τῆς ἀφράστου καὶ θείας λαμπρότητος, ἔνθα τῶν Μαρτύρων χοροὶ καταξίωσον, οὓς ἀπὸ γῆς μετέστησας, παντουργῷ σου βουλήσει φιλάνθρωπε."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m2_gr_GR_cog.EnAvyssoPtaismaton.name
ocMA.OdeW6C22.text = "Ἐλλαμφθῆναι τῷ κάλλει τῆς δόξης σου, τοὺς καταλιπόντας τὸν βίον ἀξίωσον, καὶ πρὸς τὸ φῶς σου Δέσποτα, μετακεχωρηκότας τὸ ἄφραστον."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m2_gr_GR_cog.EnAvyssoPtaismaton.name
ocMA.OdeW6C2T.text = "Ῥυομένη προθύμως ἐπίφανον, τοὺς προσκαλουμένους σε, Πάναγνε Δέσποινα, ἡ τὸν Χριστὸν γεννήσασα, τὸν ζωῆς καὶ θανάτου δεσπόζοντα."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
ocMA.OdeW7C11.text = "Ὑπομονῇ κατέβαλον, οἱ Ἅγιοι τὸν Βελίαρ, ὑπενεγκόντες πᾶσαν πεῖραν βασάνων χαλεπῶν, ὅτι ἐν ἀληθείᾳ τὸν Θεὸν ἠγάπησαν, τὸν παθόντα διὰ τὰς ἁμαρτίας ἡμῶν. Αὐτῶν προσευχαῖς, σῶσον πάντας Λόγε, πειρασμῶν καὶ κινδύνων."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
ocMA.OdeW7C12.text = "Φωτοειδεῖς Ἱεράρχαι, καὶ Ὅσιοι καὶ Προφῆται, καὶ Ἱερομαρτύρων ἡ ἀξιάγαστος πληθύς, καὶ Γυναικῶν ἁγίων, ἅγιος κατάλογος, ἐν ἀσκήσει καὶ ἀθλήσει λάμψας φαιδρῶς, ὑπὲρ ἡμῶν ἀεί, δέησιν ποιεῖσθε, ὅπως ἐλεηθῶμεν."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
ocMA.OdeW7C13.text = "Ἔνθα ὀδύνη πέφευγε, στεναγμός τε καὶ λύπη, ὅπου τὸ φῶς ἐκλάμπει, Χριστέ, τοῦ σοῦ προσώπου, ἔνθα ἁγίων δῆμοι, νῦν περιχορεύουσι, τάξον πάντας τοὺς μεταστάντας ἀφ' ἡμῶν, τὰ πταίσματα αὐτῶν, πάντα παραβλέψας, ὡς μόνος ἐλεήμων."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m2_gr_GR_cog.TaCherouvimMimoumenoi.name
ocMA.OdeW7C1T.text = "Μετὰ μαρτύρων ἄχραντε, καὶ Ὁσίων Πατέρων, καὶ Προφητῶν πασῶν τε, τῶν ὁσίων Γυναικῶν, τὸν ἐν ἁγίοις μόνον ἐπαναπαυόμενον, ἐκδυσώπει ἡμᾶς καθαγιάσαι ἁγνή, ἁγίαις ἐν φωναῖς, τοὺς δοξολογοῦντας, αὐτὸν εἰς τοὺς αἰῶνας."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
ocMA.OdeW7C21.text = "Ὁ ὢν καταβέβηκας, ἐπὶ τὸ σῶσαι, τὸ πάλαι πλανώμενον, ἀνθρώπων γένος Κύριε· διό σε οἱ Μάρτυρες, ἀεὶ καθικετεύουσι, τούτους οὓς μετέστησας ἐκ γῆς, ἐν γῇ πραέων, Σῶτερ ἀνάπαυσον."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
ocMA.OdeW7C22.text = "Νεκρώσει τὴν νέκρωσιν, ἀπετινάξω, ἐν νεκροῖς ἐλεύθερος, αὐτὸς μόνος γενόμενος, καὶ νῦν τῆς νεκρώσεως, τῆς ἁμαρτίας Δέσποτα, λύτρωσαι τοὺς δούλους σου, δεικνύς, καὶ κληρονόμους, τῆς Βασιλείας σου."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
ocMA.OdeW7C23.text = "Πολλή σου καὶ ἄφατος, ἡ εὐσπλαγχνία, ἀμέτρητος ἄβυσσος, ἡ τῆς φιλανθρωπίας σου· πταισμάτων οὖν ἄφεσιν, τοῖς μεταστᾶσι Χριστὲ δίδου, καὶ τῇ χάριτι τῇ σῇ κεκαθαρμένους, αὐτοὺς ἀνάδειξον."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m2_gr_GR_cog.AntitheonProstagma.name
ocMA.OdeW7C2T.text = "Λυχνία γεγένησαι, τῆς θείας δόξης, ἀπαύγασμα φέρουσα, Παρθένε διὰ Πνεύματος, ἡμῖν φανερούμενον, καὶ τὴν τοῦ ᾅδου ἀχλύν, φέγγει τῆς Θεότητος αὐτοῦ, ἐξαφανίζον θεοχαρίτωτε."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
ocMA.OdeW8C11.text = "Ῥεῦσαν τὸ αἷμα ὑμῶν, καθηγίασε πᾶσαν, Ἀθλοφόροι τὴν κτίσιν, καὶ ἐξήρανε σαφῶς, τὴν χύσιν τῆς ἀπάτης, καὶ τῶν πιστῶν τὰς ψυχάς, κατήρδευσε πλουσίως."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
ocMA.OdeW8C12.text = "Ὤφθη χορὸς Ἀσκητῶν, Ἱερέων τε δῆμος, καὶ Γυναίων ἁγίων, Προφητῶν τε εὐκλεῶν, ὁμότιμος Ἀγγέλων, ἀγγελικῶς ἐπὶ γῆς, ἐν Πνεύματι βιώσας."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
ocMA.OdeW8C13.text = "Οὓς ἐξ ἡμῶν Ἰησοῦ, προσελάβου ἐν κόλποις, Ἀβραὰμ κατατάξας, μετὰ πάντων ἐκλεκτῶν, ἀνάπαυσον, παρέχων, ἁμαρτημάτων αὐτοῖς, ὡς πανοικτίρμων λύσιν."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m2_gr_GR_cog.TonEnTiVatoMosi.name
ocMA.OdeW8C1T.text = "Θεοκυῆτορ ἁγνή, μετὰ πάντων Μαρτύρων, Προφητῶν καὶ ὁσίων, καὶ ἁγίων Γυναικῶν, καὶ Ἱερομαρτύρων, κατοικτειρῆσαι ἡμᾶς, τὸν Σωτῆρα δυσώπει."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m2_gr_GR_cog.KaminosPote.name
ocMA.OdeW8C21.text = "Ἔδειξας ἡμῖν, τοῦ δυσμενοῦς θανάτου, τὰς διεξόδους ὡς ἀθάνατος, τῇ θείᾳ δυνάμει σου, τὰς εἰσόδους δὲ ἐγνώρισας, τὰς τῆς ζωῆς ἀθάνατε, ἧς περ οἱ Μάρτυρές σου, νῦν ἐπαξίως ἐπέτυχον."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m2_gr_GR_cog.KaminosPote.name
ocMA.OdeW8C22.text = "Κάλλους νοητοῦ, τοῦ σοῦ κατατρυφῆσαι, τοὺς μεταστάντας καταξίωσον, τὸ αἶσχος τὸ δύσμορφον, ἐκκαθαίρων, ὡς φιλάνθρωπος, τῆς ἁμαρτίας Δέσποτα· μόνος γὰρ ἀνεδείχθης, τῆς ἁμαρτίας ἐλεύθερος."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m2_gr_GR_cog.KaminosPote.name
ocMA.OdeW8C23.text = "Ὤρθωσας ἡμᾶς, Χριστὲ τοὺς πεπτωκότας, εἰς χοῦν θανάτου, τῷ θανάτῳ σου ζωὴν χαριζόμενος, καὶ τρυφὴν τὴν ἀτελεύτητον, καὶ χαρμονὴν ἀΐδιον, ἧς νῦν τοὺς μεταστάντας, ὡς ἐλεήμων ἀξίωσον."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m2_gr_GR_cog.KaminosPote.name
ocMA.OdeW8C2T.text = "Μέγα καὶ φρικτόν, ὑπάρχει Θεομῆτορ, τῆς σῆς λοχείας τὸ μυστήριον· Θεὸν γὰρ ἐγέννησας, ὃν ὁ θάνατος οὐκ ἔστεξε, καὶ τάφος οὐ διέλυσεν· ὅθεν σε Παναγία, πάντα τὰ ἔθνη δοξάζομεν."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m2_gr_GR_cog.TinYperfyosSarki.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
ocMA.OdeW9C11.text = "Ἰσχυροὶ κατὰ παθῶν, δυνατοὶ κατὰ ἐχθρῶν, φανέντες Ἀθλοφόροι, ἀπηνέγκατε νίκην, ἀθλήσαντες νομίμως, καὶ στεφθέντες ἐκ Θεοῦ."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
ocMA.OdeW9C12.text = "Ὡς Θεοῦ Ἱερουργοί, καὶ Ποιμένος τοῦ καλοῦ, μιμηταὶ γεγονότες, ἐποιμάνατε αὐτοῦ, τὰ θρέμματα ὁσίως, Ἱεράρχαι εὐκλεεῖς."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
ocMA.OdeW9C13.text = "Σὺν ὁσίοις Ἀσκηταῖς, καὶ Προφήταις ἱεροῖς, τιμήσωμεν Γυναίων, ἀθλησάντων τὰ πλήθη, καὶ τῶν ἐν ἀσκήσει, καθελόντων τὸν ἐχθρόν."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
ocMA.OdeW9C14.text = "Ἡ πανένδοξος Χριστέ, τῶν Ὁσίων σου πληθύς, ἀπαύστως δυσωπεῖ σε. Οὓς μετέστησας πίστει, μετόχους αἰωνίου, δεῖξον Κύριε ζωῆς."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m2_gr_GR_cog.TinYperfyosSarki.name
ocMA.OdeW9C1T.text = "Φιλοικτίρμονα Θεόν, ἡ κυήσασα σαρκί, μετὰ πάντων Ἁγίων, καθικέτευε ἀεί, κινδύνων ἡμᾶς σῶσαι, Μητροπάρθενε ἁγνή."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m2_gr_GR_cog.ITonProIliouFostira.name
ocMA.OdeW9C21.text = "Ἔχων ἐξουσίαν καὶ ζώντων, καὶ νεκρῶν ζωαρχικώτατε, τοῖς ἀπὸ γῆς πρὸς σὲ μεταφοιτήσασι, κληρουχίαν ἐν οὐρανοῖς, παράσχου καὶ λαμπρότητα, τὴν τῶν ἁγίων Δέσποτα, καὶ τῶν ἐνδόξων Ἀθλοφόρων σου."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m2_gr_GR_cog.ITonProIliouFostira.name
ocMA.OdeW9C22.text = "Λόγος ὢν ὁ πάλαι, τὸ εἶναί μοι διδοὺς ζωαρχικώτατε, καὶ τὸ εὖ εἶναι πάλιν χαριζόμενος, ἐν τοῖς κόλποις τοῖς ὀρεκτοῖς, Ἀβραὰμ τοῦ προπάτορος, τοὺς μεταστάντας δούλους σου, ὡς ἐλεήμων κατασκήνωσον."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m2_gr_GR_cog.ITonProIliouFostira.name
ocMA.OdeW9C23.text = "Ὅλος χρηματίζεις Σωτήρ μου, γλυκασμὸς φωτοειδέστατος, ὅλος ὑπάρχεις ἔφεσις ἀκόρεστος, τὸν χειμάρρουν τὸν τῆς τρυφῆς, καὶ ὕδωρ τῆς ἀφέσεως, τοὺς κοιμηθέντας πότισον, ἀκαταπαύστως σὲ δοξάζοντας."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode2
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m2_gr_GR_cog.ITonProIliouFostira.name
ocMA.OdeW9C2T.text = "Σὲ τὴν Θεοτόκον ἀξίως, οἱ πιστοὶ νῦν μακαρίζομεν, τοῖς θεοπνεύστοις λόγοις σου ἑπόμενοι· σὺ γὰρ μόνη τοῖς ἐπὶ γῆς, Θεὸν ἐκυοφόρησας, τὸν τοῦ θανάτου λύσαντα, τὴν δυναστείαν Μητροπάρθενε."

ocMA.Lauds1.mode = misc_gr_GR_cog.Mode2
ocMA.Lauds1.melody = ""
ocMA.Lauds1.text = oc.m2.d3_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds2.mode = misc_gr_GR_cog.Mode2
ocMA.Lauds2.melody = ""
ocMA.Lauds2.text = oc.m2.d4_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds3.mode = misc_gr_GR_cog.Mode2
ocMA.Lauds3.melody = ""
ocMA.Lauds3.text = oc.m2.d5_gr_GR_cog.ocMA.Aposticha3.text

ocMA.Lauds4.mode = misc_gr_GR_cog.Mode2
ocMA.Lauds4.melody = ""
ocMA.Lauds4.text = "Οἴμοι οἷον ἀγῶνα ἔχει ἡ ψυχή, χωριζομένη ἐκ τοῦ σώματος! οἴμοι πόσα δακρύει τότε, καὶ οὐχ ὑπάρχει ὁ ἐλεῶν αὐτήν! πρὸς τοὺς Ἀγγέλους τὰ ὄμματα ῥέπουσα, ἄπρακτα καθικετεύει, πρὸς τοὺς ἀνθρώπους τὰς χεῖρας ἐκτείνουσα, οὐκ ἔχει τὸν βοηθοῦντα. Διὸ ἀγαπητοί μου ἀδελφοί, ἐννοήσαντες ἡμῶν τὸ βραχὺ τῆς ζωῆς, τοῖς μεταστᾶσι τὴν ἀνάπαυσιν, παρὰ Χριστοῦ αἰτησώμεθα, καὶ ταῖς ψυχαῖς ἡμῶν τὸ μέγα ἔλεος."

ocMA.LaudsTheotokion.mode = misc_gr_GR_cog.Mode2
ocMA.LaudsTheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.LaudsTheotokion.text = "Ὅλην τὴν ζωήν μου ἐν κακοῖς, καταδαπανήσας ὁ τάλας, νῦν κατελείφθην Ἁγνή, πάσης ὅλος ἔρημος, ἀγαθῆς πράξεως, προσεγγίζοντα βλέπων δέ, τὸν θάνατον, οἴμοι! τρέμω τὸ κριτήριον τοῦ σοῦ Υἱοῦ καὶ Θεοῦ· οὗ περ ἐξελοῦ με Παρθένε, καὶ πρὸ τῆς ἀνάγκης ἐκείνης, Δέσποινα ἐπίστρεψον, καὶ σῶσόν με."

ocMA.LaudsRubric1.text = oc.m1.d7_gr_GR_cog.ocMA.LaudsRubric1.text

ocMA.Aposticha1.mode = misc_gr_GR_cog.Mode2
ocMA.Aposticha1.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.Aposticha1.text = "Ῥύμην τοῦ θανάτου καὶ φθοράν, σοῦ τῷ ζωηφόρῳ θανάτῳ, καθεῖλες Δέσποτα, πᾶσι δὲ ἐπήγασας, ζωὴν αἰώνιον, καὶ νεκροῖς ἐξανάστασιν, Σωτὴρ ἐδωρήσω· ὅθεν σου δεόμεθα, καὶ νῦν ἀνάπαυσον, πίστει τοὺς πρὸς σὲ μεταστάντας, καὶ τῆς ἀθανάτου σου δόξης, τούτους καταξίωσον, Φιλάνθρωπε."

ocMA.Aposticha2.mode = misc_gr_GR_cog.Mode2
ocMA.Aposticha2.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.Aposticha2.text = "Ἵνα τοὺς ἀνθρώπους κοινωνούς, θείας βασιλείας ἐργάσῃ, Σταυρὸν ὑπέμεινας, θάνατον ἑκούσιον, καταδεξάμενος· διὰ τοῦτο δεόμεθα, τῇ σῇ εὐσπλαγχνίᾳ, μετόχους ἀνάδειξον τῆς βασιλείας σου, πίστει τοὺς πρὸς σὲ μεταστάντας, καὶ τοῦ γλυκυτάτου σου κάλλους, τούτους καταξίωσον, Φιλάνθρωπε."

ocMA.Aposticha3.mode = misc_gr_GR_cog.Mode2
ocMA.Aposticha3.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.Aposticha3.text = "Σῶσαι τὸ σὸν πλάσμα βουληθείς, τῆς οἰκονομίας τὸ ὄντως, βαθὺ μυστήριον, χαίρων ἐξετέλεσας, ὡς ὑπεράγαθος, καὶ τιμῆς ἐξηγόρασας, τὸν σύμπαντα Κόσμον, Αἵματι τιμίῳ σου· διὸ δεόμεθα, πίστει τοὺς πρὸς σὲ μεταστάντας, καὶ τοῦ γλυκυτάτου σου κάλλους, τούτους καταξίωσον Φιλάνθρωπε."

ocMA.Aposticha4.mode = misc_gr_GR_cog.Mode2
ocMA.Aposticha4.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.Aposticha4.text = "Τρόμῳ, τῷ φρικτῷ καὶ φοβερῷ, θρόνῳ σου Χριστὲ παρεστῶτες, οἱ ἀπ' αἰῶνος νεκροί, ψῆφον ἀναμένουσι, τὴν σὴν δικαίαν Σωτήρ, καὶ τὴν θείαν ἐκδέχονται δικαιοκρισίαν, τότε φεῖσαι Δέσποτα, Σῶτερ τῶν δούλων σου, πίστει τῶν πρὸς σὲ μεταστάντων, καὶ τῆς ἀϊδίου τρυφῆς σου, καὶ μακαριότητος ἀξίωσον."

ocMA.AposTheotokion.mode = misc_gr_GR_cog.Mode2
ocMA.AposTheotokion.melody = he.a.m2_gr_GR_cog.OteEkTouXylou.name
ocMA.AposTheotokion.text = "Δεῦτε τὴν Μητέρα τοῦ φωτός, ὕμνοις ἀσιγήτοις βοῶντες, πάντες δοξάσωμεν· αὕτη γὰρ ἐκύησε τὴν σωτηρίαν ἡμῶν, καὶ τὸ χαῖρε προσάξωμεν, ὡς μόνῃ τεκούσῃ, πάντων ἀρχαιότατον, τὸν πρὸ αἰώνων Θεόν. Χαίροις ἡ τὴν Εὔαν τεκοῦσα, πάλιν ἀναπλάσαντα. Χαίροις, ἄχραντε Παρθένε ἀπειρόγαμε."

ocMA.ApolTheotokion.mode = misc_gr_GR_cog.Mode2
ocMA.ApolTheotokion.melody = ""
ocMA.ApolTheotokion.text = oc.m2.d2_gr_GR_cog.ocMA.ApolTheotokion.text

ocLI.Beatitudes1.mode = misc_gr_GR_cog.Mode2
ocLI.Beatitudes1.melody = ""
ocLI.Beatitudes1.text = oc.m2.d1_gr_GR_cog.ocLI.Beatitudes1.text

ocLI.Beatitudes2.mode = misc_gr_GR_cog.Mode2
ocLI.Beatitudes2.melody = ""
ocLI.Beatitudes2.text = "Τοῦ Χριστοῦ τὰ παθήματα, μιμησάμενοι Μάρτυρες, πάθη ποικίλα βροτῶν, ἀεὶ θεραπεύετε."

ocLI.Beatitudes3.mode = misc_gr_GR_cog.Mode2
ocLI.Beatitudes3.melody = ""
ocLI.Beatitudes3.text = "Σὺν Προφήταις Ἀπόστολοι, σὺν Ὁσίοις Διδάσκαλοι, τῷ πάντων Δημιουργῷ, καλῶς εὐηρέστησαν."

ocLI.Beatitudes4.mode = misc_gr_GR_cog.Mode2
ocLI.Beatitudes4.melody = ""
ocLI.Beatitudes4.text = "Μετὰ πάντων ἀνάπαυσον, τῶν Ἁγίων σου Κύριε, οὓς προσελάβου πιστούς, δεόμεθα δούλους σου."

ocLI.BeatitudesGlory.mode = misc_gr_GR_cog.Mode2
ocLI.BeatitudesGlory.melody = ""
ocLI.BeatitudesGlory.text = "Ὦ Τριὰς ὁμοούσιε, τοὺς ὑμνοῦντάς σε οἴκτειρον, ἐκ τῶν σκανδάλων ἐχθροῦ, πάντας λυτρουμένη ἀεί."

ocLI.BeatitudesBoth.mode = misc_gr_GR_cog.Mode2
ocLI.BeatitudesBoth.melody = ""
ocLI.BeatitudesBoth.text = "Τὰς δεήσεις τῶν δούλων σου, μὴ παρίδῃς Πανάμωμε, σῴζουσα ἡμᾶς ἐκ πολλῶν πταισμάτων καὶ θλίψεων."

//Octoechos in Triodion

