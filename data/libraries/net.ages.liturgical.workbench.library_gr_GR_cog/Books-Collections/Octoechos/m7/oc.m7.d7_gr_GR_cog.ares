ocVE.Stichera2.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera2.melody = misc_gr_GR_cog.Automelon
ocVE.Stichera2.text = he.a.m7_gr_GR_cog.KatafronisantesPanton.text

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera3.melody = ""
ocVE.Stichera3.text = oc.m7.d5_gr_GR_cog.ocVE.Aposticha3.text

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera4.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.name
ocVE.Stichera4.text = "Σῶτερ μὴ καταισχύνῃς με, ὅταν ἔλθῃς κρῖναι κόσμον ἅπαντα, τὸν αἰσχύνης ἔργα ποιήσαντα."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera5.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.name
ocVE.Stichera5.text = "Ἀμέτρητά σοι πταίσας, ἀμετρήτους κολάσεις ἐκδέχομαι, ὁ Θεός μου οἴκτειρον καὶ σῶσόν με."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode7
ocVE.Stichera6.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.name
ocVE.Stichera6.text = "Ἀνάπαυσον τοὺς πίστει κεκοιμημένους, ἐν αὐλαῖς σου Δέσποτα, καὶ Βασιλείας σου ἀξίωσον."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode7
ocVE.SticTheotokion.melody = ""
ocVE.SticTheotokion.text = oc.m7.d1_gr_GR_cog.ocVE.SticTheotokion.text

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode7
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m7.d2_gr_GR_cog.ocVE.Aposticha3.text

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode7
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.text = "Κατ' εἰκόνα σὴν καὶ ὁμοίωσιν, πλαστουργήσας κατ' ἀρχὰς τὸν ἄνθρωπον, ἐν Παραδείσῳ τέθεικας, κατάρχειν σου τῶν κτισμάτων, φθόνῳ δὲ διαβόλου ἀπατηθείς, τῆς βρώσεως μετέσχε, τῶν ἐντολῶν σου παραβάτης γεγονώς· διὸ πάλιν αὐτὸν εἰς γῆν ἐξ ἦς ἐλήφθη, κατεδίκασας ἐπιστρέφειν Κύριε, καὶ αἰτεῖσθαι τὴν ἀνάπαυσιν."

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode7
ocVE.Aposticha3.melody = he.a.m7_gr_GR_cog.OukEtiKolyometha.name
ocVE.Aposticha3.text = "Ἀνάπαυσον Σωτὴρ ἡμῶν ζωοδότα, οὓς μετέστησας ἀδελφοὺς ἡμῶν, ἐκ τῶν προσκαίρων, κράζοντας· Κύριε δόξα σοι."

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode7
ocVE.AposTheotokion.melody = ""
ocVE.AposTheotokion.text = "Ὄρος θεῖον ἅγιον, εἶδέ σε ὁ Δανιήλ, καὶ Ἡσαΐας δὲ ὁ σοφός, Μητέρα Θεοῦ ἐκήρυξέ σε Σεμνή, Θεὸν ἐν σαρκὶ χωρήσασαν, τὸν Ἀδὰμ πλαστουργήσαντα, ὃν τὰ Χερουβείμ ἐτρόμαξαν σαρκούμενον, ὑμνοῦντα καὶ δοξάζοντα, ἀσιγήτῳ τρισαγίᾳ φωνῇ, ἀλλὰ τοῦτον αἴτησαι, σωθῆναι τὰς ψυχὰς ἡμῶν."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode7
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.text = oc.m7.d1_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocMA.MM.Alleluia.text = properties_gr_GR_cog.media.key

