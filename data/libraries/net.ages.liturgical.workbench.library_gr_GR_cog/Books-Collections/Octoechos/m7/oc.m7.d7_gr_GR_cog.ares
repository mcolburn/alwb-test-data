ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.text = oc.m7.d3_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma12.melody = ""
ocMA.Kathisma12.text = oc.m7.d4_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma13.melody = ""
ocMA.Kathisma13.text = oc.m7.d5_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma14.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma14.melody = ""
ocMA.Kathisma14.text = oc.m7.d1_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.text = oc.m7.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma22.melody = ""
ocMA.Kathisma22.text = "Ὁ θάνατός σου Κύριε, τοῖς νεκροῖς ζωὴν ἐξήνθησε· τὸν γὰρ ᾅδην ἐσκύλευσας, ἀνακαλύπτων τοὺς ἐν σκότει καθεύδοντας· διό σε Ζωοδότα, ὡς Θεὸν ἱκετεύομεν. Οὕς ἐξ ἡμῶν προσελάβου, μετὰ Δικαίων ἀνάπαυσον, ὅπως εὕρωσιν ἐν κρίσει, τὸ μέγα ἔλεος."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode7
ocMA.Kathisma23.melody = ""
ocMA.Kathisma23.text = "Χαῖρε ἡ τὸν ἀχώρητον ἐν οὐρανοῖς, χωρήσασα ἐν μήτρᾳ σου. Χαῖρε Παρθένε τῶν Προφητῶν τὸ κήρυγμα, ἐξ ἧς ἔλαμψεν ὁ Ἐμμανουήλ. Χαῖρε Μήτηρ Χριστοῦ τοῦ Θεοῦ."

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν Νεκρωσίμων καὶ τῶν Θεοτοκίων· Χοροὺς ἀνυμνῶ Ποιμένων καὶ Μαρτύρων, ἐγὼ ὁ Ἰωσὴφ."

ocMA.C2Acrostic.text = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν Θεοτοκίων· Κανὼν τὸν αὐτόν, ἕβδομος σκοπὸν φέρων"

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m7_gr_GR_cog.ToSynergisantiTheo.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m7_gr_GR_cog.ToSynergisantiTheo.name
ocMA.OdeW1C11.text = "Χορὸς Μαρτύρων ἀπλανῶν, πᾶσαν πλάνην νικήσας, τοῦ πολεμήτορος, χορεύει γηθόμενος, πρὸ προσώπου τοῦ Κτίσαντος."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m7_gr_GR_cog.ToSynergisantiTheo.name
ocMA.OdeW1C12.text = "Οἱ Ἱεράρχαι τοῦ Χριστοῦ, καὶ οἱ Ὅσιοι πάντες, ἀγωνισάμενοι, τρυφῆς ἠξιώθησαν, αἰωνίου ἐν χάριτι."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m7_gr_GR_cog.ToSynergisantiTheo.name
ocMA.OdeW1C13.text = "Ῥῦσαι ἡμᾶς πάσης ὀργῆς, Προφητῶν καὶ Ὁσίων, καὶ Γυναικῶν ἱερῶν, Χριστὲ παρακλήσεσι, καὶ σῶσον τὰς ψυχὰς ἡμῶν."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m7_gr_GR_cog.ToSynergisantiTheo.name
ocMA.OdeW1C14.text = "Ὁ διαπλάσας με ἐκ γῆς, καὶ εἰς γῆν πάλιν Λόγε, καθυποστρέφειν με, κελεύσας ἀνάπαυσον, οὓς ἐν πίστει μετέστησας."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m7_gr_GR_cog.ToSynergisantiTheo.name
ocMA.OdeW1C1T.text = "Ὑπεραγία ἡ Θεόν, ὑπεράγιον Λόγον, ἀποκυήσασα, ἁγίασον ἅπαντας, τοὺς πίστει σε δοξάζοντας."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m7_gr_GR_cog.NefseiSouProsGeodi.name
ocMA.OdeW1C21.text = "Κόσμος ὡραϊσμένος, τῶν ἀρετῶν ταῖς ἰδέαις, καὶ θεῖον ἀνάθημα, τοῖς διαφόροις κάλλεσι, λάμπον ταῖς θεοσημείαις, Μάρτυρες Χριστῷ, τῷ Θεῷ προσηνέχθητε."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m7_gr_GR_cog.NefseiSouProsGeodi.name
ocMA.OdeW1C22.text = "Αἴγλης τῆς ἀπροσίτου, καὶ τρισηλίου λαμπάδος, τυχεῖν καταξίωσον, τοὺς μεταστάντας δούλους σου, ἔνθα ἀπέδρα ὀδύνη, λύπη στεναγμός, πολυέλεε Κύριε."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m7_gr_GR_cog.NefseiSouProsGeodi.name
ocMA.OdeW1C23.text = "Νεκρώσει Ζωοδότα, κατακριθεῖσαν τὴν φύσιν, ζωὴ ἐνυπόστατος, ὑπάρχων Χριστὲ ἥγειρας· ὅθεν ὡς μόνος οἰκτίρμων, πίστει τοὺς πρὸς σέ, μεταστάντας ἀνάπαυσον."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m7_gr_GR_cog.NefseiSouProsGeodi.name
ocMA.OdeW1C2T.text = "Ὤρθωσας Παναγία, τὴν τῆς προμήτορος πτῶσιν, τεκοῦσα τὸν Κύριον, τὸν ταύτην ἀναστήσαντα, Λόγον τὸν ἐν τοῖς μνημείοις, πνέοντα ζωήν, ἐξουσίᾳ Θεότητος."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.name
ocMA.OdeW3C11.text = "Συντριβόμενοι λίθοις, καὶ βόθροις ὁμιλοῦντες, συνετρίψατε πᾶσαν, τοῦ πλάνου δυναστείαν, καὶ ἄθραυστοι τὸν νοῦν, ἐμείνατε Ἅγιοι."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.name
ocMA.OdeW3C12.text = "Αἴγλῃ θείων δογμάτων, καὶ ἀρετῶν ταῖς ἀκτῖσιν, οἱ σοφοὶ Ἱεράρχαι, πιστοὺς φωταγωγοῦντες, αἱρέσεων ἀχλύν, πᾶσαν διεσκέδασαν."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.name
ocMA.OdeW3C13.text = "Νεκρωθέντες τῷ κόσμῳ, οἱ Ὅσιοί σου Λόγε, ὑπερκόσμιον ὄντως, ζωὴν κληρονομοῦσι, δι' ὧν ἡμᾶς Χριστέ, ἅπαντας οἰκτείρησον."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.name
ocMA.OdeW3C14.text = "Ὑπὲρ τῶν μεταστάντων, ἐν πίστει καὶ ἐλπίδι, ἱκετεύομεν πάντες, τὸν ἀγαθὸν Δεσπότην, τοῦ φείσασθαι αὐτῶν, ἐν ᾥρᾳ τῆς κρίσεως."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m7_gr_GR_cog.OiOrthrizontesLoge.name
ocMA.OdeW3C1T.text = "Μὴ κενώσας τοὺς κόλπους, τοὺς Πατρικοὺς ὁ Λόγος, σοῖς ἐν κόλποις ὡς βρέφος, κρατούμενος ὡράθη ἀρχὴν ἐκ σοῦ λαβών, Ἄχραντε ὁ ἄναρχος."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m7_gr_GR_cog.OKatArchasTousOuranous.name
ocMA.OdeW3C21.text = "Νεανικὴν καὶ σταθηράν, τὴν καρτερίαν ἀνδρείως, οἱ Μαρτύρων ἔδειξαν χοροί· τῶν γὰρ αἰκισμῶν τὴν ῥύμην ἤνεγκαν, καὶ τῶν βασάνων τὰς πληγάς, τῆς ἀκηράτου σου δόξης, Σῶτερ ἐφιέμενοι καὶ τέρψεως."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m7_gr_GR_cog.OKatArchasTousOuranous.name
ocMA.OdeW3C22.text = "Τῆς αἰωνίου σου ζωῆς, τῶν ἐπ' ἐλπίδι θανέντων, Ἐλεῆμον πρόσδεξαι ψυχάς, καὶ τῶν ὀρεκτῶν καὶ θείων εὔσπλαγχνε, κόλπων τοῦ θείου Ἀβραάμ, ἔνδον σκηνώσας Λαζάρῳ καὶ τοῖς ἐκλεκτοῖς σου συναρίθμησον."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m7_gr_GR_cog.OKatArchasTousOuranous.name
ocMA.OdeW3C23.text = "Ὁ καταβὰς ἐξ οὐρανοῦ, ἐπὶ τὸ σῶσαι τὸ γένος, τῶν ἀνθρώπων εὔσπλαγχνε Σωτήρ, ἀΰλου φωτὸς καὶ θείας δόξης σου, κατατρυφῆσαι καὶ χαρᾶς, τοὺς εὐσεβῶς μεταστάντας, τῇ σῇ εὐσπλαγχνίᾳ καταξίωσον."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m7_gr_GR_cog.OKatArchasTousOuranous.name
ocMA.OdeW3C2T.text = "Νόμοι τῆς φύσεως ἐν σοί, τὸν ἀπερίληπτον Λόγον, συλλαβούσῃ λέλυνται Ἁγνή, καὶ νόμος ἡμῖν ὁ θεῖος δέδοται, πᾶσι τὴν ἄφεσιν διδούς, ἀπεγνωσμένοις πταισμάτων, χάριτι τῆς θείας ἀγαθότητος."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.name
ocMA.OdeW4C11.text = "Νευρούμενοι τῇ πρὸς Κύριον στοργῇ, οἱ Μάρτυρες, ἐξενεύρισαν ἐχθροῦ τὴν δύναμιν· ὅθεν μακαρίζονται."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.name
ocMA.OdeW4C12.text = "Ὡς θρέμματα τοῦ Ποιμένος καὶ Ἀμνοῦ, μακάριοι, Ἱεράρχαι λογικῆς προέστητε, ποίμνης θείᾳ χάριτι."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.name
ocMA.OdeW4C13.text = "Πολύφωτοι ὡς ἀστέρες ἀρετῶν, λαμπρότητι, κατηυγάσατε πιστῶν πληρώματα, Ὅσιοι Πατέρες ἡμῶν."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.name
ocMA.OdeW4C14.text = "Ὁμήγυρις στερροτάτων Γυναικῶν, καὶ σύλλογος, τῶν ἁγίων Προφητῶν τετύχηκε, τῶν ἐν οὐρανοῖς ἀγαθῶν."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m7_gr_GR_cog.TinEnsarkonParousianSou.name
ocMA.OdeW4C1T.text = "Ἱκέτευε ὃν ἐκύησας Θεόν, Πανύμνητε, πειρασμῶν παντοδαπῶν καὶ θλίψεων, ῥύσασθαι τὴν ποίμνην σου."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m7_gr_GR_cog.OPatrikousKolpous.name
ocMA.OdeW4C21.text = "Ἀθλητικοὺς πόνους καρτερῶς, ἐπιδειξάμενοι Χριστὲ οἱ Μάρτυρες, τοὺς στεφάνους ἀνεδήσαντο, τῆς δικαιοσύνης σου, καὶ ἐδόξασάν σου τὴν δύναμιν."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m7_gr_GR_cog.OPatrikousKolpous.name
ocMA.OdeW4C22.text = "Ὑπερφυοῦς δόξης σου Χριστέ, τοὺς μεταστάντας εὐσεβῶς ἀξίωσον, ὅταν ἔλθῃς ἐν τῇ δόξῃ σου, μετὰ τῶν Ἀγγέλων σου, ὡς Οἰκτίρμων καὶ πολυέλεος."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m7_gr_GR_cog.OPatrikousKolpous.name
ocMA.OdeW4C23.text = "Τρισσοφαοῦς αἴγλης ἀγαθέ, καὶ ἑνιαίας ἀστραπῇ Θεότητος, ἀπολαῦσαι καταξίωσον, τοὺς προεκδημήσαντας, καὶ δοξάζοντάς σε Φιλάνθρωπε."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m7_gr_GR_cog.OPatrikousKolpous.name
ocMA.OdeW4C2T.text = "Ὁ Πατρικοὺς κόλπους μὴ λιπών, παρθενικοῖς κόλποις ὁ Χριστὸς αὐλίζεται, καὶ θανάτου ἐλυτρώσατο, τοὺς σὲ μακαρίζοντας, Θεομῆτορ μόνη πανύμνητε."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.name
ocMA.OdeW5C11.text = "Μωλώπων σαρκὸς κατεφρονήσατε, στερροὶ Ἀθληταὶ καὶ τοὺς μώλωπας πάντων, ἰᾶσθε καὶ τὰ τραύματα, τῶν προσερχομένων ὑμῖν."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.name
ocMA.OdeW5C12.text = "Ἐλάβετε λύειν καὶ δεσμεῖν ἐπὶ γῆς, τὴν ἐξουσίαν, Ἱεράρχαι τοῦ Χριστοῦ· διὸ τὰ τῶν κακῶν ἡμῶν, ἀεὶ φορτία λύσατε."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.name
ocMA.OdeW5C13.text = "Νοῒ καθαρῷ τῷ Θεῷ ἑνούμενοι, χοροὶ Ἀσκητῶν, Προφητῶν καὶ Δικαίων καὶ Γυναικῶν, χορεύουσι, χορείαν ἀκατάλυτον."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.name
ocMA.OdeW5C14.text = "Ὡς μόνος ὑπάρχων πολυέλεος, τοὺς πίστει Χριστέ, μεταστάντας τοῦ βίου, τοῦ Παραδείσου ποίησον, Φιλάνθρωπε οἰκήτορας."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m7_gr_GR_cog.OrthrizeiToPnevmaMou.name
ocMA.OdeW5C1T.text = "Νυμφὼν ἀκατάλυτος ὀφθεῖσα Θεοῦ, δυσώπει αὐτόν, Παναγία Παρθένε, τοῦ νυμφῶνος οἰκήτορα, τοῦ νοητοῦ γενέσθαι με."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m7_gr_GR_cog.KyrieOTheosMou.name
ocMA.OdeW5C21.text = "Νέκρωσιν ζωηφόρον, διὰ τῶν βασάνων ἐξυφανθεῖσαν, πανεύφημοι Μάρτυρες, περιβεβλημένοι φαιδρῶς, ταῖς ψυχαῖς τῶν προκοιμηθέντων, τὴν θείαν αἰτήσασθε ἄνεσιν."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m7_gr_GR_cog.KyrieOTheosMou.name
ocMA.OdeW5C22.text = "Ἐκ τῶν ἀδαπανήτων, θησαυρῶν προχέων τὴν εὐσπλαγχνίαν, ψυχὰς ἃς μετέστησας, σὺν τοῖς πρωτοτόκοις σου, ἐν σκηναῖς ταῖς ἀκαταλύτοις, εὐδόκησον Σῶτερ αὐλίζεσθε."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m7_gr_GR_cog.KyrieOTheosMou.name
ocMA.OdeW5C23.text = "Βάρος ἀποθεμένους, καὶ δεσμῶν λυθέντας νῦν πρὸς τὴν ἄνω, ζωὴν τοὺς οἰκέτας σου, μεταβεβηκότας Χριστέ, ἐντρυφᾷν ταῖς τῶν σῶν Ἁγίων, λαμπρότησι Σῶτερ εὐδόκησον."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m7_gr_GR_cog.KyrieOTheosMou.name
ocMA.OdeW5C2T.text = "Δέσποινα Θεομῆτορ, τῶν ἐμῶν πταισμάτων λύσιν παράσχου, καὶ δός μοι συγχώρησιν, τῶν πλημμελημάτων μου, ἡ ζωὴν τῷ κόσμῳ τεκοῦσα Πανάμωμε τὴν ἐνυπόστατον."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m7_gr_GR_cog.OIonasEkKoilias.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
ocMA.OdeW6C11.text = "Καθείλετε τοῦ ἐχθροῦ τὰς ἐπάρσεις, ὑψούμενοι πρὸς Θεὸν ἐν τοῖς ἄθλοις, πανένδοξοι ὁπλῖται, οἰκήτορες οὐρανῶν χρηματίζοντες."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
ocMA.OdeW6C12.text = "Αἱρέσεων τὸν χειμῶνα σοβοῦντες, οἱ τοῦ Χριστοῦ ἀληθεῖς Ἱεράρχαι, πρὸς ἔαρ ἀληθείας, τὴν πληθὺν τῶν εὐσεβῶν μετεβίβασαν."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
ocMA.OdeW6C13.text = "Ἰσχύϊ σου τῶν Ὁσίων τὰ πλήθη, καὶ Προφητῶν καὶ Γυναίων ἁγίων, Χριστὲ δικαιωθέντα, τοῦ ἀδύτου φωτισμοῦ ἀπολαύουσι."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
ocMA.OdeW6C14.text = "Μετέστησας τῶν προσκαίρων Οἰκτίρμον, τοὺς δούλους σου, ἀλλ' αὐτοὺς αἰωνίου, μετασχεῖν εὐφροσύνης, καὶ ζωῆς ἀληθινῆς καταξίωσον."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m7_gr_GR_cog.OIonasEkKoilias.name
ocMA.OdeW6C1T.text = "Ἁγίασον, Παναγία Παρθένε, τοὺς δούλους σου, τὸν πανάγιον Λόγον σαρκὶ ἀποτεκοῦσα, ὃν ὑμνεῖ πᾶσα πνοὴ ἁγιάζουσα."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m7_gr_GR_cog.NaftionToSalo.name
ocMA.OdeW6C21.text = "Ὁ χορὸς τῶν Μαρτύρων, τὰς δυσφορωτάτας ὀδύνας, στερρῶς καρτερήσαντες, τὰς ἀπόνους, καὶ ἀνωδύνους τρυφὰς ἐκαρπώσαντο, τῆς δικαιοσύνης τοὺς στεφάνους, ἐκ τῆς ζωηφόρου δεξιᾶς δεξάμενοι."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m7_gr_GR_cog.NaftionToSalo.name
ocMA.OdeW6C22.text = "Μετὰ τῶν Δικαίων, ἔνθα τῶν Ἀγγέλων χορεῖαι, καὶ ὅπου λαμπρότητες τῶν Ἁγίων, καὶ αἰωνίου ζωῆς ἀπόλαυσις, τοὺς προκοιμηθέντας σου οἰκέτας, τάξον ἐλεῆμον ὡς Θεὸς ἀθάνατος."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m7_gr_GR_cog.NaftionToSalo.name
ocMA.OdeW6C2T.text = "Ὁ βουλήσει θείᾳ, δημιουργικῇ τε δυνάμει, τὸ πᾶν συστησάμενος ἐκ μὴ ὄντων, ἐκ τῆς γαστρός σου Ἁγνὴ προελήλυθε, καὶ τοὺς ἐν τῷ σκότει τοῦ θανάτου, πάντας ὡς Θεός, ἐκ φθορᾶς ἀφήρπασε."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m7_gr_GR_cog.OTonPateron.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m7_gr_GR_cog.OTonPateron.name
ocMA.OdeW7C11.text = "Ῥοαῖς αἱμάτων οἱ Ἀθλοφόροι, τῆς ἀσεβείας ἔσβεσαν φλόγα."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m7_gr_GR_cog.OTonPateron.name
ocMA.OdeW7C12.text = "Τοὺς Ἱεράρχας φωστῆρας κόσμου, γεγενημένους ἀνευφημοῦμεν."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m7_gr_GR_cog.OTonPateron.name
ocMA.OdeW7C13.text = "Ὕμνοις τιμάσθω τῶν Προφητῶν τε, καὶ τῶν Ὁσίων ἅγιος δῆμος."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m7_gr_GR_cog.OTonPateron.name
ocMA.OdeW7C14.text = "Ῥῦσαι γεέννης οὓς προσελάβου, πιστούς σου δούλους Χριστὲ οἰκτίρμον."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m7_gr_GR_cog.OTonPateron.name
ocMA.OdeW7C1T.text = "Ὡς τῶν Ἀγγέλων τιμιωτέραν, τὴν Θεοτόκον ὑμνολογοῦμεν."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
ocMA.OdeW7C21.text = "Στεφηφοροῦντες οἱ χοροί, τῶν Μαρτύρων τὸν Βασιλέα, Χριστὸν τρανῶς περιέπουσι, νοητῶς σὺν Ἀγγέλων χοροστασίαις. Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
ocMA.OdeW7C22.text = "Σὺ τοῦ θανάτου καὶ ζωῆς, ἐξουσίαν ἔχων Οἰκτίρμον, τρυφῆς ἐνθέου ἀξίωσον, τοὺς ἐν πίστει σοὺς δούλους, προκοιμηθέντας. Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
ocMA.OdeW7C23.text = "Καταλαμπρύνας τῷ φωτί, τὰς ἀΰλους φωτοχυσίας, ψυχὰς ἃς αὐτὸς μετέστησας, ἐν σκηναῖς αἰωνίοις κατασκηνώσαις. Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m7_gr_GR_cog.OiEnKaminoTouPyros.name
ocMA.OdeW7C2T.text = "Ὁ πρὶν ἀμήτωρ ἐκ Πατρός, νῦν ἀπάτωρ ἐκ σοῦ Παρθένε, Χριστὸς ἀφράστως γεγέννηται, δι' ἡμᾶς τοὺς ἀνθρώπους, ἑκὼν πτωχεύσας. Εὐλογημένος Πάναγνε, ὁ καρπός τῆς κοιλίας σου."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.name
ocMA.OdeW8C11.text = "Ναοὺς εἰδωλικοὺς κατεστρέψατε, Πανεύφημοι, καὶ οἴκους ἐδείμασθε, ἑαυτοὺς τοῦ Πνεύματος, Ἀθλοφόροι ἀνδρείως, τὸν δρόμον τελέσαντες."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.name
ocMA.OdeW8C12.text = "Ἐφάνητε ὡς κρίνα ἡδύπνοα, Ἱεράρχαι ψυχὰς ἐπιγνώσεσι, πιστῶν κατευφραίνοντες, εὐσεβείας, ἀξίως· διὸ μακαρίζεσθε."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.name
ocMA.OdeW8C13.text = "Γῆς πάσης παρεπίδημοι Ὅσιοι, ὡς οἱ τοῦ Θεοῦ Προφῆται γεγόνατε, τὴν ἄνω σκοπούμενοι, κληρουχίαν καὶ δόξαν, τὴν πάντοτε μένουσαν."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.name
ocMA.OdeW8C14.text = "Ὡς ζώντων καὶ νεκρῶν Λόγε Κύριος, τοὺς πίστει κοιμηθέντας οἰκέτας σου, χοροῖς συναρίθμησον, σῳζομένων ἁπάντων, ὡς μόνος φιλάνθρωπος."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m7_gr_GR_cog.TousEnKaminoPaidasMimoumenoi.name
ocMA.OdeW8C1T.text = "Ὁ τῶν Γυναίων ἁπάντων κατάλογος, ὁ ἀσκήσει καὶ ἀθλήσει Πανάμωμε, ζητήσας τὸν Κύριον, λιτανεύει ἀπαύστως, τὸ θεῖον σου πρόσωπον."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
ocMA.OdeW8C21.text = "Πρὸς τὴν οὐράνιον ἀτενίζοντες δόξαν, τῆς ἐπιγείου κατεφρόνησαν δόξης, οἱ Μάρτυρες τῆς Χριστοῦ παρουσίας, ὑμνοῦντες εὐσεβῶς, αὐτὸν ὡς Βασιλέα, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
ocMA.OdeW8C22.text = "Ὁ τὴν ἐπίγειον καταλύσας οἰκίαν, τῶν ἐπ' ἐλπίδι εἰς ζωὴν μεταστάντων, οὐράνιον κατοικίαν παράσχου, Δικαίων ἐν σκηναῖς, τούτους ἐπαναπαύων, εἰς πάντας τούς αἰῶνας."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
ocMA.OdeW8C23.text = "Νεκροῖς ἀνάπαυσιν, ὡς Θεὸς ὁ παρέχων, σὺ πότισον τῆς τρυφῆς τὸν χειμάρρουν, ὁ τὴν πηγὴν τῆς ζωῆς κεκτημένος, ὡς μόνος ἀγαθός, τοὺς προκεκοιμημένους, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m7_gr_GR_cog.TonMononAnarchon.name
ocMA.OdeW8C2T.text = "Φῶς τὸ ἀπρόσιτον, Θεοτόκε Παρθένε, ἐν τῇ γαστρί σου δεξαμένη ἀφράστως, ἐφώτισας τοὺς ἐν σκότει τοῦ βίου, δοξάζειν εὐσεβῶς, Χριστὸν τὸν προελθόντα, ἐκ σοῦ ἀνερμηνεύτως."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m7_gr_GR_cog.TinYperFysinMitera.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m7_gr_GR_cog.TinYperFysinMitera.name
ocMA.OdeW9C11.text = "Ἱερωτάτων Μαρτύρων, καὶ Προφητῶν καὶ Δικαίων, τῶν ἀπ' αἰῶνος καλῶς βεβιωκότων, ταῖς ἱκεσίαις Χριστέ, τὰς ψυχὰς ἡμῶν ἐλέησον."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m7_gr_GR_cog.TinYperFysinMitera.name
ocMA.OdeW9C12.text = "Ὡς λειτουργοὶ τοῦ Δεσπότου, Ἱερομύσται δειχθέντες, συνήφθητε Λειτουργοῖς ἐπουρανίοις, μεθ' ὧν τὰς ὑπὲρ ἡμῶν, ἱκεσίας προσαγάγετε."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m7_gr_GR_cog.TinYperFysinMitera.name
ocMA.OdeW9C13.text = "Σὺν Γυναιξὶ μακαρίως, τελειωσάσαις τὸν δρόμον, τιμήσωμεν Ἀσκητῶν χοροὺς ἁγίους, τοῖς τούτων ἁγιασμοῖς, ὅπως τύχωμεν ἐντεύξεως."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m7_gr_GR_cog.TinYperFysinMitera.name
ocMA.OdeW9C14.text = "Ἧς ἠξιώθησαν δόξης, αἱ τῶν Ἁγίων ἁπάντων, χοροστασίαι Χριστὲ τοὺς μεταστάντας, ἀξίωσον μετασχεῖν, ὀρθοδόξως σοι λατρεύσαντας."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m7_gr_GR_cog.TinYperFysinMitera.name
ocMA.OdeW9C1T.text = "Φιλαμαρτήμων ὑπάρχων, τὸ δικαστήριον τρέμω, τὸ φοβερὸν τοῦ ἐκ σοῦ Ἁγνὴ τεχθέντος, ἐν ᾧ με ὡς Ἀγαθή, ἀκατάκριτον συντήρησον."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m7_gr_GR_cog.MitirTheouKaiParthenos.name
ocMA.OdeW9C21.text = "Ἐπιφανῶς, οἱ γενναῖοι Μάρτυρες, καταλαμπρύνουσι κόσμον, οἱ τὴς πίστεως στῦλοι, Ἐκκλησιῶν οἱ ἀρραγέστατοι πύργοι, καὶ πρόβολοι τῆς εὐσεβείας, τούτους, ἐπαξίως εὐσεβῶς μακαρίζομεν."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m7_gr_GR_cog.MitirTheouKaiParthenos.name
ocMA.OdeW9C22.text = "Ῥῦσαι πυρός, τῆς γεέννης Δέσποτα, τοὺς ἐξ ἡμῶν μεταστάντας, τῆς πλευρᾶς σου τῇ λόγχῃ, τῆς ἁμαρτίας τὸ χειρόγραφον, τούτων ὡς μόνος φιλάνθρωπος ῥήξας, καὶ τῆς τῶν Ἁγίων ἀξιῶν λαμπρότητος."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m7_gr_GR_cog.MitirTheouKaiParthenos.name
ocMA.OdeW9C23.text = "Ὡς ἀγαθός, καὶ φιλάνθρωπος φύσει, ὡς συμπαθὴς καὶ οἰκτίρμων, ὡς ζωῆς ἀθανάτου,ὢν θησαυρὸς ὁ ἀδαπάνητος, Σῶτερ τοὺς πίστει προκεκοιμημένους, τῆς ἀκαταλήπτου σου τρυφῆς καταξίωσον."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode7
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m7_gr_GR_cog.MitirTheouKaiParthenos.name
ocMA.OdeW9C2T.text = "Νόμου σκιαί, καὶ τὰ πρὶν αἰνίγματα, τῇ σῇ γεννήσει παρῆλθον· καὶ γὰρ πλήρωμα νόμου, καὶ Προφητῶν ὁ Χριστὸς γέγονεν, ὃν ἐν δυσὶ ταῖς οὐσίαις ὑμνοῦντες, σὲ τὴν Ἀειπάρθενον Ἁγνὴν μακαρίζομεν."

