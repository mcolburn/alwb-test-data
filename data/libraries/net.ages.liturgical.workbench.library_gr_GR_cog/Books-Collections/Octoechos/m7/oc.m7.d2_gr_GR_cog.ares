octrMA.Photagogic.mode = misc_gr_GR_cog.Mode7
octrMA.Photagogic.text = "φῶς κατάπεμψον ταῖς ψυχαῖς ἡμῶν, διὰ τὸ μέγα σου ἔλεος."
