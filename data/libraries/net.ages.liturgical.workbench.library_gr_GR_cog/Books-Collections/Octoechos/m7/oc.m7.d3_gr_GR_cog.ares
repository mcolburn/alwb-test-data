octrMA.Triadic3.mode = misc_gr_GR_cog.Mode7
octrMA.Triadic3.text = oc.m7.d2_gr_GR_cog.octrMA.Triadic3.text

octrMA.Kathisma11.mode = misc_gr_GR_cog.Mode7
octrMA.Kathisma11.melody = ""
octrMA.Kathisma11.text = "Ὁ τῆς Πόρνης τὰ δάκρυα, καὶ τοῦ Πέτρου προσδεξάμενος, καὶ Τελώνην δικαιώσας ἐκ βάθους στενάξαντα, κᾀμὲ ἀπεγνωσμένον ἐξ ἔργων, οἴκτειρον Σωτήρ, καὶ σῶσόν με."

octrMA.Kathisma12.mode = misc_gr_GR_cog.Mode7
octrMA.Kathisma12.melody = ""
octrMA.Kathisma12.text = ocMA.Kathisma21.text

