A_Resource_Whose_Name = oc.m8.d6_gr_GR_cog

ocVE.Stichera1.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera1.melody = he.a.m8_gr_GR_cog.OAngelosSouKyrie.name
ocVE.Stichera1.text = "Λαὸς ὁ ἀνομώτατος, μέσον κατέκρινεν ἀνόμων, Ἰησοῦ μου σταυρωθῆναί σε, τὸν Κύριον καὶ κτίστην τοῦ παντὸς κόσμου, καὶ ὁρῶσα τὸ τόλμημα, ἡ γῆ ἐσείετο ἅπασα, πάντων ποιητήν σε, δεικνύουσα Δέσποτα."

ocVE.Stichera2.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera2.melody = he.a.m8_gr_GR_cog.OAngelosSouKyrie.name
ocVE.Stichera2.text = "Νόμου κατάρας ἅπαντες, λυτρούμεθα καὶ ἐγκλημάτων· ὁ γὰρ νόμου πέλων Κύριος, ἀνυψώθη ἐν τῷ Σταυρῷ τῷ τιμίῳ, εὐλογίαν πηγάζων ἀνελλιπῆ, καὶ χάριν καὶ ἔλεος, ἡμῖν τοῖς ὑμνοῦσιν αὐτοῦ, τὰ θεῖα παθήματα."

ocVE.Stichera3.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera3.melody = he.a.m8_gr_GR_cog.OAngelosSouKyrie.name
ocVE.Stichera3.text = "Ἰάτρευσας φιλάνθρωπε, πληγὰς ἡμῶν, πληγεὶς τὰς χεῖρας, πρὸς ὁδοὺς δὲ βαίνειν Κύριε, σωτηρίους κατεύθυνας πάντας, πόδας σου ὀρυχθεὶς ἐπὶ Σταυροῦ, οὓς πάλαι ὁ πρωτόπλαστος, ἐν Παραδείσῳ, ἰδὼν κατεκρύπτετο."

ocVE.Stichera4.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera4.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
ocVE.Stichera4.text = "Χαῖρε Θεοτόκε Δέσποινα, χαῖρε τὸ σκῆπτρον Δαυΐδ· χαῖρε πύλη οὐράνιε· χαῖρε βάτε ἄφλεκτε· χαῖρε ὄρος κατάσκιον· χαῖρε λυχνία· χαῖρε Πανάχραντε· χαῖρε ἡ στάμνος· χαῖρε παλάτιον· χαῖρε πανάμωμε, Μήτηρ τοῦ Θεοῦ ἡμῶν· χαῖρε λαβίς· χαῖρε θεία τράπεζα· χαῖρε τοῦ κόσμου ἐλπίς."

ocVE.Stichera5.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera5.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
ocVE.Stichera5.text = "Ἁμαρτιῶν μου τὰ κύματα, ὡς ἐν πελάγει δεινῷ, ὑπεράνω ὑψούμενα, κεφαλῆς ποντίζειν με, κατεπείγουσι Δέσποινα, ἀλλ' ἔκτεινόν μοι, χεῖρα ὡς Πέτρῳ πρίν, βυθιζομένῳ, καὶ εἰς λιμένα με, σῷον, ὁδήγησον, μετανοίας δέομαι, Μήτηρ Θεοῦ, ἄχραντε πανύμνητε, ἐλπὶς τοῦ κόσμου παντός."

ocVE.Stichera6.mode = misc_gr_GR_cog.Mode8
ocVE.Stichera6.melody = he.a.m8_gr_GR_cog.OTouParadoxou.name
ocVE.Stichera6.text = "Ἐσκοτισμένος τοῖς πάθεσι, τῆς ἁμαρτίας δεινῶς, εἰς κρημνὸν ἀπογνώσεως, σπεύδω ὁ ταλαίπωρος, ἐμπεσεῖν, σύ με οἴκτειρον, εὐλογημένη θεοχαρίτωτε, λάμψον μοι φέγγος, τῆς εὐσπλαγχνίας σου, καὶ ἐφοδήγησον, εἰς ὁδοὺς εὐθείας με, παρακαλῶ, Πάναγνε καὶ δέομαι· Μὴ ὑπερίδῃς με."

ocVE.SticTheotokion.mode = misc_gr_GR_cog.Mode8
ocVE.SticTheotokion.melody = he.a.m8_gr_GR_cog.OiMartyresSouKyrie.name
ocVE.SticTheotokion.text = "Ἡ Πάναγνος ὡς εἶδέ σε, Σταυρῷ κρεμάμενον ἐθελουσίως, κατανοοῦσά σου τὴν δυναστείαν, λύπῃ συνείχετο Χριστέ, καὶ ὀδυρομένη ἀνεβόα σοι· Τέκνον μή με λίπῃς τήν τεκοῦσάν σε, δός μοι λόγον Υἱέ μου, μή με σιγῶν παρέλθῃς, Λόγε Θεοῦ τὴν δούλην σου."

ocVE.Aposticha1.mode = misc_gr_GR_cog.Mode8
ocVE.Aposticha1.melody = ""
ocVE.Aposticha1.text = oc.m8.d4_gr_GR_cog.ocVE.Aposticha1.text

ocVE.Aposticha2.mode = misc_gr_GR_cog.Mode8
ocVE.Aposticha2.melody = ""
ocVE.Aposticha2.text = oc.m8.d4_gr_GR_cog.ocVE.Aposticha2.text

ocVE.Aposticha3.mode = misc_gr_GR_cog.Mode8
ocVE.Aposticha3.melody = ""
ocVE.Aposticha3.text = oc.m8.d2_gr_GR_cog.ocVE.Aposticha3.text

ocVE.AposTheotokion.mode = misc_gr_GR_cog.Mode8
ocVE.AposTheotokion.melody = he.a.m8_gr_GR_cog.OiMartyresSouKyrie.name
ocVE.AposTheotokion.text = "Ὁρῶσά σε σταυρούμενον, καὶ θανατούμενον ἡ Θεοτόκος· Οἴμοι! ἐκραύγαζε, πῶς τὰς ὀδύνας φέρεις, γλυκύτατε Υἱὲ; βάλλει τὴν καρδίαν μου ἡ λόγχη σου, φλέγει δὲ τὰ σπλάγχνα μου τὸ πάθος σου, ὅμως ὑμνολογῶ σε· θέλων γὰρ ταῦτα πάσχεις, σῶσαι τὸν ἄνθρωπον."

ocVE.ApolTheotokionVM.mode = misc_gr_GR_cog.Mode8
ocVE.ApolTheotokionVM.melody = ""
ocVE.ApolTheotokionVM.text = oc.m8.d4_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocMA.Kathisma11.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma11.melody = ""
ocMA.Kathisma11.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma11.text

ocMA.Kathisma12.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma12.melody = ""
ocMA.Kathisma12.text = oc.m8.d4_gr_GR_cog.ocMA.Kathisma12.text

ocMA.Kathisma13.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma13.melody = he.a.m8_gr_GR_cog.ToProstachthen.name
ocMA.Kathisma13.text = "Τὸν ἐξ αἱμάτων σου ἁγνῶν, σωματωθέντα, καὶ ὑπὲρ ἔννοιαν ἐκ σοῦ, σεμνὴ τεχθέντα, ἐπὶ ξύλου κρεμάμενον, μέσον τῶν κακούργων, ὁρῶσα, τὰ σπλάγχνα ἤλγεις, καὶ μητρικῶς θρηνοῦσα, ἐβόας· Οἴμοι! Τέκνον ἐμόν, τίς ἡ θεία καὶ ἄφατος, οἰκονομία σου δι' ἧς, ἐζώωσας τὸ πλάσμα σου! Ἀνυμνῶ σου τὸ εὔσπλαγχνον."

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma21.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
ocMA.Kathisma21.text = "Ἐν μέσῳ τῆς Ἐδέμ, ξύλον θάνατον ἤνθησεν, ἐν μέσῳ δὲ τῆς γῆς, ξύλον ζωὴν ἐβλάστησε· γευσάμενοι γὰρ τοῦ πρώτου, ἄφθαρτοι ὄντες, φθαρτοὶ γεγόναμεν, σχόντες δὲ τοῦ δευτέρου, τῆς ἀφθαρσίας κατετρυφήσαμεν· ἐν τῷ Σταυρῷ γὰρ ἔσωσε Χριστός, τὸ γένος τῶν ἀνθρώπων."

ocMA.Kathisma21b.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma21b.melody = he.a.m8_gr_GR_cog.TinSofianKaiLogon.name
ocMA.Kathisma21b.text = "Ὁ Σταυρὸν ὑπομείνας, ὡς ἀγαθός, τοῦ Ἀδὰμ τὴν πτωχείαν, ἀναλαβών, μόνε Ὑπεράγαθε, ἀναμάρτητε Κύριε, τοῦ θανάτου εὔσπλαγχνε, τὸ κράτος κατέλυσας, καὶ τῷ πάθει σου Δέσποτα, τὸν κόσμον διέσωσας· ὅθεν Ζωοδότα, τὴν πεσοῦσαν ψυχήν μου, οἰκτείρησον δέομαι, ὅταν κρίνῃς τὰ ἔργα μου, Δικαιοκρῖτα φιλάνθρωπε, καὶ δός μοι πανοικτίρμον ὁ Θεός, τῶν πταισμάτων ἄφεσιν ὡς εὔσπλαγχνος, ὅτι μόνος ὑπάρχεις ἀναμάρτητος."

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma22.melody = he.a.m8_gr_GR_cog.ToProstachthen.name
ocMA.Kathisma22.text = "Ἐν Παραδείσῳ μὲν τὸ πρίν, ξύλον ἐγύμνωσεν, ἐπὶ τῇ γεύσει ὁ ἐχθρός, εἰσφέρων νέκρωσιν, τοῦ Σταυροῦ δὲ τὸ ξύλον, τῆς ζωῆς τὸ ἔνδυμα, ἀνθρώποις, φέρον ἐπάγη, ἐπὶ τῆς γῆς, καὶ κόσμος, ὅλος ἐπλήσθη, πάσης χαρᾶς, ὃν ὁρῶντες ὑψούμενον, Θεῷ ἐν πίστει λαοί, συμφώνως ἐκβοήσωμεν· Πλήρης δόξης ὁ οἶκός σου."

ocMA.Kathisma22b.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma22b.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
ocMA.Kathisma22b.text = "Ἐν μέσῳ τῆς Ἐδέμ, ξύλον θάνατον ἤνθησεν, ἐν μέσῳ δὲ τῆς γῆς, ξύλον ζωὴν ἐβλάστησε· γευσάμενοι γὰρ τοῦ πρώτου, ἄφθαρτοι ὄντες, φθαρτοὶ γεγόναμεν, σχόντες δὲ τοῦ δευτέρου, τῆς ἀφθαρσίας κατετρυφήσαμεν· ἐν τῷ Σταυρῷ γὰρ ἔσωσε Χριστός, τὸ γένος τῶν ἀνθρώπων."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma23.melody = he.a.m8_gr_GR_cog.ToProstachthen.name
ocMA.Kathisma23.text = "Ἐκ τῶν αἱμάτων τῶν ἐμῶν, βροτὸς γεγέννησαι, ἐκ τῶν αἱμάτων δὲ τῶν σῶν, βροτοὺς ἐθέωσας, τοὺς βρώσει δὲ φθαρέντας, ζητῶν κατῆλθες, ὡς ὑπεράγαθος, ἐβόα πάλαι ἡ ἄμεμπτος, ἐν κλαυθμῷ, Χριστὸν θρηνοῦσα κρεμάμενον, ἐν Σταυρῷ, ὃν ὁρῶσα νεκρούμενον, δακρύων ἔρρει κρουνούς, τὰς ὄψεις καταξαίνουσα, καὶ αὐτὸν μεγαλύνουσα."

ocMA.Kathisma31.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma31.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
ocMA.Kathisma31.text = "Θεώσεως ψευσθείς, ὁ Γενάρχης ἐλπίδος, φθορᾶς τοῖς ἐξ αὐτοῦ, πᾶσιν αἴτιος ὤφθη. Σὺ δέ διὰ Σταυροῦ σου, ζωὴν πηγάζεις ὡς Ὑπεράγαθος· θέλων καὶ γὰρ ἐπάγης, ἵνα τὸ πρῶτον λύσῃς κατάκριμα· διὸ ὑμνοῦμέν σου Χριστέ, τὸ πάθος τὸ ἑκούσιον."

ocMA.Kathisma32.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma32.melody = ""
ocMA.Kathisma32.text = "Φωτὶ οὐρανίῳ καταλάμπεται, σήμερον αὕτη ἡ σκηνή· ἐν αὐτῇ γὰρ στρατιαὶ Ἀγγέλων ἀγάλλονται, σὺν αὐταῖς καὶ Μαρτύρων χοροὶ εὐφραίνονται, ἐπὶ τῇ μνήμῃ τῶν Ἀθλοφόρων. Αὐτῶν ταῖς παρακλήσεσι, κατάπεμψον τῷ κόσμῳ σου, τὴν εἰρήνην Χριστέ, καὶ τὸ μέγα ἔλεος."

ocMA.Kathisma33.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma33.melody = he.a.m8_gr_GR_cog.AnestisEkNekron.name
ocMA.Kathisma33.text = "Ὢ θαύματος καινοῦ! ὢ φρικτοῦ Μυστηρίου! ἐβόα ἡ Ἀμνάς, τὸν Υἱὸν καθορῶσα, ξύλῳ Σταυροῦ παγέντα. Τί τοῦτο Λόγε Θεοῦ ἀθάνατε; πῶς νεκρὸς ὁρᾶσαι, καὶ γῆν σαλεύεις ὡς παντοδύναμος; ἀλλὰ ὑμνῶ σου τὴν φρικτήν, καὶ θείαν συγκατάβασιν."

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχίς· Πλήρωμα πάντων τῶν καλῶν Σταυρὸς πέλει, Ἀμήν. Ἰωσήφ."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m8_gr_GR_cog.StavronCharaxasMosis.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m8_gr_GR_cog.StavronCharaxasMosis.name
ocMA.OdeW1C11.text = "Παλάμας θείας Σταυρῷ, Ἰησοῦ ἁπλώσας, πρὸς σεαυτὸν συνήγαγες, χειρῶν σου τὸ πλαστούργημα, καὶ ἐκ χειρὸς πονηροῦ, πάντας ἠλευθέρωσας, καὶ καθυπέταξας, χειρὶ κραταιοτάτῃ, Βασιλεῦ τῶν ἁπάντων· διὸ πιστοὶ ᾄσωμεν, τῷ μεγέθει σου ὅτι δεδόξασται."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m8_gr_GR_cog.StavronCharaxasMosis.name
ocMA.OdeW1C12.text = "Λυμαντικὴ ἐν Ἐδέμ, γέγονε τοῦ ξύλου, ἡ ἀπευκτὴ ἀπόγευσις, τὸν θάνατον εἰσάξασα, Χριστὸς δὲ θανατωθείς, ἐπὶ ξύλου ἅπασι, ζωὴν ἐπήγασε, καὶ ἔκτεινε τὸν ὄφιν, θεϊκῇ δυναστείᾳ· διὸ αὐτῷ ᾄσωμεν τῷ Θεῷ ἡμῶν, ὅτι δεδόξασται."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m8_gr_GR_cog.StavronCharaxasMosis.name
ocMA.OdeW1C13.text = "Ἠνδρίσαντο Ἰησοῦ, τῶν Μαρτύρων πλήθη, Σταυρῷ τε καὶ τῷ πάθει σου, παθῶν κατεξανίστασθαι, καὶ ἔναντι τῶν ἐχθρῶν, σὲ τὸν Βασιλεύοντα, ὁμολογεῖν τῆς κτίσεως, προσφέρειν τε βασάνους, καὶ κινδύνους ἀπείρους· διὸ τῆς σῆς δόξης, ἔτυχον ἅπαντες, τῆς ἀϊδίου ζωῆς."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m8_gr_GR_cog.StavronCharaxasMosis.name
ocMA.OdeW1C14.text = "Ῥαντίσματι θεουργῷ, τοῦ σεπτοῦ σου αἵματος, οἱ Μάρτυρές σου Κύριε, τὴν λύτρωσιν εὑράμενοι, τὰ αἵματα ἑαυτῶν, ἀληθῶς ἐκένωσαν, ἀδίκως τυραννούμενοι, σπονδάς προσφέρειν πᾶσι, δαίμοσι ψυχοφθόροις· ὅθεν σεπτὰ θύματα, σοὶ προσηνέχθησαν, τῷ τοῦ παντὸς Βασιλεῖ."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m8_gr_GR_cog.StavronCharaxasMosis.name
ocMA.OdeW1C1T.text = "Ὡς ἔβλεψέ σε Σταυρῷ, τὸν τὰ πάντα βλέποντα, ἡλούμενον ἡ ἄμεμπτος, ὀδυρομένη ἔλεγε· Τί τοῦτο τέκνον ἐμόν, τί σοι ἀνταπέδωκαν οἱ ἀπολαύσαντες, πολλῶν σου δωρημάτων, πῶς ὑποίσω τὸν πόνον; Δόξα τῇ σῇ εὔσπλαγχνε, καὶ μακρόθυμε, οἰκονομίᾳ φρικτῇ."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
ocMA.OdeW1C21.text = "Τὸ Θεοτόκον σε κηρύττειν ἄχραντον, πάσης αἱρέσεως, ἀποτροπὴν φέρει· σάρκα γὰρ γενόμενον, ἀναλλοιώτως ἔτεκες, τὸν ἐπέκεινα πάσης, Θεογεννῆτορ τῆς κτίσεως, Λόγον τοῦ Θεοῦ τὸν ἀΐδιον."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
ocMA.OdeW1C22.text = "Ὡς Θεοῦ Μήτηρ παρρησίαν ἔχουσα, πρὸς τὸν τεχθέντα ἐκ σοῦ, μονογενῆ Λόγον, καὶ Πατρὶ συνάναρχον, καὶ συμφυῆ τῷ Πνεύματι, δυσωποῦσα μὴ παύσῃ, κινδύνων σῶσαι Πανάμωμε, τοὺς σὲ Θεοτόκον δοξάζοντας."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
ocMA.OdeW1C23.text = "Σὲ Θεομῆτορ προστασίαν ἔχοντες, οὐ δειλιάσομεν, τὸ τῶν ἐχθρῶν στῖφος, τὸ ἀντιμαχόμενον, τοῖς εὐσεβῶς δοξάζουσι, τὸν ἐκ σοῦ σαρκωθέντα, Θεὸν τῶν ὅλων καὶ Κύριον, ὃν ὑπὲρ ἡμῶν ἐκδυσώπησον."

ocMA.OdeW1C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C24.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C24.melody = he.h.m8_gr_GR_cog.ArmatilatinFarao.name
ocMA.OdeW1C24.text = "Νοῦς καὶ οὐράνιος τὸ σὸν Μυστήριον, τὸ ἀκατάληπτον, κατανοεῖν ὄντως, ἀτονεῖ Πανύμνητε· τοῦ γὰρ Πατρὸς ὁ σύνθρονος, ἐν γαστρί σου σκηνώσας, ἐκ σοῦ τεχθῆναι εὐδόκησε, φύσεσι διτταῖς γνωριζόμενος."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m8_gr_GR_cog.RavdosEisTypon.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m8_gr_GR_cog.RavdosEisTypon.name
ocMA.OdeW3C11.text = "Μέλανα κόσμον, μετημφιάσθη ὁρῶν ὁ ἥλιος, ἐπὶ ξύλου σε ἀδίκως ἀρθέντα, καὶ διερράγησαν πέτραι, καὶ γῆ φόβῳ πᾶσα ἔφριξε, μόνε Σωτήρ, εἰς πάντων ἀπολύτρωσιν."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m8_gr_GR_cog.RavdosEisTypon.name
ocMA.OdeW3C12.text = "Ἁπλώσας χεῖρας, Μωσῆς ἐτύπου Σταυρὸν τὸν τίμιον, ὃν ἠμεις νῦν ἐκτυποῦντες ἐμφρόνως, ἔθνη ἀλλόφυλα πάντα, των δαιμόνων ἐκτρεπόμεθα, βλάβης ἐκτός, ἀπάσης διαμένοντες."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m8_gr_GR_cog.RavdosEisTypon.name
ocMA.OdeW3C13.text = "Παθῶν κρατοῦντες, οἱ Ἀθλοφόροι πάθη εἰκόνιζον, πολυτρόποις πάθεσιν ὁμιλοῦντες, τοῦ ἑκουσίως παθόντος, καὶ τὰ πάθη θανατώσαντος, καὶ τὴν ζωήν, τῷ κόσμῳ ἀνατείλαντος."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m8_gr_GR_cog.RavdosEisTypon.name
ocMA.OdeW3C14.text = "Ἀνεπιστρόφως, τοῦ μαρτυρίου ὁδὸν βαδίζοντες, τὰ προσκόμματα τῆς πλάνης ἐκ μέσου, οἱ εὐκλεεῖς Ἀθλοφόροι ἐποίησαν, καὶ κατήντησαν πρὸς καταπαύσεις, τὰς θείας εὐφραινόμενοι."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m8_gr_GR_cog.RavdosEisTypon.name
ocMA.OdeW3C1T.text = "Νομίμων δίχα, τῶν ἀνθρωπίνων σε Τέκνον ἔτεκον, καὶ πῶς ἄνομοι ὑψοῦσί σε ξύλῳ, μέσον ἀνόμων τὸν μόνον, νόμους ζωῆς ἐκτιθέμενον; ἡ Θεοτόκος, θρηνῳδοῦσα ἀπεφθέγγετο."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m8_gr_GR_cog.OStereosasKatArchas.name
ocMA.OdeW3C21.text = "Νενεκρωμένον μου τὸν νοῦν, τῇ τῆς ζωῆς ἐνεργείᾳ, τῆς ἐκ σοῦ φανερωθείσης τῷ κόσμῳ, διανάστησον, οὐλάς, καὶ τύπους ἐξαλείφουσα, τῆς ἁμαρτίας μόνη, Θεογεννῆτορ πανάμωμε."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m8_gr_GR_cog.OStereosasKatArchas.name
ocMA.OdeW3C22.text = "Ῥήσεις τῶν θείων Προφητῶν, καὶ τὰς αὐτῶν προφητείας, ἐπεσφράγισας, τεκοῦσα τὸν Λόγον, τὸν λαλοῦντα δι' αὐτῶν, καὶ τὰς αὐτῶν πληρώσαντα, θεηγορίας μόνη, εὐλογημένη Πανάμωμε."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m8_gr_GR_cog.OStereosasKatArchas.name
ocMA.OdeW3C23.text = "Ἀπειρογάμως ἐκ Πατρός, τὸν πρὸ αἰώνων ἀφράστως, ἐν γαστρί σου συλλαβοῦσα Παρθένε, ἀπεγέννησας ἡμῖν, Θεὸν ὁμοῦ καὶ ἄνθρωπον, ἐν ἑκατέρᾳ φύσει, τέλειον οὐ διαιρούμενον."

ocMA.OdeW3C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C24.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C24.melody = he.h.m8_gr_GR_cog.OStereosasKatArchas.name
ocMA.OdeW3C24.text = "Σοῦ τὴν ἁγίαν ὁ Θεός, γαστέρα κατασκηνώσας, ἐσαρκώθη Θεομῆτορ ὡς οἶδε, καὶ διέσωσεν ἡμᾶς, τοῖς ζωηφόροις πάθεσιν· ὅθεν σε σωτηρίας, πύλην Παρθένε γινώσκομεν."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m8_gr_GR_cog.EisakikoaKyrie.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C11.text = "Τῷ Σταυρῷ σου Παράδεισος, Σῶτερ ἀνεῴχθη, καὶ ὁ κατάκριτος, εἰσῳκίσθη πάλιν ἄνθρωπος, τὴν σὴν μεγαλύνων, ἀγαθότητα."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C12.text = "Ὡσεὶ κέδρον εὐσέβειαν, πίστιν ὡς κυπάρισσον, τὴν ἀγάπην τε, ὡσεὶ πεύκην περιφέροντες, τὸν Σταυρὸν τὸν θεῖον, προσκυνήσωμεν."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C13.text = "Νεκρωθέντας ἐζώωσας, Σῶτερ τοὺς ἀνθρώπους, νεκρὸς γενόμενος, καὶ τὸν ὄφιν ἀπενέκρωσας, τὸν τὴν ἁμαρτίαν ἐπεισάξαντα."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C14.text = "Τῶν παθῶν χρηματίσαντες, σύμμορφοι οἱ θεῖοι, τοῦ Χριστοῦ Μάρτυρες, κοινωνοὶ καὶ τῆς λαμπρότητος, τῆς ἐπουρανίου, ἐχρημάτισαν."

ocMA.OdeW4C15.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C15.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C15.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C15.text = "Ὡραιώθητε Μάρτυρες, Λόγῳ τῷ ὡραίῳ, εὐαρεστήσαντες, ἐλαμπρύνθητε τὸν Ἥλιον, τῆς δικαιοσύνης, ἀγαπήσαντες."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C1T.text = "Νέον βρέφος ἐκύησας, τὸν πρὸ τῶν αἰώνων, ὄντα παντέλειον, καὶ Σταυρῷ Κόρη πανάμωμε, τελειοῦντα, πάντα ἀγαθότητι."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C21.text = "Μετὰ Θεόν, σὲ προστασίαν κεκτήμεθα· σὺ γὰρ Μήτηρ, Θεοῦ ἐχρημάτισας, τοῦ ποιητοῦ καὶ δημιουργοῦ, καὶ τὴν ἡμετέραν, μορφὴν λαβόντος Πανάμωμε, καὶ ταύτην σεσωκότος, ἐκ φθορᾶς καὶ θανάτου, καὶ τῇ ἑαυτοῦ δόξῃ δοξάσαντος."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C22.text = "Ὡς ἐκ φυλῆς, τοῦ Βασιλέως ὑπάρχουσα, τὸν τῶν ὅλων, κατεξουσιάζοντα, Λόγον Θεὸν τέτοκας ἡμῖν, σάρκα γεγονότα, καὶ μείναντα ἀναλλοίωτον· διό σε Θεοτόκον, ἀληθῶς καὶ κυρίως, Παναγία Παρθένε δοξάζομεν."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C23.text = "Θεὸν ἐκ σοῦ, τὸν σαρκωθέντα δυσώπησον, τὸν ἀτρέπτως, ὃ ἦν διαμείναντα, καὶ φυσικῶς ἴσον τῷ Πατρί, καὶ σοὶ τῇ Τεκούσῃ, γενόμενον ὁμοούσιον, συγχώρησιν πταισμάτων, καὶ ψυχῶν σωτηρίαν, τοῖς ὑμνοῦσί σε πίστει δωρήσασθαι."

ocMA.OdeW4C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C24.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C24.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C24.text = "Νῦν τὴν μορφήν, τῶν ἀθετούντων κατάβαλε, τῆς εἰκόνος, Κόρη τὴν προσκύνησιν, σοῦ τῆς σεπτῆς, καὶ τοῦ ὑπὲρ νοῦν, ἐκ σοῦ σαρκωθέντος, καὶ κόσμον ὅλον φωτίσαντος, καὶ πάντων τῶν Ἁγίων, τοὺς δὲ πίστει τιμῶντας, καταφαίδρυνον μόνη Πανύμνητε."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m8_gr_GR_cog.OTrismakaristonXylon.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m8_gr_GR_cog.OTrismakaristonXylon.name
ocMA.OdeW5C11.text = "Καταστολὴν ἀφθαρσίας, τοὺς γυμνωθέντας ἡμᾶς, ἐνδῦσαι ἐφιέμενος, ἀπεξεδύθης καὶ Σταυρῷ ἐξετάθης, καὶ ἐγύμνωσας ἐχθροῦ, Χριστὲ τὰ πανουργεύματα· διὰ τοῦτό σου, τὰ παθήματα δοξάζομεν."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m8_gr_GR_cog.OTrismakaristonXylon.name
ocMA.OdeW5C12.text = "Αἷμα πλευρᾶς ἀποστάξαν, σωτηριώδους σαφῶς, τὸν κόσμον ἀπεκάθηρεν, ἐπιβώμιά τε αἵματα καταπαῦσαν, τοὺς φθαρέντας τῷ καρπῷ, τῆς βρώσεως ἀνέπλασε, καὶ ἐπήγασεν, ἀφθαρσίαν ταῖς ψυχαῖς ἡμῶν."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m8_gr_GR_cog.OTrismakaristonXylon.name
ocMA.OdeW5C13.text = "Λελαμπρυσμένοι τῷ κάλλει, πολυειδῶν αἰκισμῶν, οἱ Μάρτυρες οἱ ἔνδοξοι, θείῳ αἵματί σου σημειωθέντες, τὴν κωλύουσαν τὸ πρίν, διήλθοσαν ῥομφαίαν σαφῶς, καὶ ἐσκήνωσαν, γεγηθότες εἰς Παράδεισον."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m8_gr_GR_cog.OTrismakaristonXylon.name
ocMA.OdeW5C14.text = "Ὡς θαυμαστὸς σὺ ὑπάρχεις, ἐν τοῖς Ἁγίοις Χριστέ, τοῖς πίστει σε ποθήσασι! διὰ σοῦ γὰρ πλουτισθέντες τὰ θεῖα, ἰαμάτων ποταμούς, ἐν τῷ κόσμῳ ἀναβλύζουσι, καὶ ξηραίνουσι, διεκχύσεις τῶν παθῶν ἡμῶν."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m8_gr_GR_cog.OTrismakaristonXylon.name
ocMA.OdeW5C1T.text = "Νενοσηκότας Παρθένε, τῇ ἁμαρτίᾳ ἡμᾶς, ἰατρευσας κυήσασα, τὸν Σωτῆρα ἰατρόν τε τῶν ὅλων, καὶ ἐν ξύλῳ τοῦ Σταυροῦ, τραυματισθέντα Ἄχραντε, τὸν παρέχοντα, ἀφθαρσίαν ταῖς ψυχαῖς ἡμῶν."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C21.text = "Ἀπειράνδρως Παρθένε, τὸν ἀπεριόριστον, φύσει Θεότητος, ἀνδρωθέντα Λόγον, δι' ἡμᾶς τοὺς ἀνθρώπους συνέλαβες, ἐξ αἱμάτων Κόρη, παρθενικῶν διαπλασθέντα, ὑπέρ λόγον καὶ νόμον τῆς φύσεως."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C22.text = "Ὡραιώθης Παρθένε, τοῖς τῆς παρθενίας σου, ἀχράντοις κάλλεσι, καὶ τῆς πρώτης Εὔας, περιέστειλας αἶσχος τὸ δύσμορφον, τὸν Χριστὸν τεκοῦσα, περιβολὴν ἀθανασίας, χαριζόμενον τοῖς σὲ γεραίρουσι."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C23.text = "Ἀληθῆ Θεοτόκον, πάντες ἐπιγνόντες σε, πάναγνε Δέσποινα, τὸν ἐκ σοῦ τεχθέντα, Θεὸν Λόγον εἰδότες κηρύττομεν, ἐν δυσὶν οὐσίαις, θελητικαῖς, αὐτεξουσίαις, ὑπὲρ λόγον καὶ νόμον τῆς φύσεως."

ocMA.OdeW5C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C24.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C24.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C24.text = "Οἱ σὲ τεῖχος πλουτοῦντες, καὶ τῇ προστασίᾳ σου, περιφρουρούμενοι, τῇ σῇ θείᾳ δόξῃ, ἐγκαυχώμενοι σὲ μακαρίζομεν· σὺ γὰρ Παναγία, τὴν τῶν ψυχῶν ἡμῶν πηγάζεις, εὐφροσύνην καὶ τὴν ἀγαλλίασιν."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m8_gr_GR_cog.NotiouThiros.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m8_gr_GR_cog.NotiouThiros.name
ocMA.OdeW6C11.text = "Σταυρὸς ἐπάγη ἐν τόπῳ, Κρανίου, μέσον γῆς, καὶ τὴν ἐν μέσῳ Παραδείσου, ἐκβλαστήσασαν ξύλῳ, πληγὴν ἰάσατο· ὁ γὰρ Μεσσίας Ἰησοῦς, μέσον ἀνόμων, μόνος δίκαιος λῃστῶν κρεμάμενος, ἑαυτῷ συνανύψωσεν ἅπαντας, καὶ τὸν ἐξ ὕψους πεσόντα, εἰς χάος ἀπέρριψε."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m8_gr_GR_cog.NotiouThiros.name
ocMA.OdeW6C12.text = "Τὸ τόξον Χριστὲ ἐκτείνας τοῦ θείου, καὶ πανσέπτου Σταυροῦ ἔρριψας βέλη, κατὰ τοῦ παλαμναίου, τοὺς ἥλους σου τῶν χειρῶν, καὶ ἐγκατέπηξας αὐτοῦ, εἰς τήν ὀργίλον, καὶ παμβέβηλον καρδίαν Δέσποτα, καὶ τελείως αὐτὸν ἐθανάτωσας, καὶ ὑπ' αὐτοῦ τοὺς θανέντας, Οἰκτίρμον ἐζώωσας."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m8_gr_GR_cog.NotiouThiros.name
ocMA.OdeW6C13.text = "Αἱμάτων ῥεύσαντα ῥεῖθρα, σωμάτων, ἐξ ἁγίων Ἀθλητῶν ἔσβεσαν πᾶσαν, τῆς εἰδωλομανίας πυρὰν ἐν Πνεύματι, καὶ Ἐκκλησίας τὰς σεπτάς, αὔλακας ἢρδευσαν, καὶ ἄσταχυν βλαστάνειν ἔπεισαν, σωτηρίου ἐλπίδος καὶ πίστεως, δι' ὧν ἐκτρέφεται πᾶσα, ψυχὴ θείᾳ χάριτι."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m8_gr_GR_cog.NotiouThiros.name
ocMA.OdeW6C14.text = "Ὑφῆπτον τὸ πῦρ πλειόνως, τῆς γνώμης, τῆς ἐνθέου εὐσεβῶς οἱ Ἀθλοφόροι, ὀπηνίκα ἐνύλῳ πυρὶ φλογίζεσθαι, πονηροτάτων δικαστῶν, ψήφῳ ἐδίδοντο, καὶ ἄφλεκτοι διεφυλάττοντο, ἐνεργείᾳ καὶ χάριτι Πνεύματος, τοῦ στεφανώσαντος τούτους, νομίμως ἀθλήσαντας."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m8_gr_GR_cog.NotiouThiros.name
ocMA.OdeW6C1T.text = "Ῥομφαία τὴν σὴν διῆλθε, καρδίαν, καθορώσης σὸν Υἱὸν Ἄχραντε Κόρη, ἡπλωμένον ἐν ξύλῳ, καὶ πάθη φέροντα, καὶ λόγχῃ θείαν πλευράν, ἐξορυττόμενον, θελήματι, καὶ κατασφάττοντα, ἐν αὐτῇ τὸν πολέμιον δράκοντα· ὅθεν θρηνοῦσα μητρικῶς τοῦτον ἐμεγάλυνες."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C21.text = "Ἐπῆλθε τὸ παντουργόν, ἐπὶ σὲ Πνεῦμα Πανάμωμε, καὶ Λόγος ὁ τοῦ Θεοῦ, ἐν σοὶ κατεσκήνωσε, καὶ σάρξ ἐχρημάτισεν, ἀνεκδιηγήτως, διαμείνας ἀναλλοίωτος."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C22.text = "Ὡς ἔμψυχος κιβωτός, τὸν Νομοδότην ἐχώρησας, ὡς ἅγιος δὲ ναός, ἐδέξω τὸν Ἅγιον, γενόμενον ἄνθρωπον, ἐπ' εὐεργεσίᾳ, τῶν ἀνθρώπων Παναμώμητε."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C23.text = "Κυρίαν σε τοῦ παντός, καὶ Δέσποιναν ὀνομάζομεν, τὸν ὄντως ὄντα Θεόν· ἀρρήτως γὰρ τέτοκας, τὸν πάντα ποιήσαντα, καὶ διακρατοῦντα, καὶ συνέχοντα Πανάμωμε."

ocMA.OdeW6C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C24.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C24.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C24.text = "Θεὸν τεχθέντα ἐκ σοῦ, ἐπέγνωμεν Θεονύμφευτε, ὃν ἐξιλέωσαι νῦν, ἡμῖν ὡς ἰσχύουσα, τοῖς πίστει κραυγάζουσι, δωρηθῆναι λύσιν, τῶν πταισμάτων Παναμώμητε."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m8_gr_GR_cog.EknoonProstagma.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m8_gr_GR_cog.EknoonProstagma.name
ocMA.OdeW7C11.text = "Ὅτε ἐφήπλωσαι, ἐν ξύλῳ τοῦ Σταυροῦ, καθάπερ ἄμπελος, Λόγε τοῦ Πατρός, γλεῦκος ἀπέσταξας μυστικόν, παύων τὴν μέθην τῆς παραβάσεως, καὶ πάντας εὐφραίνων τοὺς εἰδότας σε, Θεὸν καὶ Κτίστην ἑκουσίως πάσχοντα, καὶ σῴζοντα τοὺς ψάλλοντας· ὁ ὑπερύμνητος, τῶν Πατέρων καὶ ἡμῶν, Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m8_gr_GR_cog.EknoonProstagma.name
ocMA.OdeW7C12.text = "Σταύρωσιν ἤνεγκας, Χριστέ μου Ἰησοῦ, τὴν ἐπονείδιστον, παύων τὰ βροτῶν, πάντα ὀνείδη καὶ στεναγμούς, χολῆς ἐγεύσω πικρίαν ἅπασαν, ἐξαίρων κακίας, ἐπλήγης ἡμῶν, πληγὰς Οἰκτίρμον ψυχικὰς ἰώμενος, καὶ ψάλλειν προτρεπόμενος, ὁ ὑπερύμνητος, τῶν Πατέρων καὶ ἡμῶν, Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m8_gr_GR_cog.EknoonProstagma.name
ocMA.OdeW7C13.text = "Πόνοις τὴν ἄπονον, γενναῖοι Ἀθληταί, ζωὴν ἐκτήσασθε· ὅθεν τοὺς ἡμῶν, πόνους κουφίζετε παντελῶς, χάριν λαβόντες ἐξ ὕψους ἔνδοξοι, ἰᾶσθαι τὰ πάθη καὶ πνεύματα, ἀποδιώκειν καὶ πιστῶν προΐστασθαι, καὶ σῴζειν τοὺς κραυγάζοντας· Ὁ ὑπερύμνητος, τῶν Πατέρων καὶ ἡμῶν, Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m8_gr_GR_cog.EknoonProstagma.name
ocMA.OdeW7C14.text = "Ἔστητε Μάρτυρες, πρὸ βήματος Χριστόν, ἀνακηρύττοντες, σάρκα δι' ἡμᾶς, ἀναλαβόντα δίχα φθορᾶς, καὶ μιμηταὶ τῶν παθῶν αὐτοῦ, δεικνύμενοι ὄντως, πυρὸς κάκωσιν, ὑπέστητε, καὶ πᾶσαν ἄλλην βάσανον ἠνέγκατε, ἐν εὐφροσύνῃ ψάλλοντες· ὁ ὑπερύμνητος, τῶν Πατέρων καὶ ἡμῶν, Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m8_gr_GR_cog.EknoonProstagma.name
ocMA.OdeW7C1T.text = "Λύπης πεπλήρωμαι, ὁρῶσά σε Υἱέ, ἀδίκως πάσχοντα, τέτρωμαι ψυχήν, λόγχῃ τρωθέντος σου τὴν πλευράν, ἡ Θεοτόκος ἐβόᾳ κλαίουσα, καὶ ὀδυρομένη, μόνη Δέσποινα, ἣν ἐπαξίως πάντες μακαρίζομεν, καὶ εὐσεβῶς κραυγάζομεν· ὁ ὑπερύμνητος, τῶν Πατέρων καὶ ἡμῶν, Θεὸς εὐλογητὸς εἶ."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
ocMA.OdeW7C21.text = "Ἰδοὺ νῦν πεπλήρωται, Δαυῒδ τοῦ θείου, ἡ θεία πρόρρησις, λιτανεύουσιν ὄντως, τῷ σῷ προσώπῳ Θεογεννῆτορ ἁγνή, οἱ κεκτημένοι τὸν πλοῦτον τῆς χάριτος, καὶ εὐλογοῦντες Θεόν, τὸν τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
ocMA.OdeW7C22.text = "Νῦν πάντα πεπλήρωται, φωτὸς τοῦ θείου, διὰ σοῦ Πάναγνε· σὺ γὰρ πύλη ἐφάνης, δι' ἧς τῷ κόσμῳ Θεὸς ὡμίλησε, καταφωτίζων, τοὺς πίστει κραυγάζοντας· Εὐλογητός ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
ocMA.OdeW7C23.text = "Λυτρούμενοι Πάναγνε, τῷ θείῳ τόκῳ τῆς παρθενίας σου, τῶν δεσμῶν τοῦ θανάτου, καὶ τῆς κατάρας τοῦ πρωτοπλάστου Ἀδάμ, σὲ Θεοτόκον, φρονοῦντες κραυγάζομεν· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C24.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C24.melody = he.h.m8_gr_GR_cog.TheouSynkatavasin.name
ocMA.OdeW7C24.text = "Κυρίαν σε Δέσποιναν, καὶ Θεοτόκον, πάντες δοξάζομεν· διὰ σοῦ γὰρ ὁ Πλάστης, τὴν σωτηρίαν τῇ φύσει πάσῃ σαφῶς ἐβράβευσε· καὶ διὰ τοῦτο, συμφώνως κραυγάζομεν· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m8_gr_GR_cog.EvlogeitePaides.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C11.text = "Εὐλογεῖτε Ξύλον, δι' οὗ πᾶσα ἠφάνισται, ἡ ἐν Ἐδὲμ δολία ἀρὰ φυεῖσα, βρώσει πονηρᾷ τοῦ Ξύλου, καὶ Χριστὸς ὑψοῦται ὁ ὑπεράγαθος, ὁ ἀνυψωθῆναι ἐν τούτῳ ἐθελουσίως, αἱρετισάμενος δι' εὐσπλαγχνίαν."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C12.text = "Ἱερῶς ἐδήλου, ἐναλλὰξ ὁ ἀοίδιμος, ἐπευλογῶν ἐγγόνους ποτέ, τὸν τύπον τοῦ ἡγιασμένου Ξύλου, δι' οὗ εὐλογία πᾶσι δεδώρηται, τοῖς κατηραμένοις, κακίστῃ τοῦ ξύλου βρώσει, καὶ ὀλισθήσασι κακῶν εἰς βάθη."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C13.text = "Ἀνορθοῦται ἄπαν, τὸ ἀνθρώπινον Δέσποτα, σοῦ ὀρθωθέντος ἐπὶ τοῦ Σταυροῦ, καὶ στῖφος, πίπτει πονηρῶν δαιμόνων, καὶ τὰ διεστῶτα ἕνωσιν δέχεται, καὶ ὑπερυψοῦται τὸ κράτος τῆς ἐξουσίας, καὶ τῆς δυναστείας σου εἰς τοὺς αἰῶνας."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C14.text = "Μακαρίαν δόξαν, καὶ τρυφὴν ἀδιάρρευστον, καὶ φωταυγῆ σκηνώματα νῦν κληροῦσθε, θεῖοι Ἀθληταὶ Κυρίου, καὶ ταῖς οὐρανίαις σκηνοῦσθε Τάξεσι, καὶ τῶν ἀοιδίμων ἐλπίδων ὑμῶν τὸ πέρας, νῦν κατειλήφατε ἐν εὐφροσύνῃ."

ocMA.OdeW8C15.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C15.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C15.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C15.text = "Ἡ ὑμῶν ἀνδρεία, ὑπὲρ ἥλιον ἔλαμψε, φωτοειδεῖς Χριστοῦ Ἀθληταί, καὶ πᾶσαν, τὴν τοῦ διαβόλου πλάνην, θεϊκῇ δυνάμει ἐναπημαύρωσε, καὶ πιστῶν καρδίας Μάρτυρες εὐσεβοφρόνων, ἐφωταγώγησεν εἰς τοὺς αἰῶνας."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m8_gr_GR_cog.EvlogeitePaides.name
ocMA.OdeW8C1T.text = "Νοητὴν νεφέλην, καὶ σκηνὴν ἁγιάσματος, θρόνον Θεοῦ καὶ πύλην φωτὸς λυχνίαν, καὶ ἀνατολὴν τοῦ Λόγου, οἱ πιστοί σε πάντες κατονομάζομεν, πάναγνε Παρθένε τοῦ πάντα τεκτηναμένου, Μήτηρ ὑπάρχουσα εὐλογημένη."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
ocMA.OdeW8C21.text = "Φωτὸς ἀδύτου γέγονας, ἐνδιαίτημα Πάναγνε, ταῖς τῆς παρθενίας, καλλοναῖς ἀστράπτουσα, καὶ πάντας κατηύγασας, τοὺς Θεοτόκον σε ἀληθῆ, καθομολογοῦντας ἐκ ψυχῆς καὶ βοῶντας· οἱ Παῖδες εὐλογεῖτε, Ἱερεῖς ἀνυμνεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
ocMA.OdeW8C22.text = "Λόγον Θεοῦ τὸν ἄναρχον, καὶ Πατρὶ συννοούμενον, τὸν ἐκ τοῦ μὴ ὄντος, Πατρικῷ βουλήματι, τὸ πᾶν συστησάμενον, θεοπρεπῶς γεγέννηκας, σάρκα γεγονότα δι'ἡμᾶς τοὺς ἀνθρώπους· διό σε Θεοτόκον, ὀρθοδόξως φρονοῦμεν, Χριστὸν ὑπερυψοῦντες, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
ocMA.OdeW8C23.text = "Ἱερωτέρα πέφηνας, Θεοτόκε πανάμωμε, τῆς ὑπερκοσμίου, τῶν Ἀγγέλων τάξεως· τῶν τούτων γὰρ τέτοκας, Δημιουργὸν καὶ Κύριον, ἐκ Παρθενικῆς ἀπειρογάμου νηδύος, ἐν δύο ταῖς οὐσίαις, ἀσυγχύτως, ἀτρέπτως, μιᾷ δὲ ὑποστάσει, Θεὸν σεσαρκωμένον."

ocMA.OdeW8C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C24.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C24.melody = he.h.m8_gr_GR_cog.EptaplasiosKaminon.name
ocMA.OdeW8C24.text = "Νενεκρωμένην Πάναγνε, τὴν ψυχήν μου τοῖς πταίσμασι, καὶ κατεφθαρμένην, ἰοβόλοις δήγμασι, Θεὸν τὸν δυνάμενον, ζωοποιεῖν τὰ σύμπαντα, ἡ ἀπειρογάμως ὑπὲρ φύσιν τεκοῦσα, ἀνάστησον βοῶσαν· Ἱερεῖς εὐλογεῖτε, λαὸς ὑπερυψοῦτε, Χριστὸν εἰς τοὺς αἰῶνας."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m8_gr_GR_cog.ODiaVroseos.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m8_gr_GR_cog.ODiaVroseos.name
ocMA.OdeW9C11.text = "Ἱερωτάτως σε ὑψοῦντες, Οἰκτίρμον προσκυνοῦμέν σου, λόγχην τε καὶ τὸν Σταυρόν, τὸν σπόγγον, τὸν κάλαμον, καὶ τοὺς χερσὶ παγέντας καὶ ποσίν, ἁγίους ἥλους Δέσποτα, δι' ὧν εὕρομεν τελείαν ἄφεσιν, καὶ τῆς ἐν Παραδείσῳ, διαγωγῆς ἠξιώθημεν."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m8_gr_GR_cog.ODiaVroseos.name
ocMA.OdeW9C12.text = "Ὢ πῶς ἀδίκως κατεκρίθης, παγῆναι δικαιότατε, μόνε πάντων βασιλεῦ, ἐν ξύλῳ σταυρούμενος, τοῦ δικαιῶσαι πάντας ἐκζητῶν, τοὺς πίστει τὰ παθήματα, τὰ ἑκούσια καὶ τὴν οἰκονομίαν σου, δοξάζοντας ἀπαύστως, καὶ σὲ ἀεὶ μεγαλύνοντας."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m8_gr_GR_cog.ODiaVroseos.name
ocMA.OdeW9C13.text = "Σῶμα προδόντες ταῖς βασάνοις, προθύμως οἱ Πανεύφημοι, ἤνεγκαν τοὺς αἰκισμούς, τὸν βίαιον θάνατον, τὰς ἐκτομὰς μελῶν, τοὺς σπαραγμούς, καὶ πᾶσαν ἄλλην βάσανον, τῷ πρὸς Κύριον φίλτρῳ πυρούμενοι· διὸ στεφανηφόροι, ἐν οὐρανοῖς πολιτεύονται."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m8_gr_GR_cog.ODiaVroseos.name
ocMA.OdeW9C14.text = "Ἡ Ἀποστόλων καὶ Μαρτύρων, γλυκύτης ταῖς εὐχαῖς αὐτῶν, πάντων ἡμῶν τὰς εὐχάς, ὡς εὔσπλαγχνος πλήρωσον, ἁμαρτημάτων λύσιν τῶν πολλῶν, παρέχων καὶ ταπείνωσιν, τὴν τῆς σῆς Βασιλείας οἰκείωσιν, Χριστὲ Θεὲ τῶν ὅλων, ὁ δι' ἡμᾶς ὀφθεὶς ἄνθρωπος."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m8_gr_GR_cog.ODiaVroseos.name
ocMA.OdeW9C1T.text = "Φωτοειδὴς παστὰς ὠράθης, Παρθένε τοῦ οἰκήσαντος, σοῦ τὴν ἄφθορον νηδύν, καὶ πάθος μακάριον, καθυπομείναντος ἐθελοντί, καὶ πᾶσι τὴν ἀπάθειαν, παρασχόντος δι' ἄφατον ἔλεος, ὃν πίστει προσκυνοῦντες, σὲ εὐσεβῶς μεγαλύνομεν."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
ocMA.OdeW9C21.text = "Νικῶσαν τὰς αἱρέσεις νεανικῶς, τὴν σεπτὴν Ἐκκλησίαν, ἀνάδειξον, ἣν ὁ Χριστός, αἵματι τιμίῳ τῷ ἑαυτοῦ, ὡς ἀγαθὸς ἐκτήσατο, Κεχαριτωμένη ὁ σὸς Υἱός, καὶ σῶσον ἐκ κινδύνων, καὶ τῆς ἐπικρατείας, τῆς κατεχούσης ἡμᾶς Δέσποινα."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
ocMA.OdeW9C22.text = "Ῥιφθέντας οὐρανίου διαγωγῆς, καὶ πεσόντας ἀθλίως εἰς θάνατον, πάλιν ἡμᾶς, Μήτηρ γενομένη τοῦ Λυτρωτοῦ, ἀνεκαλέσω Πάναγνε, καὶ πρὸς τὴν ἀρχαίαν πάλιν δραμεῖν, ἠξίωσας πατρίδα· διό σε Θεοτόκε, ἀκαταπαύστως μεγαλύνομεν."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
ocMA.OdeW9C23.text = "Ῥαδίως Θεοτόκε διαπερᾶν, τὸν τοῦ βίου παράσχου μοι κλύδωνα, τῶν πειρασμῶν, τὰς ἐπαναστάσεις καὶ τῶν παθῶν, ὡς ἀγαθὴ πραΰνουσα, καὶ καθοδηγοῦσα πρὸς ἀρετῆς, οὐράνιον πορείαν, ὅπως ὡς εὐεργέτιν, ἀκαταπαύστως μεγαλύνω σε."

ocMA.OdeW9C24.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C24.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C24.melody = he.h.m8_gr_GR_cog.ExestiEpiTouto.name
ocMA.OdeW9C24.text = "Σωτῆρα ὡς τεκοῦσα καὶ Λυτρωτήν, σωτηρίας με Κόρη, ἀξίωσον, ἐπιτυχεῖν, Πάναγνε πταισμάτων μου τὰς σειράς, ταῖς σαῖς πρεσβείαις λύουσα· πάντα γὰρ ὡς θέλεις σὺ κατορθοῖς, ὡς φέρουσα ἐν κόλποις, τὸν πάντων Βασιλέα, τὸν ἀγαθότητι ἀσύγκριτον."

