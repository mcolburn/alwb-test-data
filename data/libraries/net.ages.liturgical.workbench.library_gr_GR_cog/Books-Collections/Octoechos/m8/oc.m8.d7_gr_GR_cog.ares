ocMA.Kathisma13.text = oc.m8.d5_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma14.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma14.melody = ""
ocMA.Kathisma14.text = oc.m8.d1_gr_GR_cog.ocVE.ApolTheotokionVM.text

ocMA.Kathisma21.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma21.melody = ""
ocMA.Kathisma21.text = oc.m8.d2_gr_GR_cog.ocMA.Kathisma32.text

ocMA.Kathisma22.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma22.melody = he.a.m8_gr_GR_cog.ToProstachthen.name
ocMA.Kathisma22.text = "Τοὺς μεταστάντας εὐσεβῶς, ἐκ τῶν προσκαίρων, ἐν ταῖς σκηναῖς τῶν ἐκλεκτῶν, μετὰ Δικαίων, συναρίθμησον Δέσποτα, τούτους ἀναπαύων, ἐν τόποις ἑορταζόντων, καὶ ἐν τρυφῇ, ἀλήκτῳ τοῦ Παραδείσου, ὡς ἀγαθός, τὰ ἑκούσια πταίσματα, καὶ τὰ ἀκούσια αὐτῶν, δι' ἄκραν ἀγαθότητα, συγχωρήσας φιλάνθρωπε."

ocMA.Kathisma23.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma23.melody = ""
ocMA.Kathisma23.text = "Ὁ βάθει σοφίας φιλανθρώπως, πάντα οἰκονομῶν, καὶ τὸ συμφέρον πᾶσιν ἀπονέμων μόνε Δημιουργέ, ἀνάπαυσον Κύριε τὰς ψυχὰς τῶν δούλων σου· ἐν σοὶ γὰρ τὴν ἐλπίδα ἀνέθεντο, τῷ Ποιητῇ καὶ πλάστῃ καὶ Θεῷ ἡμῶν."

ocMA.Kathisma24.mode = misc_gr_GR_cog.Mode8
ocMA.Kathisma24.melody = ""
ocMA.Kathisma24.text = "Σὲ καὶ τεῖχος καὶ λιμένα ἔχομεν, καὶ πρέσβυν εὐπρόσδεκτον, πρὸς ὃν ἔτεκες Θεόν, Θεοτόκε ἀνύμφευτε, τῶν πιστῶν ἡ σωτηρία."

ocMA.C1Acrostic.text = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν νεκρωσίμων καὶ τῶν Θεοτοκίων· Τῆς Ὀκταήχου τῆς Νέας θεῖον τέλος. Πόνοι δὲ Ἰωσήφ."

ocMA.C2Poet = poets_gr_GR_cog.Theofanous
ocMA.C2Acrostic.text = " οὗ ἡ ἀκροστιχὶς μετὰ τῶν Θεοτοκίων· Θνήσκουσι πιστοῖς, ὄγδοον πλέκω μέλος."

ocMA.OdeW1C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C1H.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW1C1H.text = he.h.m8_gr_GR_cog.AsomenToKyrio.text

ocMA.OdeW1C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C11.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C11.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
ocMA.OdeW1C11.text = "Τίμιοι μαργαρῖται, ὤφθητε τὸν στέφανον φαιδρύνοντες, τῆς σεπτῆς Ἐκκλησίας, Ἀθλοφόροι Χριστοῦ γενναιότατοι."

ocMA.OdeW1C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C12.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C12.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
ocMA.OdeW1C12.text = "Ἤστραψαν τῶν δογμάτων, καὶ τῶν ἀρετῶν θείαις λαμπρότησιν, οἱ σοφοὶ Ἱεράρχαι, καὶ πιστῶν τὰς καρδίας ἐφώτισαν."

ocMA.OdeW1C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C13.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C13.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
ocMA.OdeW1C13.text = "Σὲ τὸν ἐν τοῖς Προφήταις, καὶ ἐν τοῖς Ὁσίοις θαυμαζόμενον, ἱκετεύομεν Λόγε, ταῖς αὐτῶν ἡμᾶς σῶσον δεήσεσιν."

ocMA.OdeW1C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C14.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C14.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
ocMA.OdeW1C14.text = "Ὅταν κρίσιν δικαίαν, ἔλθῃς τοῦ ποιῆσαι δικαιότατε, ἀκατακρίτους Λόγε, τοὺς πρὸς σὲ μεταστάντας ἀπέργασαι."

ocMA.OdeW1C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C1T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C1T.melody = he.h.m8_gr_GR_cog.AsomenToKyrio.name
ocMA.OdeW1C1T.text = "Κύριον τετοκυῖαν, γυναικῶν χορός σε ἐπιστάμενος, σοῦ ὀπίσω Παρθένε, ἐναθλήσας αὐτῷ προσενήνεκται."

ocMA.OdeW1C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C21.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C21.melody = he.h.m8_gr_GR_cog.YgranDiodefsas.name
ocMA.OdeW1C21.text = "Θανάτῳ τὸν θάνατον τοῦ Χριστοῦ, καὶ πάθεσι πάθος, μιμησάμενοι τὸ σεπτόν, οἱ Μάρτυρες πάντες τῆς ἐνθέου, καὶ μακαρίας ζωῆς ἔτυχον."

ocMA.OdeW1C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C22.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C22.melody = he.h.m8_gr_GR_cog.YgranDiodefsas.name
ocMA.OdeW1C22.text = "Νεότητος πταίσματα παρορῶν, καὶ τὰς ἁμαρτίας, ὑπερβαίνων Χριστὲ Σωτήρ, τῶν προκοιμηθέντων οἰκετῶν σου, τοῖς ἐκλεκτοῖς σου συναρίθμησον."

ocMA.OdeW1C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C23.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C23.melody = he.h.m8_gr_GR_cog.YgranDiodefsas.name
ocMA.OdeW1C23.text = "Ἧς ἔτυχον δόξης τε καὶ χαρᾶς, οἱ τὴν μακαρίαν, κεκτημένοι διαγωγήν, μετάδος πλουσίως τοῖς σοῖς δούλοις, οὓς προσελάβου Πολυέλεε."

ocMA.OdeW1C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW1C2T.ode = misc_gr_GR_cog.Ode1
ocMA.OdeW1C2T.melody = he.h.m8_gr_GR_cog.YgranDiodefsas.name
ocMA.OdeW1C2T.text = "Συνέλαβες Λόγον τὸν τοῦ Πατρός, σαρκὶ ἑνωθέντα, καθ' ὑπόστασιν τῇ ἐκ σοῦ, πανάμωμε Κόρη, καὶ τὸν ᾅδην, θείᾳ δυνάμει καταργήσαντα."

ocMA.OdeW3C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C1H.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW3C1H.text = he.h.m8_gr_GR_cog.OStereosasLogo.text

ocMA.OdeW3C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C11.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C11.melody = he.h.m8_gr_GR_cog.OStereosasLogo.name
ocMA.OdeW3C11.text = "Τὰς μυσαρὰς θυσίας οἱ Ἀθληταί, στερροτάτῃ γνώμῃ, ἀποσεισάμενοι, θυσίᾳ καθαρωτάτῃ, Λόγῳ τῷ τυθέντι γεγόνασιν."

ocMA.OdeW3C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C12.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C12.melody = he.h.m8_gr_GR_cog.OStereosasLogo.name
ocMA.OdeW3C12.text = "Ἀνανεοῦντες λόγοις ἱερουργοῖς, τοὺς παλαιωθέντας, παντοίοις παθήμασι, τοῦ ἀναπλάσαντος κόσμον, θεῖοι Μαθηταὶ ἀπεφάνθητε."

ocMA.OdeW3C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C13.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C13.melody = he.h.m8_gr_GR_cog.OStereosasLogo.name
ocMA.OdeW3C13.text = "Ἡ τοῖς Προφήταις πρότερον προφανῶς, δεδομένη χάρις, τοῦ Παναγίου Πνεύματος, τοὺς Ἀσκητὰς ἐπ' ἐσχάτων, θείων χαρισμάτων ἐνέπλησε."

ocMA.OdeW3C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C14.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C14.melody = he.h.m8_gr_GR_cog.OStereosasLogo.name
ocMA.OdeW3C14.text = "Χοροῖς ἁγίοις με σύνταξον ὁ Θεός, τοὺς πιστῶς τοῦ βίου, μεταχωρήσαντας, καὶ Παραδείσου πολίτας, ποίησον δι' ἄφατον ἔλεος."

ocMA.OdeW3C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C1T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C1T.melody = he.h.m8_gr_GR_cog.OStereosasLogo.name
ocMA.OdeW3C1T.text = "Ὁ μηδαμοῦ χωρούμενος Ἰησοῦς, ἀστενοχωρήτως, τὴν σὴν κατῴκησεν, ἡγιασμένην γαστέρα, ἄχραντε Παρθένε, καὶ Κύριος."

ocMA.OdeW3C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C21.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C21.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
ocMA.OdeW3C21.text = "Καθαρθέντες τῆς πάλαι, προγονικῆς πτώσεως, διὰ τοῦ Βαπτίσματος καὶ τῆς ἀναγεννήσεως, ῥείθροις αἱμάτων τε, τοῖς ἑαυτῶν ῥαντισθέντες, τῷ Χριστῷ Μακάριοι, συμβασιλεύετε."

ocMA.OdeW3C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C22.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C22.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
ocMA.OdeW3C22.text = "Ὁ νεκρὸς ἑκουσίως, κατατεθεὶς μνήματι, καὶ τοὺς κατοικοῦντας ἐν τάφοις ἀνακαλούμενος, Σῶτερ εὐδόκησον, οὓς ἐξ ἡμῶν προσελάβου, ἐν σκηναῖς Δικαίων σου, τούτους αὐλίζεσθαι."

ocMA.OdeW3C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C23.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C23.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
ocMA.OdeW3C23.text = "Ὑπὸ τῆς εὐσπλαγχνίας, τῆς θεϊκῆς Δέσποτα, καὶ τῆς συμφυῶς νοουμένης σου ἀγαθότητος, παρακαλούμενος, ἁμαρτημάτων τὴν λύσιν, παρασχὼν τοῖς δούλοις σου, Σῶτερ ἀνάπαυσον."

ocMA.OdeW3C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW3C2T.ode = misc_gr_GR_cog.Ode3
ocMA.OdeW3C2T.melody = he.h.m8_gr_GR_cog.OuraniasApsidos.name
ocMA.OdeW3C2T.text = "Σαρκωθεὶς ἐκ γαστρός σου, καὶ γεγονὼς ἄνθρωπος, ὁ διαφερόντως ὑπάρχων, μόνος φιλάνθρωπος, σώζει τὸν ἄνθρωπον, ἐκ τῶν πυλῶν τοῦ θανάτου, Θεομῆτορ ἄχραντε, μόνη πανύμνητε."

ocMA.OdeW4C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C1H.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW4C1H.text = he.h.m8_gr_GR_cog.EisakikoaKyrie.text

ocMA.OdeW4C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C11.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C11.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C11.text = "Ὑπελθόντες τὸ στάδιον, τὸ τῆς μαρτυρίας, ζέοντι πνεύματι, Ἀθληταὶ θεομακάριστοι, τὴν ὑλώδη πλάνην, κατεφλέξατε."

ocMA.OdeW4C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C12.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C12.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C12.text = "Τοὺς Ὁσίους σου Κύριε, καὶ τοὺς Ἱεράρχας, λαμπρῶς ἐδόξασας, τούτων θείαις παρακλήσεσι, κοινωνόν με δόξης, τῆς σῆς ποίησον."

ocMA.OdeW4C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C13.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C13.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C13.text = "Ἡ τοῦ θείου ἐπίπνοια, Πνεύματος Προφήτας, καταλαμπρύνασα, Γυναιξὶν ἰσχὺν δεδώρηται, τοῦ ἐχθροῦ ὀλέσαι τὰ φρυάγματα."

ocMA.OdeW4C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C14.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C14.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C14.text = "Συγχορεύειν ἀξίωσον, πᾶσι τοῖς Ἁγίοις, οὕς περ μετέστησας, ὑπεράγαθε οἰκέτας σου, ἐν σκηναῖς ἁγίων, δυσωπούμενος."

ocMA.OdeW4C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C1T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C1T.melody = he.h.m8_gr_GR_cog.EisakikoaKyrie.name
ocMA.OdeW4C1T.text = "Νόμου θείου τὸν πάροχον, φύσεως νομίμων, ἄνευθεν τέτοκας, ἀναπλάττοντα Πανάμωμε, τῶν βροτῶν τὴν φύσιν, ὀλισθήσασαν."

ocMA.OdeW4C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C21.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C21.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C21.text = "Ἵνα τὴν σήν, Δέσποτα δόξαν θεάσωνται, καὶ τῆς αἴγλης, σοῦ τῆς φωτοδότιδος, ἐν οὐρανοῖς τύχωσι φαιδρῶς, ἅπασαν βασάνων, πεῖραν ἀθλοῦντες ὑπήνεγκαν, οἱ Μάρτυρες οἱ θεῖοι, σοὶ Χριστὲ μελῳδοῦντες· τῇ δυνάμει σου δόξα Φιλάνθρωπε."

ocMA.OdeW4C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C22.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C22.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C22.text = "Πολλαὶ μοναί, παρὰ σοὶ Σῶτερ πεφύκασι, κατ' ἀξίαν, πᾶσι μεριζόμεναι, κατὰ τὸ μέτρον τῆς ἀρετῆς· ταύτης οὖν Οἰκτίρμον, πληρῶσαι τούτους ἀξίωσον, τοὺς πίστει μεταστάντας, εὐσεβῶς σοι βοῶντας· τῇ δυνάμει σου δόξα φιλάνθρωπε."

ocMA.OdeW4C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C23.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C23.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C23.text = "Ἴσος ἡμῖν, ἄνθρωπος ὤφθης Ἀθάνατε, πᾶσιν ἴσον, θάνατον ὑπέμεινας, καὶ τῆς ζωῆς ἔδειξας ὁδούς, ἧς τοὺς μεταστάντας, ἀξίωσον ὡς φιλάνθρωπος, συγχώρησιν πταισμάτων, ὡς Δεσπότης παρέχων, καὶ φωτὸς μετουσίαν δωρούμενος."

ocMA.OdeW4C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW4C2T.ode = misc_gr_GR_cog.Ode4
ocMA.OdeW4C2T.melody = he.h.m8_gr_GR_cog.SyMouIschys.name
ocMA.OdeW4C2T.text = "Σὺ τῶν πιστῶν, καύχημα πέλεις Ἀνύμφευτε, σὺ προστάτις, σὺ καὶ καταφύγιον, Χριστιανῶν, τεῖχος καὶ λιμήν· πρὸς γὰρ τὸν Υἱόν σου, ἐντεύξεις φέρεις Πανάμωμε, καὶ σῴζεις ἐκ κινδύνων, τοὺς ἐν πίστει καὶ πόθῳ, Θεοτόκον ἁγνήν σε γινώσκοντας."

ocMA.OdeW5C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C1H.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW5C1H.text = he.h.m8_gr_GR_cog.FotisonImas.text

ocMA.OdeW5C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C11.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C11.melody = he.h.m8_gr_GR_cog.FotisonImas.name
ocMA.OdeW5C11.text = "Ἔφερον πληγάς, τῆς σαρκὸς οἱ θεῖοι Μάρτυρες, καὶ διὰ τοῦτο τὰς πληγὰς ἡμῶν, ἀεὶ ἰῶνται, τραυματίζοντες τοὺς δαίμονας."

ocMA.OdeW5C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C12.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C12.melody = he.h.m8_gr_GR_cog.FotisonImas.name
ocMA.OdeW5C12.text = "Ἅγιοι Θεοῦ, Ἱεράρχαι ἱκετεύσατε, μετὰ πάντων τῶν Ὁσίων Χριστόν, ἁμαρτημάτων, δωρηθῆναι ἡμῖν ἄφεσιν."

ocMA.OdeW5C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C13.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C13.melody = he.h.m8_gr_GR_cog.FotisonImas.name
ocMA.OdeW5C13.text = "Σάρκα αἰκισμοῖς, καὶ ἀσκήσει δαπανήσασαι, γυναικῶν αἱ Ὅσιαι, καὶ Μάρτυρες, τῶν ἐν ἐλπίσιν, ἀγαθῶν κατηξιώθησαν."

ocMA.OdeW5C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C14.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C14.melody = he.h.m8_gr_GR_cog.FotisonImas.name
ocMA.OdeW5C14.text = "Θάνατον ἑλών τῷ θανάτῳ σου Ἀθάνατε, τοὺς θανόντας ἐπ' ἐλπίδι ζωῆς, πιστούς σου δούλους, ὡς φιλάνθρωπος ἀνάπαυσον."

ocMA.OdeW5C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C1T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C1T.melody = he.h.m8_gr_GR_cog.FotisonImas.name
ocMA.OdeW5C1T.text = "Ἔπαυσας Ἁγνή, προπατόρων τὸ κατάκριμα, τὸν τοὺς πάντας δικαιώσαντα, σαρκὶ Χριστὸν τεκοῦσα, Ἰησοῦν τὸν μόνον Κύριον."

ocMA.OdeW5C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C21.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C21.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C21.text = "Τῆς ἀφράστου σου δόξης, καὶ τῆς ὑπὲρ λόγον, σοῦ μακαριότητος, ἐν σκηναῖς Ἁγίων, ἔνθα ἦχος τερπνὸς ἑορτάζοντος, τοὺς μεταθεμένους, πρὸς ἀπαθῆ ζωὴν οἰκτείρας, καταξίωσον μόνε φιλάνθρωπε."

ocMA.OdeW5C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C22.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C22.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C22.text = "Ὅπου τάξεις Ἀγγέλων, ὅπου τῶν Δικαίων, οἱ δῆμοι ἀγάλλονται, Ἀβραὰμ ἐν κόλποις, κατασκήνωσον Σῶτερ τοὺς δούλους σου, καὶ σὺν παρρησίᾳ, τῷ φοβερῷ καὶ θείῳ θρόνῳ, παρεστάναι εὐδόκησον Εὔσπλαγχνε."

ocMA.OdeW5C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C23.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C23.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C23.text = "Ἱλασμὸς ἡμῖν ὤφθης, καὶ δικαιοσύνη τε, καὶ ἀπολύτρωσις, καὶ τῷ μώλωπί σου, τὰς ἡμῶν ἀσθενείας ἰάτρευσας· αὐτὸς οὖν Οἰκτίρμον, ὡς ἀγαθὸς τοὺς μεταστάντας, ἐν τρυφῇ Παραδείσου κατάταξον."

ocMA.OdeW5C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW5C2T.ode = misc_gr_GR_cog.Ode5
ocMA.OdeW5C2T.melody = he.h.m8_gr_GR_cog.InaTiMeAposo.name
ocMA.OdeW5C2T.text = "Συμπαθῶς ἐλεῆμον, τὸ τῆς ἀνθρωπότητος, μέτρον ἀνέλαβες, ὁ τῆς θείας δόξης, ὑπερτάτοις ἐμπρέπων ὑψώμασιν, ἐκ Παρθένου μήτρας, σάρκα λαβὼν ἐμψυχωμένην, λογικήν, δι' ἧς θάνατον ὤλεσας."

ocMA.OdeW6C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C1H.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW6C1H.text = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.text

ocMA.OdeW6C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C11.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C11.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C11.text = "Ἰχῶρσιν ὑμῶν σεπτοῖς, ψυχῶν ἰᾶσθε νοσήματα, καὶ σηπεδόνας ἀεί, σωμάτων ἐξαίρετε, κατατραυματίζοντες, πονηρῶν δαιμόνων, τὴν πληθύν, ἅγιοι Μάρτυρες."

ocMA.OdeW6C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C12.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C12.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C12.text = "Ὁ τῶν Ὁσίων χορός, Ἱεραρχῶν τε ὁ σύλλογος, καὶ Γυναικῶν ἱερῶν, ὁ θεῖος κατάλογος, στερρῶς ἐναθλήσαντες, ἐν ἐπουρανίοις, ἀπολαύσεις ἐκληρώσαντο."

ocMA.OdeW6C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C13.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C13.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C13.text = "Νεκρώσαντες Ἀσκηταί, τὴν σάρκα ζωῆς ἐτύχετε, ποιμάναντες δὲ καλῶς, Κυρίου τὸ ποίμνιον, Ἱεράρχαι πάνσοφοι, ἀθανάτου δόξης, μετὰ τέλος ἠξιώθητε."

ocMA.OdeW6C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C14.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C14.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C14.text = "Τῶν ζώντων Λόγε ζωή, καὶ τῶν θανόντων ἀνάπαυσις, ὑπάρχων τοὺς ἐξ ἡμῶν, τῷ θείῳ σου νεύματι, μεταστάντας δούλους σου, Ἀβραὰμ ἐν κόλποις, τοῦ θεράποντός σου σκήνωσον."

ocMA.OdeW6C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C1T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C1T.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C1T.text = "Ἐσκήνωσεν ἐπὶ σοί, Πατρὸς Ἁγνὴ τὸ ἀπαύγασμα, καὶ ταῖς ἀΰλοις αὐγαῖς, αὐτοῦ τῆς Θεότητος, τὸ σκότος διέλυσε, τῆς πολυθεΐας, καὶ τὸν κόσμον κατεφώτισεν."

ocMA.OdeW6C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C21.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C21.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C21.text = "Οἱ Μάρτυρές σου Σωτήρ, πολλὰς βασάνους ὑπέμειναν, τῷ πόθῳ σου τὰς ψυχάς, τρωθέντες οἱ Ἅγιοι, τῆς σῆς ὀρεγόμενοι, ἀϊδίου δόξης, καὶ γλυκείας σου μεθέξεως."

ocMA.OdeW6C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C22.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C22.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C22.text = "Γαστέρα τοῦ δυσμενοῦς, θανάτου Σῶτερ διέρρηξας, καὶ πάντας τοὺς ἐν αὐτῇ, δεσμώτας ἀνέστησας, ζωὴν χαρισάμενος, ἧς τοὺς μεταστάντας, Εὐεργέτα καταξίωσον."

ocMA.OdeW6C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C23.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C23.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C23.text = "Δακρύων καὶ στεναγμῶν, τῶν ἐν τῷ ᾅδῃ τοὺς δούλους σου, ἀπάλλαξον Ἀγαθέ· ὡς μόνος γὰρ εὔσπλαγχνος, ἀφεῖλες πᾶν δάκρυον, ἐκ παντὸς προσώπου, τῶν πιστῶς θεολογούντων σε."

ocMA.OdeW6C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW6C2T.ode = misc_gr_GR_cog.Ode6
ocMA.OdeW6C2T.melody = he.h.m8_gr_GR_cog.IlasthitiMoiSotir.name
ocMA.OdeW6C2T.text = "Ὁ φύσει Δημιουργός, δημιουργεῖται ἐν μήτρᾳ σου, ὁ πλήρης καθὸ Θεός, κενοῦται Πανάμωμε, ὁ μόνος Ἀθάνατος, τὸν ὑπὲρ τῆς πάντων, σωτηρίας θνῄσκει θάνατον."

ocMA.OdeW7C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C1H.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW7C1H.text = he.h.m8_gr_GR_cog.PaidesTheoseveis.text

ocMA.OdeW7C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C11.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C11.melody = he.h.m8_gr_GR_cog.PaidesTheoseveis.name
ocMA.OdeW7C11.text = "Λύσαντες τὸν φραγμὸν τῆς δυσσεβείας, δεσμοῖς ἱεροῖς ὑμῶν Πανεύφημοι, τῶν ἡμῶν ἐγκλημάτων, φορτία σκορπίσατε, καὶ βοῶντας Μάρτυρες σώσατε· Εὐλογητὸς ὁ Θεός, ὁ τῶν Πατέρων ἡμῶν."

ocMA.OdeW7C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C12.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C12.melody = he.h.m8_gr_GR_cog.PaidesTheoseveis.name
ocMA.OdeW7C12.text = "Ὄμβρῳ τῶν ἱερῶν αἱμάτων, αἱρέσεων πῦρ ἐναπεσβέσατε, καὶ φλογὶ Ἱεράρχαι, σοφοὶ κατεφλέξατε, ἀσεβείας πλάνης ζιζάνια, καὶ τῶν πιστῶν τὰς ψυχὰς κατεφωτίσατε."

ocMA.OdeW7C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C13.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C13.melody = he.h.m8_gr_GR_cog.PaidesTheoseveis.name
ocMA.OdeW7C13.text = "Σάρκα οἱ Ἀσκηταὶ δι' ἐγκρατείας, νεκρώσαντες, ζῶσι μετὰ θάνατον. Προφητῶν τε Δικαίων χορός, καὶ κατάλογος, Γυναικῶν ἀθλήσας, δεδόξασται. Αὐτῶν πρεσβείαις Χριστέ, κινδύνων ῥῦσαι ἡμᾶς."

ocMA.OdeW7C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C14.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C14.melody = he.h.m8_gr_GR_cog.PaidesTheoseveis.name
ocMA.OdeW7C14.text = "Πάντων τῶν ἐπ' ἐλπίδι κοιμηθέντων, ζωῆς τὰς ψυχὰς Χριστέ, ἀνάπαυσον, παρορῶν τὰ ἐν βίῳ, αὐτῶν πλημμελήματα, διὰ τὴν πολλὴν εὐσπλαγχνίαν σου, εὔσπλαγχνε μόνε Σωτήρ, ὁ τῶν Πατέρων Θεός."

ocMA.OdeW7C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C1T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C1T.melody = he.h.m8_gr_GR_cog.PaidesTheoseveis.name
ocMA.OdeW7C1T.text = "Ὅλη ὡς καθαρὰ καθυπεδέξω, ἐν μήτρᾳ τὸν Λόγον σωματούμενον· αὐτὸν γοῦν ἐκδυσώπει, καθᾶραι πταισμάτων με, καὶ ψυχῆς καὶ σώματος Πάναγνε, τὸν ἐπὶ σοὶ καθαρᾷ πίστει προστρέχοντα."

ocMA.OdeW7C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C21.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C21.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
ocMA.OdeW7C21.text = "Ὅλος ὁ πόθος τῶν Μαρτύρων, ἀνατέταται πρὸς μόνον τὸν Δεσπότην, δι' ἀγάπης αὐτῷ, ἑνούμενος καὶ μέλπων· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C22.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C22.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
ocMA.OdeW7C22.text = "Νέμοις τοῖς πίστει μεταστᾶσι, τὴν φαιδρότητα τῆς θείας Βασιλείας, ἀφθαρσίας στολήν, δωρούμενος βοῶσιν· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C23.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C23.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
ocMA.OdeW7C23.text = "Πλῆσον χαρᾶς καὶ εὐφροσύνης, οὓς μετέστησας οἰκέτας σου Οἰκτίρμον, ἀξιώσας αὐτούς, κραυγάζειν σοι καὶ μέλπειν· Εὐλογητὸς εἶ Κύριε, ὁ Θεὸς εἰς τοὺς αἰῶνας."

ocMA.OdeW7C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW7C2T.ode = misc_gr_GR_cog.Ode7
ocMA.OdeW7C2T.melody = he.h.m8_gr_GR_cog.PaidesEvraion.name
ocMA.OdeW7C2T.text = "Λύων τῆς Εὔας τὴν κατάραν, τὴν πανάμωμον κατῴκησας Παρθένον, εὐλογίας πηγήν, βλυστάνων τοῖς βοῶσιν· Εὐλογημένος Πάναγνε, ὁ καρπὸς τῆς σῆς κοιλίας."

ocMA.OdeW8C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C1H.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW8C1H.text = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.text

ocMA.OdeW8C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C11.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C11.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
ocMA.OdeW8C11.text = "Ναοὺς εἰδώλων καθείλετε ἀθλοῦντες, καὶ ἑαυτοὺς τῆς Τριάδος, θείους ναούς, ἐδομήσασθε, Ἀθλοφόροι Κυρίου, Ἀγγέλων συνόμιλοι."

ocMA.OdeW8C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C12.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C12.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
ocMA.OdeW8C12.text = "Οἱ Ἱερεῖς σου Χριστὲ δικαιοσύνην, ἐνδεδυμένοι Ὁσίοις, τοῖς ἀπ' αἰῶνος νῦν συναγάλλονται, σοῦ τὸ κάλλος ὁρῶντες, τὸ θεῖον τρανότερον."

ocMA.OdeW8C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C13.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C13.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
ocMA.OdeW8C13.text = "Ἱερωτάτων πρεσβείαις Προφητῶν σου, καὶ Γυναικῶν ἀοιδίμων, τῶν ἀπ' αἰῶνος Δικαίων τε, τὰ ἐλέη σου Λόγε, τῷ κόσμῳ σου δώρησαι."

ocMA.OdeW8C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C14.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C14.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
ocMA.OdeW8C14.text = "Δικαιοκρῖτα ἡνίκα μέλλεις κρῖναι, οὓς ἐξ ἡμῶν προσελάβου, ἀκατακρίτους συντήρησον, παραβλέψας τὰ τούτων, ἐγκλήματα Δέσποτα."

ocMA.OdeW8C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C1T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C1T.melody = he.h.m8_gr_GR_cog.OiTheorrimonesPaides.name
ocMA.OdeW8C1T.text = "Ἐπιφανεῖσα Παρθένε, τῆς ψυχῆς μου, τὰ σκοτεινότατα νέφη, τῷ φωτισμῷ σου ἀπέλασον, ἡ τῆς δικαιοσύνης, τεκοῦσα τὸν Ἥλιον."

ocMA.OdeW8C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C21.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C21.melody = he.h.m8_gr_GR_cog.MousikonOrganon.name
ocMA.OdeW8C21.text = "Ἐπιγείους ἄθλους διελθόντες, οὐρανίους ἔλαβον στεφάνους, οἱ Μάρτυρες οἱ ἀληθεῖς, βοῶντές σοι ἀπαύστως· Τὸν Κύριον ὑμνεῖτε, καὶ ὑπερυψοῦτε, εἰς πάντας τούς αἰῶνας."

ocMA.OdeW8C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C22.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C22.melody = he.h.m8_gr_GR_cog.MousikonOrganon.name
ocMA.OdeW8C22.text = "Καταβὰς ἐν λάκκῳ κατωτάτῳ, τοὺς ἐν τοῖς μνημείοις κατοικοῦντας, ἀνέστησας ζωαρχικῇ, παλάμῃ, καὶ παρέσχες, τοῖς δούλοις σου τοῖς πίστει, προκεκοιμημένοις, ἀνάπαυσιν Οἰκτίρμον."

ocMA.OdeW8C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C23.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C23.melody = he.h.m8_gr_GR_cog.MousikonOrganon.name
ocMA.OdeW8C23.text = "Ὡς πηγὴ ζωῆς τῆς ἀϊδίου, ὡς τρυφῆς ὑπάρχων ὁ χειμάρρους, τοὺς δούλους σου τοὺς εὐσεβῶς, πρὸς σὲ μετατεθέντας, ἀξίωσον ὑμνεῖν σε, καὶ δοξολογεῖν σε, εἰς πάντας τοὺς αἰῶνας."

ocMA.OdeW8C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW8C2T.ode = misc_gr_GR_cog.Ode8
ocMA.OdeW8C2T.melody = he.h.m8_gr_GR_cog.MousikonOrganon.name
ocMA.OdeW8C2T.text = "Μαρία Παρθένε Θεοτόκε, ἡ Θεὸν Σωτῆρα τοῖς ἀνθρώποις, γεννήσασα σωματικῶς, περίσῳζε τοὺς πίστει, ὑμνοῦντάς σου τὸν τόκον, καὶ ὑπερυψοῦντας, εἰς πάντας τούς αἰῶνας."

ocMA.OdeW9C1H.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C1H.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1H.melody = misc_gr_GR_cog.Heirmos
ocMA.OdeW9C1H.text = he.h.m8_gr_GR_cog.SeTinApeirogamon.text

ocMA.OdeW9C11.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C11.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C11.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
ocMA.OdeW9C11.text = "Ἵσταντο οἱ Μάρτυρες, πρὸ τῶν ἀδίκων κριτηρίων, πᾶσαν ἀδικίαν χάριτι, τοῦ Χριστοῦ κατακρίνοντες, καὶ τοὺς ὑπ' αὐτῶν κεκρατημένους ἐλέγχοντες, καὶ στεφάνους, δικαιοσύνης κομιζόμενοι."

ocMA.OdeW9C12.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C12.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C12.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
ocMA.OdeW9C12.text = "Ὤφθητε ὡς οἴακες, τῆς Ἐκκλησίας τὴν ὁλκάδα, πᾶσαν εὐσεβῶς ἰθύναντες, εἰς λιμένα Θεοῦ ἐντολῶν, πάντες Ἱεράρχαι καὶ Ποιμένες μακάριοι· διὰ τοῦτο, ὡς κυβερνήτας ὑμᾶς σέβομεν."

ocMA.OdeW9C13.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C13.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C13.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
ocMA.OdeW9C13.text = "Στῖφος ἱκετεύει σε, τῶν Προφητῶν καὶ τῶν Ὁσίων, σύλλογος Δικαίων Κύριε, ἀθλησάντων λαμπρότητι, καὶ διαπρεψάντων ἐν ἀσκήσει φιλάνθρωπε, δυσωπεῖ σε, τούς οἰκτιρμούς σου ἡμῖν δώρησαι."

ocMA.OdeW9C14.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C14.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C14.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
ocMA.OdeW9C14.text = "Ἧς κατηξιώθησαν, τιμῆς καὶ δόξης αἰωνίου, ἅπαντες Χριστοῦ οἱ Ἅγιοι, τοὺς πιστῶς ἐκδημήσαντας, τοῦ ματαίου βίου, μετασχεῖν καταξίωσον, Ἐλεῆμον, ταῖς ἱκεσίαις τῶν Ἁγίων σου."

ocMA.OdeW9C1T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C1T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C1T.melody = he.h.m8_gr_GR_cog.SeTinApeirogamon.name
ocMA.OdeW9C1T.text = "Φέρουσα τὸ πλήρωμα, τῶν ἀγαθῶν ἐν ταῖς ἀγκάλαις, πλήρωσον ἡμῶν Πανάμωμε, τὰς ἐντεύξεις τῶν δούλων σου, καὶ τὰς πρὸς Θεὸν ἡμῶν πορείας, κατεύθυνον, ἐναρέτως, ἐνδυναμοῦσα πολιτεύεσθαι."

ocMA.OdeW9C21.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C21.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C21.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.name
ocMA.OdeW9C21.text = "Ἔχοντες Μάρτυρες Χριστοῦ, τὴν ἀνίκητον ἰσχὺν καὶ ἀήττητον, ἐξεφαυλίσατε, τὸ τῶν τυράννων, ἄθεον πρόσταγμα, καὶ Βασιλείας οὐρανῶν, σαφῶς ἠξιώθητε, καταλαμπόμενοι, τῆς Τριάδος ταῖς αὐγαῖς ἀξιάγαστοι."

ocMA.OdeW9C22.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C22.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C22.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.name
ocMA.OdeW9C22.text = "Λέλυται ᾅδης ὁ πικρός, καταλύσαντος αὐτὸν σοῦ Φιλάνθρωπε, καὶ ἀναστήσαντος, τοὺς ἀπ' αἰῶνος ἐκεῖ καθεύδοντας, ἀλλὰ καὶ νῦν ὡς ἀγαθός, τοὺς μεταχωρήσαντας, πρὸς σὲ τὸν εὔσπλαγχνον, ἀνεσπέρου σου φωτὸς καταξίωσον."

ocMA.OdeW9C23.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C23.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C23.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.name
ocMA.OdeW9C23.text = "Ὅλος εἶ Σῶτερ γλυκασμός, ὅλος εἶ ἐπιθυμία καὶ ἔφεσις, ὄντως ἀκόρεστος, ὅλος ὑπάρχεις κάλλος ἀμήχανον· τοὺς μεταστάντας οὖν πρὸς σέ, τῆς σῆς ὡραιότητος, τρυφᾷν εὐδόκησον, καὶ τῆς θείας καλλονῆς σου ἀξίωσον."

ocMA.OdeW9C2T.mode = misc_gr_GR_cog.Mode8
ocMA.OdeW9C2T.ode = misc_gr_GR_cog.Ode9
ocMA.OdeW9C2T.melody = he.h.m8_gr_GR_cog.EfrixePasaAkoi.name
ocMA.OdeW9C2T.text = "Σῶσόν με Μῆτερ τοῦ Θεοῦ, ἡ γεννήσασα Χριστὸν τὸν Σωτῆρά μου, Θεὸν καὶ ἄνθρωπον, διπλοῦν τὴν φύσιν, οὐ τὴν ὑπόστασιν, μονογενῆ μὲν ἐκ Πατρός, ἐκ σοῦ δὲ πρωτότοκον, πάσης τῆς κτίσεως, ὃν ἐν φύσεσι διτταῖς μεγαλύνομεν."

