A_Resource_Whose_Name = other.hymnbystnektarios_gr_GR_cog

other.title1 = "ΑΓΝΗ ΠΑΡΘΕΝΕ"
other.title2 = "Ἧμνος εἰς τὴν Ὑπεραγίαν Θεοτόκον"
other.title3 = "Ποίημα Ἁγίου Νεκταρίου Πενταπόλεως"

other.refrain.text = "Χαῖρε Νύμφη ἀνύμφευτε."

other.line11.text = "Ἁγνὴ Παρθένε Δέσποινα, ἄχραντε Θεοτόκε,"
other.line12.text = "Παρθένε Μῆτερ Ἄνασα, πανένδροσε τε πόκε,"
other.line13.text = "Ὑψηλοτέρα οὐρανῶν, ἀκτίνων λαμπροτέρα,"
other.line14.text = "Χαρὰ παρθενικῶν χορῶν, ἀγγέλων ὑπερτέρα,"
other.line15.text = "Ἐκλαμπροτέρα οὐρανῶν, φωτὸς καθαρωτέρα,"
other.line16.text = "Τῶν οὐρανίων στρατιῶν πασῶν ἁγιωτέρα."

other.line21.text = "Μαρία ἀειπάρθενε κόσμου παντὸς Κυρία,"
other.line22.text = "Ἄχραντε νύμφη πάναγνε, Δέσποινα Παναγία,"
other.line23.text = "Μαρία νύμφη ἄνασα χαρᾶς ἡμῶν αἰτία,"
other.line24.text = "Κόρη σεμνὴ βασίλισσα Μήτηρ ὑπεραγία,"
other.line25.text = "Τιμιωτέρα χερουβὶμ ὑπερενδοξοτέρα,"
other.line26.text = "Τῶν ἀσωμάτων σεραφὶμ τῶν θρόνων ὑπερτέρα."

other.line31.text = "Χαῖρε τὸ ᾄσμα χερουβίμ, χαῖρε ὕμνος ἀγγέλων."
other.line32.text = "Χαῖρε ὠδὴ τῶν σεραφίμ, χαρὰ τῶν ἀρχαγγέλων."
other.line33.text = "Χαῖρε εἰρήνη καὶ χαρά, λιμὴν τῆς σωτηρίας,"
other.line34.text = "Παστὰς τοῦ Λόγου ἱερα, ἄνθος τῆς ἀφθαρσίας."
other.line35.text = "Χαῖρε παράδεισε τρυφῆς, ζωῆς τε αἰωνίας."
other.line36.text = "Χαῖρε τὸ ξύλον τῆς ζωῆς, πηγὴ ἀθανασίας."

other.line41.text = "Σὲ ἱκετεύω Δέσποινα, σὲ νῦν ἐπικαλοῦμαι."
other.line42.text = "Σὲ δυσωπῶ Παντάνασα, σὴν χάριν ἐξαιτοῦμε."
other.line43.text = "Κόρη σεμνὴ καί ἄσπιλε, Δέσποινα Παναγία,"
other.line44.text = "Θερμῶς ἐπικαλοῦμαί σε, Ναὲ ἡγιασμένε."
other.line45.text = "Ἀντιλαβοῦ μου, ρῦσαί με ἀπὸ τοῦ πολεμίου,"
other.line46.text = "Καὶ κληρονόμον δεῖξόν με ζωῆς τῆς αἰωνίου."
