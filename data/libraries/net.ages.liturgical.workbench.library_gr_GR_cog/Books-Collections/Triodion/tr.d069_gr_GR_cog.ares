trMA.C1Poet = poets_gr_GR_cog.KosmaMonachou
trMA.C1Acrostic.text = "οὗ ἡ Ἀκροστιχίς· Προσάββατόν τε."

trMA.Ode5C1H.ode = misc_gr_GR_cog.Ode5
trMA.Ode5C1H.mode = misc_gr_GR_cog.Mode6
trMA.Ode5C1H.melody = misc_gr_GR_cog.Heirmos
