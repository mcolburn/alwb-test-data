trCommemoration.text = "ΚΥΡΙΑΚΗ Βʹ ΤΩΝ ΝΗΣΤΕΙΩΝ ΤΟΥ ΑΓΙΟΥ ΓΡΗΓΟΡΙΟΥ ΤΟΥ ΠΑΛΑΜΑ ΑΡΧΙΕΠΙΣΚΟΠΟΥ ΘΕΣΣΑΛΟΝΙΚΗΣ" //full title of day
trShortCommemoration1.text = "Ἁγίου Γρηγορίου Παλαμᾶ"
trInsert1 = "τοῦ ἐν ἁγίοις Πατρὸς ἡμῶν Γρηγορίου Ἀρχιεπισκόπου Θεσσαλονίκνς τοῦ Παλαμᾶ," //for use in petitions and dismissal
trInsert2 = "οὗ καὶ τὴν μνήμην ἐπιτελοῦμεν·" //for use in petitions and dismissal
trNote.text = ""
trHO.day = ""
trHO.commemorations = ""
trHO.description1 = ""
trHO.description2 = ""
trHO.note1 = ""
