trCommemoration.text = "Μνήμην ἐπιτελοῦμεν πάντων τῶν ἀπ' αἰῶνος Κεκοιμημένων, Ὀρθοδόξων Χριστιανῶν, πατέρων, καὶ ἀδελφῶν ἡμῶν." //full title of day
trShortCommemoration1.text = "Ψυχοσάββατον"
trInsert1 = "" //for use in petitions and dismissal
trInsert2 = "" //for use in petitions and dismissal
trNote.text = ""
