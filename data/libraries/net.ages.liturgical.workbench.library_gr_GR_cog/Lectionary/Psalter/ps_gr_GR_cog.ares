psa105.v2.text = "Τίς λαλήσει τὰς δυναστείας τοῦ Κυρίου, ἀκουστὰς ποιήσει πάσας τὰς αἰνέσεις αὐτοῦ;"
psa105.v3.number = "(105:3)"
psa105.v3.text = "Μακάριοι οἱ φυλάσσοντες κρίσιν καὶ ποιοῦντες δικαιοσύνην ἐν παντὶ καιρῷ."
psa105.v4.number = "(105:4)"
psa105.v4.text = "Μνήσθητι ἡμῶν, Κύριε, ἐν τῇ εὐδοκίᾳ τοῦ λαοῦ σου, ἐπίσκεψαι ἡμᾶς ἐν τῷ σωτηρίῳ σου"
psa105.v5.number = "(105:5)"
psa105.v5.text = "Τοῦ ἰδεῖν ἐν τῇ χρηστότητι τῶν ἐκλεκτῶν σου, τοῦ εὐφρανθῆναι ἐν τῇ εὐφροσύνῃ τοῦ ἔθνους σου, τοῦ ἐπαινεῖσθαι μετὰ τῆς κληρονομίας σου."
psa105.v6.number = "(105:6)"
psa105.v6.text = "Ἡμάρτομεν μετὰ τῶν πατέρων ἡμῶν, ἠνομήσαμεν, ἠδικήσαμεν."
psa105.v7.number = "(105:7)"
psa105.v7.text = "Οἱ πατέρες ἡμῶν ἐν Αἰγύπτῳ οὐ συνῆκαν τὰ θαυμάσιά σου καὶ οὐκ ἐμνήσθησαν τοῦ πλήθους τοῦ ἐλέους σου καὶ παρεπίκραναν ἀναβαίνοντες ἐν τῇ ἐρυθρᾷ θαλάσσῃ."
psa105.v8.number = "(105:8)"
psa105.v8.text = "Καὶ ἔσωσεν αὐτοὺς ἕνεκεν τοῦ ὀνόματος αὐτοῦ τοῦ γνωρίσαι τὴν δυναστείαν αὐτοῦ·"
psa105.v9.number = "(105:9)"
psa105.v9.text = "Καὶ ἐπετίμησε τῇ ἐρυθρᾷ θαλάσσῃ, καὶ ἐξηράνθη, καὶ ὡδήγησεν αὐτοὺς ἐν ἀβύσσῳ ὡς ἐν ἐρήμῳ·"
psa105.v10.number = "(105:10)"
psa105.v10.text = "Καὶ ἔσωσεν αὐτοὺς ἐκ χειρὸς μισοῦντος καὶ ἐλυτρώσατο αὐτοὺς ἐκ χειρὸς ἐχθρῶν·"
psa105.v11.number = "(105:11)"
psa105.v11.text = "Ἐκάλυψεν ὕδωρ τοὺς θλίβοντας αὐτούς, εἷς ἐξ αὐτῶν οὐχ ὑπελείφθη."
psa105.v12.number = "(105:12)"
psa105.v12.text = "Καὶ ἐπίστευσαν τοῖς λόγοις αὐτοῦ καὶ ᾖσαν τὴν αἴνεσιν αὐτοῦ."
psa105.v13.number = "(105:13)"
psa105.v13.text = "Ἐτάχυναν, ἐπελάθοντο τῶν ἔργων αὐτοῦ, οὐχ ὑπέμειναν τὴν βουλὴν αὐτοῦ·"
psa105.v14.number = "(105:14)"
psa105.v14.text = "Καὶ ἐπεθύμησαν ἐπιθυμίαν ἐν τῇ ἐρήμῳ καὶ ἐπείρασαν τὸν Θεὸν ἐν ἀνύδρῳ."
psa105.v15.number = "(105:15)"
psa105.v15.text = "Καὶ ἔδωκεν αὐτοῖς τὸ αἴτημα αὐτῶν, καὶ ἐξαπέστειλε πλησμονὴν εἰς τὰς ψυχὰς αὐτῶν."
psa105.v16.number = "(105:16)"
psa105.v16.text = "Καὶ παρώργισαν Μωυσῆν ἐν τῇ παρεμβολῇ, τὸν Ἀαρὼν τὸν ἅγιον Κυρίου·"
psa105.v17.number = "(105:17)"
psa105.v17.text = "Ἠνοίχθη ἡ γῆ καὶ κατέπιε Δαθὰν καὶ ἐκάλυψεν ἐπὶ τὴν συναγωγὴν Ἀβειρών·"
psa105.v18.number = "(105:18)"
psa105.v18.text = "Καὶ ἐξεκαύθη πῦρ ἐν τῇ συναγωγῇ αὐτῶν, φλὸξ κατέφλεξεν ἁμαρτωλούς."
psa105.v19.number = "(105:19)"
psa105.v19.text = "Καὶ ἐποίησαν μόσχον ἐν Χωρὴβ καὶ προσεκύνησαν τῷ γλυπτῷ."
psa105.v20.number = "(105:20)"
psa105.v20.text = "Καὶ ἠλλάξαντο τὴν δόξαν αὐτῶν ἐν ὁμοιώματι μόσχου ἐσθίοντος χόρτον."
psa105.v21.number = "(105:21)"
psa105.v21.text = "Καὶ ἐπελάθοντο τοῦ Θεοῦ τοῦ σῴζοντος αὐτούς, τοῦ ποιήσαντος μεγάλα ἐν Αἰγύπτῳ,"
psa105.v22.number = "(105:22)"
psa105.v22.text = "Θαυμαστὰ ἐν γῇ Χάμ, φοβερὰ ἐπὶ θαλάσσης ἐρυθρᾶς."
psa105.v23.number = "(105:23)"
psa105.v23.text = "Καὶ εἶπε τοῦ ἐξολοθρεῦσαι αὐτούς, εἰ μὴ Μωυσῆς ὁ ἐκλεκτὸς αὐτοῦ ἔστη ἐν τῇ θραύσει ἐνώπιον αὐτοῦ τοῦ ἀποστρέψαι τὸν θυμὸν αὐτοῦ τοῦ μὴ ἐξολοθρεῦσαι αὐτούς."
psa105.v24.number = "(105:24)"
psa105.v24.text = "Καὶ ἐξουδένωσαν γῆν ἐπιθυμητήν, οὐκ ἐπίστευσαν τῷ λόγῳ αὐτοῦ·"
psa105.v25.number = "(105:25)"
psa105.v25.text = "καὶ ἐγόγγυσαν ἐν τοῖς σκηνώμασιν αὐτῶν, οὐκ εἰσήκουσαν τῆς φωνῆς Κυρίου."
psa105.v26.number = "(105:26)"
psa105.v26.text = "Καὶ ἐπῆρε τὴν χεῖρα αὐτοῦ ἐπ' αὐτοὺς τοῦ καταβαλεῖν αὐτοὺς ἐν τῇ ἐρήμῳ"
psa105.v27.number = "(105:27)"
psa105.v27.text = "Καὶ τοῦ καταβαλεῖν τὸ σπέρμα αὐτῶν ἐν τοῖς ἔθνεσι καὶ διασκορπίσαι αὐτοὺς ἐν ταῖς χώραις."
psa105.v28.number = "(105:28)"
psa105.v28.text = "Καὶ ἐτελέσθησαν τῷ Βεελφεγὼρ καὶ ἔφαγον θυσίας νεκρῶν·"
psa105.v29.number = "(105:29)"
psa105.v29.text = "Καὶ παρώξυναν αὐτὸν ἐν τοῖς ἐπιτηδεύμασιν αὐτῶν, καὶ ἐπληθύνθη ἐν αὐτοῖς ἡ πτῶσις."
psa105.v30.number = "(105:30)"
psa105.v30.text = "Καὶ ἔστη Φινεὲς καὶ ἐξιλάσατο, καὶ ἐκόπασεν ἡ θραῦσις·"
psa105.v31.number = "(105:31)"
psa105.v31.text = "Καὶ ἐλογίσθη αὐτῷ εἰς δικαιοσύνην εἰς γενεὰν καὶ γενεὰν ἕως τοῦ αἰῶνος."
psa105.v32.number = "(105:32)"
psa105.v32.text = "Καὶ παρώργισαν αὐτὸν ἐπὶ ὕδατος ἀντιλογίας καὶ ἐκακώθη Μωυσῆς δι' αὐτούς,"
psa105.v33.number = "(105:33)"
psa105.v33.text = "Ὅτι παρεπίκραναν τὸ πνεῦμα αὐτοῦ, καὶ διέστειλεν ἐν τοῖς χείλεσιν αὐτοῦ."
psa105.v34.number = "(105:34)"
psa105.v34.text = "Οὐκ ἐξωλόθρευσαν τὰ ἔθνη, ἃ εἶπε Κύριος αὐτοῖς,"
psa105.v35.number = "(105:35)"
psa105.v35.text = "Καὶ ἐμίγησαν ἐν τοῖς ἔθνεσι καὶ ἔμαθον τὰ ἔργα αὐτῶν·"
psa105.v36.number = "(105:36)"
psa105.v36.text = "Καὶ ἐδούλευσαν τοῖς γλυπτοῖς αὐτῶν, καὶ ἐγενήθη αὐτοῖς εἰς σκάνδαλον·"
psa105.v37.number = "(105:37)"
psa105.v37.text = "Καὶ ἔθυσαν τοὺς υἱοὺς αὐτῶν καὶ τὰς θυγατέρας αὐτῶν τοῖς δαιμονίοις"
psa105.v38.number = "(105:38)"
psa105.v38.text = "Καὶ ἐξέχεαν αἷμα ἀθῷον, αἷμα υἱῶν αὐτῶν καὶ θυγατέρων, ὧν ἔθυσαν τοῖς γλυπτοῖς Χαναὰν καὶ ἐφονοκτονήθη ἡ γῆ ἐν τοῖς αἵμασι"
psa105.v39.number = "(105:39)"
psa105.v39.text = "Καὶ ἐμιάνθη ἐν τοῖς ἔργοις αὐτῶν, καὶ ἐπόρνευσαν ἐν τοῖς ἐπιτηδεύμασιν αὐτῶν."
psa105.v40.number = "(105:40)"
psa105.v40.text = "Καὶ ὠργίσθη θυμῷ Κύριος ἐπὶ τὸν λαὸν αὐτοῦ καὶ ἐβδελύξατο τὴν κληρονομίαν αὐτοῦ·"
psa105.v41.number = "(105:41)"
psa105.v41.text = "Καὶ παρέδωκεν αὐτοὺς εἰς χεῖρας ἐχθρῶν, καὶ ἐκυρίευσαν αὐτῶν οἱ μισοῦντες αὐτούς."
psa105.v42.number = "(105:42)"
psa105.v42.text = "Καὶ ἔθλιψαν αὐτοὺς οἱ ἐχθροὶ αὐτῶν, καὶ ἐταπεινώθησαν ὑπὸ τὰς χεῖρας αὐτῶν."
psa105.v43.number = "(105:43)"
psa105.v43.text = "Πλεονάκις ἐρρύσατο αὐτούς, αὐτοὶ δὲ παρεπίκραναν αὐτὸν ἐν τῇ βουλῇ αὐτῶν καὶ ἐταπεινώθησαν ἐν ταῖς ἀνομίαις αὐτῶν."
psa105.v44.number = "(105:44)"
psa105.v44.text = "Καὶ εἶδε Κύριος ἐν τῷ θλίβεσθαι αὐτούς, ἐν τῷ αὐτὸν εἰσακοῦσαι τῆς δεήσεως αὐτῶν·"
psa105.v45.number = "(105:45)"
psa105.v45.text = "Καὶ ἐμνήσθη τῆς διαθήκης αὐτοῦ καὶ μετεμελήθη κατὰ τὸ πλῆθος τοῦ ἐλέους αὐτοῦ"
psa105.v46.number = "(105:46)"
psa105.v46.text = "καὶ ἔδωκεν αὐτοὺς εἰς οἰκτιρμοὺς ἐναντίον πάντων τῶν αἰχμαλωτευσάντων αὐτούς."
psa105.v47.number = "(105:47)"
psa105.v47.text = "Σῶσον ἡμᾶς, Κύριε ὁ Θεὸς ἡμῶν, καὶ ἐπισυνάγαγε ἡμᾶς ἐκ τῶν ἐθνῶν τοῦ ἐξομολογήσασθαι τῷ ὀνόματί σου τῷ ἁγίῳ, τοῦ ἐγκαυχᾶσθαι ἐν τῇ αἰνέσει σου."
psa105.v48.number = "(105:48)"
psa105.v48.text = "Εὐλογητὸς Κύριος ὁ Θεὸς Ἰσραὴλ ἀπὸ τοῦ αἰῶνος καὶ ἕως τοῦ αἰῶνος. καὶ ἐρεῖ πᾶς ὁ λαός· γένοιτο γένοιτο."

psa106.title = "Ψαλμὸς ΡϚʹ (106)."
