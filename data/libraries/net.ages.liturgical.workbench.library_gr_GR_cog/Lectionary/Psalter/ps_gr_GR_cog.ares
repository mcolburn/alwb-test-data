psa74.v5.number = "(74:5)"
psa74.v5.text = "Εἶπα τοῖς παρανομοῦσι· μὴ παρανομεῖτε, καὶ τοῖς ἁμαρτάνουσι· μὴ ὑψοῦτε κέρας,"
