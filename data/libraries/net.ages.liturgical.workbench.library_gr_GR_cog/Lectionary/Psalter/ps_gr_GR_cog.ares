psa146.v3.text = "Ὁ ἰώμενος τοὺς συντετριμμένους τὴν καρδίαν καὶ δεσμεύων τὰ συντρίμματα αὐτῶν,"
psa146.v4.number = "(146:4)"
psa146.v4.text = "Ὁ ἀριθμῶν πλήθη ἄστρων, καὶ πᾶσιν αὐτοῖς ὀνόματα καλῶν."
psa146.v5.number = "(146:5)"
psa146.v5.text = ""
psa146.v5a.number = "(146:5a)"
psa146.v5a.text = "Μέγας ὁ Κύριος ἡμῶν, καὶ μεγάλη ἡ ἰσχὺς αὐτοῦ."
