psa106.v18.text = "Πᾶν βρῶμα ἐβδελύξατο ἡ ψυχὴ αὐτῶν, καὶ ἤγγισαν ἕως τῶν πυλῶν τοῦ θανάτου·"
psa106.v19.number = "(106:19)"
psa106.v19.text = "Καὶ ἐκέκραξαν πρὸς Κύριον ἐν τῷ θλίβεσθαι αὐτούς, καὶ ἐκ τῶν ἀναγκῶν αὐτῶν ἔσωσεν αὐτούς,"
psa106.v20.number = "(106:20)"
psa106.v20.text = "Ἀπέστειλε τὸν λόγον αὐτοῦ καὶ ἰάσατο αὐτοὺς καὶ ἐρρύσατο αὐτοὺς ἐκ τῶν διαφθορῶν αὐτῶν."
psa106.v21.number = "(106:21)"
psa106.v21.text = "Ἐξομολογησάσθωσαν τῷ Κυρίῳ τὰ ἐλέη αὐτοῦ καὶ τὰ θαυμάσια αὐτοῦ τοῖς υἱοῖς τῶν ἀνθρώπων"
